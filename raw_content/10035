<!-- BEGIN CustomLink Omniture JS -->
<script type="text/javascript">
	//wrapper function that uses setTimeout in order to create a sep thread so processing the rest of the page isnt interrupted
	function omniture_tag(obj,item_type,img_file)
	{
		
		setTimeout("contact_omniture(true,'" + item_type + "','" + img_file + "')",0);
	}
	
	
	
	//main tagging function
	function contact_omniture(obj,item_type,img_file)
	{
		if (item_type == "addtl_thumb")
		{
			tag_addtl_thumb(obj,img_file);	
		}
		else if (item_type == "main_thumb")
		{
			tag_main_thumb(obj,img_file);
		}
		else if (item_type == "main_zoom")
		{
			tag_main_zoom(obj,img_file);	
		}
		else if (item_type == "addtl_zoom")
		{
			tag_addtl_zoom(obj,img_file);	
		}
		else if (item_type == "close_zoom")
		{
			tag_close_zoom(obj,img_file);
		}
		else if (item_type == "gallery_addtl")
		{
			tag_gallery_to_addtl(obj,img_file);
		}
		else if (item_type == "gallery_main")
		{
			tag_gallery_to_main(obj,img_file);
		}
		else if (item_type == "slider_nav")
		{
			//in this case, the second param is actually the navigation direction
			tag_slider_nav(obj,img_file);
		}
	}
	
	

	//common vars set for carousel/zoom gallery
	function common_tags(s)
	{
		//s.linkTrackVars='prop2,prop3,prop4,prop10,eVar21,eVar22,eVar23,eVar24,eVar25,events,products';  
		s.linkTrackEvents='event12,event13,event14';  
		s.eVar21=s.eVar25='+1';  
		s.products=';e71f' 
		return s;
	}

	//for selection of main thumbnail from carousel	
	function tag_main_thumb(obj,img_file)
	{
		var s=s_gi('thinkgeek');
		s=common_tags(s);
		s.linkTrackVars='prop2,prop3,prop4,prop10,eVar21,eVar22,eVar25,events,products';  
		s.prop2=s.prop3=s.prop4=s.prop10='Carousel Select Main';
		s.eVar22=s.prop10 + ' | Molecular Cuisine Starter Kit | e71f | ' + img_file;  
		s.events='event12';  
		s.tl(obj,'o',s.eVar22);
	}
	
	//for selection of additional image thumbnails from carousel
	function tag_addtl_thumb(obj,img_file)
	{
		var s=s_gi('thinkgeek'); 
		s=common_tags(s);
		s.linkTrackVars='prop2,prop3,prop4,prop10,eVar21,eVar22,eVar25,events,products';  
		s.prop2=s.prop3=s.prop4=s.prop10='Carousel Select Additional';  
		s.eVar22=s.prop10 + ' | Molecular Cuisine Starter Kit | e71f | ' + img_file;
		s.events='event12';  
		s.tl(obj,'o',s.eVar22);
	}

	
	//for FBox zooming of main image	
	function tag_main_zoom(obj,img_file)
	{
		var s=s_gi('thinkgeek');
		s=common_tags(s);
		s.linkTrackVars='prop2,prop3,prop4,prop10,eVar21,eVar23,eVar25,events,products';  
		s.prop2=s.prop3=s.prop4=s.prop10='Carousel Zoom Main';  
		s.eVar23=s.prop10 + ' | Molecular Cuisine Starter Kit | e71f | ' + img_file;  
		s.events='event13';  
		s.tl(obj,'o',s.eVar23);
	}
	
	//for FBox zooming of addtl large image	
	function tag_addtl_zoom(obj,img_file)
	{
		var s=s_gi('thinkgeek');
		s=common_tags(s);
		s.linkTrackVars='prop2,prop3,prop4,prop10,eVar21,eVar23,eVar25,events,products';  
		s.prop2=s.prop3=s.prop4=s.prop10='Carousel Zoom Additional';  
		s.eVar23=s.prop10 + ' | Molecular Cuisine Starter Kit | e71f | ' + img_file;  
		s.events='event13';  
		s.tl(obj,'o',s.eVar23);
	}


	//for FBox closing zoomed image
	function tag_close_zoom(obj,img_file)
	{
		var s=s_gi('thinkgeek');
		s=common_tags(s);
		s.linkTrackVars='prop2,prop3,prop4,prop10,eVar24,events,products';  
	
		s.prop2=s.prop3=s.prop4=s.prop10='Gallery Navigate Close';  
		s.eVar24=s.prop10 + ' | Molecular Cuisine Starter Kit | e71f | ' + img_file;
		
		s.events='event14';  
		s.tl(obj,'o',s.eVar24);
	}
	
	//for FBox Gallery navigation TO an additional image FROM a main/addtl gallery image
	function tag_gallery_to_addtl(obj,img_file)
	{
		var s=s_gi('thinkgeek');
		s=common_tags(s);
		s.linkTrackVars='prop2,prop3,prop4,prop10,eVar21,eVar24,eVar25,events,products';  
		s.prop2=s.prop3=s.prop4=s.prop10='Gallery Navigate Additional';  
		s.eVar24=s.prop10 + ' | Molecular Cuisine Starter Kit | e71f | ' + img_file;  
		s.events='event14';  
		s.tl(obj,'o',s.eVar24);
	
	}
	
	//for FBox Gallery navigation TO main image FROM an addtl gallery image
	function tag_gallery_to_main(obj,img_file)
	{
		var s=s_gi('thinkgeek');
		s=common_tags(s);
		s.linkTrackVars='prop2,prop3,prop4,prop10,eVar21,eVar24,eVar25,events,products';  
		s.prop2=s.prop3=s.prop4=s.prop10='Gallery Navigate Main';  
		s.eVar24=s.prop10 + ' | Molecular Cuisine Starter Kit | e71f | ' + img_file;  
		s.events='event14';  
		s.tl(obj,'o',s.eVar24);
	
	}
	
	
	//for jCarousel next/prev slider navigation
	function tag_slider_nav(obj,direction)
	{
		var s=s_gi('thinkgeek');
		s=common_tags(s);
		s.linkTrackVars='prop2,prop3,prop4,prop10,eVar22,events,products';  
		s.prop2=s.prop3=s.prop4=s.prop10='Carousel Navigate Thumbnail';  
		s.eVar22=s.prop10 + ' | Molecular Cuisine Starter Kit | e71f | ' + direction;  
		s.events='event12';  
		s.tl(obj,'o',s.eVar22);
	
	
	}
	

	//for debugging
	function listProps(obj)
	{
		var names = "";
		for(var i in obj) names += i + " = " + obj[i] + "<BR>\n";
		document.write("Properties:<BR>" + names);
	}
	
	
</script>
<!-- END CustomLink Omniture JS -->



