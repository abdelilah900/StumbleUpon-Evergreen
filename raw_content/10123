function removeBasket(id,subprice,cid){
	//alert(id+":"+subprice+":"+cid);
	var url='http://www.insidershealth.com/marketplace/remove/';
	//var subprice = parseFloat(subprice);
	var url = url+id+"/"+cid;
	$.post(url,function(res){
		//alert(res);return;
		if(res == 'empty'){
			$("#mybasket").html("<h3>Your Basket</h3><div class='prdName'><p class='prdz'>Currently, you have no products<br />in your shopping cart.</span></p></div><div class='arrowDown'></div>");
			return;
		}
		$("#divbasket"+id).hide();
	});
}
</script>
