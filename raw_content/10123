<script type="text/javascript">
	$(document).ready(function() {
		/*$("#various2").fancybox({
			'modal' : true
		});*/
 		
		$('#setFilter').change(function(){
			var categoryId = $('#setFilter').attr('value');
			$("div .downloadCategories").each(function(){
				if(!categoryId){
					this.style.display = 'block';
				}else{
					if(this.id=='category'+categoryId){
						this.style.display = 'block';
					}else{
						this.style.display = 'none';
					}
				}
			});
		});
		
		$('#previewItem').click(function(){
		});
		
		$('.addCart76').click(function(){  var msg = $('#myloading').html();
		$.fancybox(msg,{'padding': 0,'modal':true,'width':'600px','height':'50px'});$('#cartBox').load('http://www.insidershealth.com/marketplace/add/76',function(){ $('html, body').animate({scrollTop: '0px'}, 800); $.fancybox.close();}); return false;});		
		$('#id_review').click( function(){
			$("#add_review_id").toggle('slow');
		});
		
		
	
	});
	jQuery(document).ready(function() {
    jQuery('#mycarousel').jcarousel({
        vertical: true,
		auto: 7,
		wrap: 'circular',
        scroll: 2
    	});
	});
	
	
function show_review(pid,act,chk){
		var url="/marketplace/"+act+"/"+pid;
		
		if(chk==1){
			window.location.href=url;
		}else{
			$.fancybox.showActivity();
			$.post(url,{pid:pid},function(response){
				$.fancybox(response,{hideOnOverlayClick:false});
			});
		}
		
	}
</script>			<div class="clearer"></div>
