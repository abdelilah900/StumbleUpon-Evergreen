      if (!window.registeredBloggerCallbacks) {
        window.registeredBloggerCallbacks = true;

        

        
        gadgets.rpc.register('requestReload', function() {
          document.location.reload();
        });

        
        gadgets.rpc.register('requestSignOut', function(siteId) {
          
          google.friendconnect.container.openSocialSiteId = siteId;
          google.friendconnect.requestSignOut();
        });
      }
    </script>
<script type="text/javascript">
    
    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
            
            
              holder.currentPost = "http://www.blogger.com/feeds/5711314660374261745/posts/default/1348615403028059438";
            
            
            
              holder.currentComments = "http://www.blogger.com/feeds/5711314660374261745/1348615403028059438/comments/default";
            
            holder.currentPostUrl = "";
            holder.currentPostId = 1348615403028059438
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/5711314660374261745/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/5711314660374261745/comments/default";
          
          holder.currentBlogUrl = "http://www.unitedstatesofmotherhood.com/";
          holder.currentBlogId = "5711314660374261745";
        
        return holder;
      });
    }
  </script>
