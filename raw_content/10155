    var deferredAdCallback = function(DB_params) {
        if (null != PCWAds.DisplayAdManager.getDeferredAds() && PCWAds.DisplayAdManager.getDeferredAds().length > 0) {
    		var adParams = new Object();
    		if (DB_params && DB_params.forbes_2000 == true) {
    			adParams.f2k=1;
    		}
        	for (var i=0; i<PCWAds.DisplayAdManager.getDeferredAds().length; i++) {
        		var url = PCWAds.DisplayAdManager.getDeferredAds()[i].url;
        		var adId = PCWAds.DisplayAdManager.getDeferredAds()[i].adId;
        		PCWAds.DisplayAdManager.callDeferredAd(adId,url,adParams);
        	}
        }
    };
  </script>

