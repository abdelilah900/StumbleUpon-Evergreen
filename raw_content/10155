SocialBadgePath = {};
SocialBadgePath.facebook20 = "http://zapp3.staticworld.net/images/common/social_badge/facebook_20.png";
SocialBadgePath.twitter20 = "http://zapp2.staticworld.net/images/common/social_badge/twitter_20.png";
