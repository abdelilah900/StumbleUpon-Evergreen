<script type="text/javascript">
    CN.config.set({
        hasIntro   : false,
        playTimer  : 5,
        transition : 'standard',
        contentId  : 2000000001215853,
        viewAll    : false,
        carousel   : false,
        imagelist  : false,
        viewLarger : false
    });

            // Adds classes for rendering based on intro and hash state
    (function() {
        var fragment = CN.url.getFragment(),
            slide    = CN.url.params().slide;

                // For page rendering purposes, we have a visible intro if we hit the page without a fragment id
        if (CN.page.config.hasIntro === true && ((fragment === false || fragment === 'intro') && !slide)) {
            jQuery('html').addClass('slideshow-hasintro-true');
        }

                // If DCT turns off intro page, we show intro if it exists
        if (CN.page.config.hasIntro === false) {
            jQuery('html').addClass('slideshow-showintro-true');
        }

                // Showing the initial JSP-provided slide depends on whether the URL agrees with the JSP provided slide
        if (fragment === false || fragment === 'slide=1' || fragment === 'intro' || (fragment === false && slide)) {
            jQuery('html').addClass('slideshow-showinitial-true');
        } else {
            jQuery('html').addClass('slideshow-showinitial-false');
        }
    })();
</script>



    


        

     
        
     
    
    
    








 


        
                
            
    



        
    







<script type="text/javascript">

if (typeof pluckHandler === 'undefined') {

    var genericPluckData = {
      ID        :'',
      programUrl:'',
      serverUrl :"http://sitelife.gq.com/ver1.0/Direct/Process",
      articleID :'',
      userID    :'pluckUser.u'
    };

    var pluckHandler = new Pluck();

    if (typeof (Rating) != 'undefined') {
        var newRating = new Rating();
        Rating.init(5);
    }

    var gateway = new RegisteredGateway("/user/login", "/user/registration");
    pluckHandler.addGatewayListener(gateway);
    pluckHandler.crTitle = pluckHandler.crTitle || {}; // for legacy pluck.js
    pluckHandler.crType = pluckHandler.crType || {}; // for legacy pluck.js
    pluckHandler.init(genericPluckData.serverUrl, 10, "/user/registration");

    Rating.addSubmitListener(pluckHandler);

    jQuery(function() {
        loadPluck();
    });
}

function loadPluck() {
    gateway.init();
    
            if (pluckArticleIds.length > 0 && pluckArticleIds[0] !== '') {
                for (var i = 0; i < pluckArticleIds.length; i++) {
                    pluckHandler.initCommentForm('frmSubmitComment_' + pluckArticleIds[i], pluckArticleIds[i], 'comments_' + pluckArticleIds[i], 'comment');
                }
                pluckHandler.doMultArticleRequest(pluckArticleIds, pluckHandler.ReviewPageHandler);
            }
        
}
</script>


    



    

    

    





