<script type="text/javascript">

if (typeof pluckHandler === 'undefined') {

    var genericPluckData = {
      ID        :'',
      programUrl:'',
      serverUrl :"http://sitelife.gq.com/ver1.0/Direct/Process",
      articleID :'',
      userID    :'pluckUser.u'
    };

    var pluckHandler = new Pluck();

    if (typeof (Rating) != 'undefined') {
        var newRating = new Rating();
        Rating.init(5);
    }

    var gateway = new RegisteredGateway("/user/login", "/user/registration");
    pluckHandler.addGatewayListener(gateway);
    pluckHandler.crTitle = pluckHandler.crTitle || {}; // for legacy pluck.js
    pluckHandler.crType = pluckHandler.crType || {}; // for legacy pluck.js
    pluckHandler.init(genericPluckData.serverUrl, 10, "/user/registration");

    Rating.addSubmitListener(pluckHandler);

    jQuery(function() {
        loadPluck();
    });
}

function loadPluck() {
    gateway.init();
    
            if (pluckArticleIds.length > 0 && pluckArticleIds[0] !== '') {
                for (var i = 0; i < pluckArticleIds.length; i++) {
                    pluckHandler.initCommentForm('frmSubmitComment_' + pluckArticleIds[i], pluckArticleIds[i], 'comments_' + pluckArticleIds[i], 'comment');
                }
                pluckHandler.doMultArticleRequest(pluckArticleIds, pluckHandler.ReviewPageHandler);
            }
        
}
</script>




























