// temp. fix, see GQ-382
(function(){
   var resizer=function(e,key,sel,ad){
       ad.el.height('').height(ad.el.height()).css({overflow:'visible'});
       //alert([e,key,sel,ad])
   };

jQuery(window).bind('CN.customEvents.dartAdDrawn.header728x90', function(e,key,sel,ad) {
     CN.debug.info("Frame super resize",[key,sel,ad]);
     ad.frame.add(ad.frame[0].contentWindow).bind('resize',function(e){
         resizer(e,key,sel,ad)
     });//.trigger('resize')
});

jQuery(window).bind('CN.customEvents.dartAdDrawn.sponsor120x60', function(e,key,sel,ad) {
     CN.debug.info("Frame super resize",[key,sel,ad]);
    ad.frame.bind('load resize',function(e){
         resizer(e,key,sel,ad);
     })//.trigger('resize')
});
 
})()
</script>
        
    


    







    
    
    
    
    
    
    
    

    
