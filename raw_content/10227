            if (document.all) {
                if (event.keyCode == 13) // FOR  IE
                {
                    document.getElementById('imgSearchByZip').focus();
                    document.getElementById('imgSearchByZip').click();
                    return false;
                }
            }
            else {
                if (e.which == 13) // FOR MOZILAA
                {
                    document.getElementById('imgSearchByZip').focus();
                    document.getElementById('imgSearchByZip').click();
                    return false;
                }
            }
        }

