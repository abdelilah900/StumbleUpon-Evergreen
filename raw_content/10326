<script type="text/javascript">
//<![CDATA[
function myplugin_load_comments_div(results_div,query_vars)
{

   var mysack = new sack( 
       "http://samsonblinded.org/blog/wp-content/themes/samson/tiga_ajax.php" );    

  mysack.execute = 1;
  mysack.method = 'POST';
  mysack.setVar( "comments_div_id", results_div );
  for (key in query_vars)
  {
	mysack.setVar( key, query_vars[key] );
  }  
  
  mysack.onError = function() { alert('Ajax error in load left sidebar_links' )};
  mysack.onCompletion = function() {
    quicktagsL10n = {
        quickLinks: "(Quick Links)",
        wordLookup: "Enter a word to look up:",
        dictionaryLookup: "Dictionary lookup",
        lookup: "lookup",
        closeAllOpenTags: "Close all open tags",
        closeTags: "close tags",
        enterURL: "Enter the URL",
        enterImageURL: "Enter the URL of the image",
        enterImageDescription: "Enter a description of the image"
    }
    var cont = '<div id="ed_toolbar">';
    for (var i = 0; i < edButtons.length; i++) {
        var button = edButtons[i];
        if (button.id == 'ed_img') {
            cont += '<input type="button" id="' + button.id + '" accesskey="' + button.access + '" class="ed_button" onclick="edInsertImage(edCanvas);" value="' + button.display + '" />';
        }
        else if (button.id == 'ed_link') {
            cont += '<input type="button" id="' + button.id + '" accesskey="' + button.access + '" class="ed_button" onclick="edInsertLink(edCanvas, ' + i + ');" value="' + button.display + '" />';
        }
        else {
            cont += '<input type="button" id="' + button.id + '" accesskey="' + button.access + '" class="ed_button" onclick="edInsertTag(edCanvas, ' + i + ');" value="' + button.display + '"  />';
        }
        //edShowButton(edButtons[i], i);
    }
    cont += '<input type="button" id="ed_spell" class="ed_button" onclick="edSpell(edCanvas);" title="' + quicktagsL10n.dictionaryLookup + '" value="' + quicktagsL10n.lookup + '" />';
    cont += '<input type="button" id="ed_close" class="ed_button" onclick="edCloseAllTags();" title="' + quicktagsL10n.closeAllOpenTags + '" value="' + quicktagsL10n.closeTags + '" />';
    cont += '</div>';
    if (jQuery('#comment').length) jQuery('#comment').before(cont);
    edCanvas = document.getElementById('comment');
  };
  mysack.runAJAX();

  return true;

} // end of JavaScript function myplugin_cast_vote

//]]>
</script>
