//<![CDATA[
function myplugin_load_main_posts(results_div,query_vars)
{
  if (document.getElementById(results_div)) {
      var mysack = new sack("http://samsonblinded.org/blog/wp-content/themes/samson/tiga_ajax.php" );
      mysack.execute = 1;
      mysack.method = 'POST';
      mysack.setVar( "main_posts_div_id", results_div );
      for (key in query_vars)
      {
        mysack.setVar( key, query_vars[key] );
      }
      mysack.onError = function() { alert('Ajax error in load main posts' )};
      mysack.runAJAX();
  }
  return true;
} // end of JavaScript function myplugin_cast_vote

//]]>
</script>
