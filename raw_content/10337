	<div id="advertiser-container">
						    

	
		<script src="http://www.google.com/uds/jsapi" type="text/javascript"></script>
		<script type="text/javascript">
				var ad;
				google.load('ads', '1');
				google.setOnLoadCallback(init);
				google_ad_output = 'html';
				google_ad_format = '300x250';
				function init() {
					var options = {
						'format'   : google.ads.MEDIUM_RECTANGLE,
						'hints' : 'Sport',
						'ad_channel': 'channel',
						'color_bg': 'ffffff',
						'color_border': 'ffffff',
						'color_line': '0000ff',
						'color_text': '333333',
						'color_link': '005689',
						'color_url': '005689'
					};
					ad = new google.ads.Ad('ca-guardian_ajax', 'adblock', options);
				}
				function doAdUpdate(newHints) {
					ad.refresh({'hints' : newHints});
				}
		</script>
		
