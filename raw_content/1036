<script type="text/javascript" language="javascript">
	//<![CDATA[
	function checkEnterRecipeTopSearch(e)
	{
		var characterCode;
 
		if(e && e.which)
		{
			e = e
			characterCode = e.which;
		}
		else
		{
			e = event
			characterCode = e.keyCode;
		}
 
		if(characterCode == 13)
		{
			search(document.getElementById("recipeSearchTextBox").value, this, 'Header');
		}
	}
	
	function onClickRecipeTopSearch(e)
	{
		document.getElementById("recipeSearchTextBox").select();
	}
	//]]>
</script>
