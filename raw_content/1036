			//<![CDATA[
			function display_my_account_tab()
			{
				var myAccount = document.getElementById('MyAccountTab');
				myAccount.style.display = "block";
				var welcome = document.getElementById('WelcomeTab');
				welcome.style.display = "none";
			}

			function display_welcome_tab()
			{
				var welcome = document.getElementById('WelcomeTab');
				welcome.style.display = "block";
				var myAccount = document.getElementById('MyAccountTab');
				myAccount.style.display = "none";
			}

			function set_register_email_from_login_box(){
				var email = document.getElementById("join-email").value;
				call_register_lightbox_and_prepopulate_email(1,email);
			}

			function checkEnterLogin(e)
			{
				var characterCode;
				if(e && e.which)
				{
					e = e
					characterCode = e.which;
				}
				else
				{
					e = event
					characterCode = e.keyCode;
				}

				if(characterCode == 13)
				{
					set_register_email_from_login_box();
				}
			}
			//]]>
		</script>
			
