    Etsy.loader.require('etsy.facebook', function() {
        var log = function(button_name) {
            return function() {
                var loggerArgs = {
                    'php_event_name': 'share_facebook',
                    'source': 'treasury',
                    'button_name': button_name
                };
                if ("".length > 0){
                    loggerArgs['post_to_page'] = '';
                }
                Etsy.eventLogger.logEvent(loggerArgs);
            };
        };

        var etsyFb = Etsy.Facebook.getInstance();
        etsyFb.load(function(success){
                            etsyFb.eventSubscribe('edge.create', log('like'));
                etsyFb.eventSubscribe('message.send', log('send'));
            
            
                                            etsyFb.eventSubscribe('xfbml.render', function() {
                    var fbif = $('#share2-fb-like iframe.fb_ltr');
                    fbif.height(25);
                });
                    });

    });
</script></li>
