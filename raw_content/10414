<script type="text/javascript" src="/rd_scripts/article.js"></script>


<div id="article_nav">
    <ul>
        
            
                
                
                    <li class="off">
                        <a href="/articlesguides/howtocook/primers/bagels">intro</a>
                    </li>
                
            
        
            
                
                    <li class="current"><span>bagel myths</span><!-- Current Page --></li>
                
                
            
        
            
                
                
                    <li class="off">
                        <a href="/articlesguides/howtocook/primers/bagels-recipe">recipe &#38; how-to </a>
