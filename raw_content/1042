<script type="text/javascript">
//<![CDATA[
function watermarkExtender(txtBox, clear)
{
    var stored = txtBox.data('stored');
    if (clear)
    {
        if (txtBox.val() == stored.defaultText)
        {
            if(stored.defaultClass)
                txtBox[0].className=stored.defaultClass;
            txtBox.val('');
        }
    }
    else
    {
        if (txtBox.val() == '' || txtBox.val() == stored.defaultText)
        {
            if (stored.watermarkClass && stored.watermarkClass!='') 
                txtBox[0].className=stored.watermarkClass;
            txtBox.val(stored.defaultText);
        }
    }
}
function initWatermarkExtender(id, watermarkText, watermarkCssClass, bypass)
{
    var txtBox = $('#' + id);
    if (txtBox.length > 0) 
    {
        txtBox.data('stored', { 
            defaultClass:txtBox[0].className , watermarkClass:watermarkCssClass, defaultText:watermarkText 
        });
        watermarkExtender(txtBox, false);
    }
    
    if(bypass=='false'){
        txtBox.focus(function() { watermarkExtender($(this), true); })
            .blur(function() { watermarkExtender($(this), false); })
    }
}//]]>
</script>

<script type="text/javascript">
//<![CDATA[
AR.AdData.Site='ar.video';
AR.AdData.Zone='sidedish';
AR.AdData.KeyValues='v=325;k=27;k=28;k=57;k=95;k=192;k=233;k=247;k=295;k=296;k=325;k=363;k=460;k=501;k=515;k=618';
AR.AdData.KeyWords='';
AR.AdData.Ord = aradsord;
AR.AdData.VisitorID = '';
AR.AdData.VisitorStatus = 'unrecognized';
AR.AdData.VisitorStatusAbbr = 'u';
AR.AdData.SessionGroup = '0';
AR.AdData.ItemID = '325';
AR.AdData.ItemAdKey = 'v';
function getDisplayName() {return '';}
            var activeControl;
            var activeImage;
            var downImage = new Image();
            downImage.src = "http://images.media-allrecipes.com/site/allrecipes/area/community/toggle_arrow_down.gif";
            var rightImage = new Image();
            rightImage.src = "http://images.media-allrecipes.com/site/allrecipes/area/community/toggle_arrow_right.gif";

            function CloseComment()
            {
                if(activeControl)
                {
                    activeControl.style.display="none";
                    if(activeImage)
                    {
                        activeImage.src = rightImage.src;
                    }
                }
            }

            function OpenComment(id, imageID)
            {
                var ctrl = document.getElementById(id);
                var image = document.getElementById(imageID);
                
                if(ctrl)
                {
                    if(ctrl != activeControl)
                    {
                        CloseComment();
                        activeControl = ctrl;
                        ctrl.style.display="block";

                        if(image)
                        {
                            activeImage = image;
                            image.src = downImage.src
                        }
                    }
                }

                return false;
            }

            function CheckRequired(id, errorID)
            {
                var ctrl = document.getElementById(id);
                var errorCtrl = document.getElementById(errorID);
                var success = false;    
            
                if(ctrl)
                {
                    if(ctrl.value != '')
                    {
                         success = true;
                    }
                    else
                    {
                        if(errorCtrl)
                        {
                            errorCtrl.style.display="inline";
                        }
                    }
                }

                return success;
            }

            var submitcount=0;//]]>
</script>

