<script type="text/javascript">
/* <![CDATA[ */
function mtUpdateSignInWidget(u) {
    var separator = '&nbsp;&nbsp;|&nbsp;&nbsp;';
    var el = document.getElementById('signin-widget-content');
    var content = '';
    // alert (document.URL);
    //alert (encodeURIComponent(document.URL));
    if (!el) return;
    if (u) {
        if (u && u.is_authenticated) {
            user = u;
            mtSaveUser();
        } else {
            // user really isn't logged in; so let's do this!
            return mtSignIn();
        }
    } else {
        u = mtGetUser();
    }
    if (u && u.name) {
        var url;
		content += '<div id="login-tab" class="loggedIn"><div id="login-tab-inner">';
        if (u.is_authenticated) {
            if (u.is_author) {

				url = 'http://www.seriouseats.com/user/profile/'; 


            } else {
                url = u.url;
            }
        } else if (u.url) {
            url = u.url;
        } else {
            url = null;
        }
        if (url)
            content += '<p><strong><a href="' + url + '">' + u.name + '</a></strong></p>';
        else
            content += '<p><strong>' + u.name + '</strong></p>';
        if (u.is_author)
            content += '<p><a href="http://www.seriouseats.com/cgi-bin/mte/mt-cp.cgi?__mode=edit&blog_id=30&return_to=' + encodeURIComponent(document.URL) + '">Account</a>';
        
        content += ' | <a href="javascript:void(0)" onclick="return mtSignOutOnClick()">Log out</a>.';
        
		content += '</div></div><div class="clearer"><!-- --></div>'
    } else if (u && u.is_banned) {
        content = 'You do not have permission to sign in to this blog.';
    } else {
		content = '<div id="login-tab" class="notLoggedIn"><div id="login-tab-inner"><a onclick="return mtSignInOnClick(\'login-tab-inner\')" href="javascript:void(0)" id="login-link">log in</a></div></div><div class="clearer"><!-- --></div>'
    }
    //alert ("content is: " + content);
    el.innerHTML = content;
}
mtAttachEvent('usersignin', mtUpdateSignInWidget);
mtUpdateSignInWidget();
/* ]]> */
</script>
