<script language="javascript" type="text/javascript">
    function ViewSportNews(id, val) {
       // alert('sport');
           var len = id.lastIndexOf('_tpNewsControl');
           curID = id.substr(0, len);

           var param = val.split('$');
           var hdnUserId = $get(curID + "_tpNewsControl_hdnUserId");
           var hdnSportId = $get(curID + "_tpNewsControl_hdnSportId");
           var hdnSportName = $get(curID + "_tpNewsControl_hdnSportName");
           var hdncnt = $get(curID + "_tpNewsControl_hdncnt");
           var cnt = hdncnt.value;
//           alert(hdncnt);
//           alert(hdncnt.value);
           
           if (param[0] == "0") {
               hdnSportId.value = null;
               hdnSportName.value = "";
             //  hdncnt.value = null;

           }
           else {
               hdnSportId.value = param[0];
              // alert(hdnSportId.value);
               hdnSportName.value = param[1];
              // alert(hdnSportName.value);
               
             

           }
           var All = $get(curID + "_tpNewsControl_lblAllNews");
           All.innerHTML = param[1];
           StatusFeed.RecentNews( param[0],cnt, onViewNewsCompleted);
       }

       function onViewNewsCompleted(result) {
           //alert(result);
           var curTopDiv = $get(curID + "_tpNewsControl_divRecNews");
               curTopDiv.innerHTML = result;
       }
       

</script>
  


 
                    
                    
                    
