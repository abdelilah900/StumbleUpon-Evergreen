              //                var db = StatusFeed.AddComment(hdnst.value, txt.value, huid.value);
              //                alert('DB');
              //                alert(db);
              StatusFeed.AddNewsComment(stat, txt.value, huid.value, newid,
          onGetCommentCompleted);

              StatusFeed.GetNewsCommentAll(stat, newAllid, onGetCommentAllCompleted);
              StatusFeed.GetCommentCount(stat, onGetCommentCountCompleted);

          }



          function DeleteComment(id, val) {
              if (confirm('Do you want to remove this comment?') == true) {
                  var stid = val.split('$');
                  //ctl00_ContentPlaceHolder1_gvStatus_ctrl0_gvComments_ctl02_del
                  var len = id.indexOf('_gvComments');
                  var commid = id.substr(0, len);
                  var hndid = (commid + "_hdnStatusId");
                  var hdnst = $get(hndid);
                  //  alert('Statusid: ' + hdnst.value);
                  //  alert('FeedType: ' + hdnFeedType.value);

                  var gvid = (commid + "_gvComments");
                  var gvComments = $get(gvid);
                  curgv = gvComments;
                  var returnid = id.substr(0, len) + "_gvComments_ctl";
                  var returnAllid = id.substr(0, len) + "_gvCommentsAll_ctl";

                  var gvidAll = (commid + "_gvCommentsAll");
                  var gvCommentsAll = $get(gvidAll);
                  curgvall = gvCommentsAll;

                  var divGvid = (commid + "_divGv");
                  var divGv = $get(divGvid);
                  curdivGv = divGv;

                  var divGvAllId = (commid + "_divGvAll");
                  var divGvAll = $get(divGvAllId);
                  curdivGvAll = divGvAll;


                  var FeedType = (commid + "_hdnFeedType");
                  // alert(FeedType);
                  var hdnFeedType = $get(FeedType);
                  var stat = stid[1] + "#" + hdnFeedType.value;
                  //  alert(stat);
                  var lnkCommentsid = (commid + "_ViewAll");
                  var lnkComments = $get(lnkCommentsid);
                  curlnkComments = lnkComments;
                  StatusFeed.DeleteNewsComment(stat, stid[0], returnid, onGetCommentCompleted);
                  StatusFeed.GetNewsCommentAll(stat, returnAllid, onGetCommentAllDeleteCompleted);
                  StatusFeed.GetCommentCount(stat, onGetCommentCountCompleted);
              }
              else
                  return false;
          }


          function DeleteStatus(id, val) {

              if (confirm('Do you want to remove this feed?') == true) {
                  var curStatusDivid = id.replace("del", "pnl1");
                  curStatusDiv = $get(curStatusDivid);
                  StatusFeed.DeleteStatus(val, onDeleteStatustCompleted);
              }
              else
                  return false;
          }


          function onDeleteStatustCompleted(result) {
              curStatusDiv.innerHTML = "";
          }


          function onGetCommentCountCompleted(result) {
              curlnkComments.innerHTML = result;
          }

          function onGetCommentAllDeleteCompleted(result) {
               curdivGvAll.innerHTML = result;
          }


          function DeleteCommentHTML(id, val) {
              if (confirm('Do you want to remove this comment?') == true) {
                  var stid = val.split('$');
                  //ctl00_ContentPlaceHolder1_gvStatus_ctrl0_gvComments_ctl02_del
                  StatusFeed.DeleteNewsComment(stid[1], stid[0], onGetCommentCompleted);
                  StatusFeed.GetNewsCommentAll(stid[1], onGetCommentAllDeleteCompleted);
                  StatusFeed.GetCommentCount(stid[1], onGetCommentCountCompleted);

              }
              else
                  return false;


          }




          function onGetCommentAllCompleted(result) {
              var str = "not loggedin";
              // alert('str:' + str + ',result:' + result);
              if (result == str) {
                  window.location.href = "JoinToday.aspx";

              }
              else { curdivGvAll.innerHTML = result; }
          }




          function onGetCommentCompleted(result) {
          var str = "not loggedin";
              // alert('str:' + str + ',result:' + result);
          if (result == str) {
              window.location.href = "JoinToday.aspx";

          }
          else {
              txtcomments.value = "";
              curdivGv.innerHTML = result;
          }
          }



          function ItemMouseOver(oRow, del) {
              try {
                  oImage1 = document.getElementById(del);

                  oImage1.style.display = 'block';


                  oRow.originalBackgroundColor = oRow.style.backgroundColor
              } catch (e)
{ }

          }

          function ItemMouseOut(oRow, del) {
              try {
                  oImage1 = document.getElementById(del);


                  oImage1.style.display = 'none';


                  oRow.style.backgroundColor = oRow.originalBackgroundColor;
              } catch (e)
{ }
          }

    </script>

