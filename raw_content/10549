<script type="text/javascript">
          function show(k) {
              //alert(1);
              var s = k;
              var p = s.replace("lnk_comment_1", "pnlComments");
              //alert(s);
              //alert(p);


              var comment = document.getElementById(s);
              var panel = document.getElementById(p);
              //alert(comment);
              //alert(panel);
              comment.style.display = 'none';
              panel.style.display = 'block';


          }
          function showDiv(id) {
              var newdiv = id.replace('ViewAll', 'divGvAll');
              //        animatedcollapse.addDiv(newdiv, 'fade=0,speed=400');
              //        animatedcollapse.show(newdiv);
              var divh = $get(newdiv);
              divh.style.display = "";
              var oldlink = id;
              var ViewAll = $get(oldlink);
              ViewAll.style.display = "none";
              var newlink = id.replace('ViewAll', 'ViewRecent');
              var ViewRecent = $get(newlink);
              ViewRecent.style.display = "";
          }

          function hideDiv(id) {
              var newdiv = id.replace('ViewRecent', 'divGvAll');
              ////        animatedcollapse.addDiv(newdiv, 'fade=0,speed=400');
              //////        animatedcollapse.init();
              ////        animatedcollapse.hide(newdiv);
              var divh = $get(newdiv);
              divh.style.display = "none";

              var oldlink = id;
              var ViewAll = $get(oldlink);
              ViewAll.style.display = "none";
              var newlink = id.replace('ViewRecent', 'ViewAll');
              var ViewRecent = $get(newlink);
              ViewRecent.style.display = "";
          }



