<script language="javascript" type="text/javascript">



    window.onload = function() {
        getResolution();
    }

    function getResolution() {

        var sPath = window.location.pathname;

        var sPage = sPath.substring(sPath.lastIndexOf('/') + 1);
       // alert(sPage);
        if (sPage.toLowerCase() != "fb.aspx" && sPage.toLowerCase() != "tw.aspx" && sPage.toLowerCase() != "index.aspx" && sPage.toLowerCase() != "login.aspx" && sPage.toLowerCase() != "forgotpassword.aspx" && sPage.toLowerCase() != "confirmaccount.aspx" && sPage.toLowerCase() != "confirmnewsletter.aspx" && sPage.toLowerCase() != "") {
            var middle_left_con2 = document.getElementById("middle_left_con2");
            middle_left_con2.style.minHeight = (screen.height - 350) + "px";
            
            var gapdiv = document.getElementById("gapdiv");
            gapdiv.style.height = "46px";

            var middle_botcor = document.getElementById("middle_botcor");
            middle_botcor.style.display = "none";

          // alert('done');
        }
    }


    var openpp = 2;
    function Setgray() {
        var btnSignin = document.getElementById("ctl00_lvMember_imgHOF1");
        btnSignin.className = "mhofimgUP"; ;
        return "mhofimgUP";
    }

    function Setblue() {
        var btnSignin = document.getElementById("ctl00_lvMember_imgHOF1");
        btnSignin.className = "mhofimg"; ;
        return "mhofimg";
    }

    function Setgraysign() {
        var btnSignin = document.getElementById("ctl00_lvMember_btnSignin");
        btnSignin.className = "sigin_buttonactive"; ;
        return "sigin_buttonactive";
    }

    function Setbluesign() {
        var btnSignin = document.getElementById("ctl00_lvMember_btnSignin");
        btnSignin.className = "sigin_button"; ;
        return "sigin_button";
    }

    function hidePopup() {
        var pp = $find('ctl00_lvMember_PopExPopupBehavior');
        if (openpp == 0) {
            $find('ctl00_lvMember_PopExPopupBehavior').hide();
            openpp = 1;
        }
        else if (openpp == 1) {
            $find('ctl00_lvMember_PopExPopupBehavior').show();
            openpp = 0;
        }
        else {
            openpp = 0;
        }
    }

    if (!document.all) {
        window.onbeforeunload = function() {
            Sys.WebForms.PageRequestManager.getInstance().add_endRequest(endRequest);
        }
    }

    function endRequest(sender, e) {
        err = e.get_error();
        if (err) {
            //if (err.name == "Sys.WebForms.PageRequestManagerServerErrorException") {
            e.set_errorHandled(true);
            //}
        }
    }
</script>

 <script type="text/javascript">

     function chkOptionsmm(sender, args) {
         // alert ('Hi');
         //int cnt;
         var txt1 = document.getElementById('ctl00_LoginView1_txtOption1mm');
         //  alert(txt1);
         //  alert(txt1.value);

         var txt2 = document.getElementById('ctl00_LoginView1_txtOption2mm');
         var txt3 = document.getElementById('ctl00_LoginView1_txtOption3mm');
         var txt4 = document.getElementById('ctl00_LoginView1_txtOption4mm');
         var txt5 = document.getElementById('ctl00_LoginView1_txtOption5mm');
         var cnt = 0;
         // alert(cnt);
         if (txt1.value == "") {
             cnt = cnt + 1;
             // alert(cnt);
         }

         if (txt2.value == "") {
             cnt = cnt + 1;
             //alert(cnt);

         }
         if (txt3.value == "") {
             cnt = cnt + 1;
             //  alert(cnt);
         }
         if (txt4.value == "") {

             cnt = cnt + 1;
             //   alert(cnt);
         }
         if (txt5.value == "") {
             cnt = cnt + 1;

             //  alert(cnt);
         }


         //   alert(cnt);
         // return cnt;

         if (cnt >= 4) {
             args.IsValid = false;
         }
         else {
             args.IsValid = true;
         }


     }
     
     
     // Find the autocompleteextender and set the text as value selected
     function OnItemSelectedQstmm(event) {
          var txtSportsCast = document.getElementById("ctl00_LoginView1_txtQuestionmm");
          var value = txtSportsCast.value;
         var comment = txtSportsCast.value.lastIndexOf('@');
         var cast = value.substring(0, comment);
         var selInd = $find("AutoCompleteQstmm")._selectIndex;
         if (selInd != -1) {
             var comment = $find("AutoCompleteQstmm").get_completionList().childNodes[selInd]._value;
             var ocom = cast + '@' + comment.split('$')[0];
             //alert(ocom);
             $find("AutoCompleteQstmm").get_element().value = ocom;
         }
     }



     function OnClientPopulatedQstmm(sender, eventArgs) {

         //Find the autocompleteextender list
         var autoList = $find("AutoCompleteQstmm").get_completionList();
         for (i = 0; i < autoList.childNodes.length; i++) {
             // Consider value as image path
             var imgeUrl = autoList.childNodes[i]._value;
             //First node will have the text
             var text = autoList.childNodes[i].firstChild.nodeValue.replace(/\s+/g, "");

             var image = imgeUrl.split('$')[0];
             var category = imgeUrl.split('$')[1].replace(/\s+/g, "");
             var usertype = imgeUrl.split('$')[2];
             if (usertype == "2") {

                 autoList.childNodes[i]._value = category + "/" + text + '$' + usertype + '$' + category;
             }
             else {
                 autoList.childNodes[i]._value = text + '$' + usertype + '$' + category;
             }

             //Height and Width of the mage can be customized here...
             //autoList.childNodes[i].innerHTML = "<div style='width:420px;float:left;margin-top:5px;'>" + "<img height=30 width=30 src=" + image + " style='float:left;'/>&nbsp;" + "<div  style='float:left;margin-left:5px;'>" + text + "<br />" + category + "</div>" + "</div>";
             //autoList.childNodes[i].innerHTML = "<img height=30 width=30 src=" + image + " />&nbsp;" + text;
             autoList.childNodes[i].innerHTML = "<span>" + "<div>" + "</div>" + "<img height=30 width=30 src=" + image + " style='margin-bottom:5px;' />" + "<div style='width:250px; text-align:left; vertical-align:top;float:right;margin-bottom:5px;color:#4088CD;'>" + text + "<br />" + "<span style='color:gray;font:normal 11px Arial, Helvetica, sans-serif;'>" + category + "</span>" + "</div>" + "</span>";
         }
     }
    </script>

    <script type="text/javascript">
        // Find the autocompleteextender and set the text as value selected
        function OnItemSelectedOpt1mm(event) {
            var txtSportsCast = document.getElementById("ctl00_LoginView1_txtOption1mm");
            var value = txtSportsCast.value;
            var comment = txtSportsCast.value.lastIndexOf('@');
            var cast = value.substring(0, comment);
            var selInd = $find("AutoCompleteOpt1mm")._selectIndex;
            if (selInd != -1) {
                var comment = $find("AutoCompleteOpt1mm").get_completionList().childNodes[selInd]._value;
                //alert(comment);
                var ocom = cast + '@' + comment.split('$')[0];
                //alert(ocom);
                $find("AutoCompleteOpt1mm").get_element().value = ocom;
            }
        }


        function OnClientPopulatedOpt1mm(sender, eventArgs) {
            //Find the autocompleteextender list
            var autoList = $find("AutoCompleteOpt1mm").get_completionList();
            for (i = 0; i < autoList.childNodes.length; i++) {
                // Consider value as image path
                var imgeUrl = autoList.childNodes[i]._value;
                //First node will have the text
                var text = autoList.childNodes[i].firstChild.nodeValue.replace(/\s+/g, "");
                var image = imgeUrl.split('$')[0];
                var category = imgeUrl.split('$')[1].replace(/\s+/g, "");
                var usertype = imgeUrl.split('$')[2];
                if (usertype == "2") {

                    autoList.childNodes[i]._value = category + "/" + text + '$' + usertype + '$' + category;
                }
                else {
                    autoList.childNodes[i]._value = text + '$' + usertype + '$' + category;
                }

                autoList.childNodes[i].innerHTML = "<span>" + "<div>" + "</div>" + "<img height=30 width=30 src=" + image + " style='margin-bottom:5px;' />" + "<div style='width:250px; text-align:left; vertical-align:top;float:right;margin-bottom:5px;color:#4088CD;'>" + text + "<br />" + "<span style='color:gray;font:normal 11px Arial, Helvetica, sans-serif;'>" + category + "</span>" + "</div>" + "</span>";
            }
        }
    </script>

