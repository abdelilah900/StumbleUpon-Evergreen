<script language="javascript" type="text/javascript">

    function showpollmm() {
        $find('pollPopupmm').show();
    }




    function AddLatestPoll() {
//        var procStatus = $get("procStatus");
//        if (procStatus != null)
//            procStatus.style.display = "";
        var qst = document.getElementById('ctl00_LoginView1_txtQuestionmm');
        var txt1 = document.getElementById('ctl00_LoginView1_txtOption1mm');
        var txt2 = document.getElementById('ctl00_LoginView1_txtOption2mm');
        var txt3 = document.getElementById('ctl00_LoginView1_txtOption3mm');
        var txt4 = document.getElementById('ctl00_LoginView1_txtOption4mm');
        var txt5 = document.getElementById('ctl00_LoginView1_txtOption5mm');

        if (qst.value == "") {
            return false;
            // alert(cnt);
        }
    
        var cnt = 0;
        // alert(cnt);
        if (txt1.value == "") {
            cnt = cnt + 1;
            // alert(cnt);
        }

        if (txt2.value == "") {
            cnt = cnt + 1;
            //alert(cnt);

        }
        if (txt3.value == "") {
            cnt = cnt + 1;
            //  alert(cnt);
        }
        if (txt4.value == "") {

            cnt = cnt + 1;
            //   alert(cnt);
        }
        if (txt5.value == "") {
            cnt = cnt + 1;

            //  alert(cnt);
        }


        //   alert(cnt);
        // return cnt;

        if (cnt >= 4) {
            return false;
        }
        else {
        }




        var uid = $get("ctl00_hdnuserid");
        var hdncnt = document.getElementById("ctl00_ContentPlaceHolder1_hdncnt");
        var hdnstcount = document.getElementById("ctl00_ContentPlaceHolder1_hdnstcount");
        var hdnnxtcnt = $get("reccnt");
        var nxtcnt;
        if (hdnnxtcnt == null || hdnnxtcnt.value == "") {
            if (hdnstcount == null) {
                nxtcnt = 20; 
            }
            else {
                nxtcnt = hdnstcount.value;
            }
        }
        else {
            nxtcnt = hdnnxtcnt.value;
        }


        StatusFeed.GetPollLatest(uid.value, qst.value, txt1.value, txt2.value, txt3.value, txt4.value, txt5.value, nxtcnt, AddLatestPollCompleted);

    }


    function AddLatestPollCompleted(result) {
        // alert(result);
        var SatatusList = $get("SatatusList");
        //alert(result + SatatusList.innerHTML);
        var hdncnt = document.getElementById("ctl00_ContentPlaceHolder1_hdncnt");
        var hdnstcount = document.getElementById("ctl00_ContentPlaceHolder1_hdnstcount");
        var hdnnxtcnt = $get("reccnt");
        if (SatatusList == null || hdncnt == null || hdnstcount == null || hdnnxtcnt == null) {
            window.location.href = "../../home.aspx";
        }
        SatatusList.innerHTML = result + SatatusList.innerHTML;
        var hdnstcount = document.getElementById("ctl00_ContentPlaceHolder1_hdnstcount");
        hdnstcount.value = parseInt(hdnstcount.value) + 1;
        var qst = document.getElementById('ctl00_LoginView1_txtQuestionmm');
        var txt1 = document.getElementById('ctl00_LoginView1_txtOption1mm');
        var txt2 = document.getElementById('ctl00_LoginView1_txtOption2mm');
        var txt3 = document.getElementById('ctl00_LoginView1_txtOption3mm');
        var txt4 = document.getElementById('ctl00_LoginView1_txtOption4mm');
        var txt5 = document.getElementById('ctl00_LoginView1_txtOption5mm');
        qst.value = "";
        txt1.value = "";
        txt2.value = "";
        txt3.value = "";
        txt4.value = "";
        txt5.value = "";
        var st = $find('mpePoll');
        st.hide();


    }

    function SetOptionHome(id, val) {
        var hdnOption = $get("ctl00_ContentPlaceHolder1_hdnOption");
        hdnOption.value = val;
    }
    function openPollPopUp() {
    
        $find('mpePoll').show();

    }

    function hideall() {
        var panel_votehof = $get('ctl00_LoginView1_Panel_hofPollmm');
        // alert(panel_votehof);
        panel_votehof.style.display = "none";

    }
</script>

