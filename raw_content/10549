          var curidc = "";

          function VoteStatus(id, val) {
            //  alert(val);
              var idc = $get(id);
              var divVoteDescid = id.replace("LinkButton2", "divVoteDesc");
              var divVoteDesc = $get(divVoteDescid);
              curdivVoteDesc = divVoteDesc;
            //  alert(curdivVoteDesc);
              curidc = idc;
              //alert(idc);
              //alert(idc.innerHTML);
              var test = val.split('$');
              //  alert(test);

              var huid = $get('ctl00_ContentPlaceHolder1_hdnuserid');
              //  alert(huid);
              var uid = huid.value;


              // Added by Prajakta on May 28,2011
              if (uid == "") {

              }
              else {
                  if ((idc.innerHTML).toLowerCase() == "vote") {
                      //   idc.innerHTML = "Unvote";
                      idc.name = "unvote$" + test[1];
                  }
                  else {
                      // idc.innerHTML = "Vote";
                      idc.name = "vote$" + test[1];
                  }
              }
             // alert('1');
              StatusFeed.VoteNewsStatus(id, test[0], test[1], uid, onVoteStatusCompleted);
              StatusFeed.GetNewsVoteDescAll(test[1], uid, onVoteDescCompleted);
          }


          function onVoteStatusCompleted(result) {
              //alert(result);
              var test = result.split('$');
              // Added by Prajakta on May 28,2011
              //ctl00_ContentPlaceHolder1_gvStatus_ctrl0_LinkButton2
              //alert(test);
              //            if (test == "") {
              if (test == "not loggedin") {
                  var str1 = curidc.innerHTML;
                  //alert(str1);
                  curidc.innerHTML = str1;
                  window.location.href = "JoinToday.aspx";
              }
              else {
                  curidc.name = test[0] + "$" + test[2];
                  curidc.innerHTML = test[1];
              }


          }

          function onVoteDescCompleted(result) {
              // Added by Prajakta on May 28,2011
              //alert(result);
              // alert(curdivVoteDesc);
              //curdivVoteDesc.innerHTML = result; //ctl00_ContentPlaceHolder1_gvStatus_ctrl0_LinkButton2
              var str = "not loggedin";
              // alert('str:' + str + ',result:' + result);
              if (result == str) {
                  window.location.href = "JoinToday.aspx";

              }
              else {
                  curdivVoteDesc.innerHTML = result;
              }
          }




          var curgv = "";
