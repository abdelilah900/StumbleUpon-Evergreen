              //    alert(divGv);
              curdivGv = divGv;
              curgv = gvComments;
              curgvall = gvCommentsAll;
              txtcomments = txt;
              //////                var uid = huid.value;
              //////                StatusFeed.VoteStatus(id, test[0], test[1], uid, onVoteStatusCompleted);

              //////                var hstid = $get('ZipCode');
