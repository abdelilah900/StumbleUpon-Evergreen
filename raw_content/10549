          var newid = "";
          var newAllid = "";

          function AddComments(id) {
              // alert(id);
              var btn = $get(id);
              //  alert(btn);
              var huid = $get('ctl00_ContentPlaceHolder1_hdnuserid');
              //  alert("UserId " + huid.value);
              var txtid = id.replace("CommentsShare", "txtcomments");
              var txt = $get(txtid);
              if (txt.value == 'Write a comment...' || txt.value == '')
                  return false;
              //  alert("Comment Text " + txt.value);
              var hndid = id.replace("CommentsShare", "hdnStatusId");
              newid = id.replace("CommentsShare", "gvComments_ctl");
              newAllid = id.replace("CommentsShare", "gvCommentsAll_ctl");

              var hdnst = $get(hndid);
              // alert("StatusId " + hdnst.value);
              var gvid = id.replace("CommentsShare", "gvComments");
              var gvComments = $get(gvid);
              var gvidAll = id.replace("CommentsShare", "gvCommentsAll");
              var gvCommentsAll = $get(gvidAll);
              // alert(gvCommentsAll);

