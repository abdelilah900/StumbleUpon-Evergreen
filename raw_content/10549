              var divGvAll = $get(divGvAllId);
              curdivGvAll = divGvAll;

              var divVoteDescid = id.replace("CommentsShare", "divVoteDesc");
              var divVoteDesc = $get(divVoteDescid);


              var lnkCommentsid = id.replace("CommentsShare", "ViewAll");
              var lnkComments = $get(lnkCommentsid);


              var hndft = id.replace("CommentsShare", "hdnFeedType");
              var hdbfeedtype = $get(hndft);
              var stat = hdnst.value + "#" + hdbfeedtype.value;



              //   alert(lnkCommentsid);

              //  ctl00_ContentPlaceHolder1_gvStatus_ctrl9_ViewAll
