					<li class="reclips"><span class="value">[!= current.reclips !]</span> Reclips</li>
				</ul>
			</section>
			[! 	counter++; }; !]
		</div>
	</script>

	<script type="text/javascript">
		var mosaic = new Mosaic({
			element: $('#boardMosaic')
		});
	</script>
</div>


</div><!--end Main Section-->
