<script>
var typeahead_settings = {
	url:'/spark/services/search/spark/suggest/',
	maxResults:8,
	minCharacters:2,
	onSelect:function (data) {
		if (typeof _gat != 'undefined') {
			var pageTracker = _gat._getTrackerByName();

			// track time to click
			var clickTracker = new TimeTracker([
				5000, 10000, 20000, 30000, 40000, 50000, 60000, 90000, 120000
			]);
			clickTracker._recordStartTime(data.startTime);
			clickTracker._recordEndTime();
			if (clickTracker._getTimeDiff() < 120000) { // discard times over 2 minutes
				clickTracker._track('TypeaheadTime', 'spark', pageTracker);
			}

			// track result position
			if (data.item.index != undefined) {
				var index = 1 + data.item.index;
				pageTracker._trackEvent('Typeahead', 'Position', 'spark', index);
			} else {
				// else more results
				pageTracker._trackEvent('Typeahead', 'MoreResults', 'spark');
			}

			// track number of searches
			//var VarIndex = 2;
			//var numSearches = parseInt(pageTracker._getVisitorCustomVar(VarIndex) || 0);
			//dmjs.GA.segment('Searches', numSearches + 1, VarIndex, dmjs.GA.scope.SESSION);
		}

		if (data.item.url) {
			// give trackers some time to log
			if (typeof mixpanel !== 'undefined') { mixpanel.track('autocomplete search'); }
			setTimeout(function() { window.location.href = data.item.url}, dmjs.GA.track_delay);
			return false;
		}
		else if (data.item.title && window.search && window.search.search) {
			search.search({
				query:data.item.title
			});
		}
	},
	onSubmit:function(event){
		// only trigger once
		$(this).unbind(event);

		if (typeof _gat != 'undefined') {
			var pageTracker = _gat._getTrackerByName();
			pageTracker._trackEvent('Typeahead', 'Search', 'spark');
		}

		setTimeout((function(form) { return function(){ $(form).trigger('submit'); }; })(this), dmjs.GA.track_delay);

		return false;
	},
	complete:false,
	highlightMatches:false,
	maxHeight:999,
	width:350,
	moreResults:'/spark/search/',
	onRequest:function (text, obj) {
		// track first box impression
		if (typeof _gaq != 'undefined') {
			if ( ! typeahead_settings.firstRequest) {
				typeahead_settings.firstRequest = true;
				_gaq.push(['_trackEvent', 'Typeahead', 'Impression']);
			}
		}

		if (text && window.search && window.search.search) {
			search.search({
				method:search.method,
				query:text,
				title:search.query_text
			});
		}
	},
	firstRequest : false,
	template:'searchSuggestResultsTemplate'
};
</script>
<script>
(function($) {
	$('#searchHeader input[data-type=searchinput]')
		.typeahead(typeahead_settings);
})(jQuery);
</script>
