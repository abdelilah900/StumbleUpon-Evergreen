<div style="text-align:right">Conversation powered by <a style="text-decoration:none; color:inherit" target="_blank" href="http://livefyre.com">Livefyre</a></div>

<script type="text/javascript">

    var myListeners = {
        handle_auth_login: function(data) {
            // Whatever called this requires a login. Send through registration loop.
            window.location = 'http://www.cbssports.com/login?master_product=2580&xurl=http%3A%2F%2Fwww.cbssports.com%2Fmcc%2Fauthors%2Fnew-author%3Fxurl%3Dhttp%3A%2F%2Fwww.cbssports.com%2Fnfl%2Fblog%2Feye-on-football%2F20341899%2Fnfl-week-3-as-told-by-tweets%3FprofFR%3D1&lfxurl=http%3A%2F%2Fwww.cbssports.com%2Fmcc%2Fauthors%2Fnew-author%3Fxurl%3Dhttp%253A%252F%252Fwww.cbssports.com%252Fnfl%252Fblog%252Feye-on-football%252F20341899%252Fnfl-week-3-as-told-by-tweets%253FprofFR%253D1';
        },
        handle_user_logged_in: function(data) {
            // this happens when Livefyre believes the login/authorization process has completed successfully
            changeDOM();
        },
        handle_comment_shown: function(data) {
            var elem = data.comment.find('.lf_comment_user');
            if (elem) {
                data.comment.find('.hastooltip').removeClass('hastooltip');
                $('#livefyre .hastooltip,[title="Quick profile"]').removeAttr('title');
            }
        },
        handle_comments_loaded: function() {
            $('#lf_comments > .lf_comment_container').has('.lf_comment_user').find('.hastooltip').removeClass('hastooltip');
            $('#livefyre .hastooltip,[title="Quick profile"]').removeAttr('title');
        }
    };

    changeDOM = function() {
        // DOM hacks.  Change the UI for the dropdown box
        var userBox = $('.lf_user_box.lf_box');
        // Change link label
        $('.lf_user_links.lf_edit_profile_link',userBox).html('Edit Settings');
        // Nuke logout link
        userBox.find('.lf_logout_link').remove();
        if (userBox.length > 0) {
            userBox.append('<li class="lf_user_links"><a style="color:inherit;text-decoration:none" target="_blank" href="'+userObj.profile.profile_url+'">Edit Profile</a></li>');
        }
        // Change the text from Sign In
        var loggedin = readCookie('pid');
        if (typeof loggedin == "string") {
            if (loggedin.match(/^L:/)) {
                // They need a screen name
                $('.lf_auth_section a.lf_user_loggedout').html('Get a Screen Name to Comment').addClass('loginLink');
            }
            else {
                // No need to change the text
                $('.lf_auth_section a.lf_user_loggedout').addClass('loginLink');
            }
        }
    };
 

    var userObj = {};

    $(document).ready( function () {

        var conv = LF({
            domain: 'cbssports.fyre.co',
            site_id: '309983',
            article_id: 'c:20341899',
            conv_meta: {
                article_url: 'http://www.cbssports.com/nfl/blog/eye-on-football/20341899/nfl-week-3-as-told-by-tweets',
                title: 'NFL Week 3 as told by tweets',
                sig: '413960058dffcb7c979eb90bdabe8624'
            }
        }).ready(function() {
            // Run this listener once on initial load
            myListeners.handle_comments_loaded();
            // Run the changes to the DOM
            LF.Dispatcher.addListener(myListeners);
            // Log in the user if we got a token for them
            // Change the DOM after login since things get re-rendered
            if (userObj.token) {
                LF.login(userObj, function() {
                    CBSi.log("Livefyre Login success! ", userObj);
               }, function() {
                    CBSi.log("Livefyre LOGIN ERROR"); 
                });
            }
            else {
                changeDOM();
            }
            // Load the comment counts for our bubbles
            $('div.commentsBubble').addClass('livefyre-commentcount').attr('data-lf-site-id','309983').attr('data-lf-article-id','c:20341899');
            LF.CommentCount({
                replacer: function(element, count) {
                    if (count > 1){
                        $(element).html('<a href="#commentsAnchor">' + count + ' &nbsp;&nbsp;| &nbsp;Comments</a>');
                        $(element).show();
                    }
                    else{
                        $(element).html('<a href="#commentsAnchor">' + count + ' &nbsp;&nbsp;| &nbsp;Comment</a>');
                        $(element).show();
                    }
                }
            });
       });
    });
    

</script>


