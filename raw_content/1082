				data-slidetype="image"
				
				
				
				
				data-thumbsrc="/cm/realbeauty/images/1l/rby-all-night-updos-annalynn-mccord-smn.jpg"
				
				data-src="/cm/realbeauty/images/nL/rby-all-night-updos-annalynn-mccord-mdn.jpg">
				
				
					<div class="imageContainer" style="width:300px;height:300px">
						
					
					<div class="imageCredit">Jim Spellman/WireImage.com</div>
