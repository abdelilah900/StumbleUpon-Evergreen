}

.ui-autocomplete .ui-menu-item a {
border-bottom: 1px solid #eee;
border-radius: 0;
color: #666;
display: block;
padding: 7px 5px;
}

.ui-autocomplete .ui-menu-item a:hover, #ui-active-menuitem {
background-color: #00d3fa!important;
border: none;
border-bottom: 1px solid #eee;
border-radius: none;
color: #fff;
cursor: pointer;
}
</style>
<script type="text/javascript">
$(function() {
	$( "#queryname" ).autocomplete({
		source: function( request, response ) {
			$.ajax({
				url: "http://clients1.google.com/complete/search?hl=en&client=partner&source=gcsc&partnerid=004830092955692134028%3Abkw_22qvviq&callback=google.search.CustomSearchControl.GcrcGcsc17089&ds=cse",
				dataType: "jsonp",
				data: {
					featureClass: "P",
					style: "full",
					maxRows: 10,
					q: request.term
				},
				success: function( data ) {
					response( $.map( data[1], function( item,i ) {
						return {
							label: item[0],
							value: item[0]
						}
					}));
				}
			});
		},
		minLength: 2,
		select: function( event, ui ) {
                              window.location = "http://engadget.search.aol.com/search?q="+ui.item.label+"&invocationType=wl-gadget-a";
		},
		open: function() {
			$( this ).removeClass( "ui-corner-all" ).addClass( "ui-corner-top" );
		},
		close: function() {
			$( this ).removeClass( "ui-corner-top" ).addClass( "ui-corner-all" );
		}
	});
});

</script>
<script type="text/javascript" src="http://www.blogsmithmedia.com/www.engadget.com/include/autocomplete.js"></script></body>

