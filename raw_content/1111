<!-- SOME JS THAT IS MAGICAL -->
<script type="text/javascript">
	function typeImg(str) {
		if (str=="") {
  			return;
  		} 
		if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
  			xmlhttp=new XMLHttpRequest();
  		}
		else { // code for IE6, IE5
  			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  		}
		xmlhttp.onreadystatechange=function() {
  		if (xmlhttp.readyState==4 && xmlhttp.status==200) {
    		document.getElementById("type_select").innerHTML=xmlhttp.responseText;
    		
    		//binding js to dynamically loaded content
    		//show_error();
    	}
  	}
	
	xmlhttp.open("GET","http://iwastesomuchtime.com/do.php?typeImg",true);
	xmlhttp.send();
	
	}
	function typeVid(str) {
		if (str=="") {
  			return;
  		} 
		if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
  			xmlhttp=new XMLHttpRequest();
  		}
		else { // code for IE6, IE5
  			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  		}
		xmlhttp.onreadystatechange=function() {
  		if (xmlhttp.readyState==4 && xmlhttp.status==200) {
    		document.getElementById("type_select").innerHTML=xmlhttp.responseText;
    		
    		//binding js to dynamically loaded content
    		//show_error();
    	}
  	}
	
	xmlhttp.open("GET","http://iwastesomuchtime.com/do.php?typeVid",true);
	xmlhttp.send();
	
	}
	function typeImgl(str) {
		if (str=="") {
  			return;
  		} 
		if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
  			xmlhttp=new XMLHttpRequest();
  		}
		else { // code for IE6, IE5
  			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  		}
		xmlhttp.onreadystatechange=function() {
  		if (xmlhttp.readyState==4 && xmlhttp.status==200) {
    		document.getElementById("type_select").innerHTML=xmlhttp.responseText;
    		
    		//binding js to dynamically loaded content
    		//show_error();
    	}
  	}
	
	xmlhttp.open("GET","http://iwastesomuchtime.com/do.php?typeImgl",true);
	xmlhttp.send();
	
	}
	function typeReset(str) {
		if (str=="") {
  			return;
  		} 
		if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
  			xmlhttp=new XMLHttpRequest();
  		}
		else { // code for IE6, IE5
  			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  		}
		xmlhttp.onreadystatechange=function() {
  		if (xmlhttp.readyState==4 && xmlhttp.status==200) {
    		document.getElementById("type_select").innerHTML=xmlhttp.responseText;
    		
    		//binding js to dynamically loaded content
    		//show_error();
    	}
  	}
	
	xmlhttp.open("GET","http://iwastesomuchtime.com/do.php?reset",true);
	xmlhttp.send();
	
	}

</script>

