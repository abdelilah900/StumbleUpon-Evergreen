    <script type="text/javascript">
               window._ghearst_vars = (function() {
        var
            metaKeyWords = (function(){
                var
                    i                  = 0,
                    metaKeywordContent = '',
                    nameAttribute      = null,
                    metaCollection     = document.getElementsByTagName('meta');

                for (i = 0; i < metaCollection.length; i++) {
                    nameAttribute = metaCollection[i].name.search(/keywords/);
                    if (nameAttribute!= -1) {
                        metaKeywordContent = metaCollection[i].content;
                        metaKeywordContent = metaKeywordContent.replace(/ /g,''); // to replace space
                    }
                }
                return encodeURIComponent(metaKeywordContent);
            })();

        if( (navigator.userAgent.match(/iPad/i) !== null) && (CN.cookie.get('ipadTakeover') !== 'true') ){
            return {
                'ams_ads_script_src': 'http://subscribe.wmagazine.com/ams/page-ads.js?ad_category_prefix=&browser_path=%2F&cat_prefixes=%2C&keywords='+metaKeyWords+'&position_list=AMS_WMG_POPUP_FLOATINGAD%2CAMS_WMG_GLOBAL_NAVBAR_ROLLOVER%2CAMS_WMG_GLOBAL_RIGHTRAIL_A%2CAMS_WMG_GLOBAL_RIGHTRAIL_B%2CAMS_WMG_GLOBAL_PROMOTOP%2CAMS_WMG_GLOBAL_EMBEDDEDFORM%2CAMS_WMG_GLOBAL_NAVBAR,AMS_WMG_GLOBAL_IPAD_TAKEOVER&site_prefix=wmagazine&subdomain=www&url_name='
            }
        } else {
            return {
                'ams_ads_script_src': 'http://subscribe.wmagazine.com/ams/page-ads.js?ad_category_prefix=&browser_path=%2F&cat_prefixes=%2C&keywords='+metaKeyWords+'&position_list=AMS_WMG_POPUP_FLOATINGAD%2CAMS_WMG_GLOBAL_NAVBAR_ROLLOVER%2CAMS_WMG_GLOBAL_RIGHTRAIL_A%2CAMS_WMG_GLOBAL_RIGHTRAIL_B%2CAMS_WMG_GLOBAL_PROMOTOP%2CAMS_WMG_GLOBAL_EMBEDDEDFORM%2CAMS_WMG_GLOBAL_NAVBAR&site_prefix=wmagazine&subdomain=www&url_name='
            }
        };
    })();

    jQuery(document).ready(function(){
        CN.hearstQue.register(function(pageAds){
            var div;
            if ((typeof pageAds !== 'undefined') && (typeof pageAds.AMS_WMG_GLOBAL_IPAD_TAKEOVER !== 'undefined') && (CN.url.params('nojoy') != 1) ) {
                div = jQuery('<div id="ipad-takeover-wrapper"></div>');
                jQuery('body').append(div);
                div.html(pageAds.AMS_WMG_GLOBAL_IPAD_TAKEOVER.replace(/document.write\(.*\)/gi,"/*filtered by amg-magnet:document.write(...)*/"));
            }
        });
    });

        (function($) {
            var show = function() {
                $('.ecom-placement').each(function() {
                    $(this).css('visibility', 'visible');
                });
            };
            
            $LAB.script(_ghearst_vars.ams_ads_script_src)
                .wait(function() {
                    
                    CN.hearstQue.run(window.pageAds);
                });

            
            setTimeout(show, 10 * 1000);
        }(jQuery));
    </script>











    
    


































    
