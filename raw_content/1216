	if (top !== self && document.referrer.match(/digg\.com\/\w{1,8}/)) {
	  top.location.replace(self.location.href);
	}

	function tabListener(event) {
		var $target = $(event.currentTarget);
		if(!$target.is('ul.tab.selected')){
			$('ul.tab.selected li.t').animate({paddingLeft: 0,paddingRight: 0}, 500);
			$('ul.tab').removeClass('selected');
			$('ul.tab-content').removeClass('selected');
			//Call any ajax here to load or switch content.
			switch($(this).attr('id')){
				case "by-price-tab":
					$("ul.tab-content.by-price").addClass('selected');
					break;
				case "alphabetical-tab":
					$("ul.tab-content.alphabetical").addClass('selected');
					break;
				case "body-style-tab":
					$("ul.tab-content.body-style").addClass('selected');
					break;
				case "by-segment-tab":
					$("ul.tab-content.by-segment").addClass('selected');
					break;
			}
			$(this).addClass('selected');
			$('ul.tab.selected li.t').animate({paddingLeft: 10,paddingRight: 10}, 500);
		}
	}

	$(function(){
		$('ul.tab').bind("click", tabListener);
		$('#search-box').bind("focus",inputClearOnFocus);
		$('#search-box').bind("blur", inputResetOnBlur);
	});

	function toggleComments(){
		if($('#review div.comments div.w').is(":hidden")){
			$('#review div.comments').removeClass('collapse');
			$('#review div.comments div.w').slideDown('normal');
		}else{
			$('#review div.comments div.w').slideUp('normal',function(){$('#review div.comments').addClass('collapse')});
		}
	}

	$(function(){
		$('#navigation_bar').mouseenter(function(){
				$('#nav li').mouseover(function(){
					var tab = $(this).find('a').attr('href');
					tab_id = tab.replace('#tab','');
					for(var i=1;i<=5;i++){
						if(i==parseInt(tab_id)){
							$('#nav_'+i).css('display','block');
							$('#tab_'+i).addClass('active');
						}
						else{
							$('#nav_'+i).css('display','none');
							$('#tab_'+i).removeClass('active');
						}
					}

				})
		})
		$('#navigation_bar').mouseleave(function(){
			for(var i=1;i<=5;i++){
				if(i==1){
					$('#nav_'+i).css('display','block');
					$('#tab_'+i).addClass('active');
				}
				else{
					$('#nav_'+i).css('display','none');
					$('#tab_'+i).removeClass('active');
				}
			}
		})

	});

	// added by jai on 14th Of Sept 2010
	function setmodelyear( modelId )
	{
		var modelname = 'modelid='+modelId;
		//alert(modelname);
		$.ajax({
			url: 'http://www.leftlanenews.com/carsnew/getmodelsbymake.php',
			type: 'GET',
			dataType: 'html',
			data: modelname,
			cache: false,
			error: function(){
				alert('Error loading data. Please try again later..');
			},
			success: function(data){
				// do something with html
				$("input#crmodelyear").val(data);

			}
		});
	}
	// jai end here

	// ]]>
