    function toggleCommentsTab(tab) {
        if (tab == "mt") {
            $("#commentsOnFacebook").attr("src", "/images/site/comment-fb-g.jpg");
            $("#facebookComments").hide();
            $("#commentsOnMT").attr("src", "/images/site/comment-ml-a.jpg");
            $("#mtComments").show();
            $("#comments-form-wrapper").show();
        } else {
            $("#commentsOnMT").attr("src", "/images/site/comment-ml-g.jpg");
            $("#mtComments").hide();
            $("#comments-form-wrapper").hide();
            $("#commentsOnFacebook").attr("src", "/images/site/comment-fb-a.jpg");
            $("#facebookComments").show();
        }
    }
    </script>
    <!--  end: facebook comments widget code -->

