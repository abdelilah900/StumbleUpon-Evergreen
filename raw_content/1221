<script type="text/javascript">
var mFeed = {
    feeds: [], // array of feeds that we'll be loading,
    cachetime: 30,

    load: function(feeds, cachetime) {
        var url = 'http://www.momlogic.com/includes/readfeed_multi.php?feeds=';
        this.feeds = feeds.replace(/[^a-zA-Z0-9_\-,]/g, "").split(",");
        url += this.feeds.join(",");
        if (cachetime)
            this.cachetime = parseInt(cachetime);
        url += "&cachetime=" + this.cachetime;
        jQuery.get(url, this.parse);
    },

    parse: function(data) {
        jQuery('feed', data).each(function() {
            mFeed.render(this);
        });
    },

    render: function(data) {
        var feed = jQuery(data).attr("id");
        var container = jQuery("#mFeed-" + feed);
        var html = "";
        var max = 3;
        var ctr = 0;
        jQuery('item', data).each(function() {
            if (ctr < max) {
                var title = jQuery("title", this).text();
                var link = jQuery("link", this).text();
                var len = 33;
                if( title ) {
                    var trunc = title;
                    if( trunc.length > len ) {
                        /*
                            truncate the content of the P, then go back to the
                            end of the previous word to ensure that we don't truncate
                            in the middle of a word
                        */
                        trunc = trunc.substring(0, len);
                        trunc = trunc.replace(/\w+$/, '');

                        /*
                            Add an ellipses to the end and make it a link that expands
                            the paragraph back to its original size
                        */
                        trunc += '...';
                        title = trunc;
                    }
                }

                var theLink = link;
                html += '<div class="rssitemdp"><ul><li><a href="' + theLink + '" target="_blank"><strong>' + title + '</strong></a></li></ul></div>';
            }
            ctr++;
        });
        container.html(html);
    }
}

// you must have corresponding id'ed elements on the page that 
// have mFeed-[feedName] in order for it to work.
// example: <div id="mFeed-ellen"></div>
jQuery(document).ready(function(){
    mFeed.load("extra,ellen,tyra,essence,celebbabyscoop,popeater,shineparenting,toofab"); // takes comma delimited list of feeds to load
});
</script>

