window.fbAsyncInit = function() {
  FB.init({
    appId      : '116832620224', // App ID
    channelUrl : '//www.threadless.com/channel.html', // Channel File
    status     : true, // check login status
    cookie     : true, // enable cookies to allow the server to access the session
    oauth      : true, // enable OAuth 2.0
    xfbml      : true  // parse XFBML
  });

  // Additional initialization code here
};

// Load the SDK Asynchronously
(function(d){
   var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
   js = d.createElement('script'); js.id = id; js.async = true;
   js.src = "//connect.facebook.net/en_US/all.js";
   d.getElementsByTagName('head')[0].appendChild(js);
 }(document));

function thFBShortCut() {
	FB.login(function(response) {
   if (response.authResponse) {
	window.location = 'http://www.threadless.com/facebookauth/?ref=/product/2552/Mozart_Decomposing_Since_1791/tab,girls';
   }
   }, {scope: 'email,user_about_me,user_hometown,user_interests,user_likes,user_status,user_website,user_birthday,publish_stream,publish_actions,offline_access'});
}

function thFBLogin() {
	//$('.popbox').fadeOut();	
	FB.getLoginStatus(function(response) {
		if(response.status == 'connected') {
			window.location = 'http://www.threadless.com/facebookauth/?ref=/product/2552/Mozart_Decomposing_Since_1791/tab,girls';
		}
	});
}

</script>
