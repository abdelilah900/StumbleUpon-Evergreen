			//<![CDATA[
	
			function vProduct_packageProductsForm_productFormSubmitHandler_1()
			{
				var suffixes = "1".split(/,/);
				for (var i = 0; i < suffixes.length; i++)
				{
					if (vProduct_packageProductsForm_productFormObj_1['quantity_' + suffixes[i]].getValue() * 1 > 0)
					{
						return true;
					}
				}
				alert("You must enter a quantity for at least one item.");
				return false;
			}

			var vProduct_packageProductsForm_productFormObj_1 = new qForm("vProduct_packageProductsForm_productForm_1");
			vProduct_packageProductsForm_productFormObj_1.suffixList.description = "Product";
			vProduct_packageProductsForm_productFormObj_1.suffixList.required = true;
			vProduct_packageProductsForm_productFormObj_1.quantity_1.required = true;
vProduct_packageProductsForm_productFormObj_1.quantity_1.validateInteger();
vProduct_packageProductsForm_productFormObj_1.quantity_1.validateRange(0,20,'The quantity must be a valid number between 1 and 20.');

			//]]>
		</script>
		
