<script type="text/javascript">
	//<![CDATA[
		
			function setElementsVisibility(eContainer, aTagNames, sVisibility)
			{
				var tmp, i, j;
				var elements = [];
				for (i = 0; i < aTagNames.length; i++)
				{
					tmp = eContainer.getElementsByTagName(aTagNames[i]);
					for (j = 0; j < tmp.length; j++)
					{
						elements[elements.length] = tmp[j];
					}
				}
				for (i = 0; i < elements.length; i++)
				{
					elements[i].style.visibility = sVisibility;
				}
			}
			function getScrollingOffset()
			{
				var x,y;
				if (self.pageYOffset) // all except Explorer
				{
					x = self.pageXOffset;
					y = self.pageYOffset;
				}
				else if (document.documentElement && document.documentElement.scrollTop) // Explorer 6 Strict
				{
					x = document.documentElement.scrollLeft;
					y = document.documentElement.scrollTop;
				}
				else if (document.body) // all other Explorers
				{
					x = document.body.scrollLeft;
					y = document.body.scrollTop;
				}
				return {x:x,y:y};
			}
			if (document.getElementById)
			{
				var vProduct_detailImageZoom_container_1 = document.getElementById("vProduct-detailImageZoom-container-1");
				var vProduct_detailImageZoom_image_1 = document.getElementById("vProduct-detailImageZoom-image-1");
				var vProduct_detailImageZoom_viewImagesContainer_1 = document.getElementById("vProduct-detailImageZoom-viewImagesContainer-1");
				if (vProduct_detailImageZoom_viewImagesContainer_1)
				{
					vProduct_detailImageZoom_viewImagesContainer_1.onclick = function(oEvent)
					{
						oEvent = oEvent || window.event;
						var e = (oEvent.target || oEvent.srcElement).parentNode;
						if (e.tagName.toLowerCase() == "a")
						{
							if (vProduct_detailImageZoom_image_1)
							{
								vProduct_detailImageZoom_image_1.src = e.href;
							}
							return false;
						}
					}
				}
				var vProduct_detailImageZoom_controlClose_1 = document.getElementById("vProduct-detailImageZoom-controlClose-1");
				if (vProduct_detailImageZoom_controlClose_1)
				{
					vProduct_detailImageZoom_controlClose_1.onclick = function()
					{
						vProduct_detailImageZoom_container_1.style.display = "none";
						setElementsVisibility(vProduct_detailImageZoom_container_1.parentNode, ['select'], 'visible');
					}
				}
				function detailImageZoom(src)
				{
					if (document.getElementById)
					{
						if (src)
						{
							vProduct_detailImageZoom_image_1.src = src;
						}
						setElementsVisibility(vProduct_detailImageZoom_container_1.parentNode, ['select'], 'hidden');
						vProduct_detailImageZoom_container_1.style.display = "block";
						var scrollingOffset = getScrollingOffset().y;
						var parentOffset = vProduct_detailImageZoom_container_1.parentNode.offsetTop;
						tmp = scrollingOffset - parentOffset + 31;
						if (tmp < 31)
						{
							tmp = 31;
						}
						vProduct_detailImageZoom_container_1.style.top = tmp + "px";
					}
					else
					{
						if (src)
						{
							window.location = src;
						}
					}
				}
			}
		
	//]]>
</script> 

