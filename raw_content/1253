<script type="text/javascript">
	//<![CDATA[
		
			function setElementsVisibility(eContainer, aTagNames, sVisibility)
			{
				var tmp, i, j;
				var elements = [];
				for (i = 0; i < aTagNames.length; i++)
				{
					tmp = eContainer.getElementsByTagName(aTagNames[i]);
					for (j = 0; j < tmp.length; j++)
					{
						elements[elements.length] = tmp[j];
					}
				}
				for (i = 0; i < elements.length; i++)
				{
					elements[i].style.visibility = sVisibility;
				}
			}
			function getScrollingOffset()
			{
				var x,y;
				if (self.pageYOffset) // all except Explorer
				{
					x = self.pageXOffset;
					y = self.pageYOffset;
				}
				else if (document.documentElement && document.documentElement.scrollTop) // Explorer 6 Strict
				{
					x = document.documentElement.scrollLeft;
					y = document.documentElement.scrollTop;
				}
				else if (document.body) // all other Explorers
				{
					x = document.body.scrollLeft;
					y = document.body.scrollTop;
				}
				return {x:x,y:y};
			}
			if (document.getElementById)
			{
				var vProduct_simpleImageZoom_container_1 = document.getElementById("vProduct-simpleImageZoom-container-1");
				var vProduct_simpleImageZoom_image_1 = document.getElementById("vProduct-simpleImageZoom-image-1");
				var vProduct_simpleImageZoom_controlClose_1 = document.getElementById("vProduct-simpleImageZoom-controlClose-1");
				vProduct_simpleImageZoom_container_1.onclick = function()
				{
					vProduct_simpleImageZoom_container_1.style.display = "none";
					setElementsVisibility(vProduct_simpleImageZoom_container_1.parentNode, ['select'], 'visible');
					vProduct_simpleImageZoom_image_1.src = "/resources/_shared/images/transparent.gif";
				}
			}
			function simpleImageZoom(src)
			{
				if (document.getElementById)
				{
					vProduct_simpleImageZoom_image_1.src = src;
					setElementsVisibility(vProduct_simpleImageZoom_container_1.parentNode, ['select'], 'hidden');
					vProduct_simpleImageZoom_container_1.style.display = "block";
					var scrollingOffset = getScrollingOffset().y;
					var parentOffset = vProduct_simpleImageZoom_container_1.parentNode.offsetTop;
					tmp = scrollingOffset - parentOffset + 30;
					if (tmp < 30)
					{
						tmp = 30;
					}
					vProduct_simpleImageZoom_container_1.style.top = tmp + "px";
				}
				else
				{
					window.location = src;
				}
			}
		
	//]]>
</script>
<div id="vProduct-detailImageZoom-container-1" class="vProduct-detailImageZoom-container">
