            function submitSearchForm_1238210449(formEle) {
                var input = formEle.elements[0].value;
                var pattern = /\S+/;
                var notEmpty = pattern.test(input);

                if (notEmpty && input != "Search PopSugar Network") {
                    // only submit the form when a non-whitespace value, other than the default
                    // text, is found in the search field
                    formEle.submit();
                }

                return false;
            }
