    FB.Event.subscribe('xfbml.render', function(response) {
      var e = document.createElement('script');
      e.src = document.location.protocol +
        '//www.facebook.com/connect.php/js/FB.Share';
      e.async = true;
      document.getElementById('fb-root').appendChild(e);
      setTimeout('checkFacebookShareCounts()', 1000);
    });
    ONSUGAR.Event.observe(window, 'load', function() {
        FB.Event.subscribe('edge.create', function(response) {
            if(response) {
                if(typeof this.window['facebookEnqueue'] == 'function') {
                    var parts = response.split('-');
                    var nid = '';
                    if(parts.length>1) {
                        nid = parts[parts.length-1];
                    }
                    facebookEnqueue(nid);
                }
            }

          });
    } );
  };

    function checkFacebookShareCounts() {
    var getByClass = function(c) {
        var nodes = [];
        var elements = document.getElementsByTagName('*');

        for (var i = 0, len = elements.length; i < len; i++) {
            if (elements[i].className.indexOf(c) >=0 ) {
                nodes.push(elements[i]);
            }
        }
        return nodes;
    };
    var fbShares = getByClass('fb_share_count_inner');
    if(fbShares.length > 0) {
      for (var index = 0, len = fbShares.length; index < len; ++index) {
        var item = fbShares[index];
        if (item.innerHTML == '&nbsp;') {
          item.innerHTML = '0';
        }
      }
    }
}

