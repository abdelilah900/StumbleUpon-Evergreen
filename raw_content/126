<script language="javascript">
$().ready(function() {
$('.cnm').hover(
function() {
$(this).addClass('cnm_over');
}, function() {
$(this).removeClass('cnm_over');
}
);
$('.cnl').hover(
function() {
$(this).addClass('carousel_nav_hovered');
}, function() {
$(this).removeClass('carousel_nav_hovered');
}
);
// mini subscription
$.fn.clearfield = function() {
return this.focus(function() {
if( this.value == this.defaultValue ) {
this.value = "";
}
}).blur(function() {
if( !this.value.length ) {
this.value = this.defaultValue;
}
});
};
$("#EMAIL").clearfield();
$("#newsletter_subscription").submit(function(){
var my_email = $("#EMAIL").val();
var my_cid = $("#CID").val();
var mini_form_url = '/newsletter/mini.phtml?email='+my_email+'&cid='+my_cid;
var sec_form_url = '/newsletter/secondary.phtml?email='+my_email+'&cid='+my_cid;
$("#newsletter_subscription_response").load(mini_form_url,function() {
$("#mini_sub").hide();
window.open(sec_form_url,'secondary_form','menubar=1,resizable=1,width=750,height=550');
});
return false;
});
// end mini subscription
});
</script>
