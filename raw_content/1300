    function changeCity(city) {
        var dataString = "city=" + city;

        $.ajax({
            type: "POST",
            url: location.protocol + "//" + location.host + "/changeCity.php",
            data: dataString,
            cache: false,
            success: function(data){
                $.fancybox.close();
                window.location.href = "http://" + location.host + "/" + city + "/all-buys";
            },
            error: function() {
            }
        });
    }

    function changeLanguage(lang) {

        var language = lang;

        var dataString = 'language=' + language;

        $.ajax({
            type: "POST",
            url: location.protocol + "//" + location.host + "/changeLanguage.php",
            data: dataString,
            cache: false,
            success: function(data){
                window.location.href = "";
            },
            error: function() {
                // TODO
            }
        });
    }
	
    $('#subscribe').click(function(){
        var city = $("select[name=city]").val(),
        email = $("input[name=email]").val(),
        errmsg='',
        frm=null
        displayError = function(msg){
            $.fancybox(
                "<div class='lightbox'><h3>Subscribing</h3><p>"+msg+"</p></div>",
                {
                'autoDimensions'	: false,
                'width'         		: 350,
                'height'        		: 'auto',
                'transitionIn'		: 'none',
                'transitionOut'		: 'none'
                }
            );		
        };
        if( !city || city ==='undefined'){
            errmsg = "Please Select a city";
        }
        if( !email || email ==='undefined'){
            errmsg += (errmsg?" and":"Please")+" enter an email address";
        }
        if(errmsg){
            displayError("Error "+errmsg);
        }else{
            frm = $(this).parents('form');

            $.post($(frm).attr('action')
                ,{'city':city,'email':email}
                ,function(data){
                    if(data.length){
                        data = $.parseJSON( data );
                        if(data.error){
                            displayError("Error "+data.error.message);
                        }else{
                            displayError("Your subscription was sucessful");
                        }
                    }else{
                        $.fancybox(
                            "<div class='lightbox'><h3>Subscribing</h3><p>Your subscription was sucessful</p></div>",
                            {
                            'autoDimensions'	: false,
                            'width'         		: 350,
                            'height'        		: 'auto',
                            'transitionIn'		: 'none',
                            'transitionOut'		: 'none'
                            }
                        );
                        $("#header-close").click();
                    }
                });
        }
        return false;
    });

    function facebook_connect(response)
    {
        if( response.authResponse )
        {
            $.ajax(
                { url: location.protocol + "//" + location.host + "/account/facebook"
                , dataType: 'json'
                , cache: false
                , data: {'access_token':response.authResponse.accessToken}
                , type: "POST"
                , success: function( data )
                  {
                      if( data.error )
                      {
                          switch( data.error.code )
                          {
                            case 31300:
                            case 31301:
                                // TODO: Display error message
                                break;
                            case 31302:
                                // Offer to link emails
                                FB.api('/me', function(response) {
                                    window.location.href = "https://www.teambuy.ca/signIn.php?tb_exist=" + response.email + "&fbuid=" + response.id;
                                });
                                break;
                            case 31303:
                                // Offer to create an account
                                window.location.href = "https://www.teambuy.ca/signIn.php?register=true";
                                break;
                            default:
                                // Unknown Error
                                break;
                          }
                      }
                      else
                      {
                          window.location.href = "";
                      }
                  }
                , error: function()
                  {
                      // TODO: Display error message
                  }
                }
            );
        }
        else
        {
            FB.logout(function(response) {
               //console.log('Logged out.');
            });
        }
    }

    $('.fb-login-button').on( 'click', function(e) {
        FB.Event.subscribe('auth.authResponseChange', facebook_connect);
        FB.getLoginStatus(facebook_connect);
    });


    /**
     * Logout of Facebook when logging out of Teambuy, as required in their
     * policy and as required for out integration to work correctly (Anson Kao)
     *
     */
    $("#menu-item-logout").click(function(){
        FB.logout();
        return true;
    });
