  <script>
$(document).ready(function() {

    $("#header-close").click(function(){
        $("#header-subscribe").fadeTo(300, .1);
        $("#header-subscribe").slideUp(300);
        setCookie("headerSubscribe",true);
        return false;
    });

    function setCookie(c_name,value,exdays)
    {
        var exdate=new Date();
        exdate.setDate(exdate.getDate() + exdays);
        var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
        document.cookie=c_name + "=" + c_value;
    }

    $(".fancybox").fancybox({
        "speedIn":0,
        "titleShow":false,
        "scrolling":"no",
        "showCloseButton":true,
        "autoDimensions":true,
        "padding":0
    });

    $(".fancyboxNoPad").fancybox({
        "speedIn":500,
        "titleShow":false,
        "scrolling":"no",
        "showCloseButton":true,
        "autoDimensions":true,
        "padding":0
    });


    $(".fancyboxRefresh").fancybox({
        "speedIn":500,
        "titleShow":false,
        "scrolling":"no",
        "showCloseButton":true,
        "autoDimensions":true,
        "padding":0,
        "onClosed":function(){
            window.location.href = "";
        }
    });

    $("#changeEnglish").click(function(){
        changeLanguage('english');
    });

    $("#changeFrench").click(function(){
        changeLanguage('french');
