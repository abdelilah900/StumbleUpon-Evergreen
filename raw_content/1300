    	try{
    		tb_geo_locate.initialize(window.location.pathname == "/");
    	}catch(e){
    		//console.warn('failing gracefully');
    	}
    }function countdown(end_date, format, element)
{
     Today = new Date();
     Todays_Year = Today.getFullYear();
     Todays_Month = Today.getMonth();

     //Convert today's date into miliseconds.
     Todays_Date = (new Date(Todays_Year, Todays_Month, Today.getDate(),
                             Today.getHours(), Today.getMinutes(), Today.getSeconds())).getTime();

     //Find their difference, and convert that into seconds.
     Time_Left = Math.round((end_date - Todays_Date) / 1000);

     if(Time_Left < 0)
        Time_Left = 0;

     var innerHTML = '';

     switch(format)
       {
       case 0:
            //The simplest way to display the time left.
            innerHTML = Time_Left + ' seconds';
            break;
       case 1:
            //More datailed.
            days = Math.floor(Time_Left / (60 * 60 * 24));
            Time_Left %= (60 * 60 * 24);
            hours = Math.floor(Time_Left / (60 * 60));
            Time_Left %= (60 * 60);
            minutes = Math.floor(Time_Left / 60);
            Time_Left %= 60;
            seconds = Time_Left;

            innerHTML = "";

            if (days != 0)
                innerHTML = days + 'd ';
            innerHTML += hours + 'h ';
            innerHTML += minutes + 'm ';
            innerHTML += seconds + 's';
            break;
       default:
            innerHTML = Time_Left + ' seconds';
       }

       $('#'+element).html(innerHTML);

     //Recursive call, keeps the clock ticking.
     setTimeout(function(){ countdown(end_date, format, element) }, 1000);
}/* 
 * To change this template, choose Tools | Templates
 * and open the template in the editor.
 */

/**
 * Countdown for the more buys deals
 */

get_countdown();

function get_countdown()
{
    $('input[id|="deal_id"]').each(function() {
        var id = this.value;
        var end_date = $('#deal_end_date-'+id).val();
        end_date = end_date * 1000; //In milliseconds for javascript
        countdown(end_date, 1 ,"deal_timer-" + id);
    });
}$("#close-announcement").click(function(){
    $("#announcement-container").fadeTo(300, .1);
    $("#announcement-container").slideUp(300);
    var date = new Date();
    date.setTime(date.getTime()+(30*24*60*60*1000))
    document.cookie = 'announcement=true; expires='+date.toGMTString()+'; path=/'
    return false;
});

$("#banner_top_close_button").click(function(){
    $("#banner_container").fadeTo(300, .1);
    $("#banner_container").slideUp(300);
    var date = new Date();
    date.setTime(date.getTime()+(30*24*60*60*1000))
    document.cookie = 'close_banner=true; expires='+date.toGMTString()+'; path=/'
    return false;
});});
