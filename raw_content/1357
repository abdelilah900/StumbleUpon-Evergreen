  /* iPad Redirect */
  var regex = new RegExp("Mozilla/5.0(.*)AppleWebKit/(.*) \\(KHTML, like Gecko\\) Version/.* Mobile/(.*) Safari/(.*)"); 
  var matches = regex.exec(navigator.userAgent); 
  if (matches && matches[1].indexOf("(iPad") == 1) {
		var isExpired = getCookie("ipadInterstitial");
		var today = new Date();
		var deltaPeriod = (1 * 24 * 60 * 60 * 1000);
		today.setTime( today.getTime() );
		if(isExpired != "" && isExpired < today.getTime()) {
			setCookie("ipadInterstitial", today.getTime() + deltaPeriod, deltaPeriod);
			window.location="http://www.nypost.com/ipadredirect";
		} else if(isExpired == "") {
			setCookie("ipadInterstitial", today.getTime() + deltaPeriod, deltaPeriod);
			window.location="http://www.nypost.com/ipadredirect";
		}
  }

  // check for iphone
  regex = new RegExp("Mozilla/5.0(.*)AppleWebKit/(.*) \\(KHTML, like Gecko\\) Version/.* Mobile/(.*) Safari/(.*)"); 
