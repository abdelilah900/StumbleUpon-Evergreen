$.getJSON('http://api.chartbeat.com/toppages/?host=nypost.com&limit=10&apikey=ce554dfaa830f3c9e12b5a377253edaa&jsonp=?', function(data) {
	var i = 0;
	var maxItems = 5;
	for(i = 0; i < data.length; i++) {
		var url = data[i].path;
		var title = data[i].i;
		title = title.replace("- NYPOST.com", "");
		title = title.replace("- N", "");
		title = title.replace("'", "&apos;");
		if(url.indexOf('/p/') == 0 && maxItems > 0) {
	      	if ( maxItems == 5 ) {
	      		$("<li>").attr("class", "first").attr("onclick", "href('" + url + "')").html('<a href="'+url+'">'+title+'</a>').appendTo("ol#most_read");
	      	} else if ( maxItems == 1 ) {
	      		$("<li>").attr("class", "last").attr("onclick", "href('" + url + "')").html('<a href="'+url+'">'+title+'</a>').appendTo("ol#most_read");
