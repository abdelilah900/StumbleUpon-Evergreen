<script type="text/javascript">
var arrCount=0;
var img_id = 1;
function rotateAddBanners()
{
var strImg= new

Array("images/advertisement.jpg","images/advertisement01.jpg","images/advertisement04.jpg","images/advertisement05.jpg","images/advertisement02.jpg","images/advertisement07.jpg");
var hrefString = new Array("http://www.myonlinecareerspace.com","http://www.10biggestresumemistakes.com","http://www.10biggestinterviewmistakes.com","http://www.myonlinecareercoach.com","http://www.itsnotyouitsyourresume.com","http://www.thehighposts.com");
$("#img"+img_id).fadeOut("slow");
document.getElementById("img"+img_id).src = strImg[arrCount];
$("#img"+img_id).fadeIn("slow");
document.getElementById("imga_"+img_id).href=hrefString[arrCount];
if (arrCount < strImg.length-1){
arrCount++;
} else {
arrCount = 0;
//alert("one is round over!")
}
if(img_id == 2 )
img_id = 1;
else
img_id++;
}
function removeArrayElement(arrayName,arrayElement)
{
     for(var i=0; i<arrayName.length;i++ )
     { 
         if(arrayName[i]==arrayElement)
         {
             arrayName.splice(i,1); 
             return i;
         }
      } 
}
function in_array(arrName,arrValue)
{
      for(var i=0; i<arrName.length;i++ )
      { 
          if(arrName[i]==arrValue)
         {
        return true;         
         }
     }
     return false; 
}
</script>


