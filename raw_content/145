                        function() {
                   if (!CN.isMobile) {
                       return '/js/livefyre/cn.livefyre.js';
                   } else {
                       return '';
                   }
                }
                    )

    .wait(function(){
                CN.videoplaylist.playlistReady();
                CN.promises.scripts.livefyre.resolve();
                    jQuery(function(){
                        if(!CN.isMobile){
                           if(jQuery("#livefyre").length !== 0){
                                        // Render Lifefyre Comment Stream
                                        CN.livefyre.main.loader().loadLivefyre();
                           }
                           if(jQuery(".livefyre-commentcount").length !== 0){
                                        // Render Livefyre Comment Count
