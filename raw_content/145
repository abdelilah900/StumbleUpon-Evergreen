    <script type="text/javascript">
         /*
          *  Promotheus init
          *  Sets up the promotheus url to be fetched.
          */

        window._ghearst_vars = (function() {
        var
            metaKeyWords = (function(){
                var
                    i                  = 0,
                    metaKeywordContent ='',
                    nameAttribute      = null,
                    metaCollection     = document.getElementsByTagName('meta');

                for (i = 0; i < metaCollection.length; i++) {
                    nameAttribute = metaCollection[i].name.search(/keywords/);
                    if (nameAttribute!= -1) {
                        metaKeywordContent = metaCollection[i].content;
                        metaKeywordContent = metaKeywordContent.replace(/,/g,'%2C'); // to replace ,
                        metaKeywordContent = metaKeywordContent.replace(/ /g,''); // to replace space
                    }
                }
                return metaKeywordContent;
            })();
        return {
            'ams_ads_script_src': 'http://subscribe.glamour.com/ams/page-ads.js?ad_category_prefix=&browser_path=%2F&cat_prefixes=%2C&keywords='+metaKeyWords+'&position_list=AMS_GLM_GLOBAL_INTER%2CAMS_GLM_GLOBAL_LEFTNAV%2CAMS_GLM_GLOBAL_HEADERCALLOUT%2CAMS_GLM_GLOBAL_FOOTER%2CAMS_GLM_MOBILE_GLOBAL_FOOTER%2CAMS_GLM_TABLET_GLOBAL_HEADERCALLOUT%2CAMS_GLM_CONTENTRIVER_TOP%2CAMS_GLM_CONTENTRIVER_BOTTOM%2CAMS_GLM_MOBILE_CONTENTRIVER_TOP%2CAMS_GLM_MOBILE_CONTENTRIVER_BOTTOM&site_prefix=glamour&subdomain=www&url_name='
        };
    })();

        


















    



    $LAB.setOptions({})


    .script(window._ghearst_vars.ams_ads_script_src)


    ;



        /*
         *  Asynchronous script loading
         *  Use LAB.js to load resources in parallel, utilizing wait() to pause
         *  for dependencies and configuration.
         */

        


















    
    



    $LAB.setOptions({AlwaysPreserveOrder : true})


    .script(['/js/jquery.min.js'])

    .wait(function() {
                    CN.promises.scripts = {
                        base       : new jQuery.Deferred(),
                        ads        : new jQuery.Deferred(),
                        ecom       : new jQuery.Deferred(),
                        stats      : new jQuery.Deferred(),
                        livefyre   : new jQuery.Deferred(),
                        site       : new jQuery.Deferred(),
                        brightcove : new jQuery.Deferred(),
                        typekit    : new jQuery.Deferred(),
                        pinterest  : new jQuery.Deferred(),
                        last       : new jQuery.Deferred()
                    };

                    CN.promises.render = {
                        ecom       : new jQuery.Deferred()
                    };
                })



        


















    
        
    
    



    $LAB.setOptions({})


    .script(['/js/glamour_global.js'])

    .wait(function() {
                    CN.promises.scripts.base.resolve();
                })


        


















    




    .script(
            function() {
                if (CN.isMobile) {
                    return ["/js/cn-fe-ads/v2/cn.ads.js"];
