                    return ["/js/cn-fe-ads/cn.dart.js"];
                }
            }
        )



        


















    




    .script(
                function() {
                    if (CN.isMobile) {
                        return "/js/cn-fe-ads/v2/cn.ads.admarvel.js";
                    } else {
                        return ["/js/cn-fe-ads/cn.ad.modifiers.js",
                                "/js/cn-fe-ads/cn.ad.lotame.js",
                                "/js/cn-fe-ads/cn.ad.yieldex.js"];
                    }
                }
            )

    .wait(function() {
                    CN.promises.scripts.ads.resolve();
                })


        
            


















    
    




    .script()

    .wait(function() {
                        CN.promises.scripts.typekit.resolve();
                    })


        
        


















    
    




    .script(['/js/lib/cn-fe-stats/v3/s_code.js',
                    '/js/eventTracker.js',
