<script>
  //<![CDATA[
    $(document).ready(function() {
    if(Cookie.exists("user_credentials")) {
    	$("#session_block").load('/session', function(response, status) {
    		$("#session_block").show();			
    		if(status=='error') {
    			$("#session_block").html("<center>Sorry - a login error occured</center>");
    			$("#session").css('background-color','#C33');
    			$("#session").css('background-image','none');
    		} else {
    		$('.dropdown-menu').hover(function() {
    	  		$(this).addClass('hover');
    			}, function() {
    	  		$(this).removeClass('hover');
    		});
    		if(fb_loaded) {
    		
    		FB.XFBML.parse(document.getElementById("session_block"));
    		}
    		}
    	})
    } else {
    	$("#session_block").show();
    	if(fb_loaded) {
    		FB.XFBML.parse(document.getElementById("session_block"));
    	}
    	
    }
    })
  //]]>
</script>
