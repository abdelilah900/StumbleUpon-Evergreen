function thFBShortCut() {
	FB.login(function(response) {
	   if (response.authResponse) {
			window.location = 'http://www.threadless.com/facebookauth/?ref=/submission/447021/Football';
		}
	}, {scope:'email,user_about_me,user_hometown,user_interests,user_likes,user_status,user_website,user_birthday,publish_stream,publish_actions,offline_access'});
}

function thFBLogin() {
	//$j('.popbox').fadeOut();
	FB.getLoginStatus(function(response) {
		if(response.status == 'connected') {
			window.location = 'http://www.threadless.com/facebookauth/?ref=/submission/447021/Football';
		}
	});
}
</script>
