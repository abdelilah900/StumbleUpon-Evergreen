    function previewComment(formName){
        theForm = document.getElementById(formName);
        theDiv = document.getElementById('comment-preview-sfl-edge-message-board');
        currentDate = new Date();
        hours = currentDate.getHours()
        minutes = currentDate.getMinutes();
        if(minutes < 10) {
            minutes = '0' + minutes;
        }
        if(hours > 12) {
            hours = hours - 12;
            end = ' PM'
        } else {
            end = ' AM'
        }
        theDiv.innerHTML = '<p class="preview">Preview:</p><p class="comment-text">' + theForm.comments.value + '</p><p class="username">' + theForm.username.value + ' @ <span class="date">' + hours + ':' + minutes + end + ' , Apr 6, 2011</span></p>';
        theDiv.className = "response";
    }
