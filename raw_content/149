    function validateGraffiti (theForm) {
        if (theForm.username.value == '') {
            alert('You must supply a name.');
            theForm.username.focus();
            return false;
        }

        if (theForm.comments.value == '') {
            alert('You must supply some comments.');
            theForm.comments.focus();
            return false;
        }

        if (theForm.comments.value.length > 300) {
