<script type="text/javascript" language="javascript">
//<![CDATA[
    $j(document).ready(function() {
        var this_url = self.location.href.toString();
        $j('.newsletter-signup #sheet #joinNewsletter input:text').unbind('blur').unbind('focus').val('');
        $j('.newsletter-signup #sheet #joinNewsletter input:button').unbind("click").click(function() { optInNewsletter('.newsletter-signup #sheet #selectAge', '.newsletter-signup #sheet #newsletterEmail', (this_url.match("newsletter-signup") != "newsletter-signup" ? "Footer" : (this_url.match("utm_") == "utm_" ? "Rainbow_Ad" : "Organic"))); });
        $j('#footer .interact input:button').unbind("click").click(function() { optInNewsletter('#footer .interact #selectAge', '#footer .interact #newsletterEmail', (this_url.match("newsletter-signup") != "newsletter-signup" ? "Footer" : (this_url.match("utm_") == "utm_" ? "Rainbow_Ad" : "Organic"))); });
        $j('.open-newsletter-modal').simpleLightbox({
            lightboxContentBlock: '.newsletterModal',
            closeLink: 'a.close-newsletter-modal'
        });
    });
	

	function optInNewsletter(ctrlage, ctrlemail, source, divSuccess, divForm)
	{
	    var age = $j(ctrlage).val();
	    var optInSucess = false;
	    if (age == "18-34" || age == "35-49" || age == "50plus" || age == "")
	    {
	        var re = new RegExp("^[A-Z0-9._%-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$");
	        var email = $j(ctrlemail).val().toUpperCase();
            
            if (email.match(re) != null && email.replace(' ', '') != '')
            {
                if (age == "")
                {
                    $j(".newsletterModal .sectionHead h3").html("<span> We're Sorry</span> We can't complete your subscription");
	                $j(".newsletterModal .body p").html("Please select your age.");
                }
                else
                {
                    
                    //var code = (this_url.match("newsletter-signup") != "newsletter-signup" ? "Footer" : (this_url.match("utm_") == "utm_" ? "Rainbow_Ad" : "Organic"));
                    var code = source;
                    $j.get(getUrlPrefix() + "/webservice/silverpop.asmx/NewsletterOptIn?email=" + email + "&host=" + "www.tablespoon.com" + "&code=" + code, function(data) { setEmailOptInPageView(data) });
                   
                    setGoogleEvent('MMA', 'Newsletter', 'newsletter-opt-in 2012_3_MMA');
                    if($j('#log').text() != 'True')
                        setGoogleEvent('email signup', 'Newsletter', 'anonymous');
                   
                    $j("#newsletterEmail, #newsletterEmailMMA").val("");
	            
	                $j(".newsletterModal .sectionHead h3").html("You're on the list!");
	                $j(".newsletterModal .body p").html("You’re on the list! Thanks for signing up for Tablespoon newsletters.");
	                optInSucess = true;
	            }
	        }
	        else
	        {
	            $j(".newsletterModal .sectionHead h3").html("<span>We're Sorry</span> We can't complete your subscription");
	            $j(".newsletterModal .body p").html("Please enter a valid e-mail.");
	        }
	        
	    }
	    else
	    {
	        $j("#newsletterEmail, #newsletterEmailMMA").val("");
	        $j(".newsletterModal .sectionHead h3").html("<span>We're Sorry</span> We can't complete your subscription");
	        $j(".newsletterModal .body p").html("You’re currently not eligible to sign up. But if you’re looking for inspiration, you can always check out our <a href='/top-rated-recipes/'>Top Recipes</a>.");
	    }
	    if (typeof divSuccess != 'undefined' && optInSucess == true) {
	        $j(divSuccess).removeClass('hidden');
	        if (typeof divForm != 'undefined')
	            $j(divForm).addClass('hidden');
	    }
	    else
	        $j(".open-newsletter-modal").click();
	}

	function setEmailOptInPageView(data)
	{
	    var s = $j(data).find("string").text();
	    if (s != "")
	    {
	        setGooglePageView("/email-signup-optin/" + s);
	    }
	}
//]]>
</script>

