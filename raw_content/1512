<script type="text/javascript">
    //<![CDATA[
    var recipesPage = 2;
    var recipesFullyLoaded = false;
    $j(document).ready(function()
    {
        $j('.newRecipes .expand a').click(function() { checkrecipesExpand(this) });
    });

    function checkrecipesExpand(obj)
    {
        var mover = $j('.newRecipes .expandMover');
        var children = $j('.newRecipes .expandMover > ul').length;
        if ($j(obj).hasClass('left'))
            moveSideModule(mover, 'l', 300, children);
        else
        {
            if (mover.width() + parseInt(mover.css('left').replace('px', '')) <= 300)
                $j.get(getUrlPrefix() + '/webservice/recipes.asmx/GetNewRecipesPage?page=' + recipesPage, getRecipesCallback);
            else
                moveSideModule(mover, 'r', 300, children)
        }
    }

    function getRecipesCallback(data)
    {
        recipesPage++;
        ajaxExpandCallback(data, $j('#moreNewRecipes'), 'ul', '#moreNewRecipes', $j('.newRecipes .expandMover'), 300, $j('.newRecipes .expandMover > ul').length);
    }
    //]]>
</script></div>
