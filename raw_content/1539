    function ChangeColor(tableRow, highLight)
    {
    if (highLight)
    {
      tableRow.style.backgroundColor = '#d8d8be'; tableRow.style.cursor="hand";
    }
    else
    {
      tableRow.style.backgroundColor = '#f7f7e6';
    }
  }

  function DoNav(theUrl)
  {
  document.location.href = theUrl;
  }
