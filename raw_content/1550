	for (var i=0;i<=4;i++)
	{
		var title = data.stories[i].title
		if (title.length > maxlen) title = title.substring(0,maxlen).replace(/^(.*) (.*?)$/,"$1")+"...";
		trackingPosition = i + 1;
		html += '<li><a href="' + data.stories[i].link + '?csp=ipmps" title="' + data.stories[i].title + '" onclick="s_objectID=\'usat_insidePage|MostPopular|popular-news|'+trackingPosition+'\'">'+ title + '</a></li>'+"\n";
		if (i == 2) html += '</ul><ul class="addPopularStories">';
	}
	html += '</ul>';
	document.getElementById('mostpopularstories').innerHTML=html;
</script><!-- /EdSysObj -->
<!-- /EdSysObj -->

