<script type="text/javascript">
function atEventHandler(evt) {
    switch (evt.type) {
        case "addthis.menu.open":
			uoTrack('ipAddThis' + evt.data.pane);
            break; 
        case "addthis.menu.share":
            uoTrack('ipAddThisShare:' + evt.data.service);
			break;
        default:
            console.log('received an unexpected event', evt);
	} 
} 
addthis.addEventListener('addthis.menu.open', atEventHandler); 
addthis.addEventListener('addthis.menu.close', atEventHandler); 
addthis.addEventListener('addthis.menu.share', atEventHandler); 
</script><!-- /EdSysObj -->
