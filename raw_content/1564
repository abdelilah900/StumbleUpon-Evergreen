	<script type="text/javascript">
	//<![CDATA[
	
	$("#footer-topic-search-area input").focus(function() {
		if($(this).val() == this.defaultValue) {
			$(this).val("");
		}
		$(this).addClass('focus');
	}).blur(function() {
		if($(this).val() == "") {
			$(this).val(this.defaultValue);
			$(this).removeClass('focus');
		}
	});
	
	$("#footer-topic-search-area input").BXautocomplete("/ajax/topic-search.tn", 
		{
			max: 100,
			scrollHeight: 100,
			selectFirst: true,
			delay: 400,
			width: 255,
			minChars: 1,
			matchSubset: false,
			noResultsMessage: "No topics match the keyword"
		}
	).result(
			function(event, data, formatted) {
				window.location.href = "/" + data[1] + "/";
			}
	);
