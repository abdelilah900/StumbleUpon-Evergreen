	<script type="text/javascript">
	//<![CDATA[
	
	$("#footer-user-search-area input").focus(function() {
		if($(this).val() == this.defaultValue) {
			$(this).val("");
		}
		$(this).addClass('focus');
	}).blur(function() {
		if($(this).val() == "") {
			$(this).val(this.defaultValue);
			$(this).removeClass('focus');
		}
	});
	
	$("#footer-user-search-area input").BXautocomplete("/ajax/user-search.tn", 
		{
			max: 25,
			scrollHeight: 100,
			selectFirst: true,
			delay: 400,
			width: 255,
			minChars: 1,
			cacheLength: 1,
			matchSubset: false,
			formatItem: function(row) { return "<strong>" + row[0] + "</strong>" + "<br/><span class=\"user-search-position\">" + row[2] + "</span>"; },
			resultsClass: "ac_user_search_results",
			noResultsMessage: "No users matching"
		}
	).result(
		function(event, data, formatted) {
			window.location.href = data[1];
		}
	);
	
	//]]>
	</script>
