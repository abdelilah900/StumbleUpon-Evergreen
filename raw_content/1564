								//<![CDATA[
									$('#browse-members-area').hide();
								
									var footerBrowseArea = $('#footer-browse-area'),
										tabs = footerBrowseArea.find('.tab');
										tabSections = footerBrowseArea.find('.tab-section');
									
									footerBrowseArea.find('.tab').bind('click', function() {
										if($(this).hasClass('active-tab')) return false;
										
										tabs.toggleClass('active-tab');
										tabSections.toggleClass('active-section');
										tabSections.hide().filter('.active-section').show();
									});
								//]]>
