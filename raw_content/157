	/* Conversion Variables */
	s.campaign=s.getQueryParam("cid");
	s.eVar16=s.getQueryParam("cid");
	s.prop10=s.getQueryParam("cid");
	s.eVar17=s.getQueryParam("cid");
s.prop14 = location.href; 
s.eVar14 = location.href;

