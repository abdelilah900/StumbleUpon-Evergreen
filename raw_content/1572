function loadNextRecipe(recipeUrl,recipeid){
	if((document.getElementById("nextrecipeids").value).indexOf(recipeid) == -1){
		document.getElementById("nextrecipeids").value=document.getElementById("nextrecipeids").value+","+recipeid;
