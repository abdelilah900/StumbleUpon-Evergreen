<script language="javascript">
	function setServing(obj){
		if(isNaN(obj.value))
			return;
		if(oldServing==parseFloat(obj.value))
			return;
		document.location = "http://www.food.com/recipe/egg-free-no-bake-cookie-dough-for-eating-391880?scaleto="+obj.value+"&mode=null&st=true";
	}
	function setServingSize(inAmount){
		if(isNaN(inAmount)){
			document.getElementById("ddNewValue").value = oldServing;
			document.getElementById("ddNewValue").focus();
			return;
		}
		if(oldServing==parseFloat(inAmount)){
			document.getElementById("ddNewValue").focus();
			return;
		}
		document.location = "http://www.food.com/recipe/egg-free-no-bake-cookie-dough-for-eating-391880?scaleto="+inAmount+"&mode=null&st=true";
	}
</script>
