<script type="text/javascript">
if(document.getElementById("p2-food-home") != null){
var S_url = document.location+"";
if(S_url.indexOf("?")!=-1){
	S_url = S_url.substring(0,S_url.indexOf("?"));
}
if(S_url.indexOf("test1")!=-1){
result = "http://www.test1-food.com";
}
else if(S_url.indexOf("staging")!=-1){
result = "http://www.test2-food.com" ;
}
else{
result = "http://www.food.com";
}
document.getElementById("p2-food-home").href=unescape(result);
if(document.getElementById("p2-food-sitemap") != null) {
	result = result + "/sitemap.do";
	document.getElementById("p2-food-sitemap").href=unescape(result);
}

}
</script>

