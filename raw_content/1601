<input type="text" value="your email here" name="EMAIL" class="required email" width="120px" id="mce-EMAIL" onfocus="if (this.value == 'your email here') {this.value = '';}" onblur="if (this.value == '') {this.value = 'your email here';}"/>
</div>
<div id="mce-responses">
			<div class="response" id="mce-error-response" style="display:none"></div>
			<div class="response" id="mce-success-response" style="display:none"></div>
	  </div>
		<div><input type="submit" value="Get Recipe Updates by Email" name="subscribe" id="mc-embedded-subscribe" class="sidebarsubscribe"></div>
	</fieldset>	
</form>
