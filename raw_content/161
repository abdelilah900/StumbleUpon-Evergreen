	<script type="text/javascript" charset="utf-8">
	/* <![CDATA[ */

		$.setStaticUrl('http://cf.foodista.com/static');
		$.setSearchFocus(false);
		$.facebox.settings.closeImage = 'http://cf.foodista.com/static/images/facebox/closelabel.gif'
		$.facebox.settings.opacity = 0.2
			

	    $(document).ready(function() {
		
			// Make sidebar stretch to full height of content container
			var contentH = $('#content').height(),
				colH = $('#col3').outerHeight(true);
			if (contentH > colH) {
				$('#col3').height(contentH-20);
			}
		
			
			if ($("#search-query").val() == "") {
				showlabelen();
			}
			

			

			if(!window.location.hash && $.getSearchFocus()) {
				$('#search-query').focus();
			}

			$('#search-query').bind('click', function(event) {
				hidelabelen();
			});
			$('#search-query').bind('blur', function(event) {
				if ($('#search-query').val() == '') {
					showlabelen();
				}
	        });
			$('#search-query').bind('keypress', function(event) {
				hidelabelen();
			}); 
			
			$('.new-random-recipe').click(function() {
				var randomRecipe = $(this).parent().find(':first');
				$.callApi('content.random_recipe', {}, {onsuccess: function(packet) {
					if (packet.response) {
						var recipe = packet.response.recipe
						randomRecipe.attr('href', recipe.url);
						randomRecipe.html(
							'<img src="http://cloud.foodista.com/content/images/' + recipe.id + '_sm" />'+ 
							recipe.title
						);
					}
				}});
				return false;
			});
			
			$('#col3 .event-feed').feed({
				itemHeight: 55,
				maxVisible: 4,
				showActee: false
				
			});
			
			

			
	$('#search-results-banner')
		.slideDown()
		.find('.close')
		.click(function() { 
			$(this).parent().hide();
			return false;
		});

	

	// async file upload progress handler
	$('#upload-photo').uploadProgress({
		jqueryPath: "http://cf.foodista.com/static/js/_jquery.js",
		progressBar: '#progress-indicator',
		progressUrl: '/progress',
		start: function() {
			$("#upload-photo").children().hide();
			filename = $("#photo").val().split(/[\/\\]/).pop();
			$("#progress-filename").html('Uploading ' + filename + "...");
			$("#progress-container").show();
		},
		uploadProgressPath: "http://cf.foodista.com/static/js/jquery.uploadprogress.js",
		uploading: function(upload) {
			if (upload.percents == 100) {
				window.clearTimeout(this.timer);
				$("#progress-filename").html('Processing ' + filename + "...");
			} else {
				$("#progress-filename").html('Uploading ' + filename + ': ' + upload.percents + '%');
			}
		},
		interval: 1000
	});
	
	var photomatic_settings = {
		photoElement: '#main-photo img',
		previousControl: '#previousButton',
		nextControl: '#nextButton',
		firstControl: '#firstButton',
		lastControl: '#lastButton',
		suppressFirst: true,
		onPhotoClick: function(photo) {
			setVoteMessage(photo.id);
		}
	};

	$('#thumbs').jcarousel({
        size: imageIds.length,
		scroll: 5,
		initCallback: function(c) {
			carousel = c;
		},
		itemLoadCallback: {
			onBeforeAnimation: function(carousel, state) {
				var id,
					img,
					html = '';
				for (var i = carousel.first; i <= carousel.last; i++) {
					if (carousel.has(i)) {
						continue;
					}

					if (i > imageIds.length) {
						break;
					}
					
					id = imageIds[i-1];
					img = imageInfo[id];
					html = '<img id="' + id + '" src="' + img.thumb_url + '" width="32" height="32" alt="' + img.title + '" title="' + img.title + '"  />';

					carousel.add(i, html);
					carousel.get(i).find('img').photomatic(photomatic_settings);
				}
			}
		}
    });

	$('#thumbs img').photomatic(photomatic_settings);
	
	$('#main-photo').photovote({
		contentId: 'CYQ5RD2B',
		onSubmit: function(contentId, imageId, score) {
			imageInfo[imageId].vote = (score > 0);
			setVoteMessage(imageId);
		}
	});

	// Pre-load thumbnails
	for (object_id in imageInfo) {
	    //imageInfo[object_id]['_imagepreload'] = new Image(1,1);
	    //imageInfo[object_id]['_imagepreload'].src = imageInfo[object_id]['url'];
	    imageInfo[object_id]['_thumbpreload'] = new Image(1,1);
	    imageInfo[object_id]['_thumbpreload'].src = imageInfo[object_id]['thumb_url'];
	}
	
	

	$('#photo-module .show-upload').click(function() {
		$('#upload-photo').show();
		return false;
	});
	
	$('.lightbox').fancybox({
		type: 'image'
	});
	
	$.toolbox({
		objectId: 'CYQ5RD2B',
		userId: '',
		ratingAvg: 0.0,
		ratingRedirect: '/signin?next=/recipe/CYQ5RD2B/copycat-krispy-kreme-doughnuts',
		contentType: 'recipe',
		contentTitle: 'Copycat Krispy Kreme Doughnuts'
	});
		

	    });

	/* ]]> */
	</script>

	
	

<script type="text/javascript">
/* <![CDATA[ */

function setVoteMessage(imageId) {
	
	var personal = false;
	
	if (imageInfo[imageId].vote === undefined) {
		$('.photo-voted').hide();
		$('.photo-vote').show().find('a').attr('href', imageId);
		return;
	}

	$('.photo-vote').hide();

	var html = !personal ? 'Photo helpful? ' : 'Moved ';

	if (imageInfo[imageId].vote == true) {
		html += (!personal ? '<img src="http://cf.foodista.com/static/images/photo_yes.png" width="12" height="9" alt="Photo Helpful" title="Photo Helpful" /> You voted yes' : 'up');
		$('.photo-voted').html(html).show();
	} else {
		html += (!personal ? '<img src="http://cf.foodista.com/static/images/photo_no.png" width="9" height="9" alt="Photo Not Helpful" title="Photo Not Helpful" /> You voted no' : 'down');
		$('.photo-voted').html(html).show();
	}
}

imageIds = [];
imageInfo = {};


imageIds[0] = 'c75dbd2d233a6a59dceaf1745bdc53048a2a9e87';
imageInfo['c75dbd2d233a6a59dceaf1745bdc53048a2a9e87'] = {
	'url': 'http://dyn.foodista.com/content/images/c75dbd2d233a6a59dceaf1745bdc53048a2a9e87_240x180c.jpg',
	'thumb_url': 'http://dyn.foodista.com/content/images/c75dbd2d233a6a59dceaf1745bdc53048a2a9e87_32x32c.jpg',
	'title': 'Uploaded photo',
	'attrib_text': '\x3Ca href\x3D\x22/profile/FB44DXS2/melissa\x22\x3EMelissa\x3C/a\x3E' 
};

imageIds[1] = '7dbadc531fefafb03f69f147c6941401bb54e319';
imageInfo['7dbadc531fefafb03f69f147c6941401bb54e319'] = {
	'url': 'http://dyn.foodista.com/content/images/7dbadc531fefafb03f69f147c6941401bb54e319_240x180c.jpg',
	'thumb_url': 'http://dyn.foodista.com/content/images/7dbadc531fefafb03f69f147c6941401bb54e319_32x32c.jpg',
	'title': 'Uploaded photo',
	'attrib_text': '\x3Ca href\x3D\x22/profile/FB44DXS2/melissa\x22\x3EMelissa\x3C/a\x3E' 
};


