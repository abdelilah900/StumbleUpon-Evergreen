var mobileDetect = function() {
			var agent = navigator.userAgent.toLowerCase(),
			url = window.location.toString(),
			cookie = document.cookie.toString();
			
			redirect = (url.match(/cookinglight.com\//)) ? url.replace(/cookinglight.com\//, "cookinglight.com/m/") : url.replace(/cookinglight.com/, "cookinglight.com/m/");
			
				
			if (cookie.match(/TI_PREFS=default/) == null) {
				if (url.match(/nomobile=1/)) {
					//alert("nomobile=1, resetting to default");
					document.cookie = "TI_PREFS=default";
				}

				else if (cookie.match(/TI_PREFS=phone/) !== null) {
					//alert("TI_PREFS=phone, redirecting to moble");
					window.location = redirect;
				} 

				else if (agent.match(/iphone/i)  || agent.match(/ipod/i)  || agent.match(/android/i) || agent.match(/blackberry/i)) {
					//alert("this is mobile device setting TI_PREFS=phone and redirecting to mobile");
					document.cookie = "TI_PREFS=phone";
					window.location = redirect;
				} else {
					//alert("this is not a mobile device, resetting to default");
					document.cookie = "TI_PREFS=default";
				}
			}
	}();
</script>



