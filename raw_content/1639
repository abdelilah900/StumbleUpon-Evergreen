<script type="text/javascript">
    var imaxscroll = 1;
    var imaxscroll2 = 0;
    var vmaxscroll = 0;
    var qmaxscroll = 0;
    var storyImageXPOS = ((getPageSize()[2]-1000)/2+80);
    var editmode = '';
    var largeImages = false;
    var numClicksB4Ad = 3;
    var mediaClicks = 0;

    function changeMediaTab(type)
    {
        if (imaxscroll > 0)
        {
            jQuery('#images_panel').css("display","none");
            jQuery('#ImagesOpenBracket').css("display","none");
            jQuery('#ImagesCloseBracket').css("display","none");
            jQuery('#ImagesTab').removeClass();
        }
        if (vmaxscroll > 0)
        {
            jQuery('#videos_panel').css("display","none");
            jQuery('#VideosOpenBracket').css("display","none");
            jQuery('#VideosCloseBracket').css("display","none");
            jQuery('#VideosTab').removeClass();     
        }
        if (qmaxscroll > 0)
        {
            jQuery('#quotes_panel').css("display","none");
            jQuery('#QuotesOpenBracket').css("display","none");
            jQuery('#QuotesCloseBracket').css("display","none");
            jQuery('#QuotesTab').removeClass();  
        }
        if(imaxscroll2 > 0)
        {
            jQuery('#sports_panel').css("display","none"); 
            jQuery('#SportsOpenBracket').css("display","none");
            jQuery('#SportsCloseBracket').css("display","none");
            jQuery('#SportsTab').removeClass(); 
        }
        switch(type)
        {
            case "i":
                jQuery('#images_panel').css("display","inline"); 
                jQuery('#ImagesOpenBracket').css("display","inline"); 
                jQuery('#ImagesCloseBracket').css("display","inline"); 
                jQuery('#ImagesTab').addClass("active");
                hideAd();
                break;
            case "v":
                jQuery('#videos_panel').css("display","inline"); 
                jQuery('#VideosOpenBracket').css("display","inline"); 
                jQuery('#VideosCloseBracket').css("display","inline");  
                jQuery('#VideosTab').addClass("active");               
                break;
            case "s":
                jQuery('#sports_panel').css("display","inline");  
                jQuery('#SportsOpenBracket').css("display","inline");
                jQuery('#SportsCloseBracket').css("display","inline"); 
                jQuery('#SportsTab').addClass("active");
                hideAd2();
                break;
            case "q":
                jQuery('#quotes_panel').css("display","inline"); 
                jQuery('#QuotesOpenBracket').css("display","inline");  
                jQuery('#QuotesCloseBracket').css("display","inline"); 
                jQuery('#QuotesTab').addClass("active");
                break;
        }
    }
</script>

