<!-- Include ONCE for ALL buttons in the page -->
<script type="text/javascript">
(function() {
    window.PinIt = window.PinIt || { loaded:false };
    if (window.PinIt.loaded) return;
    window.PinIt.loaded = true;
    function async_load(){
        var s = document.createElement("script");
        s.type = "text/javascript";
        s.async = true;
        s.src = "http://assets.pinterest.com/js/pinit.js";
        var x = document.getElementsByTagName("script")[0];
        x.parentNode.insertBefore(s, x);
    }
    if (window.attachEvent)
        window.attachEvent("onload", async_load);
    else
        window.addEventListener("load", async_load, false);
})();
</script>
<script>
$(document).ready(function(){
    $('.result').hide();
    $('.result.one').show();
    var questions = [];
    $('#graphic').find('.qbox').each(function () {
        var name = $(this).attr('id');
        $(this).find('a').click(function () {
            var point = $(this).attr('rel');
            var parentID = $(this).parent().attr('id');

            if (parentID == 'peeps'){
                if(!questions[parentID])
                    questions[parentID] = [];
                
                if(questions[parentID][$(this).index()])
                    questions[parentID][$(this).index()] = undefined;
                else
                    questions[parentID][$(this).index()] = point;
            }
            else
                questions[parentID] = point;
            
            if ($(this).parent().attr('id') == 'peeps') {
                $(this).toggleClass('selected');
            } else {
                if ($(this).parent().hasClass('location1')) {
                    $(this).parents('#location').find('a').removeClass('selected');
                    $(this).addClass('selected');
                } else {
                    if ($(this).parent().hasClass('location2')) {
                        $(this).parents('#location').find('a').removeClass('selected');
                        $(this).addClass('selected');
                    } else {
                        if ($(this).parent().hasClass('how')) {
                            $(this).parents('#size').find('a').removeClass('selected');
                            $(this).addClass('selected');
                        } else {
                            $(this).parent().find('a').removeClass('selected');
                            $(this).addClass('selected');
                        }
                    }
                }
            }
            var total = 0;
            var question;
            var value;

            for(question in questions){
                value = questions[question];
                
                if(question !== undefined){
                    if(question == 'peeps'){
                        for(var i = 0; i < questions[question].length; i++){                        
                            if(questions[question][i] !== undefined) {
                                value=questions[question][i];
                                total += parseInt(value); 
                            }
                        }
                    } else {
                        total += parseInt(value);
                    }
                }
                else{
                    alert('error!');
                    exit;
                }
                
            }               
            if(total<=10) {
                $('.result').hide();
                $('.result.one').show();
            } else if (total<=20) {
                $('.result').hide();
                $('.result.two').show();
            } else if (total<=30) {
                $('.result').hide();
                $('.result.three').show();
            } else if (total<=40) {
                $('.result').hide();
                $('.result.four').show();
            } else if (total<=50) {
                $('.result').hide();
                $('.result.five').show();
            } else if (total>50) {
                $('.result').hide();
                $('.result.six').show();
            }
        });
    });
    $('a.popup').fancybox();
 });
</script>
