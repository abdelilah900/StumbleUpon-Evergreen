      function opendiv(divid)
	  {
		jQuery('#p1').removeClass('sidebar_snap_menuselect');
		jQuery('#p2').removeClass('sidebar_snap_menuselect');
		jQuery('#p3').removeClass('sidebar_snap_menuselect');

		  if(divid=='recentdiv')
		  {
			  //divid.className = divid.className == 'sidebar_snap_menu' ? 'Selectedsidebar_snap_menu' : 'sidebar_snap_menu';
				jQuery('#p1').addClass('sidebar_snap_menuselect');
				document.getElementById('recentdiv').style.display='';
				document.getElementById('cmntdiv').style.display='none';
				document.getElementById('populardiv').style.display='none';
		  }
						
		   if(divid=='cmntdiv')
		   {
				jQuery('#p2').addClass('sidebar_snap_menuselect');
				document.getElementById('cmntdiv').style.display='';
				document.getElementById('populardiv').style.display='none';
				document.getElementById('recentdiv').style.display='none';
		   }
		   if(divid=='populardiv')
		   { 
				jQuery('#p3').addClass('sidebar_snap_menuselect');
				document.getElementById('populardiv').style.display='';
				document.getElementById('cmntdiv').style.display='none';
				document.getElementById('recentdiv').style.display='none';
		   }
	  }
    
    </script>
    
