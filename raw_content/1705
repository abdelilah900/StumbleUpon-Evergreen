<div class="yog-wrap yog-grid yog-24u">
    <div class="yog-col yog-16u yom-primary">
        <!-- --><div class="yom-mod yom-art-branding"><div class="bd"><div class="yom-logo-ad"><!-- SpaceID=0 robot -->
