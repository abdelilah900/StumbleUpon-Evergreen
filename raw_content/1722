<script type="text/javascript">
    //<![CDATA[
    var newPostsPage = 2;
    var newPostsFullyLoaded = false;
    $j(document).ready(function() {
        $j('.newPosts .expand a').click(function() { checkNewPostsExpand(this) });
        $j(".newPosts .title-link").live("click", function() { setGoogleEvent("article-referrer", "new articles", $j(this).attr("href")); });
    });

    function checkNewPostsExpand(obj)
    {
        var mover = $j('.newPosts .expandMover');
        var children = $j('.newPosts .expandMover > ul').length;
        if ($j(obj).hasClass('left'))
            moveSideModule(mover, 'l', 300, children);
        else
        {
            if (mover.width() + parseInt(mover.css('left').replace('px', '')) <= 300)
                $j.get(getUrlPrefix() + '/webservice/Blog.asmx/GetNewPostsPage?page=' + newPostsPage, function(data) { newPostsPage++; getNewPostsCallback(data); });
            else
                moveSideModule(mover, 'r', 300, children)
        }
    }

    function getNewPostsCallback(data)
    {
        ajaxExpandCallback(data, $j('#moreNewPosts'), 'ul', '#moreNewPosts', $j('.newPosts .expandMover'), 300, $j('.newPosts .expandMover > ul').length);
    }
    //]]>
</script>


