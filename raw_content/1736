	<table width="100%" border="0" cellpadding="0" cellspacing="0">
    <tr>
      <td height="30" valign="top"><input type="text" value="vaša e-mail adresa" name="EMAIL" class="required email" id="mce-EMAIL" style="width:152px; height:22px; color:#666; padding:2px 5px; font-size:12px; border:1px solid #d4d4d3; margin-bottom:5px;" onblur="if (this.value == this.defaultValue || this.value == '') {this.value = this.defaultValue;this.style.color='#666';}" onfocus="if (this.value == this.defaultValue) {this.value = '';this.style.color='#000';}"></td>
      <td align="right" valign="top"><input type="image" value="Prijavite se" name="subscribe" id="mc-embedded-subscribe" class="btn" src="/images/prijavi.gif"></td>
