	<div id="vijesti-tabs">
	<script type="text/javascript">
	var avijesti = 0; var abih = 0; var asvijet = 0; var abiznis = 0; var akultura = 0; var adw = 0;
	var aksport = 0; var anogomet = 0; var akosarka = 0; var atenis = 0; var askijanje = 0; var arukomet = 0; var aformula1 = 0;
	var amagazin = 0; var ashowtime = 0; var ascitech = 0; var aauto = 0; var alifestyle = 0; var azanimljivosti = 0;

	function resetNums (except, section) {
		var callback = aktiviraj;
		if (section == 'vijesti') {
			if (except != 'avijesti') avijesti = 0;
			if (except != 'abih') abih = 0;
			if (except != 'asvijet') asvijet = 0;
			if (except != 'abiznis') abiznis = 0;
			if (except != 'akultura') akultura = 0;
			if (except != 'adw') adw = 0;
		} else if (section == 'sport') {
			if (except != 'aksport') aksport = 0;
			if (except != 'anogomet') anogomet = 0;
			if (except != 'akosarka') akosarka = 0;
			if (except != 'atenis') atenis = 0;
			if (except != 'askijanje') askijanje = 0;
			if (except != 'arukomet') arukomet = 0;
			if (except != 'aformula1') aformula1 = 0;
		} else if (section == 'magazin') {
			if (except != 'amagazin') amagazin = 0;
			if (except != 'ashowtime') ashowtime = 0;
			if (except != 'ascitech') ascitech = 0;
			if (except != 'aauto') aauto = 0;
			if (except != 'alifestyle') alifestyle = 0;
			if (except != 'azanimljivosti') azanimljivosti = 0;
		} else {
			resetNums ('', 'vijesti');
			resetNums ('', 'sport');
			resetNums ('', 'magazin');
		}
	}

	function newsScroller (section, id, rel) {
		if (id == 'avijesti') { resetNums ('avijesti', 'vijesti'); avijesti = 0; } else if (id == 'abih') { resetNums ('abih', 'vijesti'); abih++; }
		else if (id == 'asvijet') { resetNums ('asvijet', 'vijesti'); asvijet++; } else if (id == 'abiznis') { resetNums ('abiznis', 'vijesti'); abiznis++; }
		else if (id == 'akultura') { resetNums ('akultura', 'vijesti'); akultura++; } else if (id == 'adw') { resetNums ('adw', 'vijesti'); adw = 0; }
		else if (id == 'aksport') { resetNums ('aksport', 'sport'); aksport = 0; } else if (id == 'anogomet') { resetNums ('anogomet', 'sport'); anogomet++; }
		else if (id == 'akosarka') { resetNums ('akosarka', 'sport'); akosarka++; } else if (id == 'atenis') { resetNums ('atenis', 'sport'); atenis++; }
		else if (id == 'askijanje') { resetNums ('askijanje', 'sport'); askijanje++; } else if (id == 'arukomet') { resetNums ('arukomet', 'sport'); arukomet++; }
		else if (id == 'aformula1') { resetNums ('aformula1', 'sport'); aformula1++; } else if (id == 'amagazin') { resetNums ('amagazin', 'magazin'); amagazin = 0; }
		else if (id == 'ashowtime') { resetNums ('ashowtime', 'magazin'); ashowtime++; } else if (id == 'ascitech') { resetNums ('ascitech', 'magazin'); ascitech++; }
		else if (id == 'aauto') { resetNums ('aauto', 'magazin'); aauto++; } else if (id == 'alifestyle') { resetNums ('alifestyle', 'magazin'); alifestyle++; }
		else if (id == 'azanimljivosti') { resetNums ('azanimljivosti', 'magazin'); azanimljivosti++; } else { resetNums ('', 'sve'); }
		
		if (section == 'vijesti') callback = aktiviraj;
		if (section == 'sport') callback = aktivirajs;
		if (section == 'magazin') callback = aktiviraj2;
		
		CitajSadrzaj (section, rel);
		callback (id);
		
		var retVal = false;
		
		if (avijesti >= 2 || abih >= 2 || asvijet >= 2 || abiznis >= 2 || akultura >= 2 || adw >= 2 || aksport >= 2 || anogomet >= 2 || akosarka >= 2 || atenis >= 2 || askijanje >= 2 || arukomet >= 2 || aformula1 >= 2 || amagazin >= 2 || ashowtime >= 2 || ascitech >= 2 || aauto >= 2 || alifestyle >= 2 || azanimljivosti >= 2) {
			retVal = true;
		}
		
		return retVal;
	}

	</script>
		<ul>
			<li><a id="avijesti" onclick="return newsScroller ('vijesti', this.id, this.rel);" href="/" class="vijesti_item active" rel="sve">Vijesti &#8250;</a></li>
