								var comment_icon = '<img src="http://blogs.vvmedia.com/common/img/icon-comment.png">';
								LF.CommentCount({
									replacer: function(element, count) {
										if (count === 0) label = 'Write Comment';
										else label = count +' Comment'+ (count === 1 ? '' : 's');
										element.innerHTML = label + comment_icon;
									}
