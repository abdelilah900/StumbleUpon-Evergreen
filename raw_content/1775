<script type="text/javascript">

  
$(document).ready(function() {
  
    $("#video-accordian h3 a").hover(function(e){
    e.preventDefault();
    var imgSrc = $(this).attr("title");
    $(this).attr("title", "");
    var popUp = document.createElement("img");
    $(this).parent().append(popUp);
    $(popUp).attr("src", imgSrc)
     .css("display","none")
     .css("position","absolute")
     .css("top","0")
     .css("z-index","100")
     .fadeIn("slow");
   },function(){
    var popUp = $(this).parent().children("img:last-child");
    $(this).attr("title", $(popUp).attr("src"));
    $(popUp).fadeOut(function(){ $(popUp).remove(); });
  });
  
  $('.slideshow').cycle({
      fx:'scrollLeft',  
      easing: 'easeInQuad',
      speed:           500,
      pager:'#nav',
      next:'.slideshow',
      pause:1
  });

  $("#video-accordian").accordion({ active: false });
 
  
  $('#main-text-extra').hide('fast');
  $('.close-main-text-link').hide('fast');
    
  // shows the slickbox DIV on clicking the link with an ID of "slick-show"
  $('a.main-text-link').click(function() {
    $('#main-text-extra').slideDown('slow');
    $('.close-main-text-link').show('fast');
    $('.main-text-link').hide('fast');      
    return false;
  });

   // shows the slickbox DIV on clicking the link with an ID of "slick-show"
   $('a.close-main-text-link').click(function() {
      $('#main-text-extra').slideUp('slow');
      $('.close-main-text-link').hide('fast');
      $('.main-text-link').show('fast');      
      return false;
  });

});

</script>
