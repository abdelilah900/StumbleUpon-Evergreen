									<div id="toolbar" class="clear-block">
						
<div class="nodetoolbar"></div>

<script type="text/javascript">

  $(document).ready(function() {

    $('.nodetoolbar').html('<iframe src="/sociallinks.php?url=http%3A%2F%2Fwww.popsci.com%2Ftechnology%2Farticle%2F2012-12%2Fnew-metamaterial-hydrogel-flows-liquid-remembers-its-shape&amp;title=Shapeshifting+Metamaterial+Could+Revolutionize+How+We+Treat+Wounds++" width="550px" height="40px" scrolling="no" marginheight="0" marginwidth="0" frameborder="0"></iframe>');

