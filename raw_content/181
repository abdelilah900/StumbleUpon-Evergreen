                var cookieStr = 'MeasureType=' + measureTypeList.val() + ';'+
                                'expires=' + expDate.toGMTString() + ';'+
                                'path=/';
                document.cookie = cookieStr;
                
               $j.get(formatUrl("views/content/InstructionsHandler.ashx"), { MeasureType : measureTypeList.val(), UrlName : 'how-to-make-chocolate-truffles' }, 
