<script language="javascript">
    $('.more').live('click', function(){
        $('.episode-summary').hide();
        var y=$('.episode-full-description').height()+30; //adding a 30 px padding bc this doesnt include margins, etc.
        $('.episode-full-description').css("height",$('.episode-summary').height()).show().attr("style", "height:"+y+";display:block;");
    });
    $('.less').live('click', function(){
        $('.episode-full-description').hide();
        var y=$('.episode-summary').height();
        $('.episode-summary').css("height",$('.episode-full-description').height()).show().css({height:y});

        $('.episode-full-description').hide();
    });
</script>


                 
