<script language="javascript">
var y = $('#news-flyout-content').height();
$('.news-flyout-close').live('click', function(){
    
    
    $('#news-flyout-content').css("height","0").show().animate({
        "height":y
    });
    
    $(this).addClass('news-flyout-open').removeClass('news-flyout-close');
    
});
$('.news-flyout-open').live('click', function(){
    var y = $('#news-flyout-content').height();
    
    $('#news-flyout-content').animate({
        "height":0
    }, function(){
        $(this).hide();
    });
    
    $(this).addClass('news-flyout-close').removeClass('news-flyout-open');
    
});
</script>

