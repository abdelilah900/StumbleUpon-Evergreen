                    .append($('<a/>').attr('href', 'http://www.myvoicenation.com/profile/newsletters/stl/').text('Newsletters')).append($('<br/>'))
                    .append($('<span data-omni-action="logout"/>').text('Logout'))
            );
        } else {
                            if (/(blog(s|test)\.)|[\?&]customad=/.test(window.location.href) || $("span.affinity").length > 0) {
                    return;
                }
                if (!VVM.getCookie('submodal') && VVM.cookiesEnabled()){
                    MVN.omni.buildModal('subscribe');
                    if (MVN.omni.Modals['subscribe']) {
                        MVN.omni.Modals['subscribe'].show();
                    }
                }
                    }
    })
</script>
