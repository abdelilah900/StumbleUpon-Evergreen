    <script type="text/javascript">
        MVN.onReady(function(){
            MVN.load({
                'LF':{
                    component: LF_Component,
                    params: {}
                }
            });
            var page_title = $(document.getElementsByTagName('head')).find('meta[property="og:title"], meta[name="webtitle"]').attr('content');
            window.conv = fyre.conv.load({
                network: 'myvoicenation.fyre.co',
                authDelegate: MVN.LF.binds(),
                strings: {
                    shareDefaultText: page_title ? "Check out %s's post on \"" + page_title + "\"!" : "Check out %s's post!"
                }
            }, [{
                'articleId': 'L2Jlc3RvZi9hd2FyZC8xNjk4NzU3',
                'checksum': '668d0525be456720b274c86717ec3248',
                'collectionMeta': 'eyJ0eXAiOiJqd3QiLCJhbGciOiJIUzI1NiJ9..1YSdQDV1gXlTNkhcdReLwHxNoNNq_dVeRLtqQe5uJ-Q',
                'el': 'livefyre',
                'siteId': '304536',
                'authPageReload': true
            }], function(widget){
                MVN.LF.login();
                widget.on('initialRenderComplete', MVN.LF.clean);
                widget.on('commentPosted', MVN.LF.clean);
                widget.on('commentFlagged', MVN.LF.clean);
                widget.on('commentLiked', MVN.LF.clean);
                widget.on('commentShared', MVN.LF.clean);
                widget.on('commentCountUpdated', function(data){
                    MVN.LF.clean();
                    MVN.queue(function(){
                        $('[data-lfv3-counter]').text(data);
                    })
                });
                MVN.LF.change = function(conv){
                    MVN.queue(function(){
                        widget.changeCollection(conv);
                    })
                }
                MVN.queue(function(){
                    // Register Button
                    $('.lfreg', '#livefyre').live('click', function(){
                        if(!MVN.user){
                                                        MVN.omni.actionSrc = 'lf';
                            MVN.omni.action(MVN.omni.buildAction('join', {sid: MVN.LF.getSID()}));
                                                    }else{
                            MVN.LF.login();
                        }
                    });
                    // hack our hovercards onto the livefyre user imgs/links
                    $('#livefyre').delegate('.fyre-comment-wrapper', 'mouseenter', function(){
                        if (this.attributes.hasOwnProperty('mvn-hovercard-en')) return;

                        var $this = $(this),
                            $user = $this.find('.fyre-comment-user'),
                            uid   = $user.data('author-id');

                        if (!uid || uid.indexOf('anon_') === 0) {
                            $this.attr('mvn-hovercard-en', false);
                            return;
                        }
                        
                        $user.add($this.find('.fyre-comment-username[href]'))
                            .attr('data-omni-user', uid.split('@').slice(0,1).join())
                            .data('omni-username', $user.data('from'))
                            .data('omni-userphoto', $user.find('img').attr('src'));

                        $this.attr('mvn-hovercard-en', true);
                    });
                    // Move Notification Window
                    var setNotifyPosition = function(){
                        $('.fyre-notifier-container').css('right', function(){
                                return ($('body').width() - $('#livefyre').offset().left - parseInt($(this).css('width').replace('px', '')));
                            }
                        );
                    }
                    setNotifyPosition();
                    $(window).bind('resize scroll', setNotifyPosition);
                })
            });
        });

        var LF_Component = function(controller){
            var me = this;
            me.MVN = controller;
            me.version = 'LiveFyre for MVN 2.0';
            me.autoInit = false;
        }
        LF_Component.prototype = {
            cookieName: 'livefyre_token',
            anonName: 'MVN[anon]',
            anon: null,
            login: function(newToken){
                var me = this;
                var token = newToken||VVM.getCookie(me.cookieName)||false;
                if(token){
                    try{
                        fyre.conv.login(token);
                        me.clean(true);
                    }catch(e){
                        VVM.delCookie(me.cookieName);
                        if(!newToken){
                            me.login();
                        }
                    }
                }else{
                    MVN.queue(function(){
                        if(me.MVN.user||VVM.getCookie(me.anonName)){
                            VVM.debug.p('Requesting New Token');
                            me.MVN.sso.getLfToken(function(obj){
                                if(obj && obj.status == 'success'){
                                    VVM.setCookie(me.cookieName, obj.token.token, false, "/");
                                    me.login(obj.token.token);
                                }
                            });
                        }
                    });
                }
            },
            binds: function(){
                var me = this;
                if(!me.auth){
                    var auth = new fyre.conv.RemoteAuthDelegate();
                    auth.login = function(handleAs){
                        MVN.queue(function(){
                            if(!me.MVN.user){
                                                                me.MVN.omni.actionSrc = 'lf';
                                me.MVN.omni.action(me.MVN.omni.buildAction('login', {sid: me.getSID()}));
                                                            }else{
                                MVN.queue(function(){
                                    MVN.LF.login();
                                });
                            }
                        });
                        handleAs.failure();
                    }
                    auth.logout = function(handleAs){
                        MVN.queue(function(){
                            if(me.MVN.user||VVM.getCookie(me.anonName)){
                                                                VVM.delCookie(me.cookieName, '/');
                                VVM.delCookie(me.anonName, '/');
                                me.MVN.omni.action('logout');
                                                            }
                        });
                        handleAs.success();
                    }
                    auth.viewProfile = function(handleAs, author){
                        if(!author.id.match(/^anon_/) && author.profileUrl != ""){
                            window.location = author.profileUrl;
                            handleAs.success();
                        }else{
                            me.clean();
                        }
                    }
                    auth.editProfile = function(handleAs, author){
                        if(author.id.match(/^anon_/)){
                            me.clean();
                            handleAs.failure();
                        }else{
                            window.location = "http://www.myvoicenation.com/profile/settings/";
                            handleAs.success();
                        }
                    }
                    me.auth = auth;
                }
                return me.auth;
            },
            clean: function(calledAfterLogin, attempt){
                var me = this;
                MVN.queue(function(){
                    // Check if ANON
                    if(calledAfterLogin || me.anon == null){
                        if(fyre.conv.user && fyre.conv.user.id){
                            me.anon = (fyre.conv.user.id.match(/^anon_/)) ? true : false;
                        }else if(calledAfterLogin){
                            attempt = (attempt) ? attempt + 1 : 1;
                            if(attempt > 5){
                                VVM.debug.p('LF Login Failed');
                                VVM.delCookie(me.cookieName);
                            }else{
                                VVM.debug.p('LF Login Check Attempt: '+attempt);
                                window.setTimeout(function(){me.clean(true, attempt)}, 1000);
                            }
                        }
                    }
                    // ANON Clean Up
                    if(me.anon == true){
                        $('li > a[title="Edit Profile"]', '.fyre-box-list').parent('li').remove();
                    }
                    
                    // Create Account Button
                    if (!MVN.user && !VVM.getCookie(MVN.LF.anonName)) {
                        var $el = $('div.fyre-login-bar', '#livefyre');
                        if($el.length && $el.children().length == 1 && !$('.fyre-user-loggedin', $el).length){ 
                            $el.append('<span class="create-mvn-account"> or <span class="lfreg">Create Account</span></span>');
                        }
                    }
                    
                    // Each Comment/Reply
                    $('.fyre-comment-wrapper').each(function(){
                        var $el = $('.fyre-comment-user[data-author-id^="anon_"]', this);
                        if($el.length){
                            // Comment Avatar
                            $('a', $el).replaceWith($('a img', $el)
                                .attr('title', 'Anonymous User')
                            );
                            // Comment Author
                            $el = $el.parent().find('header a');
                            $el.replaceWith($('<span/>')
                                .addClass('anon_username')
                                .text($el.text())
                                .attr('title', 'Anonymous User')
                            );
                        }
                        // Comment Mentions
                        $('a.fyre-mention[resource^="acct:anon_"]', this).each(function(){
                            $(this).replaceWith($('<span/>')
                                .addClass('anon_username')
                                .text('@' + $('span[property="name"]', this).text())
                                .attr('title', 'Anonymous User')
                            );
                        });
                        // Comment Likes
                        $('.fyre-avatar', this).filter(function(){
                            return ($('img[src*="/anon/"]', this).length) ? true : false;
                        }).each(function(){
                            $('a', this).replaceWith($('a img', this)
                                .attr('title', 'Anonymous User')
                            );
                        })

                        $(this).filter('[class*="fyre-comment-author-tag-"]').each(function () {
                            var classes = this.className.split(' '), tags, i,
                                $this = $(this), $name = $this.find('.fyre-comment-username');
                            if ($name.next().is('.fyre-moderator')) {
                                $name.next().remove();
                            }
                            for(i = 0; i < classes.length; i++) {
                                if (classes[i].indexOf('fyre-comment-author-tag-') === 0) {
                                    tag = classes[i].split('-').pop();
                                    $this.addClass('has-mvn-badge').removeClass(classes[i])
                                    $name.after('<i class="mvn-badge '+tag+'">');
                                }
                            }
                        });
                    });
                });
            },
            getSID: function(){
                var src_desc = '';
                if(window.OAS_query_category||false){
                    switch(OAS_query_category.toLowerCase().replace(/\s/,'')){
                        case 'arts':
                            src_desc = 1033;
                            break;
                        case 'film':
                        case 'movies':
                            src_desc = 1034;
                            break;
                        case 'music':
                            src_desc = 1035;
                            break;
                        case 'news':
                            src_desc = 1036;
                            break;
                        case 'restaurants':
                        case 'dining':
                        case 'food':
                            src_desc = 1037;
                            break;
                                                case 'bestof':
                            src_desc = 1038;
                            break;
                                            }
                }
                return '8|53|'+src_desc;
            }
        }
    </script>
    <div id="powered_by_livefyre_new"><a href="http://livefyre.com" target="_blank">Powered by Livefyre</a></div>
</div><script>
    $(document).ready(function() {
        $('#CommentCreateContainer').hide();
    });
</script>
