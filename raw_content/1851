				<div id="eNewsSignUp_h">
					<script>
					function trackThis(){
						_gaq.push(['_trackEvent', 'Newsletter Sign Up', 'Widget Submit', document.getElementById("eLetterEmail").value]);
					}
					</script>
					<form action="http://www.post-gazette.com/email/newsletters/" onsubmit="javascript:trackThis();">
						<div id="eLetterInputs">
							<input id="eLetterEmail" name="eLetterEmail" type="text" style="font-style:italic;" size="30" value="Enter Email" onfocus="focusLetterEmail(this);" onblur="blurLetterEmail(this);"/>
							<input id="eLetterSubmit" name="eLetterSubmit" type="submit" value="Submit" />
							<!--input id="eLetterSubmit" name="eLetterSubmit" type="submit" value="Submit" onClick="javascript:_gaq.push(['_trackEvent', 'Newsletter Sign Up', 'Widget Submit', 'document.getElementById("eLetterEmail").value']);" /-->
							<!--input id="eLetterSubmit" name="eLetterSubmit" type="submit" value="Submit" onclick="javascript:_gaq.push(['_trackEvent', 'Newsletter Sign Up', 'Widget Submit', document.eLetterEmail.value]);" /-->
						</div>
					</form>
				</div>
			
				<script type="text/javascript">
					function focusLetterEmail(thisBox) {
						if(thisBox.value==thisBox.defaultValue) {
							thisBox.value="";
							thisBox.style.fontStyle="normal";
						}
					}
					function blurLetterEmail(thisBox) {
						if(thisBox.value=="") {
							thisBox.value=thisBox.defaultValue;
							thisBox.style.fontStyle="italic";
						}
					}
				
					$(document).ready(function () {
						document.getElementById("eNewsSignUp").appendChild(document.getElementById("eNewsSignUp_h"));
						document.getElementById("eNewsSignUp_h").style.display = "block";
					});
				</script>
			</body>
