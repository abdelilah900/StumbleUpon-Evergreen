  	<script>
  		//this is for the lottery tabs
		$(function() {
			$(".tabrow li").click(function(e) {
				e.preventDefault();
				$(".tabrow li").removeClass("selected");
				$(this).addClass("selected");
				var state = $(this).children().attr('class');
				//alert(show);
				$("#"+state).show().siblings("div").hide();
			});
		});
	</script>

