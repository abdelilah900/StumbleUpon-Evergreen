	<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
	<script type="text/javascript">

$(document).ready(function(){
	//Navigation Menu
   $(".subNav").hide();
   $(".menu_arrow_down").hide();
   $(".subNav_holder_t").hide();
   $(".close_box").hide();



	 function show2() {
		 var menu = $(this);

		 menu.children(".actions").slideDown("fast");
		 menu.children(".menu_arrow_down").fadeIn("fast");
		 $(".subNav_holder").show();
		 $(".subNav_holder_t").slideDown("fast");
		 //alert(menu.children().attr("id"));
		 $(".close_box").delay(1000).fadeIn("slow");

	   }

	   function hide2() {
				 var menu = $(this);
				 menu.children(".actions").slideUp("fast");
				 menu.children(".menu_arrow_down").slideUp("fast");
	   }

	   $("#nav_holder").mouseleave(function(){
				//alert("mouse out of nav holder");
				$(".subNav_holder_t").slideUp("fast");
				$(".close_box").hide();
	   });

	   $(".hover-menu").hoverIntent({
		 sensitivity: 10, 	// number = sensitivity threshold (must be 1 or higher)
		 interval: 50,		// number = milliseconds for onMouseOver polling interval
		 over: show2,     	// function = onMouseOver callback (required)
		 timeout: 300, 	  	// number = milliseconds delay before onMouseOut
		 out: hide2       	// function = onMouseOut callback (required)
	   });


	$(".close_box").hover().css("cursor","pointer").click(function(){
		//console.log("close box clicked");
		$(".actions").slideUp("fast");
		$(".menu_arrow_down").slideUp("fast");
		$(".subNav_holder_t").slideUp("fast");
	});

	//searchbox default text
	$("#searchbox_label").example('Search the Post-Gazette',{
		className: 'searchbox_text'
	});




	/*pikachoose*/
	var a = function(self){
		$("#pikame").show();
		self.anchor.fancybox({
			'overlayShow'	: true,
			'transitionIn'	: 'elastic',
			'transitionOut'	: 'elastic'
		});
	};

	$("#pikame").PikaChoose({
		buildFinished:a,
		thumbOpacity: 0.7,
		autoPlay: false,
		carousel:true
	});

//fancy box (aka login window)
	$("#example2").fancybox({
		'overlayShow'	: true,
		'transitionIn'	: 'elastic',
		'transitionOut'	: 'elastic'
	});

	$(".popup").fancybox({
		'overlayShow'	: true,
		'transitionIn'	: 'elastic',
		'transitionOut'	: 'elastic'
	});

    $("a#example2").fancybox().trigger('click');

	$("#email_to").example('jack@email.com, jill@email.com',{
		className: 'email_to_text'
	});

    $(".popup_email").fancybox({
		'overlayShow'	: true,
		'transitionIn'	: 'elastic',
		'transitionOut'	: 'elastic',
        'height' 		: 720,
        //'scrolling'     : 'no',
		'type' 			: 'iframe',
		'z-index'		: 100000,
		'onComplete'	: function(){
			_gaq.push(['_trackEvent','Share Story','Email Story', $(this).attr('href')]);
		}
	});
	$(".popup_qr").fancybox({
		'overlayShow'	: true,
		'transitionIn'	: 'elastic',
		'transitionOut'	: 'elastic',
        'height' 		: 420,
        'width'			: 420,
		'type' 			: 'iframe',
		'z-index'		: 100000,
		'onComplete'	: function(){
			_gaq.push(['_trackEvent','Share Story','QR Code', $(this).attr('href')]);
		}
	});



//equalize divs
	var maxHeight = 0;

	$(".news_box_skinny_top").each(function(){
	   if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
	});
	$(".news_box_skinny_top").height(maxHeight);

	$(".news1").each(function(){
	   if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
	});
	$(".news1").height(maxHeight);

	$(".news2").each(function(){
	   if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
	});
	$(".news2").height(maxHeight);

	$(".news3").each(function(){
	   if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
	});
	$(".news3").height(maxHeight);

	$(".local1").each(function(){
	   if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
	});
	$(".local1").height(maxHeight);

	$(".local2").each(function(){
	   if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
	});
	$(".local2").height(maxHeight);

	$(".local3").each(function(){
	   if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
	});
	$(".local3").height(maxHeight);



	//timeago
	$("abbr.breaking_time").timeago();

