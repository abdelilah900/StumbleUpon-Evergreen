						<script type="text/javascript">
							$(window).load(function () {	
								var n = $("#oas_Top1").children().length;
								//console.debug(n);
								if (n > 3){ 
									//n greater than 3 means there is an ad that is not an image or a div
									document.getElementById("oas_ad_Top1").appendChild(document.getElementById("oas_Top1"));
									document.getElementById("oas_Top1").style.display = "";
									$("#ad_top").slideDown();
									$("#ad_top_margin").slideUp();
								} else {
									var img = $("#oas_Top1:has('img')");
									var div = $("#oas_Top1:has('div')");
									var iframe = $("#oas_Top1:has('iframe')");
									
										if (div.length != 0){
											//console.debug("has div");
											//slide down the top1 ad
											document.getElementById("oas_ad_Top1").appendChild(document.getElementById("oas_Top1"));
											document.getElementById("oas_Top1").style.display = "";
											$("#ad_top").slideDown();
											$("#ad_top_margin").slideUp();
										} else {
											//console.debug("no div");
										}
										
										if (img.length != 0){
											//console.debug("has image");
											var empty_img = $("#oas_Top1 img:first").attr("src").search("empty.gif");
											//console.debug(empty_img);
											if (empty_img < 0) {
												//console.debug("has an ad, slide down");
												document.getElementById("oas_ad_Top1").appendChild(document.getElementById("oas_Top1"));
												document.getElementById("oas_Top1").style.display = "";
												$("#ad_top").slideDown();
												$("#ad_top_margin").slideUp();
											}
										} else {
											//console.debug("no image");
										}
										
										if (iframe.length != 0){
											//console.debug("has iframe");
											//slide down the top1 ad
											document.getElementById("oas_ad_Top1").appendChild(document.getElementById("oas_Top1"));
											document.getElementById("oas_Top1").style.display = "";
											$("#ad_top").slideDown();
											$("#ad_top_margin").slideUp();
										} else {
											//console.debug("no iframe");
										}
								}
							});
						</script>
					
