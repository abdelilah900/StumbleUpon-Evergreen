		if( (e.srcElement.identify() === 'close') || (e.srcElement.identify() === 'close-img')){
			setTimeout(function(){$('ipad-promo').hide();}, 400);
			BFW_Util.setCookie({name:'no_ipad_promo' , value:'1', seconds:2592000});
