<script type="text/javascript">
	function replaceSmallLike(el) {
		var element = $(el);
		if( ! element.hasClassName('real_btn') ) {
			element.addClassName('real_btn');
   	 	var like_xfbml = new Element('fb:like', { 'class': 'like_xfbml', 'send': 'false', 'rel:page_type': 'Buzz', 'rel:buzz_id': '793431', 'href': 'http://www.buzzfeed.com/johnb7/shit-shit-shit-shit-shit-lfh', 'layout': 'button_count', 'show_faces': 'false', 'width' : '110' });
			element.update('');
			element.appendChild(like_xfbml);
			FB.XFBML.parse(element);			//make sure this is last
		}
  }
</script>

<div class="facebook_like_send_small share_button" onmouseover="replaceSmallLike(this);" style="width:110px !important;">
	<div class="like preload-fb-like"></div>
	
	
