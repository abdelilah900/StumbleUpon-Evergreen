   			$('#logged-in').removeClass('active');
   		});
   	
   	$('.login-form .username').focus(function() {
   		$('.login-form .username').val('');
   	});
   	
   	$('.login-form .password').focus(function() {
   		$('.login-form .password').val('');
   	});
   	
});

document.onclick = jsddm_close;

