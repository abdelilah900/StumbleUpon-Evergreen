                jQuery('input[id$="gigya.services.socialize.plugins.share.showShareUI_tbYourEmail"]').val(loginId);
                }
                }
              // onSendDone event handler.
                    function onSendDoneHandler(event){
                    if (event.providers == 'email') { 
                    // add this to email logger
