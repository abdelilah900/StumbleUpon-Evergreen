function writeImage(method, data, action, current) {
    $(document.createElement("img")).attr({
        id: 'hswOmniImage',
        src: data
    }).load(function () {
        if (method !== 'none') {
            clearTimeout(t);
            doAction(method, action, current);
        }
    });
    if (typeof method === 'string' && method !== 'none') {
        var t = setTimeout(function () {
            doAction(method, action, current);
        }, 500);
    }
}

function doAction(method, action, current) {
    if (method == 'form') {
        current.submit();
    } else {
        document.location.href = action;
    }
}

function isPurchaseEvent(tag, lookupArray) {
    var found = false;
    for (key in lookupArray) {
        if (tag.indexOf(lookupArray[key]) >= 0) {
            found = lookupArray[key];
            break;
        }
    }
    return found;
}

function inArray(tag, lookupArray) {
    var found = false;
    for (key in lookupArray) {
        if (tag.indexOf(lookupArray[key]) >= 0) {
            found = true;
            break;
        }
    }
    return found;
}

function sendTags(tag, vars, current, eventObj, noRedirect) {
    if (tag != '') {
        var hswOmniImage = document.getElementById('hswOmniImage');
        var omniImageData = "http://metrics.howstuffworks.com/b/ss/" + s_account + "/1/H.17/s45086097826030?";
