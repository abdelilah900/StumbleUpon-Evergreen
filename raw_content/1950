        if (_href) {
            if (omniImageData.indexOf('c25=') >= 0 && omniImageData.indexOf('c7=') < 0) {
                var endPos = _href.indexOf('?');
                if (endPos < 0) {
                    endPos = _href.length;
                }
                var startPos = _href.lastIndexOf('/', endPos);
                var shortHref = _href.substr(startPos + 1, endPos - startPos - 1);
                if (shortHref == '') {
                    shortHref = _href;
                }
                omniImageData += '&c7=' + shortHref;
                omniImageData += '&v9=' + shortHref;
            }
        }
        omniImageData += '&pe=' + 'lnk_o';
        omniImageData += '&pev2=' + tag.replace(/&/g, ' and ');
        omniImageData += '&pid=' + encodeURIComponent(location.href);
        omniImageData += '&AQE=1';
        if (noRedirect == undefined) {
            if (current.action) {
                gotoAction = current.action;
                tagName = current.nodeName || '';
                if (omniImageData.indexOf('pev1=') < 0) {
                    omniImageData += '&pev1=' + encodeURIComponent(gotoAction);
                    omniImageData += '&oid=' + encodeURIComponent(gotoAction);
                }
                omniImageData += '&ot=' + tagName;
                if ((current.method.toLowerCase() == 'get') && (current.name.substr(0, 15) == 'frmSelMakeModel')) {
                    writeImage('url', omniImageData, current.action, current);
                } else {
                    writeImage('form', omniImageData, current.action, current);
                }
            } else if (_href) {
                gotoAction = _href;
                gotoTarget = current.target;
                tagName = current.nodeName || '';
                if (omniImageData.indexOf('pev1=') < 0) {
                    omniImageData += '&pev1=' + encodeURIComponent(gotoAction);
                    omniImageData += '&oid=' + encodeURIComponent(gotoAction);
                }
                omniImageData += '&ot=' + tagName;
                writeImage('url', omniImageData, gotoAction, current);
            } else if (eventObj.target.href) {
                gotoAction = eventObj.target.href;
                tagName = current.nodeName || '';
                if (omniImageData.indexOf('pev1=') < 0) {
                    omniImageData += '&pev1=' + encodeURIComponent(gotoAction);
                    omniImageData += '&oid=' + encodeURIComponent(gotoAction);
                }
                omniImageData += '&ot=' + tagName;
                writeImage('url', omniImageData, gotoAction, current);
            } else {
                gotoAction = document.location.href;
                tagName = current.nodeName || '';
                if (omniImageData.indexOf('pev1=') < 0) {
                    omniImageData += '&pev1=' + encodeURIComponent(gotoAction);
                    omniImageData += '&oid=' + encodeURIComponent(gotoAction);
                }
                omniImageData += '&ot=' + tagName;
