        var day = timestamp.getDate();
        var hours = timestamp.getHours();
        var mins = timestamp.getMinutes();
        var secs = timestamp.getSeconds();
        var weekday = timestamp.getDay();
        var timezoneOffset = timestamp.getTimezoneOffset();
        var gValue = location.protocol + '//' + location.hostname;
        var omniPrefix = '';
        if (location.pathname) {
            gValue += location.pathname;
        }
        omniImageData += '&AQB=1';
        omniImageData += '&t=' + month + '/' + day + '/' + year + ' ' + hours + ':' + mins + ':' + secs + ' ' + weekday + ' ' + timezoneOffset;
        omniImageData += '&vmt=' + '4467DB86';
        omniImageData += '&ns=' + 'hswmedia';
        omniImageData += '&g=' + encodeURIComponent(gValue);
        var subDomain = location.href;
        subDomain = subDomain.substring(7, subDomain.indexOf('.', 7));
        if (subDomain == 'recipes') {
            if (vars) {
                vars = vars.replace(/=/gi, '=TLC : ');
                vars = vars.replace(/events=TLC :/gi, 'events=');
                vars = vars.replace(/products=TLC :/gi, 'products=');
            }
        }

        if (vars) {
            omniImageData += vars;
        }

