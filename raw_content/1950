$(document).ready(function () {
    $('[omniselect]').unbind('change');
    $('#hswOmniImage').unbind('load');
    $("a").live('click', function (e) {
        if ((!$.browser.msie && e.button == 0) || $.browser.msie) {
            var tag = $(this).attr('omni');
            var vars = $(this).attr('omnivars');
            if (tag != '' && tag !== undefined) {
                if ($(this).attr('target') == '_new' || $(this).attr('target') == '_blank') {
                    sendTags(tag, vars, this, e, true);
                } else {
                    sendTags(tag, vars, this, e);
                    return false;
                }
            }
        }
    });
    $("form").submit(function (e) {
        var tag = $(this).attr('omni');
        var vars = $(this).attr('omnivars');
        if (tag != '' && tag !== undefined) {
            if ($(this).attr('target') == '_new' || $(this).attr('target') == '_blank') {
                sendTags(tag, vars, this, e, true);
            } else {
                sendTags(tag, vars, this, e);
                return false;
            }
        }
    });
    $('[omniselect]').change(function (e) {
        var tag = this.options[this.selectedIndex].getAttribute('omni');
        var vars = this.options[this.selectedIndex].getAttribute('omnivars');
        sendTags(tag, vars, this, e);
        return false;
    });
});
