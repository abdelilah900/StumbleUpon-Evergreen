			pagename += ':' + subch;
		} else if((typeof(zone1) !== 'undefined') && (typeof(subch) !== 'undefined') && zone1 != 'NULL' && subch != 'NULL') {
			pagename += ':' + zone1;
		}
		
		
		
		
				
		
				
		
	}		
       

	//special case for newsletter
	if(omniture_pagename == "newsletter signup") {
		pagename="el";
	}
</script>	




<script language="JavaScript" type="text/javascript">
	if(omniture_pagename != "") {
		s.pageName = (pagename + ':' + omniture_pagename).toLowerCase();
    } else {
		s.pageName = pagename.toLowerCase();
    }
	


		
</script>


