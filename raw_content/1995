<script type="text/javascript" charset="utf-8">
    function closePlzLogin()
    {
        $$('.require_login').each(function(item,index){
            if (item.hasClassName('focus'))
            {
                exitTop = item.top - 112;
            }
            item.removeClassName('focus')
            });
        $('plz_login').fade({ duration: 0.5, from: 1, to: 0 });
        new Effect.Move('plz_login', { x: -500, y: exitTop, mode: 'absolute',duration:0.2 });
    }
</script>		
