<script type="text/javascript">
// <![CDATA[
function equalHeight(one,two) {
	var tallest = 0;
	if (one.height() <= two.height()) {
		one.height( two.height() );}
}	

$(document).ready(function(){
	equalHeight($("#sidebar-left"), $("#main"));
	
	var div = $('#topnav');
    var start = $(div).offset().top;
 
    $.event.add(window, "scroll", function() {
        var p = $(window).scrollTop();
        $(div).css('position',((p)>start) ? 'fixed' : 'static');
        $(div).css('top',((p)>start) ? '0px' : '');
    });
	
	var myhost = document.location.hostname
	var outhost = '/go/';
	
	$('#main a[href^="http"]:not([href*="'+myhost+'"])').click(function(){
		var url = $(this).attr('href').substring(7);
		$(this).attr('target','dblink');
		_gaq.push(['_trackEvent', 'Outbound',  url ]);
		setTimeout(function(){}, 100);
	});
	
	$('#main a[href*="'+outhost+'"]').click(function(){
		var url = $(this).attr('href').substring(28);
		var source = document.URL.substring(24);
		$(this).attr('target','dblink');
		_gaq.push(['_trackEvent', 'Link',  url, source ]);
		setTimeout(function(){}, 100);
	});
	
});

// ]]>
</script> 
