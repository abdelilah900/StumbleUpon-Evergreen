		function fadein_out(val,val_num)
		{
			if(document.getElementById(val).style.display == "block")
				var empty_value = 0;
			else
			{
				jQuery(".display_videoList").hide("slow");
				jQuery("#"+val).show("slow");
				document.getElementById("mPage_"+val_num).style.borderBottom = "2px solid #000000";
				document.getElementById("mPage_"+previous_page).style.borderBottom = "0px";

				// dynamically load images for new pages
				$('#page'+val_num+' img').each(function(index){
					var img_id = $(this).attr('id');
					var img_src = $(this).attr('src');
					if(img_src != '')
						return;

					$(this).attr('src', vimages[img_id]);
				});

				previous_page = val_num;
			}
		}
		function load_next_page()
		{
			if(previous_page == total_pages)
				var empty_value = 0;
			else
			{				
				new_page = parseInt(previous_page) + 1;
				jQuery(".display_videoList").hide("slow");
				jQuery("#page"+new_page).show("slow");				
				document.getElementById("mPage_"+new_page).style.borderBottom = "2px solid #000000";
				document.getElementById("mPage_"+previous_page).style.borderBottom = "0px";				

				// dynamically load images for new pages
				$('#page'+new_page+' img').each(function(index){
					var img_id = $(this).attr('id');
					var img_src = $(this).attr('src');
					if(img_src != '')
						return;

					$(this).attr('src', vimages[img_id]);
				});

				previous_page = new_page;
			}
		}
		function load_prev_page()
		{
			if(previous_page == "1")
				var empty_value = 0;
			else
			{				
				new_page = parseInt(previous_page) - 1;
				jQuery(".display_videoList").hide("slow");
				jQuery("#page"+new_page).show("slow");				
				document.getElementById("mPage_"+new_page).style.borderBottom = "2px solid #000000";
				document.getElementById("mPage_"+previous_page).style.borderBottom = "0px";				

				// dynamically load images for new pages
				$('#page'+new_page+' img').each(function(index){
					var img_id = $(this).attr('id');
					var img_src = $(this).attr('src');
					if(img_src != '')
						return;

					$(this).attr('src', vimages[img_id]);
				});

				previous_page = new_page;
			}	
		}
	</script>

