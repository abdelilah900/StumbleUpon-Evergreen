<script type="text/javascript">//<![CDATA[
	// source: http://snippets.dzone.com/posts/show/2630
	function removeClass(objElement, strClass) {
	   // if there is a class
	   if ( objElement.className ) {
		  // the classes are just a space separated list, so first get the list
		  var arrList = objElement.className.split(' ');
		  // get uppercase class for comparison purposes
		  var strClassUpper = strClass.toUpperCase();
		  // find all instances and remove them
		  for ( var i = 0; i < arrList.length; i++ ) {
			 // if class found
			 if ( arrList[i].toUpperCase() == strClassUpper ) {
				// remove array item
				arrList.splice(i, 1);
				// decrement loop counter as we have adjusted the array's contents
				i--;
			 }
		  }
		  // assign modified class name attribute
		  objElement.className = arrList.join(' ');
	   }
	}


	function get_random_number(elemsLength) {
	  var number = parseInt(elemsLength);
	  var ranNum= Math.floor(Math.random()*number);
	  return ranNum;
	}
	
	function random_select(ulID, classname) {
	  var elems = select_child_elements_by_classname(ulID, classname);
	  var whichElem = get_random_number(elems.length);
	  for(var i = 0; i < elems.length; i++) {
//		elems[i].className.replace(/selected$/, '');
		removeClass(elems[i], "selected"); 		// UI NOTE: Replaced with function due to malfunction of 'replace' method
	  }
	  elems[whichElem].className += ' selected';
	}
	
	function select_child_elements_by_classname(ulID, classname) {
	  var ulEl = document.getElementById(ulID);
	  var elemsarr = ulEl.getElementsByTagName('li');
	  var elems = Array();
	  for(var i = 0; i < elemsarr.length; i++){
		if (elemsarr[i].className.match(classname)) {
		  elems.push(elemsarr[i]);
		}
	  }
	  return elems;
	}
	
	function navigateSmallDigihubBlock(ulID, prev, next) {
	  var selectedSet = false;
	  var nextEl = '';
	  var elems = select_child_elements_by_classname(ulID, 'tab');
	  while (selectedSet == false) {
		for(var i = 0; i < elems.length; i++){
		  var class_array = elems[i].className.split(' ');
		  if (class_array[1] == 'selected') {
			elems[i].className = class_array[0];
			if (prev == true) {
			  var nextEl = i-1;
			  if (nextEl == -1) {
				nextEl = elems.length - 1;
			  }
			}
			if (next == true) {
			  var nextEl = i+1;
			  if (nextEl == elems.length) {
				nextEl = 0;
			  }
			}
		  }
		  if (i === nextEl) {
			elems[i].className += ' selected';
			selectedSet = true;
			break;
		  }
		}
	  }
	}
	
	window.onload = function() {
          var dh = $("digiHub");
          if (dh) {
	    dh.className.replace(/accessibleTab$/, '');
          }


	  random_select('digiHub', 'tab');
	  interval = setInterval("navigateSmallDigihubBlock('digiHub', false, true)", 7500 );
	};
//]]></script>
<!-- END digihub js  -->


