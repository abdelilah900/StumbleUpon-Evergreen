  	<script type="text/javascript">
  	$(document).ready(function() {
	  $('#mr_email_popup').dialog({ 
		  autoOpen: false, 
		  modal: true,
		  dialogClass: "flora",
		  overlay: {opacity: 0.5, background: "black"},
		  open: function(event, ui) { 
			  	  $(".ui-dialog-titlebar-close").hide();
			  	  $('#mr_email_popup .content-form').show(); 
			  	},
		  show: "blind",
		  hide: "explode",
		  width: 450,
		  close: function(ev, ui) { 
          	       $('#mr_email_popup .content-processing').hide();
          	       $('#mr_email_popup .content-result').hide(); 
			     }
		  });

      $('#mr_email_popup_form_submit').click(function() {
        var form_data = $('#mr_email_popup_form').serialize();
        $('#mr_email_popup .content-form').fadeOut('fast');
        $('#mr_email_popup .content-processing').fadeIn();
        $.ajax({ 
          type:'POST', 
          url:'/member-recipes?action=ajaxrequest&needslogin=false&type=components&class=EmailPopup', 
          data:form_data,
          success: function(html){ 
            	     if(html == '') {
              	       html = "Sorry there was an error sharing!";
            	     }
            	     $('#mr_email_popup .content-processing').fadeOut();
            	     $('#mr_email_popup .content-result').html(html).fadeIn(); 
            	   }
        });
      });
  	});
  	var show_email_share_link_dialog = function() { $('#mr_email_popup').dialog('open'); return false; };
    var hide_email_share_link_dialog = function() { $('#mr_email_popup').dialog('close'); return false; };
    </script>
