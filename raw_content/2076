			}
			if (w.addEventListener) { w.addEventListener("load", go, false); }
			else if (w.attachEvent) { w.attachEvent("onload",go); }
		}(window, document, 'script'));
	</script>
		<script type="text/javascript">
		document.onkeydown = chang_page;
		function chang_page(e) {
		    var e = e || event,
		    keycode = e.which || e.keyCode;
			var obj = e.target || e.srcElement;
			if(obj.tagName.toLowerCase()=="textarea"){return;}
			if(obj.tagName.toLowerCase()=="input"){return;}
			if (keycode == 37) { $(".toolbar a.new-icon-prev").addClass("hover"); location = "http://thenextweb.com/2009/12/26/web-podcast-episode-4/"; }		    if (keycode == 39) { $(".toolbar a.new-icon-next").addClass("hover"); location = "http://thenextweb.com/shareables/2009/12/26/geek-gangs-these-days-this-is-for-using-comic-sans/"; }		    
	    }
