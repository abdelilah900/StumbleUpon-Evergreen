	<script type="text/javascript" language="javascript">
		var numTabs = 1;

		function showTab(selectedTab) {
			var i = 0;
			//loop through all tabs and set tab highlight/visibility
			for (i=1; i<=numTabs; i++) {
				document.getElementById('most-popular-tablink'+i).className = selectedTab == i ? 'selected' : '';
				document.getElementById('most-popular-tab'+i).style.display = selectedTab == i ? '' : 'none';
			}
		}
	</script>

