<script language="javascript">						
		var articleId = "7things.php";
		var articleUrl = "http://www.menshealth.com/spotlight/heart/7things.php";
	
	function submitRequest() {
		//	alert('articleId ' + articleId);
		var requestBatch = new RequestBatch();   
		var articleKey = new ArticleKey(articleId);    
		//	alert('articleKey ' + articleKey);
		requestBatch.AddToRequest(articleKey);    
		//	alert('serverUrl ' + serverUrl);
		requestBatch.BeginRequest(serverUrl, articlePageCallback);   

	}
	
	function articlePageCallback(responseBatch) {
		//	alert('articlePageCallback responseBatch.Responses.length ' + responseBatch.Responses.length);
		if (responseBatch.Responses.length == 0) {   
			// Do nothing. The request must have failed.   
		} else {
			//var articleCommentsCount = document.getElementById('articleCommentsCount');   
			//	alert('articleCommentsCount ' + articleCommentsCount);
			//var articleRecommendCount = document.getElementById('articleRecommendCount');   
			//	alert('articleRecommendCount ' + articleRecommendCount);
			var article = responseBatch.Responses[0].Article;   
			var numComments = article.Comments.NumberOfComments;
	  		var numRecommend = article.Recommendations.NumberOfRecommendations;
				//alert('article ' + article);
	  			//alert('article.Comments.NumberOfComments ' + numComments);
	  			//alert('article.Recommendations.NumberOfRecommendations ' + numRecommend);
	  		
	  		
	 		document.getElementById('commentsAndRecommendCount').innerHTML = '' 
	 												+ numComments 
	 												+ ' <a href=\"#ReaderComments\">Comments</a> '
	 												+ numRecommend
	 												+ ' Recommended';
		}   
	}   
	</script>
	 
					
