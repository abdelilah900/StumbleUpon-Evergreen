<script type="text/javascript">

//POWERS MORE LINK IN HAT
styleJsObj.moreToggle = {
	toggletimeout : null,
	hatMoreLinksElement : $("#sl-hat-more"),
	hatMoreLink : $("#sl-hat-more-link"),
	toggletime : 1000
};



styleJsObj.moreToggle.init = function(id) {
	//set events on more link
	$("#sl-hat-more-link").unbind('click');
	$("#sl-hat-more-link").mouseover(function(e) {
		styleJsObj.moreToggle.toggleMoreChannels();
	});	
	$("#sl-hat-more-link").mouseout(function(e) {
		styleJsObj.moreToggle.toggleOffMoreChannels();
	});	
	
	//set events on link container
	$("#sl-hat-more").mouseover(function(e) {
		styleJsObj.moreToggle.toggleMoreChannels();
	});	
	$("#sl-hat-more").mouseout(function(e) {
		styleJsObj.moreToggle.toggleOffMoreChannels();
	});	
}

styleJsObj.moreToggle.toggleMoreChannels = function(){
	styleJsObj.moreToggle.cancelCloseFlyout();
	var toggleDiv = $("#sl-hat-more");
        toggleDiv.slideDown('fast');
}

styleJsObj.moreToggle.toggleOffMoreChannels = function(){
	styleJsObj.moreToggle.toggletimeout = window.setTimeout(styleJsObj.moreToggle.closeMoreChannels,styleJsObj.moreToggle.toggletime);
}

styleJsObj.moreToggle.closeMoreChannels = function(){
	var toggleDiv = $("#sl-hat-more");
	toggleDiv.slideUp('fast');
}


styleJsObj.moreToggle.cancelCloseFlyout = function(){
	if(this.toggletimeout){
		clearTimeout(styleJsObj.moreToggle.toggletimeout);
		this.toggletimeout = null;
	}

}


</script>
  
<script type="text/javascript"> 
   $(document).ready(function() {
 
      	//INIT NAVIGATION
    	if ($("ul.sl-nav-list").length) {
    		$('ul.sl-nav-list').superfish({
	    		delay:500,
				disableHI:false
   		 	});
 
    	}
 
        //INIT MORE LINKS HAT POPUP
    	if ($("#sl-hat-more").length && $("#sl-hat-more-link").length) {
      		 styleJsObj.moreToggle.init();
		}
		
      //INIT NAV DATA MANIPULATION 
      styleJsObj.navDataManipulation();

  	  // INIT DL HUB
	  styleJsObj.dlHub();

	 // INIT DON'T MISS
	 styleJsObj.dontMiss();

         // INIT NAV HOVER STATE
	 styleJsObj.navHoverState();
         
         // INIT HEADLINE TITLE HOVER EFFECTS
         styleJsObj.modTitleHover();

//To hide the empty LI's in the nav dropdowns. 
$('.sl-subnav-list').find('li a:empty').parent().hide();

// Removes underline from last element on the news-list-post module
$('#roc .postborder:last').remove();

// Removes the last pipe from "Latest On Stylelist" module..
$('#latestOnStyle ul li a:last').css("border-right","none");


 });


 </script> 

<script type="text/javascript"> 
(function($){
$(document).ready(function(){
	var $pictureUrl = $('#picture-url');
	
	$('#customLink').multiAuth({
		devId: 'ao1NGh4EO09Dgh',
		getTokenCallback: function(json){
		
			var $elem = $(this.authLink),
				response = json.response,
				userAttr;
				
			if (response.statusCode === 200) {
			
				userAttr = response.data.userData.attributes;
				$elem.html('Sign out</a>');
				$('#auth-user-name').html('Hi ' + userAttr.displayName);
                                $('.sl-hat-list-right').css('padding-right','45px');
				if (userAttr.pictureUrl) {
					$pictureUrl.html('<img src="' + userAttr.pictureUrl + ' /> ');
				}
				
			} else {
			
				$elem.html('Sign In / Register');
				$pictureUrl.html('');
                                $('#auth-user-name').empty();
                                $('.sl-hat-list-right').css('padding-right','35px');				
			}
		},
		successUrl: 'http://main.stylelist.com/_uac/authReceiver.html'
		
	});
 
});
})(jQuery);
</script>

<script type="text/javascript">    
$(document).ready(function(){
	$(".onlyOnRightImg img").each(function() {
 		if($(this).attr("src") != '') $(this).css("display","block")	
 		else $(this).css("display","none")
	})

	$(".mostPopularContent img").each(function() {
 		if($(this).attr("src") != '') $(this).css("display","block")	
	 	else $(this).css("display","none")
	})
	$(".onlyOnRightImg img").each(function() {
 		if($(this).attr("src") != '') $(this).css("display","block")	
	 	else $(this).css("display","none")
	})
});
</script> <!-- JS Bottom -->

