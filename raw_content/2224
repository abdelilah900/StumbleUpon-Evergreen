	//shows and hides items in those modules that contain more than ten items
	
	$j(function() {
		$j("ol.lst-blogs-categories").each(function() {
			$j("li.cat-item:gt(9)", this).hide(); /* :gt() is zero-indexed */
			$j("li.cat-item:nth-child(10)", this).after("<li class='catMore'><a href='#'>More...</a></li>");
		});
		$j("li.catMore a").live("click", function() {
			var liCat = $j(this).parents("li:first");
			liCat.parent().children().show();
			$j("li.cat-item:last-child").after("<li class='cat-item catLess'><a href='#'>Show Fewer...</a></li>");
			liCat.remove();
			return false;
		});
		$j("li.catLess a").live("click", function(){
			$j("ol.lst-blogs-categories").each(function() {
				$j("li.cat-item:gt(9)", this).hide(); /* :gt() is zero-indexed */
				$j("li.cat-item:nth-child(10)", this).after("<li class='cat-item catMore'><a href='#'>More...</a></li>");
				$j("li.cat-item:last-child").remove();
			});
		});
	});
</script>





    
