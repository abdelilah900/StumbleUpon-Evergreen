// BEGIN DOL Web Analytics 1.0

try {
    cto = new CTO();
    
    // account info
    cto.account="familyfuncom";
    cto.category="dfam";
    cto.site="ffn";
    cto.contentType="Recipe";
    
    // omniture values
    
      cto.siteSection="xmas:xmas-recipes:gifts-from-the-kitchen:xmas-mixes";
      cto.pageName="687223-hot-chocolate-cones";
 
         
    //internal search values
    cto.property="";
    
    
    cto.numSearchResults = "";
    
    cto.searchRefinement="";
    
    
    cto.searchType="";
    
    
    cto.internalSearchPhase = "";
    
    
    
    
    cto.track(); 
} catch (err) {
    //alert(err.description);
}
// END DOL Web Analytics 1.x
    
    
    var ratingsUserId = null;
    
    var rater1Info = null;
    
    function organizerStatusUserCallback(userName, userId, loggedIn) {
      
        if (loggedIn == false) {
          return;
        }
      
        dojo.xhrGet({      
            url: "/svc_organizer_v1/organizer/status/" + userId + ".json;contenttype=FFR;contentRefId=687223",
            headers: { "Accept" : "application/*+json" },
            handleAs: "json",
            timeout: 500000,
            load: function( response, ioArgs ) {
                try {
                  //window.console.debug( response );
                  if (response.organizerStatus.inOrganizer == true) {       
                    var el = dojo.byId("saveToOrganizer");
                    var parentNode = el.parentNode;
                    parentNode.setAttribute("id", "savedToOrganizerParentNode");
                    parentNode.removeChild(el);
                    var span = document.createElement('SPAN');
                    span.setAttribute("id", "savedToOrganizerBoxSpan");
                    span.setAttribute("class", "fontGreen savedToRecipeBox");
    
                    if (response.organizerStatus.contentTypeCode == 'FFC') {
                      span.innerHTML = "This craft is in your craft box."
                    } else {
                      span.innerHTML = "This recipe is in your recipe box."
                    }
                    
                    dojo.place(span, parentNode);
                  }
                }  // try
                catch( x ) {
                  return; 
                }
        },
        error: function( response, ioArgs ) {
          window.console.debug( "error xhrGet", response, ioArgs );
        } // error()
       }); // dojo.xhrGet(...
    }
    
    function ratingsUserCallback(userName, userId, loggedIn) {
    
    
      dojo.require("dojo.cookie");
      
      if (loggedIn == false) {
        ratingsUserId = -1;
      } else {
        ratingsUserId = userId;
      }
    
        dojo.xhrGet({      
            url: "/svc_rating_v1/687223/averagerating.json;;contenttype=FFRecipe",
            headers: { "Accept" : "application/*+json" },
            handleAs: "json",
            timeout: 500000,
            load: function( response, ioArgs ) {
                try {
                  var isDisabled = false;
                  if ( dojo.cookie("voted687223") === "yes") {
                    isDisabled = true;
                  }

                  rater1Info = {
                      disabled : isDisabled,
                      containerId : "rater1",
                      rating    : response.rating.averageRating,
                      ratingCount         : response.rating.ratingCount,
                      id      : 687223,
                      type    : "FFRecipe",
                      loggedin  : loggedIn,
                      sorryMsgId  : "sorryMsg",
                      loginMsgCN  : "loginMessage",
                      starsIdBase : "star-",
                      starSpanId  : "star-label",
                      ratedStarsIdBase: "rated-star-",
                      raterRatingCountId: "rater-rating-count",
                      ratedRatingCountId: "rated-rating-count",
                      imageFresh      : "http://a.dolimg.com/familyfun/assets/images/mainContent/orangeStar11x10.gif",
                      imageRotten     : "http://a.dolimg.com/familyfun/assets/images/mainContent/greyStar11x10.gif",
                      loggedInImgName   : "",
                      loggedOutImgName  : "&lpos=Not_So_Fast_Popup&lid=bottom_page",
                      ratingPhrases   : ["Not for me", "Just okay", "I like it", "I like it a lot", "I love it"],
                      defaultText     : "Click star to rate",
                      categoryId      : "food",
                      articleTitle    : "Strawberry-Rhubarb+Crisp+"
                  };

                  rating_attach(rater1Info);

                  var outerMostDiv = dojo.byId("ratingsOuterMostDiv")
                  outerMostDiv.style.display = "block";        
                  dojo.byId( "star-label" ).title = "average: " + response.rating.averageRating;
                }  // try
                catch( x ) { return; }
          },
          error: function( response, ioArgs ) {
          var outerMostDiv = dojo.byId("ratingsOuterMostDiv")
          outerMostDiv.style.display = "block";      
    
          window.console.debug( "error xhrGet", response, ioArgs );
          } // error()
        }); // dojo.xhrGet(...
    } // ratingsUserCallback
          
    var userServiceCallbacks = new Array(ratingsUserCallback, organizerStatusUserCallback);
    
    //preload
    new Image().src="http://a.dolimg.com/familyfun/assets/images/mainContent/orangeStar11x10.gif";
    new Image().src="http://a.dolimg.com/familyfun/assets/images/mainContent/greyStar11x10.gif";
 
  </script>
 
