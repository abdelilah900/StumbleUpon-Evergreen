			if (segs_beg>=0){ segs_beg=document.cookie.indexOf('=',segs_beg)+1;
				if(segs_beg>0){var segs_end=document.cookie.indexOf(';',segs_beg);
					if(segs_end==-1) segs_end=document.cookie.length;
					rsi_segs=document.cookie.substring(segs_beg,segs_end).split('|');
