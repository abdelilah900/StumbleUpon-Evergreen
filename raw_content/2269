                                var act = new gigya.services.socialize.UserAction();
                                
                                act.setDescription(gal_abstract);
                                act.addMediaItem({ type: 'image', src: "/polopoly_fs/1.32827!/img/httpImage/image.jpg_gen/derivatives/index_300/image.jpg", href: gal_url });
                                act.setTitle(gal_title);    
                                act.setLinkBack(gal_url);
                                
                                var showShareBarUI_params=
                                {   
                                    containerID: 'horizontal',
                                    shareButtons: [
                                        { provider: 'Facebook-Like',action: 'recommend',tooltip: 'Recommend',enableCount: 'true'},
                                        { provider: 'Twitter',enableCount: 'true'},
                                        { provider: 'Google-plusone', enableCount: 'false'},
                                        { provider: 'Stumbleupon', enableCount: 'false'},
                                        { provider: 'Email'}
                                    ],
                                    userAction: act,
                                    onLoad: onLoadHandler,
                                    onSendDone: onSendDoneHandler
                                }
                                function onLoadHandler(event){
                                    // Workaround: Remove the og tags on the javascript level so Gigya won't use as the highest priority.
                                    // Gigya will fix bug on 3/25.
                                    var og_ele = document.getElementById("og_url");
                                    if(og_ele != null) {
                                        og_ele.parentNode.removeChild(og_ele);
                                    }
                                    og_ele = document.getElementById("og_description");
                                    if(og_ele != null) {
                                        og_ele.parentNode.removeChild(og_ele);
                                    }
                                    og_ele = document.getElementById("og_image");
                                    if(og_ele != null) {
                                        og_ele.parentNode.removeChild(og_ele);
                                    }
                                    og_ele = document.getElementById("og_title");
                                    if(og_ele != null) {
                                        og_ele.parentNode.removeChild(og_ele);
                                    }
                                    if(loginId.length > 0){
                                        jQuery('input[id$="gigya.services.socialize.plugins.share.showShareUI_tbYourEmail"]').val(loginId);
                                    }
                                }
                                // onSendDone event handler.
                                function onSendDoneHandler(event){
                                    if (event.providers == 'email') { 
                                        // add this to email logger
                                        jQuery('.logger').empty();
                                        jQuery('.logger').append('<img src="/logger/p.gif?type=EMAILED&d=$path&a=1.32826"/>');
                                    }
                                    else{
                                        // add this to share logger
                                        jQuery('.logger').empty();
                                        jQuery('.logger').append('<img src="/logger/p.gif?type=SOCIALNETWORK&d=$path&a=1.32826"/>');
                                    }
                                }
                               
                                gigya.services.socialize.showShareBarUI({},showShareBarUI_params);
                            </script> 
