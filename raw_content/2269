function doNextPrev() {
    if(currImage == gal_size - 1) {  
        document.location = 'http://www.nydailynews.com/life-style/fashion/2008-victoria-secret-angels-fashion-show-gallery-1.32826?pmSlide=' + gal_size;
    } else {
        LoadGallery(nextImage, false);
    }
    return true;
}         
var slideNo = 0;
if(!isNaN(slideNo) && slideNo > 0 && slideNo < gal_size) {
    LoadGallery(slideNo, false);
    if(window.history && window.history.pushState) {
        window.history.pushState({}, "2008 Victoria\'s Secret Angels Fashion Show", 'http://www.nydailynews.com/life-style/fashion/2008-victoria-secret-angels-fashion-show-gallery-1.32826');
    } else {
        document.location = document.location.protocol + "//" + document.location.host + document.location.pathname;
    }
}

if(currImage == 0) {
    LoadGallery(0, true);
