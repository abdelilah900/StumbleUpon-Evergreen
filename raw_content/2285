      $('#recipe_slides').slides({
        preload: true,
        generatePagination: false
      });

      if ($(window).width() > 480) // Scroll tracking. Disable for mobile devices
      {
        var position = $(".footer").offset().top - $("#single_recipe").height() - 60;
        $("#single_recipe").followTo($("#single_recipe").offset().top, position);
      }
      else
      {
        $(document).ready(function() {
            $("#single_recipe").css({
                position: 'static'
            });
        });
      }

    });
  </script>

