    <h1 class="individual">Pasta Al Forno with Pumpkin and Pancetta</h1>
    
    
      <h2 class="arial_bold">
        <a href="http://www.food52.com/recipes/search?ca=85">
          <span class="orange">Community Pick!</span>
        </a>
      </h2>
    
    <div id="tabs">
    <ul class="nav">
      <li><a href="#recipe_tab">Recipe</a></li>
      
      
      <li>
        <a href="#foodpickle_tab" onclick="tab();">
          
              4 Questions
          
        </a>
      </li>
      <!-- <li><a href="#cooking_view">Cooking View</a></li> -->
    </ul>
    <div id="recipe_tab">
      <div class="recipe">
        <div id="recipe_slides">

          
            <div id="single_slide">
              <div class="image_container">
                <img alt="Pasta Al Forno with Pumpkin and Pancetta" src="http://d2k9njawademcf.cloudfront.net/indeximages/10797/nine_col/5095379791_2b0be6fffb_z.jpg?1289326769" />
              </div>
              <div class="caption" style='bottom:0;' >
                <p>Photo by Sarah Shatz</p>
              </div>
            </div>
          

          
          
              <a href="#slideshow_open" name="modal" id="slideshow" onclick="scrollTo(0,0);"><span>Slideshow</span></a>
          

        </div>
