			<div id="shares_for_post_51062" class="shares"><div class="share-counts large-icons"><iframe src="http://finder.good.is/like_buttons/?layout=30px&amp;url=http%3A%2F%2Fwww.good.is%2Fpost%2Fsix-dangerous-products-men-use-daily-and-what-to-use-instead%2F&amp;image_url=http%3A%2F%2Fcms1.good.is%2Fposts%2Fslide_slidethumb_1287067371man_products_header.jpg&amp;tags=Lifestyle%2Chealth%2Cenvironment%2Cmen%2Chow-to%2Cnatural+health%2Cnatural+personal+care+products%2Corganic+beauty+products%2Cshaving+cream%2Csperm+count&amp;description=Six+Dangerous+Products+Men+Use+Daily%E2%80%94And+What+To+Use+Instead" scrolling="no" frameborder="0" allowTransparency="true" class="its-good"></iframe><script type="text/javascript" src="http://finder.good.is/javascripts/share_button.js"></script><span class="fold"></span></div><script type='text/javascript'>
$(document).ready(function() {
  var shares = $('#shares_for_post_51062'),
      content = $('div.content'),
      sharesHeight = shares.outerHeight(true),
      sharesLeft = shares.offset().left,
      yMin = shares.offset().top - parseInt(shares.css('margin-top'), 10),
      yMax = content.offset().top + content.outerHeight() + parseInt(shares.css('margin-top'), 10),
      position = 'default',
      positionCSS = {
              'default': {},
              'fixed': {position: 'fixed', top: 0},
              'bottom': {position: 'absolute', top: yMax - sharesHeight}
      };
  
  function setSharesPosition() {
    var w = $(this), x = w.scrollLeft(), y = w.scrollTop();
    if (y > yMin) {
      if (y + sharesHeight < yMax) {position = 'fixed';}
      else {position = 'bottom';}
    }
    else {position = 'default';}
    // Clear all styles, then set appropriately.
    shares.removeAttr('style');
    shares.css(positionCSS[position]);
    // Adjust for horizontal scroll.
    if (position === 'fixed' && x > 0) {shares.css({left: sharesLeft - x});}
  }
  
  // Only slide the shares box on articles that are long enough.
  if (yMax - sharesHeight > yMin) {
          $(window).scroll(setSharesPosition);
          setSharesPosition();
  }
})
</script>
