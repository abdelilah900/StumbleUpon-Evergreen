		<script type="text/javascript">
			function searchFocus() {
				input = document.getElementById('s');
				if( input.value == 'Search Kayotic Kitchen' ) {
					input.value = '';
				}
			}
			function searchBlur() {
				input = document.getElementById('s');
				if( input.value == '' ) {
					input.value = 'Search Kayotic Kitchen';
				}
			}
		</script>	</div>
