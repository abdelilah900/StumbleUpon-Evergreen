//MORE IN NAV IN LEFT RAIL
var morInLinks = $("#moreIn").find("ul li");
var morInLinkMatch = morInLinks.children("a[href*='/easter/']");
if(morInLinkMatch.length >0){
morInLinkMatch.parent().hide();
morInLinkMatch.parents('ul').show();
}else{
$("#moreIn").find("ul li.featHead:contains('Easter')").parent().show();
}
