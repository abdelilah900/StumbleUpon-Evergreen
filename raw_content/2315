<script type="text/javascript">

$(document).ready(function() {

var dtext = $('#GH_search_field').val();


var footerHTML = "<form id='ftrSrchid' name='bb_topform' action='/search'><input type='text' name='query' id='ftrtopquery'  value='"+$("#GH_search_field").val()+"' class='search'/><button id='ftrsrchLnk' title='Title' class='searchButton' type='submit'>Search<img border='0' src='http://www.aolcdn.com/channels/hed_mag' alt='Title' class='magnGlass'/></button></form><div class='clear'/> ";
 
$('#footer div.hdrSrch').html(footerHTML);

});

(function(){
var d = document, s = d.createElement('script');
s.type = 'text/javascript';
s.src = "http://o.aolcdn.com/feedback/feedback1.js";
d.getElementsByTagName('head')[0].appendChild(s);
})();

$(document).ready(function() { 

$(".ftrBtm").before("<div><a class='fllogo' href='http://living.aol.com'></a></div>");

var fdl = document.getElementById("GH_fb_link").innerHTML;

$("#cv-feedback-link").remove();

$(".ftrSrch").after(fdl);

$("#GH_search_field, #ftrtopquery").focus(function(){
		$(this).val('');
});
 
$("#GH_search_field, #ftrtopquery").blur(function(){
               var value = $(this).val();
                var defValue = 'Search for Recipes';
                (value == '') ? $(this).val(defValue):'';	

});


});
</script>


