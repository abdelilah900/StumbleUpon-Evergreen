<script>
YAHOO.Sports.fantasyPerformersTabsInit = function() {
    YAHOO.util.Event.addListener("hot-toggle", "click", YAHOO.Sports.toggleHotNot, true);
  YAHOO.util.Event.addListener("not-toggle", "click", YAHOO.Sports.toggleHotNot, false);
  YAHOO.Sports.toggleHotNot(null, true);
    YAHOO.Sports.tabs.init("fantasy-perf");
}
</script>

        
        
</div>
  
<script>
YAHOO.Sports.fantasyCollapsed = false;

YAHOO.Sports.toggleFantasyCollapse = function() {

  // collapse if open
  if (!YAHOO.Sports.fantasyCollapsed) {
    YAHOO.util.Dom.addClass("fantasy-control", "collapsed");
    YAHOO.util.Dom.addClass("fantasy", "collapsed");
    // update cookie
    YAHOO.Sports.fantasyCollapsed = true;
    YAHOO.Sports.Cookie.set("fantasy_fupdate", 0);

  } else {
    YAHOO.util.Dom.removeClass("fantasy-control", "collapsed");
    YAHOO.util.Dom.removeClass("fantasy", "collapsed");
    // reset tab panes
    if(YAHOO.Sports.fantasyPerformersTabsInit) {
      window.setTimeout(YAHOO.Sports.fantasyPerformersTabsInit,600);
    }
    if (YAHOO.Sports.fantasyUpdatesTabsInit) {
      window.setTimeout(YAHOO.Sports.fantasyUpdatesTabsInit,600);
    }
    // update cookie
    YAHOO.Sports.fantasyCollapsed = false;
    YAHOO.Sports.Cookie.set("fantasy_fupdate", 1);
  }
}

// collapse initially if cookie says to collapse
if(YAHOO.Sports.Cookie.get("fantasy_fupdate") == 0) {
  YAHOO.util.Event.onContentReady("bd", YAHOO.Sports.toggleFantasyCollapse);
} else {
  }

YAHOO.Sports.fantasyCollapseContents = YAHOO.util.Dom.getElementsByClassName("contents", "div", "fantasy-updates");
YAHOO.util.Event.addListener("fantasy-control", "click", YAHOO.Sports.toggleFantasyCollapse);

YAHOO.util.Event.onContentReady("fantasy-perf", YAHOO.Sports.fantasyPerformersTabsInit);
YAHOO.util.Event.onContentReady("fantasy-updates", YAHOO.Sports.fantasyUpdatesTabsInit);
</script>
