<script>
(function() {
    var area = document.getElementById("lrec_area"),
        loader = document.getElementById("lrec_loader"),
        parent = YAHOO.util.Dom.getElementsByClassName("yui-b", "div", "yui-sub")[0];
    if (! parent) { return false; }
    var loaderFirstChild = YAHOO.util.Dom.getFirstChild(loader);
    if(loaderFirstChild && loaderFirstChild.tagName === "STYLE") {
         loader.removeChild(loaderFirstChild);
         loader.appendChild(loaderFirstChild);
    }
    parent.replaceChild(loader, area);
    YAHOO.util.Dom.setStyle(loader, "visibility", "visible");
})();
</script>
