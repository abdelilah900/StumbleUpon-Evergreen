			if (totalVids) {
				allVids = totalVids;
				backAction = -1;
				nextAction = 1;
			} else {
				allVids = theSource.length;
				backAction = '\"back\"';
				nextAction = '\"next\"';
			}	
		}
		if (div === "searchvidoutput" && theSource.length === 0) {
			searchVidsNull();
