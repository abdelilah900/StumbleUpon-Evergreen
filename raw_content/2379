jQuery(document).ready(function(){
	jQuery('.imagecol').each(function(){
		var t = jQuery(this).find('.wpcart_gallery .thickbox');
		t.colorbox({
			maxWidth :'90%',
			maxHeight :'90%',
			returnFocus : false
		});
		
