<script type="text/javascript">
    var FeaturedPost = (function() {
        var fp = {};

        /* ********************* */
        /* Start of customization */
        /* ********************* */

        // the text that appears next to the image
        fp.text = 'The Memory Mapper lets you make a video keepsake.';

        // the url users will be taken when they click on the text next to the image
        fp.textUrl = 'http://ad.doubleclick.net/clk;241081292;63764456;j;pc=[TPAS_ID]';

        // this is the path the image thumbnail
        fp.imageSrc = 'http://desmond.imageshack.us/Himg59/scaled.php?server=59&filename=yfrogimage02.jpg&res=crop';

        // this is the url user will be taken to when clicking on the thumbnail
        fp.imageUrl = 'http://ad.doubleclick.net/clk;241081292;63764456;j;pc=[TPAS_ID]';

        // see more text, the text for the see more link
        fp.seeMoreText = 'Create yours now';

        // see more url, link to the see more page
        fp.seeMoreUrl = 'http://ad.doubleclick.net/clk;241081292;63764456;j;pc=[TPAS_ID]';

        // facebook like url
        fp.fbLikeUrl = 'http://www.facebook.com/visa?sk=app_215095905176343';

        // status text, shows up in Twitter status box when clicked on Tweet button
        fp.tweetText = 'Check out the Memory Mapper on Facebook. Just upload photos and map your memories to make a video keepsake. http://tinyurl.com/3qa49ao';

        // This is *OPTIONAL*. If no url is desired, leave it empty like fp.tweetUrl = ''
        // Url to share with Twitter when Tweeting, it will be shortened with t.co
        fp.tweetUrl = '';

        // This is *OPTIONAL*. If no screen name is desired, leave it empty like fp.tweetVia = ''
        // A screen name to associate with the Tweet. The provided screen name will be appended to the end of the tweet with the text: "via @username"
        fp.tweetVia = '';

        // This is *OPTIONAL*. If no hashtag(s) is/are desired, leave it empty like fp.tweetHashes = ''
        // Add context to the pre-prepared status update by appending #hashtags. Omit the "#" symbol and separate multiple hashtags with commas.
        // Proper syntax example : fp.tweetHashes = 'hello,winning'
        fp.tweetHashes = '';

        // This is *OPTIONAL*. If no related account(s) is/are desired, leave it empty like fp.tweetRelated = ''
        // Suggest accounts related to the your content or intention by comma-separating a list of screen names. After Tweeting, the user will be encouraged to follow these accounts.
        // Proper syntax example : fp.tweetRelated = 'yfrogpics,moehosseini'
        fp.tweetRelated = '';

        /* ********************* */
        /* End of customization */
        /* ********************* */

        // Google Analytics category
        fp.category = 'yfrog2/featured_post';

        // build the intent url for twitter
        fp.intentUrl = 'http://twitter.com/intent/tweet?text=' + encodeURIComponent(fp.tweetText);
        if (fp.tweetUrl)
            fp.intentUrl += '&url=' + fp.tweetUrl;
        if (fp.tweetVia)
            fp.intentUrl += '&via=' + fp.tweetVia;
        if (fp.tweetHashes)
            fp.intentUrl += '&hashtags=' + fp.tweetHashes;
        if (fp.tweetRelated)
            fp.intentUrl += '&related=' + fp.tweetRelated;

        return fp;
    })();
</script>
