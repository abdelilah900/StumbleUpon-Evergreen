<script type="text/javascript">
    var recipeName = "Oven-Fried Ranch Chicken";
    var recipeId = '63015';

    $j(document).ready(function() {
        $j('.item-container').live('click', function(e) {
            var relatedTitle = $j(this).find(".strip-rec-link-title").text();
            ntptEventTag('ev=relatedRecipes_click&RecipeTitle=' + relatedTitle);
        });

        if (typeof FB != 'undefined') {
            FB.Event.subscribe('edge.create', function(response) {
                ntptEventTag('ev=RecipeFacebookLike_bt&RecipeID=' + recipeId + ' &RecipeTitle=' + recipeName);
            });
        }
    });
