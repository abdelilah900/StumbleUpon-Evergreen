<script type="text/javascript">
//<![CDATA[
    $j(document).ready(function() {
        var authenticated = $j("#log").text().toLowerCase();
        var type = $j(".hdnCommentType").val().toLowerCase();
        formatUnicaCommentTag(authenticated, type);
        $j(".post-button").click(function() { setGoogleEvent("comment", type, self.location.href); });
        $j(".post-button").click(function() { setGoogleEvent(type, "comment", self.location.href); });
        $j(".post-button").click(function() { setGoogleEvent("comment", (authenticated == "false" ? "anonymous" : "logged in"), self.location.href); });
    });
    
    function formatUnicaCommentTag(authenticated, type) {
        if (authenticated != "false") {
            if (type == "recipe") {
                $j(".post-button").click(function() { ntptEventTag('ev=RecipeComment_user'); });
            }
            if (type == "collection") {
                $j(".post-button").click(function() { ntptEventTag('ev=postcollectioncomment'); });
            }
        }
    }
//]]>
</script>
    

