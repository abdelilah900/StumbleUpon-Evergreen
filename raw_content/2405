<script type="text/javascript">

$(document).ready(function(){
    $(".cl_tabber .tabs li").click(function(){
        var tabID = 1;
        var theTab = this;
        $(".cl_tabber .tabs li").each(function(index,elem){
            if(theTab == elem){ 
                tabID = index + 1;
            }
        });
        var child = $(".cl_tabber .tabContent li:nth-child(" +tabID+")");
        $(".cl_tabber .tabContent li").hide();
        var child = $(".cl_tabber .tabContent li:nth-child(" +tabID+")").show();
        $(".cl_tabber .tabs li").removeClass("selected");              
        $(this).addClass("selected");
    });

    var blogSearchDefault = 'Search the Clicker blog';
    $('#findInputBlog').click(function(){
        if($(this).val() == blogSearchDefault){
            $(this).val('');
        }
    }).blur(function(){
            $(this).val(blogSearchDefault);
    });


        $('.widgettitle').each(function(i, elem){
        var titleArr = $(elem).text().trim().split(' ');
        if(titleArr.length > 0){
            $(elem).removeClass('red'); 
            var first = '<span class="red">' + titleArr[0] + '</span> ';
            var rest = titleArr.splice(1).join(' ');
            $(elem).html(first + rest);
        }
    });

});
</script>




