	try {
		autoT = new AutoCompleter('findInput', 'http://www.clicker.com/s2?', 'autoT', {maxTitleLen: 38, extraWidth:71, offsetX:-4});
		autoB = new AutoCompleter('findInputBottom', 'http://www.clicker.com/s2?', 'autoB', {maxTitleLen: 25, offsetX:-4, extraWidth:78 });
	}
	catch(e) {};
	
	$('#findInput').focus();

		
	$(window).scroll(function() {$("#socialSide").css("right", "-" + $(window).scrollLeft() + "px");});
	
	function main_handleFBLogin(response) {
		
		setTimeout(function(){
			Clicker.reloadProfile();	
		},1);
	}

	function main_fbdisable() {
		if ( window.top.l.tempFBAcct ) {
			window.top.Clicker.delTmpFB(function(success){
				window.top.Clicker.FBGraph.doRevoke(true, function(response){
					window.top.location.href='/j_acegi_logout';
				});
			});
		}
		else {
			window.top.Clicker.fbdisconnectExistingUser();
		}
	}

	function main_fblearnmore(callback) {
		var url="/popup/fbWhyConnect/";
		Clicker.Dialog.show(url, callback);
		return false;
	}

	function main_hideRSWelcome() {
		$('#clickerWelcome').slideUp();
		Clicker.tokenpost("/user/AJAX/modUI/", {clickerWelcome: 0});
	}

	

	Clicker.FBGraph.init('885a1bcc845775d2b72336e7744eb596', 'fb-root');
	
	$(document).ready(function(){
		var pb = $("#profileBar");
		if ( pb.length > 0 ) {
			var fsrc = '/utility/profileBar/';
			if ( pb.hasClass('useCss2') ) {
				fsrc += '?useCss2=1';
			}
			pb.html('<iframe id="profileBarFrame" name="profileBarFrame" class="" src="' + fsrc + '" style="height: 55px; width:100%; overflow:hidden;"></iframe>');
		}
		if ( document.referrer.indexOf("www.stumbleupon.com") > 0 && location.pathname.indexOf("/popup/") < 0 ) {
			var sCookie = new Cookie('stumbled');
			if( !sCookie.exists() ){
				sCookie.bake(1, 315360000000, "/");
				Clicker.Dialog.show('/popup/stumbleupon/');
			}
		}
	});
	
</script>


<script type="text/javascript">
	var _gaq = _gaq || [];
	_gaq.push(['_setAccount', 'UA-3515502-4']);
	_gaq.push(['_setDomainName', '.clicker.com']);

	var isTempFBAccount = !! '';
	var fbIPDone = !! '';
	var FBAccountStatus = "none";
	if (fbIPDone) {
		FBAccountStatus = "fbipDone";
	} else if(isTempFBAccount) {
		FBAccountStatus = "tempFBAccount";
	}

	_gaq.push(['_setCustomVar',
			1, 					// slot 1 (of 1-5)
			'FBAccountStatus',	// our custom variable name
			FBAccountStatus, 	// needs to be a string value
			2					// set the scope to session level(2).
			]);

	_gaq.push(['_trackPageview']);

	(function() {
		var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
		ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
		var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
	})();
</script>



