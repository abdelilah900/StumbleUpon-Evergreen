                    /** custom getter */
                    var blogsRssValueGetters = defaultRssValueGetters
                        .clone()
                        .addItemGetter("descriptionImage", function(itemJson) {
                            var $description = $(itemJson.xmlNode).find("description");
                            return $($description.text()).filter("img").attr("src");
                        });
                </script>
                




