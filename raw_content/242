<script type="text/javascript" language="javascript">
    function redirectTOColumnist(ddlColumnist) {
        var myindex = ddlColumnist.selectedIndex
        var SelValue = ddlColumnist.options[myindex].value
        var baseURL = SelValue;
        top.location.href = baseURL;
    }
    function getColumnist(ddlColumnistCategory) {
        var myindex = ddlColumnistCategory.selectedIndex
        var SelValue = ddlColumnistCategory.options[myindex].value
        $("#td-columnist_finder_ajaxloader").show();
        $("#columnist").load("/includes/columnist?Query=" + escape(SelValue), function(responseText) {
            $("#td-columnist_finder_ajaxloader").hide();
        });
    };
</script>     


        
