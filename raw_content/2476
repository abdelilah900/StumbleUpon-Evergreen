}


function kdinit(){


$('#GH_hat').append("<ul><li class='promotxt'><a href=' http://survey.questionmarket.com/surv/764409/ai_start.php?site=10&from_ec=0' target='_blank'>Complete our survey for  a chance at $1000!</a></li></ul>");


