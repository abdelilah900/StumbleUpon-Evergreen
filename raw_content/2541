<script type="text/javascript">
  var $j = jQuery.noConflict();
	$j(document).ready(function(){
		$j('.thumb-big-mask.thumb-big-captionfull').hover(function(){
			$j(".cover", this).stop().animate(
			{
			left:'0px', 
			opacity: .9
			},
			{
			queue:false,
			duration:600
			}
			);
			
		}, function() {
			$j(".cover", this).stop().animate({
			left:'0px', 
			opacity: '0'
			},
			{queue:false,
			duration:600
			});
			
		});
		
		$j(".thumb-big-mask").click(function() {
		  window.location = $j(this).find("h2:first a:first").attr("href");
		});
	});
</script>

