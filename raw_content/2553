new QuickDegreeFinder().populate('.edudirect-degree_level_id', '.edudirect-category_id', '.edudirect-subject_id')
								 .setDefaultDegree('Bachelor')
								 .setDefaultCategory('Health')
								 .setDefaultSubject('Nursing')			
.onReady(function() {
    
    var selectOption = function(value, select) {
        var selector = 'option:contains("' + value + '"), option[value="' + value + '"]';
        
        $(selector, select).attr('selected', 'selected').trigger('change');
    };
    
    $('.edudirect-career').change(function() {
        var selections = this.value.split(',');
        
        try {
            selectOption(selections[0], '.edudirect-degree_level_id');
            selectOption(selections[1], '.edudirect-category_id');
            selectOption(selections[2], '.edudirect-subject_id');
								 
        } catch(e) {
            // Couldn't find the element, and we need to fail gracefully
        };
        
        // Uncomment this line to log out what the widget selections are & have the form popup in a new window with search results
        // console.log($(this).parents('form').attr('target', '_blank').attr('method', 'get').serializeArray());
    }).trigger('change');
    
});

</script>

