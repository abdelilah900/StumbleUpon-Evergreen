		jQuery('#slingshothref').attr('class','');
		jQuery('#ybhref').attr('style','color:#0081C2;');
		jQuery('#otherhref').attr('style','color:#000000;');
		jQuery('#slingshothref').attr('style','color:#0081C2;');
                clearInterval(inter);
	return false;
});



