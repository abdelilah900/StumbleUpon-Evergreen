jQuery(".selectBx").change(function() {

	var teamname = jQuery(this).find("option:selected").val();
	var dataString='teamname='+teamname;
	jQuery('#schoolfilter').empty();
	jQuery('#schoolfilter').append('<span style="align:center">Loading...</span>');	

	jQuery.ajax({
		type: "POST",
		url: "http://www.lostlettermen.com/get_school_stories.php",
		data: dataString,
		success: function(msg){
			jQuery('#schoolfilter').empty();
			jQuery('#schoolfilter').append(msg);
		}
	});     

});		

