jQuery(".oncampusnum").live("click", function(event){
		jQuery('.oncampusnum').children().attr('class','');
		jQuery(this).find('a').attr('class','selectedPage');
		jQuery('#oncampus').hide();
		jQuery('#oncampus1').hide();
		jQuery('#oncampus2').hide();
		jQuery('#oncampus3').hide();
		jQuery('#oncampus4').hide();
		jQuery('#oncampus5').hide();
		jQuery('#oncampus6').hide();
		jQuery('#oncampus7').hide();
		jQuery('#oncampus8').hide();
		jQuery('#oncampus9').hide();
		var number = jQuery(this).attr('value');
		
		if( number == 1)
		{
			jQuery('#oncampus').show();
			
		}
		if( number == 2)
		{
			jQuery('#oncampus1').show();
		}
		if( number == 3)
		{
			jQuery('#oncampus2').show();
		}
		if( number == 4)
		{
			jQuery('#oncampus3').show();
		}
		if( number == 5)
		{
			jQuery('#oncampus4').show();
		}
		if( number == 6)
		{
			jQuery('#oncampus5').show();
		}
		if( number == 7)
		{
			jQuery('#oncampus6').show();
		}
		if( number == 8)
		{
			jQuery('#oncampus7').show();
		}
		if( number == 9)
		{
			jQuery('#oncampus8').show();
		}
		if( number == 10)
		{
			jQuery('#oncampus9').show();
		}
                clearInterval(inter);
	return false;
});

