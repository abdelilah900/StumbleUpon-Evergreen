<script type="text/javascript">
	function openNewWindow(URLtoOpen,windowName,width,height) {
		windowFeatures ="menubar=no,scrollbars=no,location=no,favorites=no,resizable=no,status=no,toolbar=no,directories=no";
		winLeft = (screen.width-width)/2;
		winTop = (screen.height-(height+110))/2;
		myWin= open(URLtoOpen,windowName,"width=" + width +",height=" + height + ",left=" + winLeft + ",top=" + winTop + windowFeatures);
	}
//---------------------------------

    
$(window).bind("load", function() {

    var myHeight = $('#mainProdTable td.mainProdCell').height();
     $('#guruRecom').css('height', myHeight + 31); //31px to account for the top+bottom padding of mainProdCell


        var prodContH =  $('.prodCont').height();
        var prodContImgH = $('.prodCont .prodImg').height();

        var imgHFinal = (prodContH - prodContImgH) / 2;

        $('.prodCont .prodImg').css('margin-top',imgHFinal)


        $('.guruImgCont .guruImg').each(function(){
            var guruImgContH =  $(this).parents('.guruImgCont').height();
            var guruImgContImgH =  $(this).height();

            var imgHFinal = (guruImgContH - guruImgContImgH) / 2;
                                                                             
            $(this).css('margin-top',imgHFinal);


        })


});


</script>


