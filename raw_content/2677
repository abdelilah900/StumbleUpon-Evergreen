  <TR valign="top">
    <TD height="40" align="left" valign="top"><H1>Resting Heart Rate</H1></TD>
    <TD align="right" width="316" valign="top" class="Search">
	<FORM method="get" action="/newsletter.htm">
	<LABEL for="Email"><IMG align="absmiddle" alt="" src="/img/email.jpg" />&nbsp;<SPAN class="purple small">FREE workout tips</SPAN></LABEL>
	<INPUT id="email" type="text" value="Enter Email" class="n" onblur="searchboxes(this,'Enter Email',0);" name="email" size="17" onfocus="searchboxes(this,'Enter Email',1);" maxlength="128" />&nbsp;<INPUT name="submit" type="submit" value="Go" />
