<script>


function pimg()
{
    this.xOffset = 5;
    this.yOffset = 50;
        
    $(".pimg").hover(function (e)
    {        
        this.img_title = this.title;
        this.title = "";
        var img_src = $(this).attr('img_src');
        var desc = "";//"(this.img_title != "") ? "<h3>" + this.img_title + "</h3>" : "";
        var image = (img_src) ? img_src : this.src;
        if (img_src!='/' && img_src!='')$("body").append("<div id='pimg'><img src='/thumb.php?img=" + image + "' alt='Image preview' height='136' />" + desc + "</div>");        
        //$("#pimg").css("top", (e.pageY - xOffset-140) + "px").css("left", (e.pageX + yOffset) + "px");                
        $("#pimg").css("top", ($(this).offset().top-154) + "px").css("left", ($(this).offset().left) + "px");        
        $("#pimg").show();
    }, function ()
    {
        this.title = this.img_title;
        $("#pimg").remove();

    });
    /*$(".pimg").mousemove(function (e)
    {        
        $("#pimg").css("top", (e.pageY - xOffset-140) + "px").css("left", (e.pageX + yOffset) + "px");
    });*/
};
function tooltip()
    {
    this.xOffset = 5;
    this.yOffset = 50;
        
    $(".tooltip").hover(function (e)
    {        
        $.ajax({
                 type: "GET",
                 url: "dir/dir_health_guide2.php",
                 data: "search="+$(this).text(),
                 dataType: "xml",
                 success: function(xml) {
        var desc = "";//"(this.img_title != "") ? "<h3>" + this.img_title + "</h3>" : "";
        //var image = (img_src) ? img_src : this.src;
        var image;
        $(xml).find('item:first').each(function(){
            image=$(this).attr('img');
        });
        if (image)$("body").append("<div id='pimg'><img src='/thumb.php?img=" + image + "' alt='Image preview' height='136' />" + desc + "</div>");        
        $("#pimg").css("top", (e.pageY - xOffset-140) + "px").css("left", (e.pageX + yOffset) + "px");                
        //$("#pimg").css("top", ($(this).offset().top-154) + "px").css("left", ($(this).offset().left) + "px");        
        $("#pimg").show();
                 }   
                 });
        
        
    }, function ()
    {
        //this.title = this.img_title;
        $("#pimg").remove();

    });
    /*$(".pimg").mousemove(function (e)
    {        
        $("#pimg").css("top", (e.pageY - xOffset-140) + "px").css("left", (e.pageX + yOffset) + "px");
    });*/

}


