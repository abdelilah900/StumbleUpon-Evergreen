function checkemail(str){
var filter=/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i
if (filter.test(str))
testresults=true
else{
alert("Please input a valid email address!")
testresults=false
}
return (testresults)
}
    $('.thanksForSubscribe').hide();

	$('#ssGoBtn').click(function(){
	   if (checkemail($('#inputTopSubscriber').attr('value'))){
		$.post("subscribe.php", { email: $('#inputTopSubscriber').attr('value') },function(data){
		  if (data!='')alert(data);
          else $('.thanksForSubscribe').show();
		});
		//$('.subscriptionDropDown').slideUp();
        
        }
	});

	$('#sBottomGoBtn').click(function(){
	   if (checkemail($('#inputBottomSubscriber').attr('value'))){
		$.post("subscribe.php", { email: $('#inputBottomSubscriber').attr('value') },function(data){
		  if (data!='')alert(data);
          else $('#bottomSubscriber').fadeOut(); 
		});
		
        }
	});
	$('#inputTopSubscriber').keyup(function(e) {
      //alert(e.keyCode);
      if(e.keyCode == 13)$('#ssGoBtn').click();
    });
	$('#inputBottomSubscriber').keyup(function(e) {
      //alert(e.keyCode);
      if(e.keyCode == 13)$('#sBottomGoBtn').click();
    });      
 
});	
    function addoverlay(){
        $('.overlay').show();
