<script>
function thanks(from){
var i=0;

 $.ajax({
                 type: "GET",
                 url: "dir/dir_thanks.php",
                 data: "idc="+398,
                 dataType: "xml",
                 success: function(xml) {
                    $('.thankWindowResult').remove();
                    $('.pagerLic2').remove();
                 	$(xml).find('item').each(function(){

						if (i<from+5 && i>=from)$('#thankWindowResultContainer').append('<div class="thankWindowResult"><div class="thankWindowResultTitle"><a href="'+$(this).attr('href')+'" target="_blank">'+$(this).attr('title')+'</a></div><div class="thankWindowResultDesc"><a href="'+$(this).attr('href')+'" target="_blank">'+$(this).attr('desc')+'</a>'+($(this).attr('nohyp')?'<br>'+$(this).attr('nohyp'):'')+'</div></div>');
						i++;
						
                 		});
                	
						
                 		var num=Math.floor(i/5);
                 		if (num!=i/5)num++;
                 		var fromp=from/5+1;
                   if (num>1){
                    $('#thankWindowResultContainer').append('<div class="pagerLic2"></div>');
					if (fromp>1){$('.pagerLic2').append('<div class="pagerArrow"><a onclick="thanks('+((fromp>1)?(fromp-2)*5:'0')+')"><img src="img/general/arrow22.png" /></a></div>');}
					for (var b=((fromp>4)?fromp-4:1);b<((((fromp>4)?fromp+4:9)<num+1)?((fromp>4)?fromp+4:9):num+1);b++){
						
						if (b!=fromp)$('.pagerLic2').append('<div class="pagerNotCurrent"><p><a onclick="thanks('+(b-1)*5+')">'+b+'</a></p></div>');
						else $('.pagerLic2').append('<div class="pagerCurrent"><p><a onclick="thanks('+(b-1)*5+')">'+b+'</a></p></div>');
					}
					
					if (fromp!=num && num>1)$('.pagerLic2').append('<div class="pagerArrow"><a onclick="thanks('+((fromp)*5)+')"><img src="img/general/arrow11.png" /></a></div><div class="clear"></div>');
					
					
					$('.pagerLic2 a').css('cursor','pointer')
                    }
                }
                
            });
}

thanks(0);
</script>
