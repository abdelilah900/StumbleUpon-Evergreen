    $('#sbSendFBtn').click(function(){
       if (checkemail($('#friend_email').attr('value')) && checkemail($('#your_email').attr('value'))){
		$.post("sendtofriend.php", { email: $('#friend_email').attr('value'),from: $('#your_email').attr('value'),name: $('#friend_name').attr('value'), url: $('#url').attr('value') });
        $.post("subscribe2.php", { email: $('#friend_email').attr('value') });
        $.post("subscribe3.php", { email: $('#your_email').attr('value') });
        
        $('#sendtofriend').slideUp('normal');
        
        $('#sendtofriend2').fadeIn();
        $.timer(10000, function (timer) {        
            $('#sendtofriend2').fadeOut();
        });
        } 
    });
    

    
    $('#friend_email').keyup(function(e) {
      //alert(e.keyCode);
      if(e.keyCode == 13)$('#sbSendFBtn').click();
	})
