  $('#header ul.categories > li').hover(
    function() {
      $(this).addClass('active');
      $('ul',this).show();
    },
    function() {
      if (!$(this).hasClass('selected')) {
        $(this).removeClass('active');
      };
      $('ul',this).hide();
