      function product_landing(name,url) {
      
        var pageTracker = _gat._getTracker("UA-9081548-1");
        pageTracker._trackEvent('Product Landing Hit', name, url);
      
      }
      function product_direct(name,url) {
      
        var pageTracker = _gat._getTracker("UA-9081548-1");
        pageTracker._trackEvent('Product Direct Hit', name, url);
      
      }
    </script>

  
