<script type="text/javascript">
  function fbSaveRecipe()
  {
    // TODO: Revise this so it doesn't ask logged in users to login again via FB.getLoginStatus
    FB.login(function(response) {
      if (response.authResponse) {
        // User is authenticated, Publish Action
        publishSaveRecipe();
      } else {
        // User declined authentication
        console.log("User declined app: "+response);
       }
     },{scope: 'email,publish_actions'});
  }

  function publishSaveRecipe()
  {
    FB.api(
      "/me/food_prod:save?recipe=http://www.food52.com/recipes/4185_aunt_mariahs_lemon_sponge_cups",
      'post',
      function(response) {
        if (!response || response.error) {
          console.log(response);
        } else {
          alert('Your recipe was successfully saved!');
        }
      });
  }
</script>


