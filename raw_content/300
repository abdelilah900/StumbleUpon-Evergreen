    Event.observe(document.body, "mouseover", function(e) {
      var el = e.element();
      if (el.tagName == 'BODY') {
        el.setStyle({
          cursor: 'pointer'
        });
      }
    });

    Event.observe(document.body, "mouseout", function(e) {
      var el = e.element();
      if (el.tagName == 'BODY') {
        el.setStyle({
          cursor: 'auto'
        });
      }
    });
