/* some js to help ie handle pngs */
function iecheck(inc_seven) {
	// alert(navigator.platform);
    if (navigator.platform == "Win32" && navigator.appName == "Microsoft Internet Explorer" && window.attachEvent) {
        var rslt = navigator.appVersion.match(/MSIE (\d+\.\d+)/, '');
        var iever = (inc_seven)?(rslt != null && Number(rslt[1]) >= 5.5):(rslt != null && Number(rslt[1]) >= 5.5 && Number(rslt[1]) < 7 );
	} else {
		iever=false;
	}
    return iever;
}

var useFilter=iecheck(false);
var ie6Brand='<a href="http://www.surphace.com" target="_new" style="cursor: hand; display: block; width:135px; height:20px; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src=\'http://cdn11.surphace.com/images/poweredby/poweredbysurphace-inline.png\', sizingMethod=scale);"><img align="absmiddle" border="0" style="margin-left: 2px;" src="http://cdn11.surphace.com/images/iespacer.png"/></a>';
var normalBrand='<a href="http://www.surphace.com" target="_new"><img align="absmiddle" border="0" style="margin-left: 2px;" src="http://cdn11.surphace.com/images/poweredby/poweredbysurphace-inline.png"/></a>';
var brand='';
if(useFilter==false) {
	brand=normalBrand;
} else {
	brand=ie6Brand;
}
</script>

