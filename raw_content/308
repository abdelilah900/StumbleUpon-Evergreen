        $(document).ready(function () {
            $("#topbar").find("#sim").click(function () {
                this.style.behavior = 'url(#default#homepage)'; this.setHomePage('http://www.publico.pt')
                $('#topbar').hide(100);
                var exdate = new Date();
                exdate.setDate(exdate.getDate() + 365);
                var c_value = escape(1) + ((7 == null) ? "" : "; expires=" + exdate.toUTCString());
                document.cookie = "PUBLICO_HOMEPAGE=" + c_value;
            });
            $("#topbar").find("#nao").click(function () {
                $('#topbar').hide(100);

                var exdate = new Date();
                exdate.setDate(exdate.getDate() + 7);
                var c_value = escape(1) + ((7 == null) ? "" : "; expires=" + exdate.toUTCString());
                document.cookie = "PUBLICO_HOMEPAGE=" + c_value;
            });
            $("#topbar").find("#close").click(function () {
                $('#topbar').hide(100);
            });
        });

    </script>
    
    <script type="text/javascript">
        var keyCode = 0;
        var cancel = false;
        document.onkeydown = checkKeyCode;
        document.onkeyup = function () {
            if (!cancel) keyCode = 0;
        };
        function checkKeyCode(e) { 
            if (window.event) {
                keyCode = window.event.keyCode;
            } else if (e) {
                keyCode = e.which;
            }
        }

        $(document).ready(function () {
            setTimeout("bindClick()", 1000);
        });

        function bindClick() {
            $("a").each(function () {
                if (!$.data(this, "events")) {
                    var url = $(this).attr("href");
                    if (url.indexOf("javascript") == -1) {
                        $(this).bind("click", function (d) {
                            cancel = true;
                            try {
                                var url = $(this).attr("href");

                                var div = $(this).closest("div");
                                var parentDiv = div.parent().closest("div");
                                var maxDiv = parentDiv.parent().closest("div");

                                var location = "";
                                var divClass = div.attr("class");
                                var parentDivClass = parentDiv.attr("class");
                                var maxDivClass = maxDiv.attr("class");

                                if (div.find("h2").length == 1) {
                                    location = getLocation(div);
                                } else if (parentDiv.find("h2").length == 1) {
                                    location = getLocation(parentDiv);
                                } else if (maxDiv.find("h2").length == 1) {
                                    location = getLocation(maxDiv);
                                }

                                if (location == "" || location == null) {
                                    if (parentDivClass == "headlines") {
                                        if (divClass.indexOf("main") > -1)
                                            location = "DESTAQUE ESQUERDO";
                                        else
                                            location = "DESTAQUE CENTRAL"
                                    } else if (parentDivClass.indexOf("subscriber") > -1 || divClass.indexOf("subscriber") > -1) {
                                        location = "CAIXA ASSINANTES"
                                    } else if (divClass == "maisnoticias box") {
                                        location = "ULTIMAS NOTICIAS"
                                    } else if (divClass.indexOf("videos") > -1) {
                                        location = "VIDEO HP"
                                    } else if (maxDivClass == "topStories") {
                                        location = "DESTAQUES CABEÇA"
                                    } else if (divClass == "mainMenu") {
                                        location = "MENU SECÇOES"
                                    } else if (parentDivClass == "topBar") {
                                        location = "MENU TOPO"
                                    } else if (parentDivClass == "subBar") {
                                        if (divClass == "left") {
                                            location = "MENU CENTRAL ESQUERDO"
                                        } else {
                                            location = "MENU CENTRAL DIREITO"
                                        }
                                    } else if (divClass == "maisnoticias") {
                                        location = "CAIXA TOPS"
                                    } else if (parentDivClass == "suplement-entry") {
                                        location = getLocation(maxDiv.parent().closest("div").parent());
                                    } else if (divClass == "subMenu") {
                                        location = "SUBMENU";
                                    }
                                }

                                if (location == "" || location == null) {
                                    location = "OUTROS"
                                }

                                try {
                                    _gaq.push(['_trackEvent', location, "CLICK", url]);
                                } catch (e) {
                                    console.log(e);
                                }

                                if (keyCode != 17 && keyCode != 91) {
                                    setTimeout(function () {
                                        document.location = url;
                                    }, 1000);
                                    return false;
                                } else { 
                                    keyCode = 0
                                }
                            } catch (e) { }
                        });
                    }
                };
            });

            setTimeout("bindClick()", 1000)
        }

        function getLocation(container) {
            if (container.find("h2").first().find("span").length > 0) {
                if (container.find("h2").first().find("span").length > 1) {
                    if (container.find("h2").first().find("span").find("a").length > 0) {
                        return container.find("h2").first().find("span").first().find("a").html();
                    } else {
                        return container.find("h2").first().find("span").first().html();
                    }
                } else {
                    if (container.find("h2").first().find("span a").length > 0) {
                        return container.find("h2").first().find("span a").html();
                    } else {
                        return container.find("h2").first().find("span").html();
                    }
                }
            }
            return "";
        }
    </script>

</body>
