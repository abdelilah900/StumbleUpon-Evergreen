        function setCounter(id, Origin, type, section, title) {
            var ord = Math.random() * 10000000000000000;
            if (!isNaN(id)) {
                if (!document.getElementById("statPic")) {
                    var pic1 = new Image(1, 1);
                    pic1.id = "statPic";
                } else {
                    var pic1 = document.getElementById("statPic");
                }
                var params = "&id=" + id + "&Origin=" + Origin + "&type=" + type + "&section=" + section + "&title=" + title + "&url=" + escape(document.location.href) + "&ord=" + ord;
                pic1.src = "http://www.publico.pt/includes/lib/statistics.aspx?" + params;
            }
        }

        var doReload = true;
        var timerRefresh = setTimeout("go()", 300000);

        function go() {
            if (doReload)
                document.location.reload();
        }
