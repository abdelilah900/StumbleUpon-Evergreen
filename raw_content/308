<script type="text/javascript">
    var hideMenu;
    $(document).ready(function() {
        $("#subMenu-float").hide();

        $("#More").hover(function() {
            $("#subMenu-float").show('fast');
            $("#More").parent().removeClass().addClass("item on red");
        }, function() {
            hideMenu = setTimeout('hideContent()', 100);
            $("#More").parent().removeClass().addClass("item red");
        });

        $("#subMenu-float").hover(function() {
            try {
                clearTimeout(hideMenu);
                $("#More").parent().removeClass().addClass("item on red");
            } catch (e) { }
        }, function() {
            $("#subMenu-float").hide('fast');
            $("#More").parent().removeClass().addClass("item red");
        });
    });

    function hideContent() {
        $("#subMenu-float").hide('fast');
    }
</script>
