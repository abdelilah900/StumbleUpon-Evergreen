        <script type="text/javascript">
            function loadUtilities(url) {
                var arr = url.split("/");
                switch (arr[2]) {
                    case "getNetworkUtilities":
                        $('.utilities-menu li').removeClass('maisnoticias-active');
                        $('.utilities-menu li:eq(0)').addClass('maisnoticias-active');
                        break;
                    case "getFormatsUtilities":
                        $('.utilities-menu li').removeClass('maisnoticias-active');
                        $('.utilities-menu li:eq(1)').addClass('maisnoticias-active');
                        break;
                    case "getUsefulUtilities":
                        $('.utilities-menu li').removeClass('maisnoticias-active');
                        $('.utilities-menu li:eq(2)').addClass('maisnoticias-active');
                        break;
                    case "getServicesUtilities":
                        $('.utilities-menu li').removeClass('maisnoticias-active');
                        $('.utilities-menu li:eq(3)').addClass('maisnoticias-active');
                        break;
                    case "getReaderUtilities":
                        $('.utilities-menu li').removeClass('maisnoticias-active');
                        $('.utilities-menu li:eq(4)').addClass('maisnoticias-active');
                        break;
                    default:
                        $('.utilities-menu li').removeClass('maisnoticias-active');
                        $('.utilities-menu li:eq(0)').addClass('maisnoticias-active');
                }
                $("#caixaUtilidadesConteudo").load(url + " #caixaUtilidadesConteudo", function () { $("ul.CS-GERAL li:nth-child(5n)").css("margin-right", "0px"); });
            }

        </script>
