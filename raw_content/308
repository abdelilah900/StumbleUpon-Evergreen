     $().ready(function () {
         var counter = 1;
         $('#coda-slider-1').codaSlider({
             dynamicArrows: false,
             dynamicTabs: false,
             autoHeight: false,
             autoSlide: true,
             autoSlideStopWhenClicked: true
         });
         $('#ops-wrapper').mouseenter(function () {
             if (counter == 1) {
                 $(this).css('width', '600px');
                 counter += 1;
             }
         });
         $('#ops-wrapper').mouseleave(function () {
             $(this).css('width', '300px');
         });
         $('#fechar').click(function () {
             $('#ops-wrapper').css('width', '300px');
         });
     });
</script>
