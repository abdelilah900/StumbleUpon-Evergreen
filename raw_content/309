function post_submit( postform ) {

	if ( postform.user.value == "" ) {
      alert("please input your name");
      return false;
  }
  if ( postform.comment.value == "" ) {
      alert("please input your comment");
      return false;
  }
  
  var Cmsg = new Object();
  Cmsg.m_channel  = cmnt_channel;
  Cmsg.m_newsid   = cmnt_newsid;
  Cmsg.m_content  = postform.user.value + ":\n" + postform.comment.value;
  Cmsg.m_config   = "c=" + postform.country.value;
  cmnt_post( Cmsg );   
  alert( "thanks for your comments" ); 
  
  postform.user.value='';
  postform.comment.value='';
  window.location.reload();
}
