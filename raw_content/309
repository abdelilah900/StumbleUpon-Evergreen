function changeForm(formType, oForm)
{
	switch(formType)
	{
		case 'local':
			oForm.action = "http://search.english.sina.com/en_search.php";
			document.getElementById('s_keyword').name = 'k';
			break;
		case 'web':
			oForm.action = "http://www.google.com/search";
			document.getElementById('s_keyword').name = 'q';
			break;
	}
}
</script>
