	<script type="text/javascript">
    var cmnt_channel	= "en";		
    var cmnt_newsid	= "200-1-313120"; 
    var cmnt_group		= 0; 
    </script>
    
    <script>
    function user_tile( Cmsg ) {
      var local_datetime = new Date( Cmsg.m_datetime.replace(/-/g,"/") );
      var serial = local_datetime.getTime() % 17 + 1;
      return "http://comment.news.sina.com/comment/images/user_tile/tile_" + serial + ".jpg";
    }
    
    // prefix zero
    function prefix_zero( i ) {
      if ( i < 10 ) {
        return "0" + i;
      } else {
        return i;
      }
    }
    // local time to GMT
    function gmt_datetime( datetime ) {
      var local_datetime = new Date( datetime.replace(/-/g,"/") );
      return ( local_datetime.getUTCFullYear() + "-" +
         prefix_zero((parseInt(local_datetime.getUTCMonth())+1)) + "-" +
         prefix_zero(local_datetime.getUTCDate()) + " " +
         prefix_zero(local_datetime.getUTCHours()) + ":" +
         prefix_zero(local_datetime.getUTCMinutes()) + ":" +
         prefix_zero(local_datetime.getUTCSeconds()) + " GMT" );
    }
    function parse_country( config ) {
    	var pos = config.indexOf( "c=" );
    	if ( pos != -1 ) {
    	    var pos_end = config.indexOf("&",pos);
    	    var result="";
            if ( pos_end != -1 )
                result =   trim( config.substr(pos+2,pos_end-pos-2)) ;        
            else 
                result = trim( config.substr(pos+2)) ;
    		return result!=""? ("(" + result + ")"):"";
    	} 
    	else {
    		return "";
    	}
    }
    //
    function cmnt_print() {
        var limit = 4;
        var html_content = "";
        if ( CmsgList.length > 0 ){
        	html_content += '<div class="ArticleBox"><div class="ArticleTitle"><h3>Comments(' + Count.c_show + ')</h3><span class="right"><a href="http://comment.news.sina.com.cn/comment/comment4.html?channel=en&newsid=200-1-313120&style=0">View all &gt;&gt;</a></span></div>';
    
        	if ( CmsgList.length <= limit )
        	limit = CmsgList.length;
    
        	for ( var i=0; i<limit; i++ ) {
    
        		html_content += '<div class="CellComm"><img src="' + user_tile( CmsgList[i] ) + '" width="58" height="58" alt="" class="photo"/><div class="aut"><strong style="color:#7A98C1">';
    
        		//if ( trim(CmsgList[i].m_user) == "" ) _s( "Sina User" );
        		//else _s( CmsgList[i].m_user ); 
    
        		var cmsg_user = "Sina User";
        		var cmsg_content = CmsgList[i].m_content;
    
        		var pos =CmsgList[i].m_content.indexOf( ":\n" );
        		if ( pos != -1 ) {
        			cmsg_user = trim( CmsgList[i].m_content.substr(0,pos) );
        			cmsg_content = trim( CmsgList[i].m_content.substr(pos+2) );
        		}
    
        		html_content+= cmsg_user;	
        		html_content+= parse_country(CmsgList[i].m_config);
        		html_content+= '</strong> wrote:<span class="time">' +  gmt_datetime( CmsgList[i].m_datetime ) + '</span></div><div class="say">';
    
        		html_content+= substr(cmsg_content,390); 
        		if ( cmsg_content.length > 390 )
        		html_content+= "..." ;
    
        		html_content+= '</div></div>' ;
        	}
    
    	    html_content+= "</div>" ;
        }
        //alert(html_content);
        document.getElementById("comment").innerHTML = html_content;
    
    }
    </script>
