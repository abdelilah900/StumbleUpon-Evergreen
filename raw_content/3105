function __TABBLO_TPT_LOAD() {

	var instructionsText = "";

	var imageUrl = "";

	var providerText= "";

	var useTemplate = "recipe_2";

	//var cp = "";

	var cpt = "(c) 2011 AOL Inc. All Rights Reserved.";
      
  providerText = "";
        
imageUrl = "http://o.aolcdn.com/os/living-food/images/kd-logo.png?cache=1";
       
 instructionsText = "";

 if(document.getElementById("rdir-body") != null)
        instructionsText = document.getElementById("rdir-body").innerHTML;       

providerText = document.getElementById("provided-by").getElementsByTagName("p")[0].innerHTML;

        providerText = 
providerText.substr(providerText.indexOf(':')+1,providerText.length);

providerText = 'Provided by:'+jQuery.trim(providerText);

//console.log(jQuery.trim(providerText));
      
 //alert(document.getElementById("provided-by").getElementsByTagName("img")[0].src);

if (document.getElementById("provided-by").getElementsByTagName("a")[0] != undefined){
    
if(document.getElementById("provided-by").getElementsByTagName("a")[0].innerHTML.length>0)
       
 providerText = 'Provided by: ' +document.getElementById("provided-by").getElementsByTagName("a")[0].innerHTML;
}
    if(document.getElementById("gallery") != null){
        
imageUrl = document.getElementById("gallery").getElementsByTagName("img")[0].src;
  
  }

else{
     
   useTemplate = "recipe_3";

}

    
 Tabblo.embedded.sites.SettingsObject.preprocess.apply({
	
   Properties:
	  
 {
	     
  template: useTemplate
	  
 },
	   
FixedContent:
	  
 {
	      
 accentcolor: '#603e1e',
	      
 accentcolor2: '#603e1e',
	       
logo:'http://o.aolcdn.com/os/living-food/images/kd-logo.png?cache=1',
	      
 provided_by: providerText,
	      
 image:imageUrl,
	     
  sectiontitle: 'Kitchen Daily',
	    
   copyright: cpt,
	    
   sitetitle: 'KitchenDaily',
	    
   instructions_head: 'Directions',
	     
  instructions_cont:'(Directions cont.)',
	      
 instructions:  instructionsText,
	      
 info: '10 min'
	    
},
	    // content definition:
	   
 Content:
	    {
		'pagetitle':   { match: 'css', selector:'h1' },

		'logo2':       {match:'css', selector: 'div.provided-by img', nodeContentType: 'image' }, 

		'grouping' : {match:'css',selector:'.category',recurse:true,ignore:true,continueHooks:true},
		
'ingredients':   { match: 'css', selector:'.recIngredientsList li' 
}  
	
    }
   
 },[]);
   
 Tabblo.embedded.printabulous();

} 

function MakePDF() {
  
  var tpS = document.createElement('script');
   
 tpS.setAttribute('type','text/javascript');
  
  tpS.setAttribute('charset','utf-8'); 
   
 tpS.setAttribute('src','http://h30405.www3.hp.com/edit/tptboot/1.0');
    
document.getElementsByTagName('body').item(0).appendChild(tpS);

}

function printthis(printType){
 
    MakePDF();

}


