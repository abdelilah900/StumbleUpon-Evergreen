var spConfig = new Product.Config({"attributes":{"124":{"id":"124","code":"size","label":"Size","options":[{"id":"48","label":"Medium","price":"0","oldPrice":"0","products":["103"]},{"id":"49","label":"Large","price":"0","oldPrice":"0","products":["104"]},{"id":"50","label":"XL","price":"0","oldPrice":"0","products":["105"]},{"id":"51","label":"XXL","price":"0","oldPrice":"0","products":["106"]}]}},"template":"$#{price}","basePrice":"64.95","oldPrice":"64.95","productId":"102","chooseText":"Choose an Option...","taxConfig":{"includeTax":false,"showIncludeTax":false,"showBothPrices":false,"defaultTax":0,"currentTax":0,"inclTaxTitle":"Incl. Tax"}});

function smdColorSwatchAttributeConfigGetNext(key) {
	f = 0;
	for (i in smdColorSwatchAttributeConfig) { if (parseInt(i) > 0 && f == 1) { return i; } if (parseInt(i) == key) { f = 1; } }
	return f = 1 ? -1 : false;
}

function smdProductFilter(products, att, option_id) {
	ret = new Array();
	att = parseFloat(att) +  '-attribute-key';
	for (pid in products) { if (products[pid][smdColorSwatchAttributeConfig[att].name] == option_id) { ret[ret.length] = products[pid]; } }
	return ret;
}

function smdGetSelectedProductOptions(att) {
	products = smdColorSwatchProductConfig;
	att.each(function(a,index) { if (typeof(a[1]) != 'object' && a[1] != false) { products = smdProductFilter(products, a[0], a[1]); } });
	return products;
}

function smdColorSwatchProductConfigAllowOption(att) {

	products = smdGetSelectedProductOptions(att);

	att.each(function(a,index) {
		if (typeof(a[1]) == 'object') {
			$$('.li-att-'+a[0]).each(function(el,i){ el.addClassName('not_available'); });
			
			for (option_id in a[1]) {
			 if (parseInt(option_id) > 0) {
					for (pid in products) { 
						if (!isNaN(pid)) {
						  
							if (products[pid][smdColorSwatchAttributeConfig[a[0]].name] == option_id) {
								img = $$('#color-swatch-img-'+parseFloat(a[0])+'-'+option_id+' img')[0];
								if (img) { img.src = products[pid].csSwatchImage; 
								}
								if ($(document.getElementById('color-swatch-img-'+parseFloat(a[0])+'-'+option_id))) {
									$(document.getElementById('color-swatch-img-'+parseFloat(a[0])+'-'+option_id).parentNode).removeClassName('not_available');
								}
							} else {
								
							}
						}
					}
				}
			}
		}

	});
}

function set_attribute_selected_value(el, attribute_id, val ) {

	if (el.parentNode.className.indexOf('not_available') != -1) {
		 if (typeof(ColorSwatchClickDisabled) == "function") { ColorSwatchClickDisabled(this, attribute_id, val); }		return false;
	}

	dropdown = document.getElementById('attribute' + attribute_id);
	
	for ( var i = 0; i < dropdown.options.length; i++ ) {
		if ( dropdown.options[i].value == val ) {
			dropdown.selectedIndex = i;
			break;
		}
	}
	spConfig.configureElement($('attribute' + attribute_id));
	
	nextAttribute_id = smdColorSwatchAttributeConfigGetNext(attribute_id);
	
	for (s in smdColorSwatchAttributeConfig[attribute_id + '-attribute-key'].option) {
		smdColorSwatchAttributeConfig[attribute_id + '-attribute-key'].option[s].selected = false;
	}
	smdColorSwatchAttributeConfig[attribute_id + '-attribute-key'].option[val].selected = val;
	
	current = false;
	addDisabled = false;
	for (i in smdColorSwatchAttributeConfig) {
		iKey = parseFloat(i);
		if (attribute_id == iKey) { current = true }	
		if (current) {
			$$('.li-att-'+iKey+' div.color-swatch-img').each(function(el,index){ el.removeClassName('active'); });
			if (addDisabled == true) {
				for (s in smdColorSwatchAttributeConfig[i].option) { smdColorSwatchAttributeConfig[i].option[s].selected = false; }
				$$('.li-att-'+iKey).each(function(el,index){ el.addClassName('not_available'); });
			}
			addDisabled = true;
		}
	}
	
	if ($('color-swatch-img-'+attribute_id+'-'+val)) {
  	$('color-swatch-img-'+attribute_id+'-'+val).addClassName('active');
	}
	smdColorSwatchProductConfigAllowOption(smdGetSelectedAttributeArray(attribute_id));
}

function smdGetSelectedAttributeArray(attribute_id, lisenSelection) {
	nextAttribute_id = smdColorSwatchAttributeConfigGetNext(attribute_id);
	
	att = new Array();
	lisenSelection = typeof(lisenSelection) == 'undefined' ? 1 : lisenSelection;
	
		for (i in smdColorSwatchAttributeConfig) {
			if (parseFloat(i) > 0) {
				selected = false;
				if (lisenSelection) {
				  for (s in smdColorSwatchAttributeConfig[i].option) { if (parseFloat(smdColorSwatchAttributeConfig[i].option[s].selected) > 0) { selected = smdColorSwatchAttributeConfig[i].option[s].selected; 
				  
				  } }
				}
				att.push([i, selected]);
			}
			
			if (parseFloat(i) == attribute_id && parseFloat(nextAttribute_id) > 0) {
				att.push([nextAttribute_id, smdColorSwatchAttributeConfig[nextAttribute_id].option]);
				break;
			}
		}
		
	return att;
}

function smdGetSelectedAttributeArrayByOption(attribute_id, option_id) {
	nextAttribute_id = smdColorSwatchAttributeConfigGetNext(attribute_id);
	att = new Array();
		for (i in smdColorSwatchAttributeConfig) {
			if (parseInt(i) > 0) {
				selected = false;
			  for (s in smdColorSwatchAttributeConfig[i].option) { if (s == option_id) { selected = option_id } else if (selected == false && parseInt(smdColorSwatchAttributeConfig[i].option[s].selected) > 0) { selected = smdColorSwatchAttributeConfig[i].option[s].selected; } }
				att.push([i, selected]);
			}
			if (parseInt(i) == attribute_id && nextAttribute_id > 0) {
				break;
			}
		}
	return att;
}
function showMoreView(el, attribute_id, val) {
	
	ulOb = $$('div.more-views ul');
	$$('div.more-views li').each(function(elLI,i){
		elLI.parentNode.removeChild(elLI);
	});
	
	nextAttribute_id = smdColorSwatchAttributeConfigGetNext(attribute_id);
	
	product = smdGetSelectedProductOptions(smdGetSelectedAttributeArray(attribute_id));
	
	moreviewImages = new Array();	

	for (k in product[0].images) {
		moreviewImages[moreviewImages.length] = {
			'csImage':{'image':product[0].images[k].csImage.image, 'label':product[0].images[k].csImage.label, 'galleryUrl':product[0].images[k].csImage.galleryUrl}, 
			'csThumb':{'image':product[0].images[k].csThumb.image, 'label':product[0].images[k].csThumb.label, 'galleryUrl':product[0].images[k].csImage.galleryUrl},
			'csOrginalImage':{'image':product[0].images[k].csOrginalImage.image, 'label':product[0].images[k].csOrginalImage.label, 'galleryUrl':product[0].images[k].csOrginalImage.galleryUrl},
			'csCustomImage':{'image':product[0].images[k].csCustomImage.image, 'label':product[0].images[k].csCustomImage.label, 'galleryUrl':product[0].images[k].csCustomImage.galleryUrl}
		}
	}
	
	if (ulOb && product.length > 0) {
		for (i=0; i < moreviewImages.length; i++) {
			liElment = document.createElement('li');
			aElment = document.createElement('a');
			imgElment = document.createElement('img');
	
			imgElment.src = moreviewImages[i].csThumb.image;
						imgElment.title = moreviewImages[i].csThumb.label;
						aElment.appendChild(imgElment);
			aElment.href = moreviewImages[i].csOrginalImage.image;
			aElment.alt = moreviewImages[i].csOrginalImage.label;
			aElment.att = attribute_id;
			aElment.oid = val;
			aElment.images = moreviewImages[i];
			aElment.onclick = function() {
				  if (typeof(ColorSwatchMoreViewClick) == "function") { ColorSwatchMoreViewClick(this, this.att, this.oid, this.images); } 					return false;
			}
			liElment.appendChild(aElment);
			
			ulOb[0].appendChild(liElment);
		}
	}
}
