                <li id="liIngredient" data-ingredientid="20244" data-grams="9">
                    <label>
                        <span class="checkbox-formatted"><input id="cbxIngredient" type="checkbox" name="ctl00$CenterColumnPlaceHolder$recipeTest$recipe$ingredients$rptIngredientsCol2$ctl05$cbxIngredient" /></span>
                        <p class="fl-ing" itemprop="ingredients">
                            <span id="lblIngAmount" class="ingredient-amount">1 tablespoon</span>
                            <span id="lblIngName" class="ingredient-name">crushed red pepper flakes</span>
                            
                        </p>
                    </label>
                </li>
            
                <li id="liIngredient" data-ingredientid="18738" data-grams="2.25">
