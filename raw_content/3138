	$(function() {
		function mycarousel_initCallback(carousel) {
				jQuery('#next').bind('click', function() {
					carousel.next();
					return false;
				});
				jQuery('#prev').bind('click', function() {
					carousel.prev();
					return false;
				});
			};
			
		$("#mycarousel").jcarousel({
				scroll: 1,
				initCallback: mycarousel_initCallback,
				buttonNextHTML: null,
				buttonPrevHTML: null,
				easing: 'easeOutBack',
				animation:600,
				wrap: 'circular'
			});
		
