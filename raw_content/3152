<script type="text/javascript">
jQuery(document).ready(function(){
    var $content = jQuery('#content');
    var $sidebar = jQuery('#sidebar');
    
    if($content.outerHeight() > $sidebar.outerHeight())
    {
        var newHeight = $content.height() - parseInt($sidebar.css('padding-top')) - parseInt($sidebar.css('padding-bottom')) - parseInt($sidebar.css('margin-top')) - parseInt($sidebar.css('margin-bottom'));
        $sidebar.css('height', newHeight + 'px');
    }
});
</script>

