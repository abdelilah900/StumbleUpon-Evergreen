	// this is delcared public so we can call "dclk" later on for things
	// such as var adaptvCompanionAdTag, on the video page.
	var dclk;

	(function(){
		var sitename = "dmd.cracked";
		var zone     = "content";
		var sz       = "300x250";
		var ugc      = "";
		var art      = "art18803";
		var auth     = "colinmurdock";
		var dbr      = "msie";
		var rating   = "pg";
		var cat      = "weirdworld";
		var loc      = "btf";
		var vid      = "";
		var envr     = "";
		var tile     = "3"; // (for roadblocking)
		var ctype	 = "articles";
        var spikes	 = "";
		var ord      = dart_ord;
		var title    = "";
		
		

					dclk = '<iframe  src="http://ad.doubleclick.net/adi/'+sitename+'/'+zone+';cat='+cat+';ctype='+ctype+';spikes='+spikes+';envr='+envr+';loc='+loc+';rating='+rating+';dbr='+dbr+';auth='+auth+';art='+art+title+';vid='+vid+';ugc='+ugc+';%dx%;tile='+tile+';sz='+sz+';ord='+ord+'?" width="300" height="250" marginwidth="0" marginheight="0" frameborder="0" scrolling="no"><\/iframe>';
		
		if (typeof dexQS !== "undefined" && dexQS.length > 0) {
			dclk = dclk.replace('%dx%', dexQS);
		} else {
			dclk = dclk.replace('%dx%', "");
		}

				document.write(dclk);
		
	})();
	</script>
