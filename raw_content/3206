            holder.commentFeed = "http://www.blogger.com/feeds/2938403175408538397/comments/default";
          
          holder.currentBlogUrl = "http://www.sprinklebakes.com/";
          holder.currentBlogId = "2938403175408538397";
        
        return holder;
      });
    }
  </script>
