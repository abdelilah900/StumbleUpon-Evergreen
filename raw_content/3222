        <script type="text/javascript">
    /**
     * Kills the flash in flash ad that has been loaded.
     */
    function killAd() {
        FD.trigger("FDVideoPlayerFlashAdCompleted");
    }
    /**
     * Pauses the video players content clip.
     */
    function pauseContent() {
        FD.trigger("FDVideoPlayerFlashAdPause");
    }
    /**
     * Plays the video players content clip.
     */
    function playContent() {
        FD.trigger("FDVideoPlayerFlashAdPlay");
    }
    /**
     * Kills the flash in flash ad that has completed, and plays the video players content clip.
     */
    function videoComplete() {
        FD.trigger("FDVideoPlayerFlashAdCompleted");
        FD.trigger("FDVideoPlayerFlashAdPlay");
    }
    /**
     * Loads explosive flash ad with the specified url.
     *
     * @param url URL of the explosive flash ad.
     * @param width Width of the explosive flash ad.
     * @param height Height of the explosive flash ad.
     */
    function loadExplosion(url, width, height) {
        if ((url) && (width) && (height)) {
            new FD.VideoPlayerFlashInFlashAdHelper({
                height: height,
                url: url,
                width: width
            });
        } else if (url) {
            new FD.VideoPlayerFlashInFlashAdHelper({
                url: url
            });
        }
    }
    /**
     * Closes the explosive flash ad.
     */
    function closeExplosion() {
        FD.trigger("FDVideoPlayerFlashAdCompleted");
        FD.trigger("FDVideoPlayerFlashAdPlay");
    }
    /**
     * Replaces specified companion ad with specified image and click through url.
     *
     * @param companionAdId Identifier of the <code>div</code> that contains the companion ad to replace.
     * @param imageUrl URL of the image to use as the companion ad.
     * @param linkUrl URL of the click through for the companion ad.
     */
    function replaceCompanionAd(companionAdId, imageUrl, linkUrl) {
        new FD.VideoPlayerCompanionAdHelper({
            companionAdId: companionAdId,
            imageUrl: imageUrl,
            linkUrl: linkUrl
        });
    }
</script>
