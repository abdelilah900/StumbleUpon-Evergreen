<script type="text/javascript">

  function rate(obj){
   
    if($('#'+obj.id).hasClass('star_off')){
      var id = obj.id.substring(4,5);
      var num = parseInt(id);

      var rating = 0;
      for(x = 1; x <= num ; x++){

        $('#star'+x).removeClass("star_off").addClass("star_on");

      }
     
      $('#rating').val(num);
    
        
    }else{

      var id = obj.id.substring(4,5);
      var num = parseInt(id) +1;
      var curr = parseInt(id);
      for(x = num; x <= 5; x++){
        
        $('#star'+x).removeClass("star_on").addClass("star_off");

      }

      $('#rating').val(curr);
    }
  }

</script>
