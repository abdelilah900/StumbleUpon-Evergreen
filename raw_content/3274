		function updateEmbedCode(languageCode) {
			var currEmbedCode = jQuery('#embedCode').val();
			var langEmbedCode;
			var embedPattern = new RegExp('&lang=[a-z]*_*[a-z]*','ig');
			if (currEmbedCode.search(embedPattern) >= 0) {
				langEmbedCode = currEmbedCode.replace(embedPattern,'&lang=' + languageCode);
			}
			else {
				langEmbedCode = currEmbedCode;
			}
			jQuery('#embedCode').val(langEmbedCode);
		}
