<script type="text/javascript" charset="utf-8">
var product_slugs = new Array();
$(document).ready(function(){
	$(".buy_button").click(function(){
		// alert($(this).parent().parent("form").attr('action'));
		// $(this).ajaxSubmit();
		$(this).parent().parent("form").ajaxSubmit(submitOptions);
		return false;
	});
	
	var submitOptions = {
	    url : '/cart/add/ajax/',
	    type : 'POST',
	    beforeSubmit : function(formArray, jqForm) {
			$("#ajaxwindow").show().html("");
	        return SSS.validate(formArray, jqForm, '#statusupdate');
	    },
	    dataType : 'json',
	    success : function(json) {
	        SSS.updatestatus(json, '#statusupdate', '#cart .red');
			$("li#cart a").click();
	    },
		error: function (XMLHttpRequest, textStatus, errorThrown) {
		  alert(XMLHttpRequest+"\n\n"+textStatus+"\n\n"+errorThrown);
		}

	};
	$('select.priced').selectbox({debug: true});	
	var shipping = new Array();
	
		shipping['uk'] = 4.50;
	
		shipping['europe-airmail-order-value-over-25'] = 7.95;
	
		shipping['international'] = 9.50;
	
		shipping['Europe'] = 4.25;
	
	$('form.options').ajaxForm(submitOptions);
});

</script>



