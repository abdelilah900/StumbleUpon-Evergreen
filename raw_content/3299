<script type="text/javascript" language="javascript">

    //Bouton Region

    //Bouton Region
    jQuery(document).ready(function () {
        jQuery("#DivRegionPied").click(function () {
            if (jQuery("#panelRegions").is(':hidden')) {
                jQuery("#panelTele").slideUp('slow'); jQuery("#btnTele a").removeClass("active");
                jQuery("#panelRadio").slideUp('slow'); jQuery("#btn-regions a").removeClass("active");
                jQuery("#panelRegions").slideDown('slow'); jQuery("#btnRadio a").addClass("active");
                jQuery('#DivRegion').css("color", "#FFFFFF");
            } else {
                jQuery("#panelRegions").slideUp('slow'); jQuery("#btnRadio a").removeClass("active");
                jQuery('#DivRegion').css("color", "#BABABA");
            }
        });
    });

</script>
    
    


