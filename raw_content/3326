            ]
        });

        var galleryMode = window.location.href.contains('/photos/galleries/') ? 'detail' : 'inline';
        var trackerFn = function(){
	    _sf_async_config.sections = window.siteSectionTag + ":" + window.siteSubSectionTag;
            _sf_async_config.title = window.siteSectionTag + ":" + window.siteSubSectionTag + ":" + this.slides[this.index].options.title + 
	    ":" + this.slideshow.slides[this.index].id;
           if (typeof pSUPERFLY !== "undefined") {
	    pSUPERFLY.virtualPage([
 	    //console.log([
                '/photos/galleries/2008/oct/30/2008-vegas-amjam/?' +
                galleryMode +
                ':' +
                slugify(this.slides[this.index].options.credit) +
                ':' +
                (this.isFullscreen ?
                    'fullscreen' :
                    this.options.width) +
                ':' +
                (this.index + 1) +
                '/' +
                this.slideshow.slides.length,
                window.siteSectionTag+":"+window.siteSubSectionTag+":"+this.slideshow.slides[this.index].id
            ]);}
            _gaq.push([
            //console.log([
                '_trackPageview',
