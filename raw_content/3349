    omniture_event66_fired = 'false';
    $(window).scroll(function (e){
      if ($('.sailthruRecommendation').css('display') == 'block' && omniture_event66_fired != 'true') {
        iVillage.omniEvent(null, 66, 'sailthru-widget');
        omniture_event66_fired = 'true';
      }
    });
</script>
      </div>
    </div>
