	<div class="amr-post post-9557 post type-post status-publish format-standard hentry category-food category-halloween category-how-tos" id="post-9557">
	    <div class="amr-post-tl"></div>
	    <div class="amr-post-tr"></div>
	    <div class="amr-post-bl"></div>
	    <div class="amr-post-br"></div>
	    <div class="amr-post-tc"></div>
	    <div class="amr-post-bc"></div>
	    <div class="amr-post-cl"></div>
