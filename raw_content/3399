                'articleId': 'YWIyNi8yMjAxMzU5LnQuanBn',
                'checksum': 'cfb349846b0e231675ccdc84774071c4',
                'collectionMeta': 'eyJ0eXAiOiJqd3QiLCJhbGciOiJIUzI1NiJ9..xb1DqmEgdJdkmzRur8kNjpbn1FjXDVNTpvPUqrKXshA',
                'el': 'livefyre',
                'siteId': '304529'
            }], function(widget){
                MVN.LF.login();
                widget.on('initialRenderComplete', MVN.LF.clean);
                widget.on('commentPosted', MVN.LF.clean);
                widget.on('commentFlagged', MVN.LF.clean);
                widget.on('commentLiked', MVN.LF.clean);
                widget.on('commentShared', MVN.LF.clean);
                widget.on('commentCountUpdated', function(data){
                    MVN.LF.clean();
                    MVN.queue(function(){
                        $('[data-lfv3-counter]').text(data);
                    })
                });
                MVN.LF.change = function(conv){
                    MVN.queue(function(){
                        widget.changeCollection(conv);
                    })
                }
                MVN.queue(function(){
                    // Register Button
                    $('.lfreg', '#livefyre').live('click', function(){
                        if(!MVN.user){
                                                        MVN.omni.actionSrc = 'lf';
                            MVN.omni.action(MVN.omni.buildAction('join', {sid: MVN.LF.getSID()}));
                                                    }else{
                            MVN.LF.login();
                        }
                    });
                    // Hide Hover for Anon
                    var $hovercard = false;
                    $('.fyre-comment-user').live('mouseenter', function(){
                        if($(this).data('author-id').match(/^anon_/)){
                            if($('div.fyre-hovercard').length){
                                if(!$hovercard){
                                    $hovercard = $('div.fyre-hovercard');
                                }
                                $('div.fyre-hovercard').remove();
                            }
                        }else{
                            if($hovercard && !$('div.fyre-hovercard').length){
                                $('body').append($hovercard);
                            }
                        }
                    });
                    // Move Notification Window
                    var setNotifyPosition = function(){
                        $('.fyre-notifier-container').css('right', function(){
                                return ($('body').width() - $('#livefyre').offset().left - parseInt($(this).css('width').replace('px', '')));
                            }
                        );
                    }
                    setNotifyPosition();
                    $(window).bind('resize scroll', setNotifyPosition);
                })
            });
        });
        var LF_Component = function(controller){
            var me = this;
            me.MVN = controller;
            me.version = 'LiveFyre for MVN 2.0';
            me.autoInit = false;
        }
        LF_Component.prototype = {
            cookieName: 'livefyre_token',
            anonName: 'MVN[anon]',
            anon: null,
            login: function(newToken){
                var me = this;
                var token = newToken||VVM.getCookie(me.cookieName)||false;
                if(token){
                    try{
                        fyre.conv.login(token);
                        me.clean(true);
                    }catch(e){
                        VVM.delCookie(me.cookieName);
                        if(!newToken){
                            me.login();
                        }
                    }
                }else{
                    MVN.queue(function(){
                        if(me.MVN.user||VVM.getCookie(me.anonName)){
                            VVM.debug.p('Requesting New Token');
                            me.MVN.sso.getLfToken(function(obj){
                                if(obj && obj.status == 'success'){
                                    VVM.setCookie(me.cookieName, obj.token.token, false, "/");
                                    me.login(obj.token.token);
                                }
                            });
                        }
                    });
                }
            },
            binds: function(){
                var me = this;
                if(!me.auth){
                    var auth = new fyre.conv.RemoteAuthDelegate();
                    auth.login = function(handleAs){
                        MVN.queue(function(){
                            if(!me.MVN.user){
                                                                me.MVN.omni.actionSrc = 'lf';
                                me.MVN.omni.action(me.MVN.omni.buildAction('login', {sid: me.getSID()}));
                                                            }else{
                                MVN.queue(function(){
                                    MVN.LF.login();
                                });
                            }
                        });
                        handleAs.failure();
                    }
                    auth.logout = function(handleAs){
                        MVN.queue(function(){
                            if(me.MVN.user||VVM.getCookie(me.anonName)){
                                                                VVM.delCookie(me.cookieName, '/');
                                VVM.delCookie(me.anonName, '/');
                                me.MVN.omni.action('logout');
                                                            }
                        });
                        handleAs.success();
                    }
                    auth.viewProfile = function(handleAs, author){
                        if(!author.id.match(/^anon_/) && author.profileUrl != ""){
                            window.location = author.profileUrl;
                            handleAs.success();
                        }else{
                            me.clean();
                        }
                    }
                    auth.editProfile = function(handleAs, author){
                        if(author.id.match(/^anon_/)){
                            me.clean();
                            handleAs.failure();
                        }else{
                            window.location = "http://www.myvoicenation.com/profile/settings/";
                            handleAs.success();
                        }
                    }
                    me.auth = auth;
                }
                return me.auth;
            },
            clean: function(calledAfterLogin, attempt){
                var me = this;
                MVN.queue(function(){
                    // Check if ANON
                    if(calledAfterLogin || me.anon == null){
                        if(fyre.conv.user && fyre.conv.user.id){
                            me.anon = (fyre.conv.user.id.match(/^anon_/)) ? true : false;
                        }else if(calledAfterLogin){
                            attempt = (attempt) ? attempt + 1 : 1;
                            if(attempt > 5){
                                VVM.debug.p('LF Login Failed');
                                VVM.delCookie(me.cookieName);
                            }else{
                                VVM.debug.p('LF Login Check Attempt: '+attempt);
                                window.setTimeout(function(){me.clean(true, attempt)}, 1000);
                            }
                        }
                    }
                    // ANON Clean Up
                    if(me.anon == true){
                        $('li > a[title="Edit Profile"]', '.fyre-box-list').parent('li').remove();
                    }
                    
                    // Create Account Button
                    if (!MVN.user) {
                        var $el = $('div.fyre-login-bar', '#livefyre');
                        if($el.length && $el.children().length == 1 && !$('.fyre-user-loggedin', $el).length){ 
                            $el.append('<span class="create-mvn-account"> or <span class="lfreg">Create Account</span></span>');
                        }
                    }
                    
                    // Each Comment/Reply
                    $('.fyre-comment-wrapper').each(function(){
                        var $el = $('.fyre-comment-user[data-author-id^="anon_"]', this);
                        if($el.length){
                            // Comment Avatar
                            $('a', $el).replaceWith($('a img', $el)
                                .attr('title', 'Anonymous User')
                            );
                            // Comment Author
                            $el = $el.parent().find('header a');
                            $el.replaceWith($('<span/>')
                                .addClass('anon_username')
                                .text($el.text())
                                .attr('title', 'Anonymous User')
                            );
                        }
                        // Comment Mentions
                        $('a.fyre-mention[resource^="acct:anon_"]', this).each(function(){
                            $(this).replaceWith($('<span/>')
                                .addClass('anon_username')
                                .text('@' + $('span[property="name"]', this).text())
                                .attr('title', 'Anonymous User')
                            );
                        });
                        // Comment Likes
                        $('.fyre-avatar', this).filter(function(){
                            return ($('img[src*="/anon/"]', this).length) ? true : false;
                        }).each(function(){
                            $('a', this).replaceWith($('a img', this)
                                .attr('title', 'Anonymous User')
                            );
                        })
                    });
                });
            },
            getSID: function(){
                var src_desc = '';
                if(window.OAS_query_category||false){
                    switch(OAS_query_category.toLowerCase().replace(/\s/,'')){
                        case 'arts':
                            src_desc = 1188;
                            break;
                        case 'film':
                        case 'movies':
                            src_desc = 1189;
                            break;
                        case 'music':
                            src_desc = 1190;
                            break;
                        case 'news':
                            src_desc = 1191;
                            break;
                        case 'restaurants':
                        case 'dining':
                        case 'food':
                            src_desc = 1192;
                            break;
                                            }
                }
                return '8|53|'+src_desc;
            }
        }
    </script>
