	<script type="text/javascript">
		jQuery(document).ready(function() {
			var searchInputUnselectedText = "Search Local Businesses";
			if(searchInputUnselectedText != jQuery("div#integratedSearch div.header span.buttons input.searchInput").val() )
			{
				jQuery("div#integratedSearch div.header span.buttons input.searchInput").addClass("focus");
			}
			
			jQuery("div#integratedSearch div.header span.buttons input.searchInput").focus( function() {
				if(searchInputUnselectedText == jQuery(this).val())
				{ 
					jQuery(this).val("");
					jQuery(this).addClass("focus");
				}
			});
			jQuery("div#integratedSearch div.header span.buttons input.searchInput").blur( function() {
				if("" == jQuery(this).val())
				{ 
					jQuery(this).val(searchInputUnselectedText);
					jQuery(this).removeClass("focus");
				}
			} );
		});
	</script>
