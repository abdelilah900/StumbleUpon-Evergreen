  var wh_subscribe_dropdown_callstate = 0;
	
  function displaysubscribe(e) {
  	//hideMenuImmed();
  	var gmnuObj = this;
  	jQuery(this).attr('ON','1');
  	jQuery(this).removeAttr('OFF');
  	setTimeout(function() {
  		var mnuObj = gmnuObj;
  		if (jQuery(mnuObj).attr('ON') == '1') {
  			jQuery('#subscribePromoArea').css('display','block');
  			jQuery(mnuObj).removeAttr('ON');
  		}
  	}, 500);
  }

  function hidesubscribe(e) {
  	wh_subscribe_dropdown_callstate = 0;
  	var gmnuObj = this;
  	jQuery(this).attr('OFF','1');
  	jQuery(this).removeAttr('ON');
  	setTimeout(function() {
  		var mnuObj = gmnuObj;
  		if((wh_subscribe_dropdown_callstate == 0) && (jQuery(mnuObj).attr('OFF') == '1')) {
  			jQuery('#subscribePromoArea').css('display','none');
  			jQuery(mnuObj).removeAttr('OFF');
  		}
  	}, 0);
  }
	
  function setCallOnState(e) {
    wh_subscribe_dropdown_callstate = 1;
  }

  function setCalloffState(e) {
  	wh_subscribe_dropdown_callstate = 0;
  	var mnuId = "#subscribePromoArea";
  	jQuery(mnuId).trigger("mouseout");
  }

  // ]]>
</script>

