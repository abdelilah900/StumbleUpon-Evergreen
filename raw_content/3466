            </script><script type="text/javascript">

            // ========================================= send vote

            // bool indicates whether the vote is being added (true) or removed (false).
            function vote(contestId, valueString, bool) {
                // Send the vote info to /contest/vote/ with a POSTED param
                new Ajax.Request('/contest/vote/',{
                    method: 'post',
                    parameters: {
                        contestId: contestId,
                        instructableId: valueString,
                        add: bool,
                        posted: new Date().getTime()
                    },
                    onSuccess: function( t ) {
                        if( bool ) {
                            // suppress the vote count!
                            //          $("voteCount").innerHTML = "Current Votes: " + t.responseText;
                            $("vote_blurb").hide();
                            $("vote_thanks").show();
                            $("vote_link").hide();
                            $("voted_link").show();
                        } else {
                            $("vote_blurb").show();
                            $("vote_thanks").hide();
                            $("voted_link").hide();
                            $("vote_link").show();
                        }
                    },
                    onFailure: function( t ) {
                        alert('voting error ' + t.status + ' -- ' + t.statusText);
                    }
                });
            }

          </script><div id="instructable-title"><div id="top-actions"><script type="text/javascript">
