					// remove the current country //
					var countryClass = $("#country-select dt").attr("class").split(/\s+/);
	
					$('#country-select dd ul li').each(function() {
						if ($(this).hasClass(countryClass)) {
							$(this).remove();
							return false;
						}
					})

					// add the dropdown
					$("#country-select dt").click(function(e) {
						$(this).css({ "cursor" : "pointer" });
						$("ul.dropdown").toggle();
						e.preventDefault();
						return false;
					});

					// remove the dropdown on click of the window //
					$(document).bind('click', function(e) {
						var $clicked = $(e.target);
						if (! $clicked.parents().hasClass("dropdown"))
							$("ul.dropdown").hide();
					});
				
