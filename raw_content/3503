		// do not change global vars below
		var opacityrunning = false;
		var showingfaderimage = 2;
		var showingimagepos;
		var faderobject;
		var activeState;
		var heroglobalrefid = '';
		var pic = new Array();
		var firstrun = true;
		function showme(refid, pos, color, avail, price, skurefid, proprice, productname, size, colorcode, skuid, invmisc) {
			/*alert('refid = '+refid 
						+'\n pos = '+pos 
						+'\n color = '+color 
						+'\n skurefid = '+skurefid 
						+'\n colorcode = '+colorcode 
						+'\n skuid = '+skuid
						);*/
			closeAuxNotify();
			if (size == 'TRUE'){
				activesizecolor = skurefid;
			}
			if (firstrun == true){
				urchinTracker('/watches/womens/' + productname +'-'+ skurefid);
				pic[pos] = new Image();
				pic[pos].src = refid;
			}
			if (pic[pos].complete != true){
				firstrun = false;
				if ($('loaderoo')){
					$('loaderoo').style.display = 'block';
				}
				setTimeout(function () { imageloadedcheck(refid, pos, color, avail, price, skurefid, proprice, productname, size, colorcode, skuid, invmisc); }, 50);
			}
			else {			
				firstrun = true;
				if ($('loaderoo')){
				$('loaderoo').style.display = 'none';
				}
				heroglobalrefid = refid;
				if (opacityrunning == false){
					
					if (showingimagepos != pos){
						
						if (showingfaderimage == 1){
							
							$('faderimg1').src = refid;
							opacity('faderimg1',0,100,500);
							opacity('faderimg2',100,0,500);
							setTimeout(function () { $('faderimg1').style.filter = ''; }, 500);
							if (findIE6 != -1){
								var imagestring = $('faderimg1').src;
								var splitimagestring = imagestring.split('png',1);
								if (splitimagestring != imagestring){
									setTimeout(function () {
										$('faderimg1').style.filter = '';
										$('faderimg1').style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, sizingMethod=scale src='"+ $('faderimg1').src +"')";
										$('faderimg1').src = 'http://image.nixonnow.com/image/common/clear.gif';
										$('faderimg2').src = 'http://image.nixonnow.com/image/common/clear.gif';										 
									}, 500);
								}
							}
							opacityrunning = true;
							faderobject = 'faderimg1';
							showingfaderimage = 2;	
						} 
						else {
							if (showingfaderimage == 2){
								$('faderimg2').src = refid;
								opacity('faderimg1',100,0,500);
								opacity('faderimg2',0,100,500);
								setTimeout(function () { $('faderimg2').style.filter = ''; }, 500);
								if (findIE6 != -1){
									var imagestring = $('faderimg2').src;
									var splitimagestring = imagestring.split('png',1);
									if (splitimagestring != imagestring){
										setTimeout(function () {
											$('faderimg2').style.filter = '';
											$('faderimg2').style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, sizingMethod=scale src='"+ $('faderimg2').src +"')";
											$('faderimg2').src = 'http://image.nixonnow.com/image/common/clear.gif';
											$('faderimg1').src = 'http://image.nixonnow.com/image/common/clear.gif';										 
										}, 500);
									}
								}
								opacityrunning = true;
								faderobject = 'faderimg2';	
								showingfaderimage = 1;	
							}
						}
						showingimagepos = pos;
					}
					if ($('loaderoo')) {
						switchSelection(refid, pos, color, avail, price, skurefid, proprice, productname, size, colorcode, skuid, invmisc);
					}
				}
			}
			permaurl = $('permalink').href;
			permasplit = permaurl.split("_");
			permaurl = permasplit[0]+'_'+colorcode+'.html';
			//alert(permasplit + "\n" +permaurl);
			//Set the permalink
			$('permalink').href = permaurl;
			if ($('permalinkfield'))
			$('permalinkfield').value = permaurl;
			addThisLoadInit('init',permaurl);
		}
		
		function fixeliteimage(){
			if (findIE6 != -1){
				//alert ('got here');
				$('faderimg1').style.filter = '';
				$('faderimg1').style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, sizingMethod=scale src='"+ $('faderimg1').src +"')";
				$('faderimg1').src = 'http://image.nixonnow.com/image/common/clear.gif';
				$('faderimg2').style.filter = '';
				$('faderimg2').style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, sizingMethod=scale src='"+ $('faderimg2').src +"')";
				$('faderimg2').src = 'http://image.nixonnow.com/image/common/clear.gif';
			}
		}
		
