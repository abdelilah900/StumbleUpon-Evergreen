			var showShippingSchedule = function(imgName){
				zrLightBoxInit();
				setprocessingheight();
				document.getElementById('lightBoxNavRight').style.display = 'none';
				document.getElementById('lightBoxNavLeft').style.display = 'none';
				var detailTeamImage = new Image();
				detailTeamImage.onload = function(){ zrLoadShippingSchedule(detailTeamImage);};				
				detailTeamImage.src = 'http://image.nixonnow.com/image/common/' + imgName;	
			}
			
			var zrLoadShippingSchedule = function(detailTeamImage){
				var pars = "PAGE=LOAD_TEAM_IMAGE";
				pars += "&src=" + detailTeamImage.src;
				pars += "&r=" + Math.random();
				var url = "/za/NXN";
				var myAjax = new Ajax.Request( url, { method: 'post', parameters: pars, onComplete: zrLightBoxDone2});
			}
			
			var zrLightBoxDone2 = function(request){
				if (request != ''){
					$('windowContent').innerHTML = request.responseText;
				}
				setprocessingheight();
				setTimeout(function(){
					if (!window.XMLHttpRequest) {
						document.getElementById("leftTopCorner").style.display = 'none';
						document.getElementById("rightTopCorner").style.display = 'none';
						document.getElementById("leftBottomCorner").style.display = 'none';
						document.getElementById("rightBottomCorner").style.display = 'none';
						document.getElementById("closePopButton").style.display = 'block';
						document.getElementById('masterWindow').style.border = '1px solid #A7A9AC';
					} 
					else {
						document.getElementById("leftTopCorner").style.display = 'block';
						document.getElementById("rightTopCorner").style.display = 'block';
						document.getElementById("leftBottomCorner").style.display = 'block';
						document.getElementById("rightBottomCorner").style.display = 'block';
						document.getElementById("closePopButton").style.display = 'block';
					}
					document.getElementById('imageLoaded').style.padding = '20px';
					document.getElementById('masterWindow').style.display = 'block';
					setprocessingheight();
					
					ZRFade.init('masterWindow',600);
				}, 400);
			}
			
    </script>
