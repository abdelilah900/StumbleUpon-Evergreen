	var activeeliteswatch = 'swatch1';
  function activateswatch(element) {
   if (activeeliteswatch != element){
    if (activeeliteswatch != ''){
    $(activeeliteswatch).className = "eliteswatch";
    }
    //opacity(element,20,100,500);
    $(element).className = "eliteswatchactive";
    activeeliteswatch = element;
   }
  }

  var activetype = '';
  var eliteOn = '';
  
  function eliteSwap(element, n) { 
   if (element != activetype) {
    if(n == 1){
     $(element+"nav").style.filter = '';
     $(element+"nav").style.opacity = '1';
    } else {
			opacity(element+"nav",0,35,1);
    }
   }
  }
	
  function showwatchinfo(type){
   if ($('blackened').style.display == 'none'){
    $('blackened').style.display = 'block';
   }
   if (activetype != type){ 
    if (watchinfohasclose != true){
     opacity('watchinfolayer',100,0,350);
    }
    
    watchinfohasclose = false;
    if (activetype != ''){ 
     opacity(activetype + 'nav',100,35,350);
    }
    opacity(type + 'nav',35,100,1); 
    activetype = type;
    setTimeout(function () { switchwatchinfo(type); }, 350);
   }
  }
  
  function switchwatchinfo(type){
   $('watchinfolayer').innerHTML = $(type + 'storage').innerHTML;
   opacity('watchinfolayer',0,100,500);
   //opacity(type + 'nav',20,100,0); 
  }
  
  var watchinfohasclose = false;
  function closewatchinfolayer() {
   opacity('watchinfolayer',100,0,350);
   opacity(activetype + 'nav',100,35,350); 
   watchinfohasclose = true;
   setTimeout(function () { closewatchinfolayerdone(); }, 500);
  }
  
  function closewatchinfolayerdone() {
   $('blackened').style.display = 'none';
   activetype = '';
  }
		
		var activesizecolor = '';
		function sizecolorindicator(skuid,trigger){
			if (trigger == 'over'){
				$('image' + skuid).src = 'http://image.nixonnow.com/image/product_detail/box_selected.gif';
			}
			if (trigger == 'out' && activesizecolor != skuid){
				$('image' + skuid).src = 'http://image.nixonnow.com/image/product_detail/box.gif';
			}
			if (trigger == ''){ 
				if (activesizecolor != ''){
					if($('image' + activesizecolor)){
						$('image' + activesizecolor).src = 'http://image.nixonnow.com/image/product_detail/box.gif';
					}
				}
				$('image' + skuid).src = 'http://image.nixonnow.com/image/product_detail/box_selected.gif';
				activesizecolor = skuid;
			}
		}
		
		function clearactiveselection() {
			if ($('image' + activesizecolor)){
				$('image' + activesizecolor).src = 'http://image.nixonnow.com/image/product_detail/box.gif';
				activesizecolor = '';
			}
		}
		
		function checkselection() {
			//alert (carturl);
			if (activesizecolor == ''){
				alert ("Please select a size!");
				return false;
			}
			else {
				//alert ('got here');
				addproducttocart();
			}
		}
		var carturl = '';
		var buyOnlineHref = '';
		function addproducttocart() {
      
    			if (carturl != ""){	
    				if (findIE6 != -1){
    					window.location.href = carturl;
    				}
    				else {
    					window.location = carturl;
    				}
    			}
    	  
		}
		
		function imageloadedcheck(refid, pos, color, avail, price, skurefid, proprice, productname, size, colorcode, skuid, invmisc) {
			showme(refid, pos, color, avail, price, skurefid, proprice, productname, size, colorcode, skuid, invmisc);
		}
		
		function switchSelection(refid, pos, color, avail, price, skurefid, proprice, productname, size, colorcode, skuid, invmisc) {
		  var buyonline = false;
		  var redButton = 'add-to-cart';
  		if ('' == 'TRUE') {
  		  buyonline = true;
  		  redButton = 'buy-online';
  		}
		  
		  
			//Change the activestate for each thumb.
				if ($('detthumb'+activeState)){
					$('detthumb'+activeState).className = 'detailthumb';
				}
				if ($('detthumb'+pos)){
					$('detthumb'+pos).className = 'activethumb';
				}
				if ($('skurefidwishlist')){
					$('skurefidwishlist').value = skurefid;
				}
				//Set the color, Availibility & Price
				if ($('selcolor')){
					$('selcolor').innerHTML = color;
				}
				if ($('selsize') && size != '' && size != 'TRUE'){
					$('selsize').innerHTML = size;
				}
				else {
					if (size == '' && $('selsize')){
						$('selsize').innerHTML = 'NO SIZE SELECTED';
						activesizecolor = '';
					}
				}
				if ($('cartbutton')){
					var currentCart = $('cartbutton').src;
				}
				if (avail == 0) {
					if ($('selavail')){
						$('selavail').innerHTML = 'Out of Stock';
					}
					if ($('cartbutton')){
						
								$('cartbutton').src = currentCart.replace(redButton, "notify-me"); 
								$('cartbutton').style.cursor = 'pointer';
								
								$('cartbutton').alt = "Please notify me when" + productname + " in " + color + " is in stock.";
								$('cartbutton').title = "Please notify me when" + productname + " in " + color + " is in stock.";
								$('cartthumb').style.cursor = 'pointer';
								$('cartthumb').alt = "Please notify me when" + productname + " in " + color + " is in stock.";
								$('cartthumb').title = "Please notify me when" + productname + " in " + color + " is in stock.";
							
					}

					carturl = '';
					buyOnlineHref = '';
					
					if ($('addtocartlinkimg')) {
						
								$('addtocartlinkimg').onclick = function(){toggleAux('auxnotify'); return false};
								if ($('addtocartlinkbutton')) {
								  $('addtocartlinkbutton').onclick = function(){toggleAux('auxnotify'); return false};
								}
							
					}
					if ($('notifyme')){
						$('notifyme').style.display = 'block';
					}
				} 
				else {
				  // avail = 1
				  
					if ($('selavail')){
						$('selavail').innerHTML = 'In Stock';
					}
					if ($('cartbutton')){
					  
					  var altTitleDescr = 'Add '+ productname  +' in ' +color+' to Your Cart';
					  if (buyonline) {
					    altTitleDescr = 'Buy Online';
					  }
					  
					  
						
								$('cartbutton').src = currentCart.replace("notify-me", redButton); 
								$('cartbutton').style.cursor = 'pointer';
								$('cartbutton').alt = altTitleDescr;
								$('cartbutton').title = altTitleDescr;
								$('cartthumb').style.cursor = 'pointer';
								$('cartthumb').alt = altTitleDescr;
								$('cartthumb').title = altTitleDescr;
							
					}
					
					
							if ($('addtocartlinkimg')) {
								$('addtocartlinkimg').onclick = function(){$('auxnotify').style.display = 'none'; addproducttocart(); return false};
								if ($('addtocartlinkbutton')) {
								  $('addtocartlinkbutton').onclick = function(){$('auxnotify').style.display = 'none'; addproducttocart(); return false};
								}
  						}
						
  				
  				buyOnlineHref = invmisc;
  				
					if (size != ''){
						carturl = '/add-to-cart/' + skurefid;
						//$('addtocartlinkimg').href = '/add-to-cart/' + skurefid;
						//$('addtocartlinkbutton').href = '/add-to-cart/' + skurefid;
					}
					else {
						carturl = '';
						//$('addtocartlinkimg').href = 'javascript:;';
						//$('addtocartlinkbutton').href = 'javascript:;';
					}
					if ($('notifyme')){
						$('notifyme').style.display = 'none';
					}
				}
				
				
				
				
				if (proprice != '') {
				  proprice = '<span style="text-decoration: line-through;">' + proprice + '</span> - ';
				}
				if ($('selprice')) {
				  if (price.indexOf('-1') != -1) {
				    if ($('skuprice')) {
				      $('skuprice').style.display = 'none';
				    }
				  }
				  else {
				    if ($('skuprice')) {
				      $('skuprice').style.display = 'inline';
				    }
				    $('selprice').innerHTML = proprice + price;
				  }
				}
				//$('addtocartlink').href = '/add-to-cart/' + skurefid;
				daThumb = refid.replace("hero","thumb");
				daThumb = daThumb.replace("hero","thumb");
				
						try {
							if (document.getElementById('prodtypeis').value == 'WATCHES'){
								daThumb = daThumb.replace("jpg","png");
							}
						}	catch(err){
						// do nothing for now
						}
							
				if ($('cartthumb')) {
					$('cartthumb').style.filter = '';
					$('cartthumb').style.background = '';
					$('cartthumb').src = daThumb;
				}
				activeState = pos;
				//alert (colorcode);
				setnotify(skurefid, productname, color, size, colorcode, skuid);
				setfindadealer (color, avail, skurefid, size, colorcode, skuid);
		}
		
		
