		var preloadover;
		function zoomandajaxcheck(){
			if (ajaxloaddone == true && zoomdone == true){
    		$('processingdisp').style.background = 'none';
				$('processingdisp').style.background = 'no-repeat top left';
				if	(findIE6 != -1){
					if (backgroundSrc != ''){
						$('processingdisp').style.backgroundImage = "url('http://image.nixonnow.com/image/common/" + backgroundSrc +"')";
					}
					else {
						$('processingdisp').style.backgroundImage = "url('http://image.nixonnow.com/image/common/product20/detail-bg.gif')";
					}
				}
				else {
					if (backgroundSrc != ''){
						$('processingdisp').style.backgroundImage = "url('http://image.nixonnow.com/image/common/" + backgroundSrc +"')";
					}
					else {
						$('processingdisp').style.backgroundImage = "url('http://image.nixonnow.com/image/common/product20/detail-bg.png')";
					}
				}
				
				$('processingdisp').innerHTML = htmlstorage;
				if ($('initialcartlink')){
				  carturl = $('initialcartlink').value;
				}
				
				
        if ($('initialskuinvmiscdata')){
          buyOnlineHref = $('initialskuinvmiscdata').value;
        }
				
				if ($('firstskurefid')){
					if ($('firstskurefid').innerHTML != '' && $('selsize').innerHTML == 'ONE SIZE'){
						activesizecolor = $('firstskurefid').innerHTML;
						carturl = '/add-to-cart/' + activesizecolor;
						//$('addtocartlinkimg').href = '/add-to-cart/' + activesizecolor;
						//$('addtocartlinkbutton').href = '/add-to-cart/' + activesizecolor;
					}
				}
				/*
				What a sec to do this one. 
				
				if ($('movieslideshowcontainer')){
					videoRefID = $('movieslideshowcontainer').title; 
					loadVideo(videoRefID);
				}*/
				if ($('scrollcontainer1')){
					var scrollconstrainerheight = $('scrollconstrainer1').offsetHeight;
					var scrollcontainerheight = $('scrollcontainer1').offsetHeight;
					scrollmaxy = scrollcontainerheight - scrollconstrainerheight;
					if (scrollmaxy <= 0){
						$('downarrow1').style.display = 'none';
					}
				}
				
				preloadover = setTimeout('enableproductelements()', 10000);
				
				if ($('initalnamesub')){
					var setnameto = $('initalnamesub').value;
				}
				if ($('initalcolorsub')){
					var setcolorto = $('initalcolorsub').value;
					dealercolorname = setcolorto;
				}
				if ($('initalskuidsub')){
					var setskuidto = $('initalskuidsub').value;
					dealerskuid = setskuidto;
				}
				if ($('initialsize')){
					var setsizeto = $('initialsize').value;
					dealersize = setsizeto;
				}
				if ($('heroglobalrefid')){
					heroglobalrefid = $('heroglobalrefid').value;
				}
				if ($('initalprodrefid')){
					var dealerprodref = $('initalprodrefid').value;
				}
				if ($('initalprodid')){
					var dealerprodid = $('initalprodid').value;
				}
				if ($('initalissoft')){
					var dealerissoft = $('initalissoft').value;
				}
				if ($('initalavail')){
					var avail = $('initalavail').value;
				}
				if ($('initalcolorcode')){
					var colorcode = $('initalcolorcode').value;
				}
				if ($('initialactive')){
					activeState = $('initialactive').value;
					activeState++;
				}
				if ($('initialskuid')){
					var initialskuid = $('initialskuid').value;
				}
				//alert (setcolorto +"/"+ avail +"/"+ setskuidto +"/"+ setsizeto +"/"+ colorcode);
				setfindadealer (setcolorto, avail, setskuidto, setsizeto, colorcode, initialskuid);
				setnotify(setskuidto, setnameto, setcolorto, setsizeto);
				fixpng('fixmeinner');
				if (pngx1fixed == false){
					fixpng('closexpng');
					pngx1fixed = true;
				}
				fixpng('fixmeinner thumber');
				// use function below to submit watch and softgoods path to google after ajax load is done /watches/ and /softgooditems/
							
				sIFRinit();
				//alert ('/watches/womens/' + setnameto +'-'+ setskuidto);
				urchinTracker('/watches/womens/' + setnameto +'-'+ setskuidto);
			} 
			else {
				setTimeout('zoomandajaxcheck()', 50);
			}
		}
		var pngx1fixed = false;
		function hidecoverdetail() {
			setprocessingheight();
			$('processing').style.display = 'none';
			$('processingdisp').innerHTML = '';
			$('processingdisp').style.background = '#FFFFFF';
			$('processingdisp').style.backgroundImage = "none";
			zoom($('processingdisp'),60,0,0);
			pathis = '';
			zoomdonecheck();
		}
		
		function zoomdonecheck(){
			if (zoomdone == true){
				$('processingdisp').style.display = 'none';				
				//$('processingdisp').innerHTML = loadinganim;
			} 
			else {
				setTimeout('zoomdonecheck()', 50);
			}
		}
		
		function scrollnow(direction,user,distance,type,containernum){
			allowscroll = false;
			var currenttop = parseFloat($('scrollcontainer' + containernum).offsetTop);
			if (direction == '+'){
				// vertical max
				if ((currenttop * -1) <= scrollmaxy){
					allowscroll = true;
				}
				currenttop -= easearray[scrollstep];
			}
			if (direction == '-'){
				//vertical min
				if ((currenttop * -1) > 0){
					allowscroll = true;
				}
				currenttop += easearray[scrollstep];
			}
			scrollpxcounter += easearray[scrollstep];
			if (scrollpxcounter >= scrolldistancey){
				allowscroll = false;
				$('scrollcontainer' + containernum).style.marginTop = currenttop + "px";
			}
			if (allowscroll == true){
				scrollstep++;
				$('scrollcontainer' + containernum).style.marginTop = currenttop + "px";
				easyscroll(direction,false,distance,type,containernum);
			}
			else {
				scrollinit = false;
				scrolldone = true;
			}
		}
				
		// zoom image handler
		var zoomstatus;
		var replacer;
		var watchtypetouse;
		var zoomblock = '';
		var zoomimageid = 'zoomimage';
		var origzoomimg = '';
		/*
		zoomimagefade function 
		direction = 
		loaded = 
		watchtype = 
		zoomtype = seeiton zoomjpg, or zoompng
		*/	
		
		var surpresszoom = false;
		function setsurpresszoom(surpressvalue){
			surpresszoom = surpressvalue;
		}
			
		function zoomimagefade(direction, loaded, watchtype, zoomtype){
			if (surpresszoom == true && zoomtype != 'seeiton'){
				$('zoombutton').src = 'http://image.nixonnow.com/image/common/product20/detail-zoom-off.gif';
				if (direction != '-'){
					return false;
				}
			}
			else {
				$('zoombutton').src = 'http://image.nixonnow.com/image/common/product20/detail-zoom.gif';
			}
			/*alert("direction = " + direction +
						"\n loaded = " + loaded +
						"\n watchtype = " + watchtype +
						"\n zoomtype = " + zoomtype +
						"\n ext = " + ext);*/
			watchtypetouse = watchtype;
			if(zoomtype != '') { //happens when clicking on ant
				zoomblock = zoomtype;
			} 
			if (direction != '-'){
				if (heroglobalrefid != ''){
					$('loaderoo').style.display = 'block';
					replacer = heroglobalrefid;
					//alert(zoomtype);
					switch(zoomtype) { 
						case 'seeiton':
							//alert(zoomtype);
							replacer = replacer.replace(/hero/g,zoomtype);
							var dashpos = replacer.lastIndexOf("-");
							replacer = replacer.substring(0,dashpos);
							replacer = replacer + ".jpg";
							break;    
						case 'zoompng':
							//alert(zoomtype);
							replacer = replacer.replace(/hero/g,'zoom');
							replacer = replacer.replace(/jpg/g,'png');
							break;
						case 'zoomjpg':
							replacer = replacer.replace(/hero/g,'zoom');
							break;
					}
					var zoomimageobj = new Image();
					zoomimageobj.onload = zoomimageloaded;
					zoomimageobj.src = replacer;
				}
				else {
					zoomnow(direction, loaded, watchtype);
				}
			}
			else { //zoom out
				zoomnow(direction, loaded, watchtype);
			}
		}
		
		function zoomimageloaded(evt){
			$('loaderoo').style.display = 'none';
			zoomnow('',true, watchtypetouse);
		}
		
		function zoomnow(direction,loaded,watchtype) {
			if (loaded == true){
				if(zoomblock == 'seeiton') { 
					$('seeitonimage').src = replacer;
					zoomimageid = 'seeitonimage';
				} else {
					$('zoomimage').src = replacer;
					zoomimageid = 'zoomimage';
				}
			} 
			// write preloader for zoom image here
			if ($('zoombutton')){
				if ($('zoombutton').alt == 'zoom-in' && direction != '-'){
					zoomstatus = 'zoom-in';
					
					$(zoomblock).style.display = 'block';
					opacity(zoomimageid,0,100,500);
					setTimeout(function () { zoomimagefadedone(zoomstatus, watchtype); }, 510);
				} 
				else {
					if ($('zoombutton').alt == 'zoom-out' || direction == '-'){
						zoomstatus = 'zoom-out';
						if	(findIE6 != -1){
							// next line causes problem in ie6
							//$(zoomimageid).style.background = 'none';
							if(origzoomimg != ''){
								$(zoomimageid).src = origzoomimg;
							}
						}
						opacity(zoomimageid,100,0,500);
						setTimeout(function () { zoomimagefadedone(zoomstatus, watchtype); }, 510);
					}
				}
			}
		}
		
		var version = navigator.userAgent;
		var findIE6 = version.indexOf('MSIE 6');
		function zoomimagefadedone(zoomstatus, watchtype) {
			//alert (watchtype);
			switch (zoomstatus){
			case 'zoom-in':
				$('zoombutton').alt = 'zoom-out';
				$('zoombutton').title = 'zoom-out';
				if (watchtype != 'ELITE'){
					$('zoombutton').src = 'http://image.nixonnow.com/image/common/product20/detail-zoom-minus.gif';
				}
				if	(findIE6 != -1){
					if (zoomblock == 'zoompng') { 
						origzoomimg = $(zoomimageid).src;
						$(zoomblock).style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='"+ $(zoomimageid).src +"')";
						$(zoomimageid).src = 'http://image.nixonnow.com/image/common/clear.gif';
					} else { 
						$(zoomblock).style.background = 'none';
						$(zoomimageid).style.border = '1px solid #CCCCCC';
					}
				}
				else {
					$(zoomimageid).style.filter = '';
				}
			break;
			case 'zoom-out':
				if(zoomblock != '') { 	
					$(zoomblock).style.display = 'none';
					$('zoombutton').alt = 'zoom-in';
					$('zoombutton').title = 'zoom-in';
					if (watchtype != 'ELITE'){
						if (surpresszoom == false){
							$('zoombutton').src = 'http://image.nixonnow.com/image/common/product20/detail-zoom.gif';
						}
					}
					zoomblock = '';
				} 
				//$('zoomimage').style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='"+ $('zoomimage').src +"')";
			break;
			}
		}
		
		function text2Password(lmnt){
			$('userPasswordDiv').innerHTML = '<input name="userPassword" type="password" id="userPassword" value="" class="ffield" onclick="this.focus();" />';
			setTimeout(function(){$('userPassword').focus();},300);
		}
		
		var userloggedin;
		var status;
		var watchrefid;
		
		var userloggedin = false;
		
		function wishlistcheck(openclose) {
			status = openclose;
			var pars = "PAGE=CHECK_LOGGED_IN";
			var url = "/za/NXN";
			var myAjax = new Ajax.Request( url, { method: 'post', parameters: pars, onComplete: wishlistcheckdone});
		}
		
		function wishlistcheckdone(originalRequest){
			userloggedin = originalRequest.responseText;
			wishlistlogic();
		}
		
		function wishlistlogic(){
			if (status == 'open' && userloggedin == 'false'){
				tabimage = $('wishlisttab').src;
				tabimage = tabimage.replace(/.gif/,'-on.gif');
				//$('wishlisttab').src = tabimage;
				var userName = $('userName').value;
				var userPassword = $('userPassword').value;
				var pars = "OPTION=LOGONUSERAJAX";
				pars += "&WDS_USER.USERID=" + userName;
				pars += "&WDS_USER.PASSWORD=" + userPassword;
				pars += "&WDS_USER.COMPANY_ID=1";
				pars += "&r=" + Math.random();
				var url = "/za/NXN";
				var myAjax = new Ajax.Request( url, { method: 'post', parameters: pars, onComplete: wishlistlogicdone});
			}
			// add product to wish list if user is logged in
			if (userloggedin == 'true'){
				// ensure wish list popup layer is closed
				$('auxwish').style.display = 'none';
				var watchskurefid = $('skurefidwishlist').value;
				window.location = '/za/NXN?OPTION=WISHLIST_INSERT&PAGE=WISH_LIST&SKU=' + watchskurefid;
			}
			else {
				// show wish list popup and let user login
				$('auxwish').style.display = 'block';	 
			}
		}
		
		function wishlistlogicdone(originalRequest) {
			if (originalRequest.responseText == 'TRUE'){
				var watchskurefid = $('skurefidwishlist').value;
				$('auxwish').style.display = 'none';
				window.location = '/za/NXN?OPTION=WISHLIST_INSERT&PAGE=WISH_LIST&SKU=' + watchskurefid;	
			}
			else {
				$('userName').value = '-- Username --';
				$('userPassword').type = 'text';
				$('userPassword').value = '-- Password --';
				$('wishnote').innerHTML = '<span class="red">Enter a valid username &amp; password</span>';
			}
		}
		// wish list end
		
		function createaccount(){
			var currentlocation  = window.location;
			window.location = "/za/NXN?PAGE=CREATE_WISH_LIST_ACCOUNT&HOMEPAGE=" + currentlocation;
		}
		
		var imageloadedcounter = 0;
		var imageloadedcountersecond = 0;
		function resetimageloader() {
			imageloadedcounter = 0;
			imageloadedcountersecond = 0;
			heroglobalrefid = '';
			activeState;
			opacityrunning = false;
			herosloaded = false;
			showingfaderimage = 2;
			showingimagepos = '';
			activesizecolor = '';
			firstcolorset = false;
			currIconDesc = '';
			dealersize = '';
		}
		// wait for image preload of first set
		function reportimageload(evt){
			if ($('imagepreloadercounter')){
				var totalimagestoload = (parseFloat($('imagepreloadercounter').innerHTML)) + 1; 
				//alert (totalimagestoload +"/"+ imageloadedcounter);
				imageloadedcounter++;
				if (imageloadedcounter == totalimagestoload){
					enableproductelements();
				}
			}
		}
		// wait for image preload of second set
		var herosloaded = false;
		function reportimageload_second(evt){
			if ($('imagepreloadercounter')){
				var totalimagestoloadsecond = (parseFloat($('imagepreloadercounter').innerHTML)) - 1; 
				imageloadedcountersecond++;
				if (imageloadedcountersecond == totalimagestoloadsecond){
					herosloaded = true;
					//alert ('all heros loaded');
				}
			}
		}
		
		function enableproductelements(){
			clearTimeout (preloadover);
			if ($('thumbscontainer')){
				$('thumbscontainer').style.display = 'block';
			}
			if ($('thumbscontainerloader')){
				$('thumbscontainerloader').style.display = 'none';
			}
			if($('faderimg1')){
				$('faderimg1').style.visibility = 'visible';
			}
			if ($('faderimg2')){
				$('faderimg2').style.visibility = 'visible';
			}
			opacity('faderimg1',0,100,500);
			setTimeout(function () { removealphafilter(); }, 500);
			if ($('faderimgloader')){
				$('faderimgloader').style.display = 'none';
			}
		}
		
		function removealphafilter() {
			if ($('faderimg1')){
				$('faderimg1').style.filter = '';
			}
			if ($('faderimg2')){
				$('faderimg2').style.filter = '';
			}
			if (findIE6 != -1){
				var imagestring = $('faderimg1').src;
				var splitimagestring = imagestring.split('png',1);
				if (splitimagestring != imagestring){
					$('faderimg1').style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, sizingMethod=scale src='"+ $('faderimg1').src +"')";
					$('faderimg1').src = 'http://image.nixonnow.com/image/common/clear.gif';
					$('faderimg2').src = 'http://image.nixonnow.com/image/common/clear.gif';		
				}
			}
		}
		
