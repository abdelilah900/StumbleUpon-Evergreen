		function productdetailloaded(request){
			ajaxresult = request.responseText;
			htmlstorage = closedetail + '<div id="innerwrapper">' + ajaxresult + '</div>';
			setprocessingheight();
			ajaxloaddone = true;
			zoomandajaxcheck();
			startclock();
		}
				
