				function openDropDown(el, ac) {					
					if(show == 1){
						$(el).setStyle({
							visibility:'hidden'
						});
						$(ac).removeClassName('selected');
						show = 0;
					} else {
						$(el).setStyle({
							visibility:'visible'
						});
						$(ac).addClassName('selected');
						show = 1;
					}
				}
        	</script>
