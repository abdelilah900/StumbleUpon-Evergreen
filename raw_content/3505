	<script type="text/javascript">
		var potd = {
			fill: function(o) {
				jQuery.ajax({
					type: 'GET', async: false, cache: false, 
					url: '/potd/fill/'+o+'/', 
					dataType: 'json', 
					success: function(d) {
						var potd_pn = jQuery('.msss .pn');
						var potd_photo = jQuery('.msss .photo');
						var potd_cap = jQuery('.msss .caption');
						var potd_enlg = jQuery('#enlg');
						potd_pn.html(d.pn);
						potd_photo.fadeOut('fast', function() {potd_photo.html(d.photo);}).fadeIn('fast');
						potd_cap.html(d.caption);
						potd_enlg.html(d.enlg);
					}
				});
			},
			cap_sh: function() {
				var potd_cap_sh = jQuery('.msss .caption');
				if(potd_cap_sh.is(":visible")) {
					potd_cap_sh.slideUp('fast');
				}else{
					potd_cap_sh.slideDown('fast');
				}
			}
		}
		jQuery(document).ready(function() {jQuery('.msss .scroll').hover(function() {var pn = jQuery(this).attr('id');jQuery(this).addClass(pn+'_h');}, function() {var pn = jQuery(this).attr('id');jQuery(this).removeClass(pn+'_h');});});
	</script>
