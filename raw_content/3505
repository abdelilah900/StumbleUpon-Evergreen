<script language="javascript">
	function load_mp(t, i) {
		jQuery.ajax({type: 'GET', async: true, cache: false, url: '/mp/'+i+'/', dataType: 'html', success: function(data) {jQuery('.mp_btn').removeClass('selected');
		jQuery(t).addClass('selected');
		var n = jQuery('#most_popular_side');n.fadeOut('fast', function() {n.html(data)}).fadeIn('fast');}});
		
		upi_pt('http://www.upi.com/mp/'+i+'/', 'Most Popular - '+i);
	}
</script>


