	<script language="javascript">
		function loadSSG(t, tab) {
			jQuery.ajax({type: 'GET', async: true, cache: false, url: '/ss/em/more/'+tab+'/', dataType: 'html', success: function(data) {
				jQuery('.np_tab').removeClass('selected');
				jQuery(t).addClass('item selected');
				var n = jQuery('#ss_em');
				n.fadeOut('fast', function() {n.html(data)}).fadeIn('fast');
			}});
		}
		loadSSG(jQuery('#np_Photos_of_the_Day'), 'Photos_of_the_Day');
		function fillSS(tab, offset) {jQuery.ajax({type: 'GET', async: true, cache: false, url: '/ss/em/more/'+tab+'/'+offset+'/', dataType: 'html', success: function(data) {jQuery('#ss_em').html(data);}});}
	</script>

	
