function KeyPress(e) {
var huh = e.keyCode;
		if(e.keyCode) // IE
		  {
		  	if (e.keyCode == 13)
		  		window.location.href='http://search.yahoo.com/search?type=rivals&p=' + document.getElementById('searchField').value;
		  }
		else if(e.which) // Netscape/Firefox/Opera
		  {
		  	if (e.which == 13)
		  		window.location.href='http://search.yahoo.com/search?type=rivals&p=' + document.getElementById('searchField').value;
		  }
	}
</script>
