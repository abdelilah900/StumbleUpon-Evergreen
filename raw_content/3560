<script type="text/javascript">

var scroll_position = 0;

function scroll_monitor(){setInterval("check_scroll()",50)}

function check_scroll(){

		var pos = 10;

		if (navigator.appName == "Microsoft Internet Explorer"){
			scroll_position = document.body.scrollTop;
			if (scroll_position > 170){
				pos += scroll_position;
				document.getElementById('leftnav-top').style.top = pos+='px'; 
			}
			else {document.getElementById('leftnav-top').style.top = '180px'; }
		}
		else {   
			scroll_position = window.pageYOffset;     
			if (scroll_position > 170){
				document.getElementById('leftnav-top').style.position = 'fixed'; 
				document.getElementById('leftnav-top').style.top = '10px'; 
			}
			else{
				document.getElementById('leftnav-top').style.position = 'absolute'; 
				document.getElementById('leftnav-top').style.top = '180px'; 
			}
		}
	return true;
}

</script>

