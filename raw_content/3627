	<!-- inject clickable area into the bvp page for use in background ads -->
	<script>
		(function createAdSpaces(){
			var height=898,width=260;
			var container=getContainer();
			if(container){
				$(container)
					.insert({before: '<div id="cnn_video_leftadarea"></div>'})
					.insert({after: '<div id="cnn_video_rightadarea"></div>'});
				videoBackgroundSkinAdAreaResize();
			} else {
				window.setTimeout(createAdSpaces, 100);
				return;
			}
			Event.observe(window, 'resize', function(){videoBackgroundSkinAdAreaResize();});
			function getContainer(){return $$('div.cnn_maincntnr')[1];}
			function videoBackgroundSkinAdAreaResize(){
				var container=getContainer(),header=$('cnn_hdr'),top='125px';
				if(container){
					var containerPos=$(container).cumulativeOffset();
					if(header){top=header.getHeight()+"px";}
					var leftAdArea=$('cnn_video_leftadarea');
					if(leftAdArea){
						leftAdArea.style.position="absolute";
						leftAdArea.style.left=(containerPos.left-width)+'px';
						leftAdArea.style.top=top;
					}
					var rightAdArea=$('cnn_video_rightadarea');
					if(rightAdArea){
						rightAdArea.style.position="absolute";
						rightAdArea.style.left=(containerPos.left+container.getWidth())+'px';
						rightAdArea.style.top=top;
					}
				}
			}
		})();
	</script>
