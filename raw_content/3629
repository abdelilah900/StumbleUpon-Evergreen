var _gaq = _gaq || [];
_gaq.push(['_setAccount', 'UA-25371336-1']);
_gaq.push(['_trackPageview']);
/*
_gaq.push(['_trackEvent', 'Videos', 'Play', 'XYZ']);
_gaq.push(['_trackEvent', 'Videos', 'Pause', 'XYZ']);
_gaq.push(['_trackEvent', 'Videos', 'Stop', 'XYZ']);
