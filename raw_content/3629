<script type="text/javascript">
	var teaserPlayer;
	$(document).ready(function(){
		if(navigator.userAgent.match(/iPad/i) == null){
			//SWF
			var params = { allowScriptAccess: "always", wMode: "transparent" };
			var atts = { id: "teaser-player" };
			swfobject.embedSWF("http://www.youtube.com/e/kh29_SERH0Y?enablejsapi=1&playerapiid=ytplayer&autohide=1&hd=1",
							"teaser-video-embed", "698", "393", "8", null, null, params, atts);
		}else{
			//IFRAME/HTML5
			$("#teaser-activate").remove();
			$("#teaser-video-fade").remove();
			$("#teaser-video").removeClass("no-show");
			$("#teaser-video-embed").html('<iframe width="698" height="393" src="http://www.youtube.com/embed/kh29_SERH0Y" frameborder="0" allowfullscreen></iframe>');
		}
	});
	function onYouTubePlayerReady(playerId) {
		teaserPlayer = document.getElementById("teaser-player");
	}
</script>

<script type="text/javascript" charset="utf-8">
	$(document).ready(function(){
		$("#teaser-activate").click(function(){
			showTrailer();
		});
	});
	function showTrailer() {
		$("#teaser-video-fade").fadeIn(750);
		$("#teaser-activate").fadeOut(750,function(){
			$("#teaser-video").removeClass("no-show");
			if(teaserPlayer){
				teaserPlayer.playVideo();
			}
			//document.getElementById("teaserplayer").playMovie(); //ooyala
			$("#teaser-activate").remove();
			$("#teaser-video-fade").fadeOut(750,function(){
				$("#teaser-video-fade").remove();
			});
		});
		trackFloodEvent('trailer-play');
		trackGAEvent('trailer-play');
	}
</script>			</div>
