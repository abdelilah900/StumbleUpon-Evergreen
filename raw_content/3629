		<div id="news-history-scroll-bar" class="custom-scroll-bar"></div>
	</div>
	<script type="text/javascript">
	$(document).ready(function(){
		if(navigator.userAgent.match(/iPad/i) == null){
			$("#news-history-scroll-bar").slider({
				orientation:"vertical",
				range: "min",
				min: 0,
				max: 100,
				value: 100,
				slide: function(event, ui) {
					scrollNewsList(ui.value);
				}
			});
		}else{
			document.addEventListener('touchmove', function(e){ e.preventDefault(); });
			var myScroll = new iScroll('news-history-list');
		}
		getTweets();
	});
	function scrollNewsList(perc) {
		var lHeight=$("#news-history-list").height();
		var maxScrollY=lHeight-$("#news-history").height();
		var percNormalized=1.00-(perc/100);
		var scrollPosY=Math.round(0-(maxScrollY*percNormalized));
		$("#news-history-list").css({top:scrollPosY+'px'});
	}
	function getTweets() {
		var twitterReq='http://api.twitter.com/1/statuses/user_timeline.json?user_id=253162230&count=50&trim_user=1&exclude_replies=1&callback=?';
		$.getJSON(twitterReq,function(data){
			var tweetCountMax=5;
			var tweetCountInc=0;
			var tweetsStr='';
			if(data.length>0 && "text" in data[0]){
				$.each(data,function(key,tweet){
					tweetCountInc++;
					if(tweetCountInc<=tweetCountMax){
						tweetsStr+='<p class="tweet">';
							tweetsStr+=parseTweetHyperlinks(tweet.text);
							tweetsStr+='<span class="tweet-date">'+getFormattedTweetTime(tweet.created_at)+'</span>';
						tweetsStr+='</p>';
					}
				});
			}
			$("#twitter-feed-list").html(tweetsStr);
			//add new window listeners for urls
			$("#twitter-feed-list p a").click(function(){
				var url=$(this).attr("href");
				var newwindow=window.open(url,'external-url');
				if (window.focus) {
					newwindow.focus()
				}
				return false;
			});
		});
	}
	function getFormattedTweetTime(stamp) {
		//running regex to grab everything after the time
		var newstamp = stamp.replace(/(\d{1,2}[:]\d{2}[:]\d{2}) (.*)/, '$2 $1');
		//moving the time code to the end
		newstamp = newstamp.replace(/(\+\S+) (.*)/, '$2 $1');
		var d1 = Date.parse(newstamp);
	 	var formatted=d1.toString('d MMM');
		return formatted;
	}
	
	function parseTweetHyperlinks(str){
		var newStr=String(str).parseTweetURLs().parseTweetHashtags().parseTweetUsernames()
		return newStr;
	}
	String.prototype.parseTweetURLs = function() {
		return this.replace(/[A-Za-z]+:\/\/[A-Za-z0-9-_]+\.[A-Za-z0-9-_:%&~\?\/.=]+/g, function(url) {
			return url.link(url);
		});
	};
	String.prototype.parseTweetHashtags = function() {
		return this.replace(/[#]+[A-Za-z0-9-_]+/g, function(t) {
			var tag = t.replace("#","%23")
			return t.link("http://search.twitter.com/search?q="+tag);
		});
	};
	String.prototype.parseTweetUsernames = function() {
		return this.replace(/[@]+[A-Za-z0-9-_]+/g, function(u) {
			var username = u.replace("@","")
			return u.link("http://twitter.com/"+username);
		});
	};
	</script>
</div>
