(function() {
  var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
  ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
  var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();
var lastGARef='';
function trackGAEvent(ref) {
	if(lastGARef!=ref){
		switch(ref){
			case 'trailer-play':
				_gaq.push(['_trackEvent', 'Videos', 'Play', 'The Art of FLIGHT Trailer 1']);
			break;
			case '#content-page-1':
				_gaq.push(['_trackEvent', 'Sections', 'Visit', 'Trailer']);
			break;
			case '#content-page-2':
				_gaq.push(['_trackEvent', 'Sections', 'Visit', 'News']);
			break;
			case '#content-page-3':
				_gaq.push(['_trackEvent', 'Sections', 'Visit', 'Videos']);
			break;
			case '#content-page-4':
				_gaq.push(['_trackEvent', 'Sections', 'Visit', 'Photos']);
			break;
			case '#content-page-5':
				_gaq.push(['_trackEvent', 'Sections', 'Visit', 'Bios']);
			break;
			case '#content-page-6':
				_gaq.push(['_trackEvent', 'Sections', 'Visit', 'World Tour']);
			break;
			case '#content-page-7':
				_gaq.push(['_trackEvent', 'Sections', 'Visit', 'Friends']);
			break;
		}
	}
	lastGARef=ref;
}
