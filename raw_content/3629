<script type="text/javascript">
	$(document).ready(function(){
		//create stub bio container
		var bioEl='<div class="rider-bio" style="display:none;">';
			bioEl+='<h3><span></span><a id="riders-backtofilm" href="#">&#171; Back to Art of FLIGHT</a><div style="clear:both;"></div></h3>';
			bioEl+='<p></p>';
			bioEl+='</div>';
		$("#riders-summary .film-description").after(bioEl);
		//rider bio listener
		$("#riders-list .riders .rider-summary").click(function(){
			ridersShowRiderBio(this);
		});
		//back to film summary listener
		$("#riders-backtofilm").click(function(){
			ridersShowFilmSummary()
			return false;
		});
	});
	function ridersShowFilmSummary() {
		$("#riders-list .active-bio").removeClass("active-bio");
		$("#riders-summary .rider-bio").fadeOut(300,function(){
			$("#riders-summary .film-description").fadeIn(300);
		});
	}
	function ridersShowRiderBio(targ) {
		var el=$(targ);
		//set currently active item if set
		var activeEl=$("#riders-list .active-bio");
		//fade in
		if(activeEl.length>0){
			activeEl=$("#riders-summary .rider-bio");
		}else{
			activeEl=$("#riders-summary .film-description");
		}
		activeEl.fadeOut(300,function(){
			//set new content
			$("#riders-summary .rider-bio h3 span").html(el.children('h4').html());
			$("#riders-summary .rider-bio p").html(el.children('p').html());
			//unset old active item
			$("#riders-list .active-bio").removeClass("active-bio");
			//set new active item
			el.addClass('active-bio');
			//fade in
			$("#riders-summary .rider-bio").fadeIn(300);
		});
	}
</script>			</div>
