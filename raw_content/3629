var lastFloodRef='';
function trackFloodEvent(ref) {
	if(lastFloodRef!=ref){
		switch(ref){
			case 'trailer-play':
				callFloodTrailerPlay();
			break;
			case '#content-page-1':
				callFlood('RB_2011_AOF_HP_Trailer');
			break;
			case '#content-page-2':
				callFlood('RB_2011_AOF_News');
			break;
			case '#content-page-3':
				callFlood('RB_2011_AOF_Videos');
			break;
			case '#content-page-4':
				callFlood('RB_2011_AOF_Photos');
			break;
			case '#content-page-5':
				callFlood('RB_2011_AOF_Bios');
			break;
			case '#content-page-6':
				callFlood('RB_2011_AOF_HP_World Tour');
			break;
			case '#content-page-7':
				callFlood('RB_2011_AOF_HP_Friends');
			break;
		}
	}
	lastFloodRef=ref;
}
function callFlood(evt){
