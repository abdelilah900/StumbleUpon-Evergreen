	$("#doubleclick-container").html('<iframe src="http://fls.doubleclick.net/activityi;src=2958734;type=count047;cat=rb_20682;u2='+encodeURIComponent(evt)+';ord=' + a + '?" width="1" height="1" frameborder="0" style="display:none"></iframe>');
}
function callFloodTrailerPlay(){
