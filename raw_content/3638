                <script type="text/javascript">
                	$("#whats-new-header").after($('#member-login-header'));
                	                	$('#taste-member-login').hide();
                	$('#member-login-header').css({backgroundPosition: '0 8px', height:'19px', padding:'10px 0 4px 11px'});
                	$('#whats-new-header').addClass('active');

                	$('#tab-content h2')
                		.mouseover(function(){
                			$(this).css({backgroundPosition: '0 0', height: '26px', padding:'7px 0 0 11px'});
                		})
                		.mouseout(function(){
                			if (!$(this).hasClass('active'))
                				$(this).css({backgroundPosition: '0 8px', height:'19px', padding:'10px 0 4px 11px'});
                		})
                		.click(function(){
	                		if (this.id == 'member-login-header') {
	            				$('#taste-member-login').show();
	            				$('#taste-whats-new').hide();
	            				$('#whats-new-header').css({backgroundPosition: '0 8px', height:'19px', padding:'10px 0 4px 11px'});
	            			} else {
	            				$('#taste-member-login').hide();
	            				$('#taste-whats-new').show();
	            				$('#member-login-header').css({backgroundPosition: '0 8px', height:'19px', padding:'10px 0 4px 11px'});
	            			}
	            			$('#tab-content h2').removeClass('active');
	            			$(this).addClass('active');
                		});
                </script>
