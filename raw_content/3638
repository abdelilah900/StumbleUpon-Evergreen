<script type="text/javascript">
	$(document).ready(function()
	{
		$('#adv-search-button').click(function() {
			var publication = $(this).siblings('#search-in-label').children('#search-in');
			if (publication.val() > 0) {
				var name = publication.attr('name');
				publication.attr('name', name+'[]');
			} else {
				publication.remove();
			}
			$(this).siblings('label').children('#searchinput').attr('name', 'keywords');

			var searchForm = $(this).parent().parent();
			searchForm[0].action = '/recipes/advanced-search.php';
			searchForm.submit();
			return false;
		});
	});
</script>

