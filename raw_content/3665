		  if ((typeof (hideAllAds) == 'undefined' || hideAllAds == false) && (typeof (AD_TRACKER) == 'undefined' || AD_TRACKER.isAdHidden('leaderboard') == false) &&
		      (typeof (hideAd_16174175) == 'undefined' || hideAd_16174175 == false)) {
            TR.writeDFPURL("http://ad.doubleclick.net/adj/" + adsrc);
		  }

		  </script>

		<noscript>
		    <a href="http://ad.doubleclick.net/jump/us.reuters;type=leaderboard;sz=728x90;tile=1;ord=2423?" target="_blank">
