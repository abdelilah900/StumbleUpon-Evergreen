<script type="text/javascript">
var validfrm = false
function removeNL(s) {
  /*
  ** Remove NewLine, CarriageReturn and Tab characters from a String
  **   s  string to be processed
  ** returns new string
  */
  r = "";
  for (i=0; i < s.length; i++) {
    if (s.charAt(i) != '\n' &&
        s.charAt(i) != '\r' &&
        s.charAt(i) != '\t') {
      r += s.charAt(i);
      }
    }
  return r;
  }



function valFrm(obj, respObj, isEmail)
{
   var addr = obj.value;
   var i = 0;
   var emailAddresses;
   var valMsg = "";
   var resp = document.getElementById(respObj);
   
   if (addr == "")
   {
      valMsg = "Required Field. Cannot be Blank \n";
   }
   if (isEmail == 1 && addr != "")
   {
       emailAddresses = addr.split(";");
       while (i < emailAddresses.length)
          {
          //  addr = emailAddresses[i];
	        if (!validateEmail(emailAddresses[i],1,0))
	        {
               valMsg = emailAddresses[i] + " is not a valid email address \n";
            }
            i++;
          }
   }
    if (valMsg == "")
       validfrm = true;
    else
       validfrm = false;
    resp.innerHTML = valMsg;
}

function clearPasswordPromptUpp() {
    j$('input[ID=passwordPromptUpp]').hide();
    j$('div[ID=loginDetail] input[ID$=Password]').show().focus();
}
</script>

