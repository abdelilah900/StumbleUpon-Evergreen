 if(obj){
  if(obj.value==' Search Lawyers.com'){obj.value='';}
  setSiteSearchBoxColor(obj);
 }return;}
function setSiteSearchBoxColor(obj) {
 if(obj.value==' Search Lawyers.com'){obj.style.color='#777';}
 else{obj.style.color='#000';}}
