        function changeCssClass(objDivID)
        {
            if(document.getElementById(objDivID).className=='wp-tabs')
            {
                document.getElementById(objDivID).className = 'wp-tabs2';
            }
            else
            {
                document.getElementById(objDivID).className = 'wp-tabs';
            }
        }
        
    </script>
