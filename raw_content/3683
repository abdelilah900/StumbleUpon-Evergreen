    <script language="javascript" type="text/javascript">
				function validateContentSearch() {
					var searchTerm;
					searchTerm = document.searchForm.s.value;
					searchTerm = searchTerm.replace(/^\s*|\s*$/g,""); //replace if only spaces were entered in the search term field
					if ((!searchTerm) || (searchTerm.toLowerCase() == "Search Lawyers.com".toLowerCase())) {
						alert("Please enter a search term.");
						document.searchForm.s.focus()
						return false;			
					}
					searchTerm = searchTerm.replace("%","");//fix for endeca not supporting % in querystring
					document.searchForm.s.value = searchTerm;
					return true;
				}
    </script>

