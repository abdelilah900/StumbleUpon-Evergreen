<script>
	function onShareButtonClicked(event) {
	}
	// Define UserAction onject
	var ua = new gigya.services.socialize.UserAction(); 
	ua.setLinkBack("http://blogs.lawyers.com/2013/05/obamacare-infographic/"); 
	ua.setTitle("How Obamacare Works [Infographic]");
	
	// Define Share Bar plugin's Parameters	
	var shareBarParams ={ 
		userAction:ua,
