<script language="javascript" type="text/javascript">
    if (document.inputForm && document.inputForm.globalSearchTerm) {
        setSiteSearchBoxColor(document.inputForm.globalSearchTerm);
    }

    function clearSiteSearchInputBox(obj) {
        if (obj) {
            if (obj.value == " Search Lawyers.com") {
                obj.value = "";
            }
            setSiteSearchBoxColor(obj);
        }
        return;
    }

    function setSiteSearchBoxColor(obj) {
        if (obj.value == " Search Lawyers.com") {
            obj.style.color = "#777";
        } else {
            obj.style.color = "#000";
        }        
    }

</script>

<!-- define Gigya -->
<script type="text/javascript" src="http://cdn.gigya.com/js/socialize.js?apiKey=2_1djUjsqFtqjy6R3OLeZMi9EGNDNGaJ2JkMSjDNMkg-jTvuGhKM04lRE1EtPrkbrQ"></script>

<script language="javascript" type="text/javascript">
var decodedAlid;
var uid;
   
    DisplayWelcomeMessage(); 
    
    document.onclick=function(){ HideDialog(); }
    
    function HideDialog(){
        var clickedElement;
        var clickedClass;
        var myTargetElement = document.getElementById('ProfileNameDiv');
        if (navigator.appName == "Microsoft Internet Explorer"){
            clickedElement = document.activeElement;
            clickedClass = clickedElement.className;
        }
        else{
            clickedElement = window.event||arguments.callee.caller.arguments[0];
            clickedClass = clickedElement.target.className;
            //alert(clickedElement.target.id);
            //alert(clickedClass);
            // if user clicks on name section then we need to display the dialog
            if (clickedElement.target.id == 'firstNameSpan'|| clickedElement.target.id == 'lastNameSpan' || clickedElement.target.id == 'signoutImg')
            {
                clickedClass = clickedElement.target.parentNode.className;
            }
        }
       
        if (myTargetElement != null || myTargetElement != undefined)
        {
            if (myTargetElement.className != clickedClass){
                document.getElementById('dialogHolder').style.display = 'none';
            }
        }
    }

    function CallProfile(){
        if (document.getElementById('dialogHolder').style.display == 'block')
        {
            document.getElementById('dialogHolder').style.display = 'none';
        }
        else{
            var hostServer = document.getElementById('windowHostHidden').value;
            CallProfileResponse(decodedAlid, hostServer);
        }
    }

    function DisplayWelcomeMessage()
    {
        var regCookie = GetCookie("reg");
        if (regCookie != null || regCookie != '' || regCookie != undefined)
        {
            var arr = '';
            if (regCookie.indexOf("&") != -1) {
                arr = regCookie.split("&");
            }
            
            if (arr != null || arr != undefined)
            {
               if (arr.length > 0)
               {
                    var alid = arr[2].split("=")[1];
                    // if alid is empty, then setting the default value as -1
                    if (trim(alid).length != 0){
                        decodedAlid = decode64(alid);
                    }
                    else{
                        decodedAlid = "-1";
                    }
                    
                    var firstname = arr[0].split("=")[1];
                    var lastname = arr[1].split("=")[1];
                    if (firstname != '' && trim(firstname).length != 0 && lastname != '' && trim(lastname).length != 0){
                        var decodedFName = decode64(arr[0].split("=")[1]);
                        var decodedLName = decode64(arr[1].split("=")[1]);
                        ConstructDialogHeader(decodedFName, decodedLName); 
                        // update the style
                        var global_searchcontainer = document.getElementById('global_searchcontainer');
                        removeClass(global_searchcontainer, 'global_search');
                        addClass(global_searchcontainer, 'global_search_changed');
                        
                        var header = document.getElementById('header');
                        addClass(header, 'header_changed');
                        
                        var socialcontainer = document.getElementById('socialContainer'); 
                        removeClass(socialcontainer, 'fbTwitter');
                        addClass(socialcontainer, 'fbTwitter_changed');
                    }
                    
               }
            }
        }
    }
    
    function CallProfileResponse(alid, hostServer){
        var uCookie = GetCookie("secuser");
        if (uCookie != null || uCookie != '' || uCookie != undefined){
            var uarr = '';
            if (uCookie.indexOf("&") != -1) {
                uarr = uCookie.split("&");
            }
            
            if (uarr != null || uarr != undefined){
               if (uarr.length > 0){
                   uid = uarr[0].split("=")[1];
               }
            }
        }
        var currentPage = location.protocol + "//" + hostServer + "/layouts/Controls/RegistrationPopup/SignOutDialogHandler.ashx?alid="+escape(alid)+"&uid="+escape(uid);
        MakeCrossRequest(currentPage, 'GET', 'dialog');
    }
    
    function MakeCrossRequest(url, action, calltype)
    {
        try
        {
            if (window.XDomainRequest) {
                getCORS(url, action, calltype);
            }
            else
            {            
                var xmlhttp;
                if (window.XMLHttpRequest){
                    xmlhttp = new XMLHttpRequest;
                } else if (window.ActiveXObject){
                    try {
                        xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
                    }catch (e) {
                        try {
                            xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
                        } catch (e) {
                            xmlhttp = false;
                        }
                    }
                }
                if (xmlhttp){
                    xmlhttp.open(action, url, false);
                    xmlhttp.send();
                    if (calltype == 'dialog')
                    {
                        load(xmlhttp.responseText);
                    }
                    else if (calltype == 'logout'){
                        RefreshPage();
                    }
                }
            }

        }
        catch(e){
            //alert(e);
                var s = null;
                if (document.getElementById("def")) {
                    s = document.getElementById("def");
                }
                else {
                    s = document.createElement('script');
                    s.type = 'text/javascript';
                    s.id = "def";
                }
                s.src = (url + "&def=Y");
                document.getElementsByTagName('head')[0].appendChild(s);
        }
    }
    
    
    function load(data)
    {
        document.getElementById('dialogHolder').innerHTML = data;
        document.getElementById('dialogHolder').style.display = 'block';
    }
    
    function getCORS(url, action, calltype){
        try{
            if (window.XDomainRequest)
            {
                var xdr = new XDomainRequest();
                if (xdr)
                {
                    //alert(url);
                    xdr.open(action, url);
                    xdr.onload = function(){
                        if (calltype == 'dialog'){
                            load(xdr.responseText);
                        }
                        else if (calltype == 'logout'){
                            RefreshPage();
                        }
                    }
                    xdr.send();
                }
                else
                {
                    alert('Failed to create');
                }
            }
            else{
                alert("XDomainRequest doesn't exist");
            }
        }
        catch(e)
        {
            alert(e);
        }
    }
    
    function ConstructDialogHeader(firstName, lastName){
        document.getElementById('firstNameSpan').innerHTML = firstName;
        document.getElementById('lastNameSpan').innerHTML = lastName;
        document.getElementById('welcomeDiv').style.display = 'block';
    }
    
    
    
    function GetCookie(c_name) {
    if (document.cookie.length > 0) {
        c_start = document.cookie.indexOf(c_name + "=");
        if (c_start != -1) {
            c_start = c_start + c_name.length + 1;
            c_end = document.cookie.indexOf(";", c_start);
            if (c_end == -1) c_end = document.cookie.length
            return unescape(document.cookie.substring(c_start, c_end));
        }
    }
    return ""
}


function mylogout()
{
  var hostServer = document.getElementById('windowHostHidden').value;
  var logoutURL = "https://" + hostServer + "/layouts/Controls/RegistrationPopup/SingleSignOut.aspx";
  PopupWindow(logoutURL);
  LogoutGigya();
}

function PopupWindow(pageURL) {
    var w = 340;
    var h = 110;
    var left = (screen.width/2)-(w/2);
    var top = (screen.height/3)-(h/2);
    var targetWin;
        
    if (navigator.appName == "Microsoft Internet Explorer"){
        var caller = window.location.href;
        targetWin = window.open(pageURL + "?p="+escape(caller), '', 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=no, resizable=no, width=340, height=110');
    }
    else{
        targetWin = window.open (pageURL, '', 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=no, resizable=no,  width='+w+', height='+h+', top='+top+', left='+left);
        targetWin.onunload = function(){ targetWin.opener.location.href = targetWin.opener.location.href; }
    }
}

function LogoutGigya()
{
    try{
        gigya.socialize.logout(true);
    }
    catch(e)
    {
         alert('error thrown during logout ' +e);
    }
}

function RefreshPage()
{
    var currentProtocolVal = location.protocol;
    if (currentProtocolVal.indexOf('https') != -1)
    {
        currentProtocolVal = 'http:';
    }
    var currentHost = currentProtocolVal + "//" + window.location.hostname;
    var currentPage = currentHost + location.pathname;
    window.location.href = currentPage.split("?")[0];
}


function trim(text) {
    return text.replace(/^\s+|\s+$/g, "");
}
function ltrim(text) {
    return text.replace(/^\s+/g, "");
}
function rtrim(text) {
    return text.replace(/\s+$/g, "");
}


function decode64(input) {
var keyStr = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
var output = "";
	     var chr1, chr2, chr3 = "";
	     var enc1, enc2, enc3, enc4 = "";
	     var i = 0;
	 
	     // remove all characters that are not A-Z, a-z, 0-9, +, /, or =
	     var base64test = /[^A-Za-z0-9\+\/\=]/g;
	     if (base64test.exec(input)) {
	        alert("There were invalid base64 characters in the input text.\n" +
	              "Valid base64 characters are A-Z, a-z, 0-9, '+', '/',and '='\n" +
	              "Expect errors in decoding.");
	     }
	     input = input.replace(/[^A-Za-z0-9\+\/\=]/g, "");
	 
	     do {
	        enc1 = keyStr.indexOf(input.charAt(i++));
	        enc2 = keyStr.indexOf(input.charAt(i++));
	        enc3 = keyStr.indexOf(input.charAt(i++));
	        enc4 = keyStr.indexOf(input.charAt(i++));
	 
	        chr1 = (enc1 << 2) | (enc2 >> 4);
	        chr2 = ((enc2 & 15) << 4) | (enc3 >> 2);
	        chr3 = ((enc3 & 3) << 6) | enc4;
	 
	        output = output + String.fromCharCode(chr1);
	 
	        if (enc3 != 64) {
	           output = output + String.fromCharCode(chr2);
	        }
	        if (enc4 != 64) {
	           output = output + String.fromCharCode(chr3);
	        }
	 
	        chr1 = chr2 = chr3 = "";
	        enc1 = enc2 = enc3 = enc4 = "";
	 
	     } while (i < input.length);
	 
	     return unescape(output);
	  }
	  
function hasClass(ele,cls) {
  return ele.className.match(new RegExp('(\\s|^)'+cls+'(\\s|$)'));
}

function addClass(ele,cls) {
  if (!hasClass(ele,cls)) ele.className += " "+cls;
}

function removeClass(ele,cls) {
  if (hasClass(ele,cls)) {
      var reg = new RegExp('(\\s|^)'+cls+'(\\s|$)');
      ele.className=ele.className.replace(reg,' ');
  }
}

</script>

