<script type="text/javascript">
/**
* TabPro Behaviour
*/
window.addEvent('domready', function() {
	//. clear the original tabs from the base ul
	$('main-navigation').empty();
	
	//. insert out li into the base ul
	$('main-navigation').set('html', $('main-navigation_tabpro').get('html') );
	
	jqcc('ul#main-navigation li.drop').live('mouseover mouseout', function(event) {
		if (event.type == 'mouseover')
		{
			jqcc(this).find('ul').css('display', 'block');
			jqcc(this).find('a').addClass('hover');
		}else{
			jqcc(this).find('ul').css('display', 'none');
			jqcc(this).find('a').removeClass('hover');
		}
	});
	
	//. remove our tabs from display
	$('main-navigation_tabpro').dispose();
	//. if we are looking at a page that has sub navigation then remove it because its already handled by our drop-downs
	$$('.sub-navigation').dispose();
});
</script>

