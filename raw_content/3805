							$('video_buzz_element_1999026_879128').innerHTML = you_tube_markup;
						}
	          } else if (vd.type == 'vimeo') {
	            var vimeo_tmpl = new Template('<iframe src="#{src}" width="#{width}" height="#{height}" frameborder="0"></iframe>');
	            var vimeo_markup = vimeo_tmpl.evaluate({ src: src_link+'?color=ee3322', width: 625, height: 376 });
	            $('video_buzz_element_1999026_879128').innerHTML = vimeo_markup;
						} else {
							try{so.addParam("wmode", "opaque");} catch(e) {}
							so.write("video_buzz_element_1999026_879128");
						}
					}
				} catch(e) {
					console.log(e);
				}

