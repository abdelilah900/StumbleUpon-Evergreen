<script type="text/javascript">
/* <![CDATA[ */
function mtUpdateSignInWidget(u) {
    var separator = '&nbsp;&nbsp;&bull;&nbsp;&nbsp;';
    var el = document.getElementById('signin-widget-content');
    var content = '';
    if (!el) return;
    if (u) {
        if (u && u.is_authenticated) {
            user = u;
            mtSaveUser();
        } else {
            // user really isn't logged in; so let's do this!
            return mtSignIn();
        }
    } else {
        u = mtGetUser();
    }
    if (u && u.name) {
        var url;
        if (u.is_authenticated) {
            if (u.is_author) {
                url = 'http://dynamic.boingboing.net/cgi-bin/mt/mt-cp.cgi?__mode=view&blog_id=1';
                url += '&static=' + encodeURIComponent( location.href );
            } else {
                url = u.url;
            }
        } else if (u.url) {
            url = u.url;
        } else {
            url = null;
        }
        if (url)
            content += '<p><a style="color:#F00;" href="' + url + '">Profile<\/a>';
        else
            content += '<p>' + '';
        if (u.is_author)
            content += ' &bull; <a href="http://dynamic.boingboing.net/cgi-bin/mt/mt-cp.cgi?__mode=edit&blog_id=1&return_to=' + encodeURIComponent(document.URL) + '">Edit<\/a>';
        
        content += ' &bull; ' + '<a href="javascript:void(0)" onclick="return mtSignOutOnClick()">Logout<\/a>';
        
    } else if (u && u.is_banned) {
        content = 'You do not have permission to sign in to this blog.';
    } else {
        content = '<p><a style="color:#F00;" href="javascript:void(0)" onclick="return mtSignInOnClick(\'signin-widget-content\')">Log in<\/a>';
        
        
        content += separator + '<a href="http://dynamic.boingboing.net/cgi-bin/mt/mt-cp.cgi?__mode=register&blog_id=1&return_to=' + encodeURIComponent(document.URL) + '">Register<\/a>';
        
        
    }
    el.innerHTML = content;
}
mtAttachEvent('usersignin', mtUpdateSignInWidget);
mtUpdateSignInWidget();
/* ]]> */
</script>


