            gasp_cb.style.width = "25px";
            gasp_p.appendChild(gasp_cb);
            gasp_p.appendChild(gasp_text);
            var frm = gasp_cb.form;
            frm.onsubmit = gasp_it;
            function gasp_it(){
            if(gasp_cb.checked != true){
            alert("Please check the box to confirm that you are NOT a spammer");
            return false;
            }
            return true;
            }
            </script>
            <input type="hidden" id="gasp_email" name="gasp_email" value="" /><p style="display: none;"><input type="hidden" id="akismet_comment_nonce" name="akismet_comment_nonce" value="9aca4a117c" /></p>					</form>
