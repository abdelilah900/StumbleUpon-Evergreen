jQuery(document).ready(function($){
$.fn.cb_field_ajaxCheck = function() {
	if ( ( $(this).val() != '' ) && ( $(this).val() != $(this).data('cblastvalsent') ) ) {
		var inputid = $(this).attr('id');
		if ( ! $('#'+inputid+'__Response').size() ) {
			var respField = '<div class=\"cb_result_container\"><div id=\"' + inputid + '__Response\">&nbsp;</div></div>';
			$(this).parent().each( function() {
				if (this.tagName.toLowerCase() == 'td') {
					$(this).append(respField);
				} else {
					$(this).after(respField);
				}
				$(inputid+'__Response').hide();
			} );
		}
		if (  $('#'+inputid+'__Response').length > 0 ) {
			$('#'+inputid+'__Response').html('<img alt=\"\" src=\"http://www.fittogether.com/components/com_comprofiler/images/wait.gif\" /> Checking...').fadeIn('fast');
			var cbInputField = this;
			var lastVal = $(this).val();
			$(this).data('cblastvalsent', lastVal );
			$.ajax( {	type: 'POST',
						url:  'http://www.fittogether.com/index2.php?option=com_comprofiler&task=fieldclass&reason=register&field='+encodeURIComponent(inputid)+'&function=checkvalue&user=0&no_html=1&format=raw',
						data: 'value=' + encodeURIComponent( lastVal ) + '&cbsecuritym3=' + encodeURIComponent('cbm_16e7e325_21a5019a_0902d041c693ce5bf5b9622870f12bef') + '&cbrasitway=' + encodeURIComponent('cbrv1_1390320204d194161f07a2cdd9d91e13_d3NUJC1WsOzUYNsg'),
						success: function(response) {
							var respField = $('#'+$(cbInputField).attr('id')+'__Response');
