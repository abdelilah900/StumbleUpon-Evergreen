    function doToggleDisplay(status) {
        if ($('scrollingContainer') && $('nonScrollingContainer')) {
            if (status == 'more') {
                $('scrollingContainer').style.display = 'none';
                $('nonScrollingContainer').style.display = '';
            }
            else {
                $('scrollingContainer').style.display = '';
                $('nonScrollingContainer').style.display = 'none';
            }
        }
    }
</script>


      
