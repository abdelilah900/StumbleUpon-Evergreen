			var url = location.href;

			function cmt(page) {
				$.get("/comentarios/cmt-ajax.html", { url: url, page: page },  function(data) {
					if( data ) {
						$('#cmtDisplay').html(data);
						if( $('#cmtDisplay').css('display')=='block' )
							$('#cmtDisplay div').css('display','inline');
					}
				});
			}
			cmt(1);
		</script>
		
