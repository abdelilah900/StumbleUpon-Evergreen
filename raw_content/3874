		var current_image = 0;
		var last_image = 11;
		$(document).keydown(function(event) {
			if (event.keyCode == '37') { previous_image(); }
			if (event.keyCode == '39') { next_image(); }
			function previous_image() {
				current_image--;
				if (current_image < 1) current_image = last_image;
				document.location = '#foto-reportaje-' + current_image;
			}
			function next_image() {
				current_image++;
				if (current_image > last_image) current_image = 1;
				document.location = '#foto-reportaje-' + current_image;
			}
		});
	</script>
