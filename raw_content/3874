			function updater_user_box()
				{
					var user_box = document.getElementById("frmCmtUserData");
					user_box.innerHTML = '';
						
					var fbc_box = document.getElementById('cmtFBC');
					fbc_box.innerHTML = "<div id='fbc-profile'>" 
					+ "<fb:profile-pic uid='loggedinuser' facebook-logo='true'></fb:profile-pic>" 
					+ "<fb:name uid='loggedinuser' useyou='false'></fb:name> " 
					+ '<span class="fbc-logout">(<a href="#" onclick="FB.Connect.logout(function() { location.reload(); }); return false;">Logout</a>)</span>'
					+ '</div>'
					+ '<div style="clear:both;"></div>';
							
					FB.XFBML.Host.parseDomTree();
				}
					
					
				FB.init('cec90ab38f945a78dc2865af1d0164f7', '/fb-connect/xd_receiver.html', {
				'ifUserConnected' : updater_user_box
			});
