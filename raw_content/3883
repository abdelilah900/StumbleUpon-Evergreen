<script type="text/javascript">

    function GetCommunityActivityPage(currentPage) {
        
            var pageNumber = 0;
            var xmlhttp;

            //Get the value from the hidden field
            var currentPageValue = document.getElementById("communityActivityPage").value;

            if (currentPage == "p") {
                pageNumber = parseInt(currentPageValue) - 1;
            }
            else if (currentPage == "n") {
                pageNumber = parseInt(currentPageValue) + 1;
            }
            else {
                pageNumber = currentPage;
            }
            
            //add the querystring parm
            var url = "/Handlers/FromtheCommunity.ashx?page=" + pageNumber.toString();

            //make the call to the server
            if (window.XMLHttpRequest) {
                // code for IE7+, Firefox, Chrome, Opera, Safari
                xmlhttp = new XMLHttpRequest();
            }
            else {// code for IE6, IE5
                xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
            }
                        
            xmlhttp.open("GET", url, false);
            xmlhttp.send();
            
            
            //Set the markup
            var displayDiv = document.getElementById('ctl00_ctl00_CommunityContent_CommunityContent_FromTheCommunityID_fromthecommunity');
            displayDiv.innerHTML = xmlhttp.responseText;

            //Set the Page
            var hiddenPageNumber = document.getElementById("communityActivityPage");
            hiddenPageNumber.value = pageNumber.toString();

            return false;
        }

    </script>
    
    <input id="communityActivityPage" type="hidden" value="1"  />
