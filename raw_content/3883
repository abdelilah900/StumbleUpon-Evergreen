            dartad_tile = typeof(dartad_tile) != 'undefined' ? dartad_tile + 1 : 1;
            var dartad_url = "http://ad.ca.doubleclick.net/N6872/adj/shaw.foodnetwork.ca/community/;loc=top;sz=250x250,300x250;pr=foodtv;kw=ron;ck=community;imp=community;tile=" + dartad_tile + ";" + adcookieTag + surroundTag + "ord=" + adOrdValue + "?";
