            // Attach this event to Ajax call if you want to enable Ad Synching
            // E.X. 
            /*
                     $(document).ready(function () {
                        $(".prev, .next").click(function () {
                            DoAdSynch();
                        });
                 });

            */
            function DoAdSynch() {
                $(".bboxContainer").each(function (i, val) {
                    var aPosition = dartad_url.indexOf("sz=");
                    var bPosition = dartad_url.indexOf(";", aPosition);

                    if (aPosition != -1 && bPosition != -1) {
                        var newUrl = dartad_url.replace("adj", "adi");
                        var sub = newUrl.substring(aPosition + 3, bPosition);
                        newUrl = newUrl.replace(sub, "300x250");
                        adWidth = 300;
                        adHeight = 250;
                        var htmlStr = "<iframe border='no' scrolling='no' frameBorder='0' width='" + adWidth + "' height='" + adHeight + "' style='border:none;' src='" + newUrl + "'></iframe>"
                        $(this).html(htmlStr);

                    }
                });
            }
