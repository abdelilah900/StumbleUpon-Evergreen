<script type="text/javascript">


    function submitLogin() {
        setTimeout(function () {
            $.ajax({
                type: "GET",
                url: "/services/login/loginHandler.ashx",
                data: {
                    username: $("#fancy_div input:text").val(),
                    password: $("#fancy_div input:password").val(),
                    rememberme: ($("#fancy_div :checkbox:checked").length > 0) ? true : false
                },
                contentType: "text/plain; charset=utf-8",
                dataType: "json",
                complete: function () {
                    $(".loginLoadingIndicator").css('display', 'none');
                },

                success: function (msg) {
                    // Failure
                    if (msg.StatusCode == 0) {
                        $(".loginMsg").html(msg.Message);
                    } else {
                        if (window.location.pathname.toLowerCase() !== "/community/signout.aspx") {
                            window.location = window.location.href;
                        }
                        else {
                            window.location.href = "http://" + window.location.host + "/community/";
                        }
                    }
                },
                error: function (msg) {
                    $(".loginMsg").html("Unable to send login request.");
                }
            });
        }, 2000);
    }

     function checkSubmit(e) {
       if(e && e.keyCode == 13)
       {
            $(".loginLoadingIndicator").css('display', 'block');
            $(".loginMsg").html('');
            submitLogin();
            return false;              
       }
    }

    $(document).ready(function () {
        $('#signInLogout').attr('href', '#overlayLoginContainer');
        var fancyboxObj = $("#signInLogout").fancybox({
            width: 400,
            height: 350,
            closeClick: false,
            showCloseButton: true,
            hideOnContentClick: false,
            modal: false
        });

        $("#submitButton").live('click', function () {
            $(".loginLoadingIndicator").css('display', 'block');
            $(".loginMsg").html('');
            submitLogin();
            return false;
        });
        

           
    });
	
</script>
