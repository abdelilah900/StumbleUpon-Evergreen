try{
    var url =(self.location.href);
            var peer = [/\d+/];
            var rtnPeer = [/rfid\d+/];
            var mkt_nws='market-news';
            var bnd='bonds';
            var fut='futures';
            var cncies='currencies';
            var intrn='international';
            var tId;

 
            if (url.match(peer)) {
                var s1 = new String(url.match(peer));
                var s2 = s1.replace('/','');
                tId = s2.replace('/','');
 
            }if (url.match(rtnPeer)) {
                var s1 = new String(url.match(rtnPeer));
                var s2 = s1.replace('/','');
                tId = s2.replace('/','');
 
            }  
 
if(tId==undefined){
if(document.getElementById('dateAndTime').innerHTML==null){
//do nothing
}
var dateTime = document.getElementById('dateAndTime').innerHTML;
                    if(url.match(bnd)){
                        tId=bnd+dateTime.substring(0,8);
                    }else if(url.match(fut)){
                        tId=fut+dateTime.substring(0,8);
                    }else if(url.match(cncies)){
                        tId=cncies+dateTime.substring(0,8);
                    }else if(url.match(intrn)){
                        tId=intrn+dateTime.substring(0,8);
                    }else {
                        tId=mkt_nws+dateTime.substring(0,8);
                    }


}}catch (err){//module is not configured on the page.

}
