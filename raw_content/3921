    <div id="col1">
    <!--Spot=Left_Rail_1 fragmentid=12992--><div id="Article" class="artdf">
    <div id="articleHeader">
        <h1>The four-day work week: Coming soon to a town near you</h1>

        <span class="byline">By <a href="/writers/bruce-watson/">BRUCE WATSON</a></span>
        <span zclass="posted">Posted 2:00 PM 08/29/09</span> <span class="categories"> <a href="/category/energy/" rel="tag">Energy</a>, <a href="/category/technology/" rel="tag">Technology</a>, <a href="/category/economy/" rel="tag">Economy</a>, <a href="/category/people/" rel="tag">People</a></span>

    </div>

    <div id="articleToolsTop" class="rightTxt">

        <span id="cmtCount" class="left"></span>
        <SCRIPT LANGUAGE="JavaScript">
    //below code configures url for facebook like feature
    document.onload(setLink(window.location));
   // document.onload();
    function setLink(link){
try{
        document.getElementById('fbLike').href= link
    }catch(err){//module is not configured on the page
}

}

