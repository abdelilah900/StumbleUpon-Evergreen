<script type="text/javascript">


function changecolor()
{

document.getElementById("webQuery").className="search";
}
function removecolor()
{

document.getElementById("webQuery").className="";
}

function submitHeaderSearch(event) {
    if (event == "onclick" || eventIsEnterKey(event)) {
        return srchSub2();        
    } else {
        return true;
    }
}
function p_o(o){return document.getElementById(o);}

function eventIsEnterKey(event) {
    if (event && event.which == 13 || window.event && window.event.keyCode == 13 || event && event.which == 3 || window.event && window.event.keyCode == 3) {
        return true;
    }
    return false;
}

String.prototype.trim = function() {
	return this.replace(/^\s+|\s+$/g,"");
}

function srchSub2() {
    var frm=p_o("dualsearch");
    var queryval = frm.webQuery.value.trim();
if(queryval == '' || queryval == 'Search the Web')
{
var newurl='http://search.aol.com/aol/webhome?'+'s_it=hdt-dailyfinance';
}
else{
    queryval = queryval.replace( /\+/, " ");
    var newurl = frm.action + queryval+'&s_it=hdt-dailyfinance'; }
    if (frm.target == '_blank') {
        window.open(newurl, '_self', '');
    } else {
        window.location = newurl;
    }
    return false;
}
</script></div>

