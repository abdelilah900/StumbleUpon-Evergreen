	soC.addParam("allownetworking", "all");
	soC.addParam("scale", "noscale");
	soC.addParam("align", "left");
	soC.addParam("salign", "lt");
	soC.addVariable("priceServer", "http://portal.pf.aol.com/dyndataus/ws?rf=http://www.dailyfinance.com");
	soC.addVariable("defaultMarkets", "$INDU:DJI:DJIA,$COMPX:NAI:NASDAQ,$INX:CMI:S%26P 500,(TC10Y:BSS:10 yr Note,/gc:cmx:Gold,/cl:nym:Oil,ukx:ise:FTSE 100,N225:FX1:NIKKEI 225,HSIX:FX1:HANG SENG,EURUSD:FX1:EURO (in USD),USDJPY:FX1:YEN (per USD)");
	soC.addVariable("refreshInterval", "20000");
    soC.addVariable("liveUpdate", carouselLiveUpdate );
	soC.addVariable("updateMechanism", "JS");
	soC.addVariable("drawerEnabled", "false");
	soC.addVariable("lang", "en");
	soC.addVariable("addSymbol", false);

        soC.addVariable("drawerEnabled", false);

	soC.write("carousel");

</script>


