// AddThis configuration

(function($){

$.addthis({
	addthis_config: { username: 'aolmoney' }
});

$(function(){
    $("[name='aol-addthis']").each(function(){

        var e = this,
            url = e.href,
            pageTitle = document.title;

        url = url.substring(url.indexOf('&body=')+6, url.length);
        pageTitle = pageTitle.substring(0,pageTitle.length-15);



        $(e).replaceWith($('<div></div>')
            .addthis({
                addthis_share: {
                   url: url,
                   title: pageTitle,
                   templates: {
                                      twitter: '{{title}}: {{url}} via @daily_finance'
                                     }
                }

            }));
    });
});

})(jQuery);



// end AddThis configuration

