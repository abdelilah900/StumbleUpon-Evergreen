<!-- Cufon Scripts -->
function showTxt() {
	var i = 0;
	var hiddenTxt = new Array;
	
	hiddenTxt = document.getElementsByTagName("h1");
	for (i=0; i<hiddenTxt.length; i++) {
		hiddenTxt[i].style.display = 'inline';
	}
	hiddenTxt = document.getElementsByTagName("h2");
	for (i=0; i<hiddenTxt.length; i++) {
		hiddenTxt[i].style.display = 'inline';
	}
	hiddenTxt = document.getElementsByTagName("h3");
	for (i=0; i<hiddenTxt.length; i++) {
		hiddenTxt[i].style.display = 'inline';
	}
	hiddenTxt = document.getElementsByTagName("h4");
	for (i=0; i<hiddenTxt.length; i++) {
		hiddenTxt[i].style.display = 'inline';
	}
}
<!-- END Cufon Scripts -->

<!-- Buzz Scripts -->
function catInFocus(category) {
	document.getElementById('about-box').style.background = 'none';
	document.getElementById('featured-box').style.background = 'none';
	document.getElementById('giveaways-box').style.background = 'none';
	document.getElementById('visionaries-box').style.background = 'none';
	document.getElementById('events-box').style.background = 'none';
	document.getElementById(category).style.background = 'url("images/bg_pk.png")';
}

function showAbout(state) {
	if (state) {
		document.getElementById('more-about').style.display = 'block';
		document.getElementById('about-show').style.display = 'none';
		document.getElementById('about-collapse').style.display = 'inline';
	} else { 
		document.getElementById('more-about').style.display = 'none';
		document.getElementById('about-show').style.display = 'inline-block';
		document.getElementById('about-collapse').style.display = 'none';
	}
}

function borderFocus(obj) {
	obj.style.borderColor = '#AEAEAE';
}

function borderDefocus(obj) {
	obj.style.borderColor = '#ccc';
}

function buzzFocus(inFocus, color) {
		
	document.getElementById(inFocus).style.background ='url(\'images/bg_gk.png\')';
	document.getElementById(inFocus).className='tag-defocus';
	document.getElementById(inFocus+'-desc').style.display='inline-block';
	document.getElementById(inFocus+'-desc').style.borderColor=color;
	document.getElementById(inFocus+'-name').style.color="#ffffff";
}

function buzzDefocus(inFocus, color) {
		
	document.getElementById(inFocus).className='tag-defocus';
	document.getElementById(inFocus+'-desc').style.display='none';
	document.getElementById(inFocus+'-name').style.color="#ffffff";
	document.getElementById(inFocus).style.background ='url(\'images/bg_dk.png\')';
	
}
<!-- END Buzz Scripts -->
<!-- Author Profile Scripts -->
function titleFocus(displayName, newTitle) {
	document.getElementById('post_title_'+displayName).innerHTML=newTitle;
}

function titleDefocus(displayName) {
	document.getElementById('post_title_'+displayName).innerHTML='';
}
<!-- END Author Profile Scripts -->

<!-- Subscribe email textfield -->
function clearText(field) {
	if (field.defaultValue == field.value) field.value = '';
	else if (field.value == '') field.value = field.defaultValue;
}
<!-- END Subscribe email textfield -->

Cufon.replace('h1', { fontFamily: 'KozukaR' });
Cufon.replace('h2', { fontFamily: 'KozukaR' });
Cufon.replace('h3', { fontFamily: 'KozukaR' });
Cufon.replace('h4', { fontFamily: 'KozukaR' });

</script><!-- General JS -->
