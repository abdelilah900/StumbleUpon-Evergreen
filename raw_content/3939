function slideOut() {
	$(".addthis_bar_vertical").animate({ marginLeft : "0" }, 500);
	$('div.at-floatingbar-share').css('width', 150);
}

function slideIn() {
	if ($.browser.msie && parseInt($.browser.version) <= 7) {
		$(".addthis_bar_vertical").animate({ marginLeft : "-158" }, 500);
	} else {
		$(".addthis_bar_vertical").animate({ marginLeft : "-78" }, 500);
	}
	$('div.at-floatingbar-share').css('width', 150);
}

var hiConfig = {
	over: slideOut,
	timeout: 300,
	out: slideIn
}

$(function(){
	$(".addthis_bar_vertical").hoverIntent( hiConfig );

});

$(window).resize(function() {
	$this = $(window);
	$visible = $(".addthis_bar_vertical:visible").length;
	if ($this.width() < 1070) {
		if ($visible == 1) {
			if ($.browser.msie && parseInt($.browser.version) <= 7) {
				$(".addthis_bar_vertical").css('marginLeft', '-158');
			} else {
				$(".addthis_bar_vertical").css('marginLeft', '-78');
			}
			$(".addthis_bar_vertical").hide('fast');
		}
	} else {
		if ($visible == 0) {
			$(".addthis_bar_vertical").show('fast');
		}
	}
});
</script>

