		<script type="text/javascript">
			// Datos del analitycs
			//<![CDATA[
						var cimg=1,imgz='',imgsrc={"1":"57118_1_1340808245-00921900.jpg","2":"57118_2_1340808245-38906000.jpg"},imz={"1":1,"2":1};
			
			/** Esconde o muestra #custom en caso de que #sizes = Custom **/
			function sc(){if($('#sizes').val()=='Custom'){$('#custom').show();}else{$('#custom').hide();}}
			
			
			$(function() {	//Funciones al inicio del documento
				
				/** Inicia la galeria que se muestra al ampliar con el boton ENLARGE **/
				$("a#first-gallery").attr('rel', 'galeria').fancybox();
				$('#enlarge').click(function () {
					$('#first-gallery').click();
					return false;
				});
				/****/
				
				//** Hace un preload de las imagenes que se mostraran en la galeria **//
				$.preloadImages('/images/zloader.gif');
				$.preloadImages('/prodimg/57118/2/315x430/57118_2_1340808245-38906000.jpg');				//***//
				
				//** Da vida a traves del evento click a los tabs que se encuentran a la derecha de la imagen principal del producto**//
				$('#prodtabs li').click(function(){
					$('.tab').hide();
					$('#prodtabs li').removeClass('sel');
					$(this).addClass('sel');
					$('#'+$(this).attr('rel')).show();
				});
				/*****/
				
				//** Envia los datos a traves de ajax de el formulario de consulta que se encuentra en el segundo "tab"**//
				$('#tab_ask form').submit(function(e){
					e.preventDefault();
					$.ajax({type:'POST',url:$(this).attr('action')+'&a=1',data:$(this).serialize(),success:function(d){$('#tab_ask').html(d)}});
				});
				//****//
				
				//** Crea el fancibox donde se muestran los size chart de los productos **//
				$('#asizechart').click(function(e){
					e.preventDefault();
					$.fancybox($('#sizechart').html(),{autoDimensions:false,width:350,height:120});
				});
			
				//** En caso de que sea dominique ansari muestra una imagen diferente con otra descripcion de las dimenciones**//
				$('#asizechart-dominique').click(function(e){
					e.preventDefault();
					$.fancybox($('#size-dominique').html(), {autoDimensions:true});
				});
				//*****//
				
				//** No estoy seguro de que funcione esto**//
				$('#asizeguide').click(function(e){
					e.preventDefault();
					$.fancybox($('#sizeguide').html(), {autoDimensions:false,width:780,height:450});
				});
				/***/
				
				$('#sizes').click(sc).change(sc).keypress(sc).click();
				
				//** En caso de hacer click en el boton "Personalize item?" muestra la caja donde se encuentran estos elementos**//
				$('#custom-btn').click(function(){
												$('#sizes').val('Custom');
							$('#custom').show();
							$('#custom textarea').focus();
									});
				/***/
			
				//** Esta funcion cambia como imagen pricipal al view_image sobre el que se haga click**//
				$('.view_image').click(function(e){
					e.preventDefault();
					cimg=$(this).attr('rel').replace(/img/,'');
					$('#mainimg img').attr('src','/prodimg/57118/'+cimg+'/315x430/'+imgsrc[cimg]);
					$('#enlarge').attr('href',$(this).attr('href'));
					imgz='/prodimg/57118/'+cimg+'/725x998/'+imgsrc[cimg];
				}).eq(0).click();
				//****//
				
				
				var zz=$('<div></div>').css({position:'absolute',top:0,left:0,width:200,height:200,background:'#222',border:'1px solid #000',cursor:'crosshair'}).fadeTo(0,0.45).hide();
				var zzc=100;
				var mp=$('#mainimg').position();
				var mpw=$('#mainimg').width();
				var mph=$('#mainimg').height();
				var zd=$('<div></div>').css({position:'absolute',top:26,left:-19,width:490,height:478,overflow:'hidden',background:'#fff url(/images/zloader.gif) no-repeat scroll center center','z-index':999}).hide();
				
				$('#prodinfo').css('position','relative').prepend(zd);
				
				//** Manejador del zoom en caso de que la imagen real tenga una mayor dimencion que la que se encuentra en maining **//
				$('#mainimg').css('position','relative').append(zz).hover(function(e){
					if(imz[cimg]){
						zz.show();
						zd.empty().append('<img src="'+imgz+'" alt="" style="position:absolute;"/>');
						mm(e);zd.show();
					}
				},function(){
					if(imz[cimg]){
						zd.hide();zz.hide();
					}
				}).mousemove(mm);
				
				//** Funcion que controla la posicion de el recuadro que recrea el zoom sobre la imagen ampliada **//
				function mm(e){
					if(imz[cimg]){
						mp=$('#mainimg').position();
						x=e.pageX-mp.left-zzc;
						y=e.pageY-mp.top-zzc;
						if(x<0)x=0; if(y<0)y=0;
						if(x>mpw-zzc*2-2) x=mpw-zzc*2-2;
						if(y>mph-zzc*2-2) y=mph-zzc*2-2;
						zz.css({left:x,top:y});
						$('img',zd).css({left:-(x*2.06),top:-(y*2.28)});
					}
				}
				
			});
			//]]>
		</script>
