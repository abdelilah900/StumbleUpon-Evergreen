<!--

function close_menus() {
	document.menus_open = 0;
	for (var i = 1; i<=7; i++) {
		if (document.getElementById('smenu'+i)) {document.getElementById('smenu'+i).style.display='none';}
	}
}

function open_menu(id) {
var d = document.getElementById(id);
if (d) {
	close_menus();
	d.style.display='block';
	document.menus_open = 1;

		//when user clicks somewhere call
		//function to close calendar
		document.onmousemove = function(e) { e = e || window.event; catch_mousemove(e); } ;
}
		return true;

}

	function catch_mousemove(e) {
	//alert("caught!\n menus_open:"+document.menus_open);
		if (document.menus_open!=1) {
		return false;
		}
		var target = e.srcElement || e.target;
		if (!(e != null && elementIsUnderID(target, "menu"))) {
			close_menus();
		}
		return false;

	}

	function getElementID(element) {
			if (element !=null) {
			if (element.attributes != null) {
			var element_attributes = element.attributes;
				for (i=0; i<element_attributes.length; i++){
				if (element_attributes[i].name == "id") {
					return element_attributes[i].value;
				}
				}
			}
			}
			return null;
	}

	function elementIsUnderID(element, desired_id) {
		var current_id = getElementID(element);
		if (current_id == desired_id) {
			return true;
		}
		if (element.parentNode == null) {
			return false;
		} else {
			return elementIsUnderID(element.parentNode, desired_id);
		}
		return false;
	}
//-->

