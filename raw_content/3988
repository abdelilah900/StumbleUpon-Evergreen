var params = jQ.grep(window.location.href.split('?'), function(param) {
								   return param.substring(0, 12) === "recipetitle="; 
								});
								
								if (params.length > 0) {
								  var param = params[0].substr(12);
								  var param = decodeURIComponent(param.replace(/\+/g, '%20'));
								  
								  var selector = jQuery('p:contains("'+param+'"):last')
										   .add(jQuery('h2:contains("'+param+'"):last'))
										   .add(jQuery('h3:contains("'+param+'"):last'))
										   .add(jQuery('h4:contains("'+param+'"):last'));
										
								  if (selector.length >= 1) {
								     jQuery(window).scrollTop(jQuery(selector[0]).offset().top-35);
								  }
								}
</script>
									
							        		
		
												
										
						
											                    
    
	
		
											
    
        
