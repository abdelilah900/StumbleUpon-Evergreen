	<script type="text/javascript">
	//<![CDATA[
	var largePictureIsLoaded;
	if(!largePictureIsLoaded) {
		postLoadImage("big-picture", "http://static.guim.co.uk/sys-images/Guardian/Pix/pictures/2011/11/3/1320323425633/Toad-in-the-hole-001.jpg");
		largePictureIsLoaded = true;
	}

	function showLargePicture(event) {
		if (event) {
			event.preventDefault();
		}

		var dialogBox = new guardian.r2.DialogBox();
		dialogBox.showDialogBox(document.getElementById('big-picture-holder'), document.getElementById('picture-frame'), true);
		window.location.hash = 'zoomed-picture';
		dialogBox.createCloseLink(document.getElementById('big-picture-holder'), document.getElementById('picture-frame'));
		
		if(!largePictureIsLoaded) {
			document.getElementById("big-picture").src = 'http://static.guim.co.uk/sys-images/Guardian/Pix/pictures/2011/11/3/1320323425633/Toad-in-the-hole-001.jpg';
			largePictureIsLoaded = true;
		}

		return false;
	}
	if (window.location.hash === '#zoomed-picture') {
		jQ(showLargePicture);
	}
	jQ('#show-big-picture-link').bind('click', showLargePicture);

	function closeLightBox(lightBoxImgID) {
		jQ(lightBoxImgID).bind('click', function(e) {
			e.preventDefault();
			jQ('.lb-close').hide();
			window.location.hash = '_';
			return false;
		});
	}

	closeLightBox('#picture-frame');

	//]]>
	</script>

	 
 
