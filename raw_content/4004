                        $(function(){
                $('.exploreCollapse').ajaxify('#exploreTopicsWindow', {
                    bind: 'click',
                    cache: false,
                    live: true,
                    preloadType: 'inline',
                    postResponse: function() {
                        if ($('#exploreTopics').hasClass('exploreCollapse')) {
                            $('#exploreTopics').removeClass('exploreCollapse');
                            $('#exploreTopics').addClass('exploreExpand');
                        } else if ($('#exploreTopics').hasClass('exploreExpand')) {
                            $('#exploreTopics').removeClass('exploreExpand');
                            $('#exploreTopics').addClass('exploreCollapse');
                        }

                        if ($('#exploreTopicsWrapper').hasClass('exploreTopicsWrapperHide')) {
                            $('exploreTopicsWrapper').removeClass('exploreTopicsWrapperHide');
                            $('exploreTopicsWrapper').addClass('exploreTopicsWrapperShow');
                        }

                        closeOthers('exploreTopics');
                    }
                });

                $('.exploreExpand').live('click', function() {
                    if ($('#exploreTopics').hasClass('exploreCollapse')) {
                        $('#exploreTopics').removeClass('exploreCollapse');
                        $('#exploreTopics').addClass('exploreExpand');
                    } else if ($('#exploreTopics').hasClass('exploreExpand')) {
                        $('#exploreTopics').removeClass('exploreExpand');
                        $('#exploreTopics').addClass('exploreCollapse');
                    }

                    if ($('#exploreTopicsWrapper').hasClass('exploreTopicsWrapperShow')) {
                        $('#exploreTopicsWrapper').removeClass('exploreTopicsWrapperShow');
                        $('#exploreTopicsWrapper').addClass('exploreTopicsWrapperHide');
                    }

                    return false;
                });

                $('.topicExplorePaginator a').ajaxify('#exploreTopicsWindow', {
                    bind: 'click',
                    cache: false,
                    live: true
                });

                $('.topicExploreItem').live('mouseover', function(){
                    $(this).addClass('topicExploreItemHover');
                }).live('mouseout', function(){
                    $(this).removeClass('topicExploreItemHover');
                });

                $('.drozAnswerCollapse').ajaxify('#drozAnswerWindow', {
                    bind: 'click',
                    cache: false,
                    live: true,
                    preloadType: 'inline',
                    postResponse: function() {
                        if ($('#drozAnswer').hasClass('drozAnswerCollapse')) {
                            $('#drozAnswer').removeClass('drozAnswerCollapse');
                            $('#drozAnswer').addClass('drozAnswerExpand');
                        } else if ($('#drozAnswer').hasClass('drozAnswerExpand')) {
                            $('#drozAnswer').removeClass('drozAnswerExpand');
