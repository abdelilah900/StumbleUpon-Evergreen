                            $('drozAnswerWrapper').removeClass('drozAnswerWrapperHide');
                            $('drozAnswerWrapper').addClass('drozAnswerWrapperShow');
                        }

                        closeOthers('drozAnswer');
                    }
                });

                $('.drozAnswerExpand').live('click', function() {
                    if ($('#drozAnswer').hasClass('drozAnswerCollapse')) {
                        $('#drozAnswer').removeClass('drozAnswerCollapse');
