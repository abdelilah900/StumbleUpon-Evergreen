    jQuery(function($){

            jQuery(".scrollable").scrollable({circular: true, prev: '.prev-p', next: '.next-n'});

            $('.products-grid li.item').hover(function(){
              $(this).find('.wrapper:first').stop(true,false).animate({opacity: 0.8}, 200, "easeInCirc")
              $(this).find('.first-product-image').stop(true,false).animate({opacity: 0}, 220, "easeInCirc")
              //$(this).find('.second-product-image').stop(true,false).animate({opacity: 1}, 400, "easeInCirc") //.css('z-index', 10)

            }, function(){
              $(this).find('.wrapper:first').stop(true,false).animate({opacity: 0}, 300, "easeOutCirc")
              $(this).find('.first-product-image').stop(true,false).animate({opacity: 1}, 330, "easeOutCirc")
