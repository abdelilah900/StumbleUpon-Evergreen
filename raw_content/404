        // Regex out the content_id
        parts = id.match(/\-(\d+)$/);
        var article_id = 'c:' + parts[1];
        $(this).attr('style','display: block').contents().remove();   
        $(this).append('<span class="livefyre-commentcount" data-lf-site-id="309983" data-lf-article-id='+article_id+'>0</span>');  
    });
});
</script>
    
