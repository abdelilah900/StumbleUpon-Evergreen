<script type="text/javascript">

    callFBApi = function() {
       var accessToken;
       var uid;
       CBSi.injectJS('//connect.facebook.net/en_US/all.js#xfbml=1', 'facebook-jssdk');
       FB.init({
           appId : '297742330311988',
           oauth : true
       });
       FB.login(function(response) {
            if (response.authResponse) {
                uid = response.authResponse.userID;
                accessToken = response.authResponse.accessToken;
                CBSi.log("== FB APP DATA ==", response);
                FB.api('/me', function(social) {
                    CBSi.log("== FB USER DATA ==", social);
                    log_in_socially(social.name, social.username, 'fb');
                });
            }
        });
    };

    log_in_socially = function(dn, userid, site) {
        $.ajax({
            url: "/data/common/livefyre/lf_social_login",
            dataType: 'json',
            data: {
                lfdn: dn,
                lfuser: userid,
                ssite: site
            }
        }).done(function(profile) {
            CBSi.log("== Social Login ==",profile,site);
            if (profile.error) {
                CBSi.log("Social login failed with error: ", profile);
                return null;
            }
            else {
                document.cookie = "lf_social_login="+ profile.socialsess +"; expires=0; path=/";
                profile.socialsess = '1';
                userObj = profile;
                LF.login(profile, function() {
                    CBSi.log("Logged in Socially");
                    myListeners.after_social_login();
                });
            }
        });
    };

    callTwitterApi = function() {
        window.open('/common/livefyre/via_twitter','twitterOauth','width=600,height=500,menu=0,status=0');
        return false;
    };

    var myListeners = {

        after_social_login: function() {
            CBSi.log("Executing social DOM changes.");
            $('#livefyre .lf_follow_link').css('display','none');    
        },
        handle_auth_login: function(data) {
            // Whatever called this requires a login. Send through registration loop.
            //open jQuery Dialog
            var optionBox = '#lf_login_options';
            var jQdialog = $(optionBox);
            //check if dialog object is open so we don't create it again
            var isDialogOpen = jQdialog.dialog("isOpen");
            if (isDialogOpen != true) {
                jQdialog.dialog({
                    autoOpen:true, zIndex:5004,
                    title:'How would you like to sign in?', closeOnEscape:true,
                    width:230, height:120, maxWidth:800,
                    modal:true, draggable:true,
                    open: function() {
                        mousePos.X += 45;
                        mousePos.Y = mousePos.Y -90 - $(window).scrollTop();
                        jQdialog.dialog("option","position",[ mousePos.X, mousePos.Y ]);
                        $(optionBox + ' #cbssports').bind('click', function() {
                            if (jQdialog.dialog("isOpen")) {
                                jQdialog.dialog("close");
                            }
                            window.location = 'http://www.cbssports.com/login?master_product=2580&xurl=http%3A%2F%2Fwww.cbssports.com%2Fmcc%2Fauthors%2Fnew-author%3Fxurl%3Dhttp%3A%2F%2Fwww.cbssports.com%2Fcollegefootball%2Fblog%2Feye-on-college-football%2F20922465%2Fwatch-texas-tech-assistant-feels-tommy-tubervilles-wrath-literally%3FprofFR%3D1&lfxurl=http%3A%2F%2Fwww.cbssports.com%2Fmcc%2Fauthors%2Fnew-author%3Fxurl%3Dhttp%253A%252F%252Fwww.cbssports.com%252Fcollegefootball%252Fblog%252Feye-on-college-football%252F20922465%252Fwatch-texas-tech-assistant-feels-tommy-tubervilles-wrath-literally%253FprofFR%253D1';
                        });
                        $(optionBox + ' #facebook').bind('click', function() {
                            if (jQdialog.dialog("isOpen")) {
                                jQdialog.dialog("close");
                            }
                            callFBApi();
                        });
                        $(optionBox + ' #twitter').bind('click', function() {
                            if (jQdialog.dialog("isOpen")) {
                                jQdialog.dialog("close");
                            }
                            callTwitterApi();
                        });
                    }
                });
            }
        },
        handle_user_logged_in: function(data) {
            // this happens when Livefyre believes the login/authorization process has completed successfully
            CBSi.log("Changing DOM for "+ data.display_name);
            changeDOM();
        },
        handle_comment_shown: function(data) {
            var elem = data.comment.find('.lf_comment_user');
            if (elem) {
                data.comment.find('.hastooltip').removeClass('hastooltip');
                $('#livefyre .hastooltip,[title="Quick profile"]').removeAttr('title');
            }
        },
        handle_comments_loaded: function() {
            $('#lf_comments > .lf_comment_container').has('.lf_comment_user').find('.hastooltip').removeClass('hastooltip');
            $('#livefyre .hastooltip,[title="Quick profile"]').removeAttr('title');
        }
    };

    changeDOM = function() {
        // DOM hacks.  Change the UI for the dropdown box
        var userBox = $('.lf_user_box.lf_box');
        if (userObj.socialsess === '1') {
            userBox.remove();
            return;
        }
        // Change link label
        $('.lf_user_links.lf_edit_profile_link',userBox).html('Edit Settings');
        // Nuke logout link
        userBox.find('.lf_logout_link').remove();
        if (userBox.length > 0) {
            userBox.append('<li class="lf_user_links"><a style="color:inherit;text-decoration:none" target="_blank" href="'+userObj.profile.profile_url+'">Edit Profile</a></li>');
        }
        // Change the text from Sign In
        var loggedin = readCookie('pid');
        if (typeof loggedin == "string") {
            if (loggedin.match(/^L:/)) {
                // They need a screen name
                $('.lf_auth_section a.lf_user_loggedout').html('Get a Screen Name to Comment').addClass('loginLink');
            }
            else {
                // No need to change the text
                $('.lf_auth_section a.lf_user_loggedout').addClass('loginLink');
            }
        }
    };
 

    var mousePos = { X:0, Y:0 };
    var userObj = {};

    $(document).ready( function () {

        var conv = LF({
            domain: 'cbssports.fyre.co',
            site_id: '309983',
            article_id: 'c:20922465',
            conv_meta: {
                article_url: 'http://www.cbssports.com/collegefootball/blog/eye-on-college-football/20922465/watch-texas-tech-assistant-feels-tommy-tubervilles-wrath-literally',
                title: 'Watch: Texas Tech assistant feels Tommy Tuberville\'s wrath (literally)',
                sig: 'efed8d426a1992c5420ef25cfeee5d79'
            }
        }).ready(function() {
            // Run this listener once on initial load
            myListeners.handle_comments_loaded();
            // Run the changes to the DOM
            LF.Dispatcher.addListener(myListeners);
            // Log in the user if we got a token for them
            // Change the DOM after login since things get re-rendered
            if (userObj.token) {
                LF.login(userObj, function() {
                    CBSi.log("Livefyre Login success! ", userObj);
               }, function() {
                    CBSi.log("Livefyre LOGIN ERROR"); 
                });
            }
            else {
                changeDOM();
            }
            // Load the comment counts for our bubbles
           
            $('div.commentsBubble').addClass('livefyre-commentcount').attr('data-lf-site-id','309983').attr('data-lf-article-id','c:20922465');
            LF.CommentCount({
                replacer: function(element, count) {
                    if (count > 1){
                        $(element).html('<a href="#commentsAnchor">' + count + ' &nbsp;&nbsp;| &nbsp;Comments</a>');
                        $(element).show();
                    }
                    else{
                        $(element).html('<a href="#commentsAnchor">' + count + ' &nbsp;&nbsp;| &nbsp;Comment</a>');
                        $(element).show();
                    }
                }
            });
            
            // Run for chat window
            if (LF.config.article_id.match(/^chat:/)) {
                CBSi.log("Disabling comment notifyer for chat.");
                LF.CommentNotifier.setEnabled(false);
            }

            // To track mouse position for the popup menu
            $('#livefyre').bind("mousedown",function(e) {
                mousePos.X = e.pageX;
                mousePos.Y = e.pageY;
            });
       });
    });
</script>

</div>
