}

function result1(obj1,val1) {
	if (obj1[val1].value) {
		if (val1=="Mililiter") 
                        obj1.Cups.value=normInput1(eval(obj1.Mililiter.value)/236.8,3);
		else if (val1=="FluidOunce")
                        obj1.Cups.value=normInput1(eval(obj1.FluidOunce.value)*.125,3);
                if (val1!="Mililiter")
			obj1.Mililiter.value=normInput1(eval(obj1.Cups.value)*236.8,3);
                if (val1!="FluidOunce")
                	obj1.FluidOunce.value=normInput1(eval(obj1.Cups.value)/.125,3);
	}
}
