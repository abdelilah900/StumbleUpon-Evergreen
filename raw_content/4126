function getDateString(nodeId){
	var node = document.getElementById(nodeId);
	var date = new Date();
	var strDate = date.getMonth() + 1;
	strDate += "/" + date.getDate();
	strDate += "/" + date.getFullYear();
	node.innerHTML = strDate;
}
