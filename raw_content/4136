			<script type="text/javascript">
var streetstylePage = 2;
var baseURL = '/streetstyle/index/merge:1/type:top/size:large';

function LoadMoreStreetstyles() {
	$.get(baseURL + '/page:' + streetstylePage++, function(data) {
		$('#streetstyle_list').append(data);
		AfterLoadMoreStreetstyles(data);
	});
	return false;
}

function AfterLoadMoreStreetstyles(ar) {
			pageTracker._trackPageview('/internal/streetstyle/scroll');
			$('.just_loaded a[rel*=colorbox]').colorbox({
		rel:'nofollow'
	});
	setupCommentListForms('.just_loaded');
	$('.just_loaded').removeClass('just_loaded');
}

	$(document).ready(function() {
		$('#streetstyle_list').infinitescroll({
			navSelector : '#streetstyle_nav',
			nextSelector : '#streetstyle_load_more',
			itemSelector : '.streetstyle_item',
			loadingImg : '/imgs/ajax-loader.gif',
			loadingText : 'Loading more Streetstyles',
			donetext : 'There are no more Streetstyles to display',
			bufferPx : 500
		}, AfterLoadMoreStreetstyles);
	});
	</script>
