<script type="text/javascript">
$(document).ready(function() {
	// show newsletter signup form
	$('a[rel="signup-newsletter"]').bind('click', function() {
		if ($(this).hasClass('sel')) {
			$('#signupNewsletter').hide();
			$(this).removeClass('sel');
		} else {
			$('#signupNewsletter').show();
			$(this).addClass('sel');
		}
		return false;
	});

	$('#signupNewsletter').submit(function(e) {
		e.preventDefault();
		var emailaddress = $("#sn_email").val();
		if (emailaddress != '') {
			$.post('/newsletter_signup', { 'sn_email' : emailaddress }, function(data) {
				$.renderCakeMessage('general_msg', data.message);
				if (data.success) {
					$('#signupNewsletter').hide();
					$('a[rel="signup-newsletter"]').removeClass('sel');
				}
			}, 'json');
		}
		return false;
	});
});
</script>	</div>
