			$.get('/facebook/login', function(response) {
				if (response == 'true') {
					location.reload();
					return false;
				} else {
					$.fn.colorbox({
						href: '/auth/login',
						innerWidth:'900',
						innerHeight:'420',
						onComplete: function() {
							$('#login').hide();
							$('#step2_registration').show();
							$('#step2_registration').load('/facebook/register');
						}
					});
				}
			});
		}
