<div id="ad-content-banner">
<div id='FF_Fashion_ATF_Top_728x90' style='width: 770px; margin: 0 auto 0 auto'></div>
<script type='text/javascript'>
$('#ad-content-banner').hide();
GlamShowIfAvailable("LeaderBoard");
if ( !GlamIsAvailable("LeaderBoard") )
    if(googletag)
        googletag.cmd.push(function() { googletag.display('FF_Fashion_ATF_Top_728x90'); });

function showBanner()
{
    $banner = $('#ad-content-banner');
    $body = $('body');
    var bgImage = $body.css('background-image');
    var topPadding = $body.css('padding-top');
    if(bgImage.indexOf('url') == -1 || topPadding == 110 || topPadding == '110px')
    {
        $banner.show();
    }
    else
    {
        $banner.hide();
    }
}

$(document).ready(function(){
    if(!$('html').is('.ie'))
    {
        showBanner();
    }
    setTimeout(showBanner, 1000);
});

$(window).load(function(){
    showBanner();
});

</script> </div>
