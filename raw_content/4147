				newThumbUrls[14] = "http://www.craftster.org/forum/index.php?topic=378654.0";
			for(i=0; i<15; i++) {
				preload_image_object = new Image(90,90);
         		preload_image_object.src = newThumbImgs[i];
         	}
			
			</script>
			
