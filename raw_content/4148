	$(document).ready(function () {
		if (window.location.hash.indexOf("gpr")>-1) {
			var c = pcw_readCookie("gpmessaged");
			if (null == c || c.length == 0) {
				$('#gpmsg').jqm({
					ajax: '/misc/gameproMessage.htm',
					closeClass: 'close',
					toTop: true,
					onLoad: function(hash){
						var d = new Date();
						d.setDate(d.getDate()+90);
						pcw_setCookie('gpmessaged', "true", d, '.pcworld.com');
					}
				});
				$('#gpmsg').jqmShow();
			}
		}
	});
	</script>

