        s += "<ul>";
    for(i=0; i < radlinks.length; ++i) {
        s += "<li><a target=\"_top\" href=\"/Special:Radlinks?radPos1=" + radPos1 + "&google_kw=" + radlinks[i].url_escaped_term + "&google_rt=" + radlinks[i].radlink_token + "&google_page_url=" + encodeURIComponent(window.location) + "\">" + radlinks[i].term + "</a></li>\n";
    }
    s += "</ul><br /><div style='float:right;padding-top:.5em;margin-top:-15px;'><a href='javascript:hideads()'>Hide all ads</a> - <a href='/wikiHow:Why-Hide-Ads'>Why?</a></div></div>";
    document.write(s);
document.write('</div><div class="sidebar_bottom_fold"></div></div>');
    return;    
}
// -->
</script>
<script language="JavaScript">
<!--
google_ad_client = "pub-9543332082073187"; 
google_ad_channel = '5047600031+7733764704+1640266093+5860073694+8962074949+2490795108+1258424658+7122150828' + radChan1;
google_ad_output = 'js';
google_max_num_ads = '0';
google_max_radlink_len = '55';
google_rl_mode = 'relevance';


document.write('<script language="JavaScript" src="http://pagead2.googlesyndication.com/pagead/show_ads.js"></' + 'script>');


// -->
</script>
	</div>
</div>

</div>
