                            <fieldset>

                                <script language="javascript" type="text/javascript">
                                    function Search(txtid) {
                                        var txt = document.getElementById(txtid);
                                        if (txt.value != '' && txt.value != 'Search our site') {
                                            document.location.href = '/Catalog.aspx?Search=' + txt.value;
                                        }
                                    }

                                    function stopEvent(e) {
                                        if (!e) e = window.event;
                                        if (e.stopPropagation) {
                                            e.stopPropagation();
                                        } else {
                                            e.cancelBubble = true;
                                        }
                                    }

                                    function cancelEvent(e) {
                                        if (!e) e = window.event;
                                        if (e.preventDefault) {
                                            e.preventDefault();
                                        } else {
                                            e.returnValue = false;
                                        }
                                    }

                                    function EnableGO(event) {
                                        if (event.keyCode) {
                                            if (event.keyCode == 13) {
                                                stopEvent(event);
                                                cancelEvent(event);
                                                Search('product_search');
                                            }
                                        } else {
                                            if (event.which)
                                                if (event.which == 13)
                                                Search('product_search');
                                        }
                                    }

                                </script>

                                <div class="text_bg">
                                    <input type="text" id="product_search" name="product_search" value="Search our site"
                                        onkeydown="EnableGO(event)" onblur="if(this.value=='') this.value='Search our site';"
                                        onfocus="if(this.value=='Search our site') this.value='';" class="text" />
                                </div>
                                <input type="button" class="button" onclick="Search('product_search');" />
                            </fieldset>
                            
