<script type="text/javascript">//<![CDATA[

//Below functions are helper ones for maintaining adspaces

//expands special offer content
jQuery.expand = function(adpos){

$('#ad' + adpos).css("display","block");

};

jQuery.expandAndSetHeight = function(adpos,height){
    $('#ad' + adpos).css("display","block");
    $('#iframe_' + adpos).css("height",height+"px");
};

//Takes a iframe id & collapses it for when there is no advert to show
jQuery.collapse = function(theIframe){

$('#iframe_' + theIframe).css("display","none");

};

//If there is an ad to serve then we want to show the 'ad feedback' link
//Clicks through to a feedback form on that particular advert
jQuery.showWrapper = function(theWrapper){
$('#feedback_' + theWrapper).css("display","inline");
}

// Cache-busting and pageid values for the ad
random = Math.round(Math.random() * 10000000000);
if (!pageNum) var pageNum = Math.round(Math.random() * 10000000000);

var ad_area_code = "s.stuff.national.health";
var ad_domain = "www.stuff.co.nz";

//Doubleclick

var dcdAdsCounter = 0;
document.dcdAdsH = new Array();
document.dcdAdsI = new Array();
document.dcdAdsU = new Array();
var dartOrd = Math.floor(100000000*Math.random());
var dartSite = "onl.stuff.national";
var dartZone = "/health";
var dartKeys = "kw=ADHD;kw=HEALTH";

function loadHserverAd(strDimension,strPos,width,height){
   //Check if there isn't already an ad piece of code in there - search for instance of iframe_strPos
   if( ($('#ad' + strPos + ':has(#iframe_' + strPos + ')').length < 1 )){
      adserver = "http://inl.adbureau.net";
      var target = "/site=s/area=" + ad_area_code + "/aamsz=" + strDimension + "/POS=" + strPos  + "/SOURCEDOMAIN=" + ad_domain;

        target = target + "/KEYWORD=" + escape("ADHD+HEALTH");

      var adpageNum = pageNum;

      var random = Math.round(Math.random() * 10000000000);
      $('#ad' + strPos).append('<IFRAME ID ="iframe_' + strPos + '" SRC="'+adserver+ '/hserver/acc_random='+random + target + '/pageid=' + adpageNum + '/?http://www.stuff.co.nz/national/health/3248111/Results-of-drug-trial-for-ADHD-sufferers-prove-astonishing' + '" NORESIZE SCROLLING=NO HSPACE=0 VSPACE=0 FRAMEBORDER=0 MARGINHEIGHT=0 MARGINWIDTH=0 WIDTH=' + width + ' HEIGHT=' + height + ' allowTransparency="true"><SCRIPT SRC="' + adserver + '/jnserver/acc_random=' + random + target + '/pageid=' + adpageNum + '"></SCR' + 'IPT></IFRAME>');
   }
}

function renderBserverAd(strPos, aambTag) {
    var divName = "ad" + strPos;
    var invDiv = 'INV' + divName;
    var content = eval(aambTag);
    content.replace(/<\/?(?:html|body|head)\/?>/g, '');
    var holder = $('<div id="' + invDiv + '"></div>');
    holder.html(content);
    $('#' + divName).append(holder);
}

function relocateAd(iframeObj, divName) { 
    var iframeDoc, 
    allScripts; 
    iframeDoc = iframeObj.contentWindow.document;

    allScripts = iframeDoc.getElementsByTagName('script'); 

    for (s = 0; s < allScripts.length; s++) 
    if (allScripts[s].src) 
    allScripts[s].src = ''; 
    document.getElementById(divName).insertAdjacentElement('beforeEnd', iframeDoc.getElementById('adDiv')); 
}

function rank_sort (a, b) {
    var ra = a.rank;
    var rb = b.rank;

    if ( rb == null ) return -1;
    if ( ra == null ) return  1;

    return ra - rb;
}

function PushDartJSAd(id, rank, width, height, aamsz) {
    dcdAdsCounter++;
    document.dcdAdsH.push("ad" + id);
    document.dcdAdsI.push("dcAd-" + dcdAdsCounter);
    document.dcdAdsU.push('http://ad-apac.doubleclick.net/adj/'+dartSite+dartZone+';pos='+id+';pid=3248111;'+dartKeys+';sz='+aamsz+';tile='+dcdAdsCounter+';ord='+dartOrd+'?');
}

var hserver_ads = [];
function PushHserverAd(id, rank, width, height, aamsz) {
    hserver_ads.push( { id: id,
                        rank: rank,
                        width: width,
                        height: height,
                        aamsz: aamsz } );
}
function RenderHserverAds() {
    hserver_ads.sort(rank_sort);
    for (var i = 0; i < hserver_ads.length; i++) {
        var ad = hserver_ads[i];
        loadHserverAd( ad.aamsz, ad.id, ad.width, ad.height );
    }
}

var bserver_ads = [];
function PushBserverAd(id, rank, width, height, aamsz) {
    bserver_ads.push( { id: id,
                        rank: rank,
                        width: width,
                        height: height,
                        aamsz: aamsz } );
}

function SetupBserverAds(){
    if (bserver_ads.length == 0) return;

    var aamRnd = Math.round(Math.random() * 100000000000);

    // Adserver URL
    adserver = "http://inl.adbureau.net/bservers";

    // Ad tag targeting values which will be appended to each ad request section in the bserver ad call

    var allAdTags = "/AAMALL/acc_random=" + aamRnd + "/pageid=" + pageNum + "/site=s/area=" + ad_area_code + "/SOURCEDOMAIN=" + ad_domain;

      allAdTags = allAdTags + "/KEYWORD=" + escape("ADHD+HEALTH");

    //loop through adArr with all the ad call strings provided
    bserver_ads.sort(rank_sort);

    var adStr = '';
    var tag_id = 0;
    for (var i=0; i<bserver_ads.length; i++) {
        var ad = bserver_ads[i];
        ad.tag = "AAMB" + tag_id++;
        eval(ad.tag + ' = 0;');
        var code = "/" + ad.tag + "/aamsz=" + ad.aamsz + "/POS=" + ad.id;
        adStr = adStr + code;
    }

    //document.write('<SCR' + 'IPT SRC="' + adserver + allAdTags  + adStr + '?" type="text/JavaScript" language="JavaScript">');
    //document.write('</SCR' + 'IPT>');
    $('body').append('<SCR' + 'IPT SRC="' + adserver + allAdTags + adStr + '?" type="text/JavaScript" language="JavaScript"></SCR' + 'IPT>');

}
function RenderBserverAds() {
    if (bserver_ads.length == 0) return;

    if (eval(bserver_ads[0].tag)) {
        for (var i = 0; i < bserver_ads.length; i++) {
            var ad = bserver_ads[i];
            renderBserverAd(ad.id, ad.tag);
        }
        bserver_ads = [];
    } else {
        setTimeout(RenderBserverAds, 1000);
    }
}
