    $(document).ready(function() {
        $('#foliogrid .post').click(function(){
            href = $(this).children('div').children("a[rel='bookmark']").attr('href');
            if (href != undefined)
            {
                window.location = href; 
            }
        }).hover(function(){
            $(this).css('cursor', 'pointer');
        }, function(){
            $(this).css('cursor', 'default');        
