	<ul class="contribution-list indented-mode" id="contribution-list">
	
	</ul>

	
</section>
  <script type="text/javascript" src="http://s3-ak.buzzfed.com/static/js/public/contribute/contribute.js?v=1369929445"></script>
  <script type="text/javascript">
  if (typeof bf_contribute !== "undefined" && !bf_contribute.facebook_toggled) {
       bf_contribute.enableForm('contribute-text-form');
    bf_contribute.buttonToggle('text-contribute-activate');
     }
  </script>
  
  </article>
</div>


