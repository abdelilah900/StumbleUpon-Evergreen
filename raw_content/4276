<script type="text/javascript">
    //build slideshow
	$('#gallery_items_14640493').cycle({
		fx: 'scrollHorz', /*fade*/
		speed: 800,
        next: '#gallery_next_14640493',
        prev: '#gallery_prev_14640493',
		timeout: 8000,
        pager: '#gallery_bullets_14640493',
        pagerAnchorBuilder: function paginate(idx, el){
            return '<a class="item" id="item' + idx + '" href="#"  name="lpos=widget[C_5_gallery_4380645_homepage]&lid=[dot]">&bull;</a>';
        }
        //before: onBefore
	});

    function onBefore(curr,next,opts, fwd){
        //set the slide height
        var $ht = $(this).height();
        $(this).parent().animate({"height": $ht});
    }

</script>
