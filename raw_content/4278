	<div id="adHeaderCirc" class="ad adHeadercirc"></div>

	<script type="text/javascript">
//<![CDATA[
	var topAdAttempts = 0;

	function topAds() {

		// Has DoubleClick sent a blank ad (as signified by a grey.gif tracking pixel)
		var blankAdLoaded = false;
		if ($("#topAds .adTopleader").html().search("grey.gif") != -1) {
			blankAdLoaded = true;
		} 
		
		// webkit seems to be having issues with visiblity detection
		var visibilityCheck;
		if (jQuery.browser.webkit) {
			visibilityCheck = function(index) {
				return $(this).css('visibility') == 'visible';
			}
		} else {
			visibilityCheck = ':visible';
		}
		
		var adWidth = maxAdWidth = Math.max(
			$("#topAds .adTopleader").find(">img").filter(visibilityCheck).width(),
			$("#topAds .adTopleader").find(">embed").filter(visibilityCheck).width(),
			$("#topAds .adTopleader").find(">div").filter(visibilityCheck).width(),
			$("#topAds .adTopleader").find(">a").filter(visibilityCheck).width(),
			$("#topAds .adTopleader").find(">object").filter(visibilityCheck).width(),
			$("#topAds .adTopleader").find(">table").filter(visibilityCheck).width(),
			$("#topAds .adTopleader").find(">iframe").filter(visibilityCheck).width(),
			$("#topAds .adTopleader").find(">span").filter(visibilityCheck).width(),
			$("#topAds .adTopleader").find(">center").filter(visibilityCheck).width()
		);
						
		// If we don't have a blank ad from DoubleClick, or the ad width is such
		// that we suspect it hasn't finished loading, try again in a bit...
		if (blankAdLoaded == false && adWidth <= 500) {
			if (topAdAttempts < 10) { // Try up to 10 times
				topAdAttempts++;
				setTimeout('topAds()', 50);
				return false;
			} else {
				// We've waited a while, doesn't look like an ad is coming
				blankAdLoaded = true;
			}
		}
				
		// If we have a blank ad or a wide push down ad, then show the wide circ
		if ( blankAdLoaded == true || adWidth > 732 )
		{			
				var circAd = '';
				circAd += '<div id="adHeaderWideCirc">';
				//circAd +=	'<a id="circCover" href="https://secure.palmcoastd.com/pcd/eSv?iMagId=23301&i4Ky=IA2S" title="Subscribe to The Atlantic">';
				circAd +=	'<a id="circCover" href="https://secure.palmcoastd.com/pcd/eSv?iMagId=23301&i4Ky=I7AD" title="Subscribe to The Atlantic">';
				circAd +=		'<img width="66" src="http://cdn.theatlantic.com/static/easel/images/onn/011315_big-cover.jpg" alt="Subscribe to The Atlantic" />';
				circAd +=	'</a>';
				circAd +=	'<ul>';
				circAd +=		'<li><a href="https://secure.palmcoastd.com/pcd/eSv?iMagId=23301&i4Ky=IA1S">Subscribe</a></li>';
				circAd +=		'<li>&bull;<a href="https://secure.palmcoastd.com/pcd/eServ?iServ=MjMzMDE1ODgzNQ==">Renew</a></li>';
				circAd +=		'<li>&bull;<a href="https://secure.palmcoastd.com/pcd/eSv?iMagId=23301&i4Ky=IFGD">Give a Gift</a></li>';
				circAd +=		'<li>&bull;<a href="http://itunes.apple.com/us/app/the-atlantic-magazine-digital/id397599894?mt=8">Digital Edition</a></li>';
				circAd +=	'</ul>';
				circAd += '</div>';		
						
				$("#adHeaderCirc").html(circAd);

			if (blankAdLoaded == false) 
			{
				$("#topAds .adTopleader").addClass('adPush');
				$("#topAds .adTopleader").removeClass('ad');
			} else {
				$("#topAds .adTopleader").height(0);
			}

		// ELSE the topleader has an ad in it, but it is less than 730px
		// So show the small circ ad that goes with it
		} else {
			$("#topAds .adTopleader").attr('style', 'width:730px');
			
			var circAd = '';
			circAd += '<div id="adHeaderSmallCirc">';
			//circAd +=	'<a id="circCover" href="https://secure.palmcoastd.com/pcd/eSv?iMagId=23301&i4Ky=IA2S" title="Subscribe to The Atlantic">';
			circAd += 	'<a id="circCover" href="https://secure.palmcoastd.com/pcd/eSv?iMagId=23301&i4Ky=I7BT">';
			
			circAd +=		'<img width="58" height="78" src="http://cdn.theatlantic.com/static/easel/images/onn/011315_big-cover.jpg" alt="Subscribe to The Atlantic" />';
			circAd +=	'</a>';
			circAd +=	'<ul>';
			circAd +=		'<li>&bull;<a href="https://secure.palmcoastd.com/pcd/eSv?iMagId=23301&i4Ky=IA1S">Subscribe</a></li>';
			circAd +=		'<li>&bull;<a href="https://secure.palmcoastd.com/pcd/eServ?iServ=MjMzMDE1ODgzNQ==">Renew</a></li>';
			circAd +=		'<li>&bull;<a href="https://secure.palmcoastd.com/pcd/eSv?iMagId=23301&i4Ky=IFGD">Give a Gift</a></li>';
			circAd +=		'<li>&bull;<a href="http://itunes.apple.com/us/app/the-atlantic-magazine-digital/id397599894?mt=8">Digital Edition</a></li>';
			circAd +=	'</ul>';
			circAd += '</div>';
			
			$("#adHeaderCirc").html(circAd);
			$("#adHeaderCirc").attr('style', 'float:right');
		}

		return true;
	}
	topAds();
//]]>	
	</script>
