<script type="text/javascript">
//<![CDATA[
var bcExp;
var modVP;
var modExp;
var modCon;
var modAd;


var mediaComplete = function (evt) {   
	var nextVideoLink = $('#selectedRelatedVideo').next().find('a')[0];
	$(nextVideoLink).trigger('click')
}


// called when template loads, this function stores a reference to the player and modules.
function onTemplateLoaded(experienceID) {
	bcExp = brightcove.getExperience(experienceID);
 
	modVP = bcExp.getModule(APIModules.VIDEO_PLAYER);
	modExp = bcExp.getModule(APIModules.EXPERIENCE);
	modCon = bcExp.getModule(APIModules.CONTENT);
	modAd  = bcExp.getModule(APIModules.ADVERTISING);
	
	modVP.addEventListener("mediaComplete", mediaComplete);
}


function loadVideo(event, self, videoId, videoTitle, related) {
	var pushState = true;
	var returnValue = false;
		
	// If we need to scroll to see the video, do so.
	var windowLocation = $(window).scrollTop();
	var videoLocation = $("#videoTitle").offset().top;

	if (windowLocation > videoLocation) {
		var selector = (document.getElementsByTagName('HTML')[0].scrollTop > 0) ? 'html' : 'body';
		$(selector).animate({scrollTop: videoLocation},'slow');
	}
		
	if (window.history == undefined || window.history.pushState == undefined) {
		pushState = false;
	}
	
	if (related == true) {
		$('#selectedRelatedVideo').removeAttr('id');
		$(self).parent().attr('id', 'selectedRelatedVideo');
	}

	$.ajax({
		type: "GET",
		url: "/video/ajax/?callback=?",
		dataType: "jsonp",
		data: {id:videoId},
		async: true,
		success: function(data) {
			$('.videoFeature').replaceWith(data.output[0]);
			if (related != true) {
				$('#relatedVideos').replaceWith(data.output[1]);
			}
			var stateObj;
			var title = videoTitle + ' - Video - The Atlantic';

			if (pushState) {
				window.history.pushState(stateObj, title, '/video/index/' + videoId + '/');
				document.title = title;
			} else {
				window.title = title;
				window.location.hash = '#!/video/index/' + videoId + '/';
			}

			var script = 'http://s7.addthis.com/js/250/addthis_widget.js#domready=1';
			if (window.addthis){
				window.addthis = null;
			}
			$.getScript( script );
			
			// Track Quantcast
			if (typeof(_qevents) != "undefined") _qevents.push({ qacct:"p-b0K-eQJGBXxXE"});
			
			// Track ComScore
			if (typeof(_comscore) != "undefined") _comscore.push({ c1: "2", c2: "6463921" });
		}
	});		

	if (typeof event == 'object' && event.returnValue) {
		event.returnValue = returnValue;
	}
	return returnValue;
}
//]]>
</script>
	<!-- Quantcast Tag, part 1 --> 
