<script type="text/javascript">

	function reloadBanners(url) {
		
		//_gaq.push(['_trackPageview', url]); //google analytics
	  	//_qevents.push( {qacct:"p-a0gOdUACucKCE", event:"refresh" }); //quantcast
	  	
	  	/*var iframe = document.createElement('iframe'); 
		iframe.style.width = '0px';						 
		iframe.style.height = '0px'; 						 
		document.body.appendChild(iframe); 
	
		iframe.src = 'http://thehoopdoctors/wp-content/themes/newsports/tracking.php?rand=' + Math.random();*/
		
		var count = 0;
		var ord = Math.random()*10000000000000000;
		var adlink = cmnunt_site + '/' + cmnunt_zone +'_norm;kw='+ cmnunt_silo + ',' + cmnunt_tier;
		
		// 728x90
		var my_ad_iframe_728 = createIFrame("http://ad.doubleclick.net/adi" + adlink + ";sz=728x90;tile=" + count + ";ord=" + ord +"", 728, 90, 'my_ad_iframe_id_728');
		jQuery("#ad728").html(my_ad_iframe_728);		
		document.getElementById('my_ad_iframe_id_728').src = "http://ad.doubleclick.net/adi/" + adlink + ";sz=728x90;tile=" + count + ";ord=" + ord +"";
		
		count++;
		
		// 300x250
		var my_ad_iframe_300 = createIFrame("http://ad.doubleclick.net/adi" + adlink + ";sz=300x250;tile=" + count + ";ord=" + ord +"", 300, 250, 'my_ad_iframe_id_300');
		jQuery("#ad300").html(my_ad_iframe_300);		
		document.getElementById('my_ad_iframe_id_300').src = "http://ad.doubleclick.net/adi/" + adlink + ";sz=300x250;tile=" + count + ";ord=" + ord +"";
			
		count++;
		
		// 160x600
		var my_ad_iframe_160 = createIFrame("http://ad.doubleclick.net/adi" + adlink + ";sz=160x600;tile=" + count + ";ord=" + ord +"", 160, 600, 'my_ad_iframe_id_160');
		jQuery("#ad160").html(my_ad_iframe_160);		
		document.getElementById('my_ad_iframe_id_160').src = "http://ad.doubleclick.net/adi/" + adlink + ";sz=160x600;tile=" + count + ";ord=" + ord +"";
		
		count++;
	
	}   
	
jQuery.noConflict();
jQuery(document).ready(function($) {

	if (window.location.hash) {
		var current = window.location.hash;
		current = parseInt(current.substring(1))-1;
	}
	else
	{
		var current = 0;
	}

	
	var first = 0;
   jQuery('#slide_list').cycle({
    fx:     'scrollLeft',
    prev:    'a[id=next]', 
    next:    'a[id=prev]',
    speed:    500,
    startingSlide: 0,
    timeout: 	0,
    after:     onAfter,
    startingSlide: current,
    before:	   onBefore
   });
   
   function onAfter(curr,next,opts) {
		var caption = '' + (opts.currSlide + 1) + ' of ' + opts.slideCount;
		var title = $('#slidetitle_' + (opts.currSlide + 1)).attr('alt');
		$('div[id=slidecaption]').html(caption);
		$('#slidetitle').html(title);	
		
		var index = opts.currSlide;
		//get the height of the current slide
		var $ht = $(this).height();
		//set the container's height to that of the current slide
		$(this).parent().css("height", $ht);
			
	}
	
	function onBefore(curr,next,opts) {
		
		if (first == 0)
		{
			first = 1;		
		}
		else
		{
			//alert('CUR: ' + opts.currSlide + ' --- NEXT: ' + opts.nextSlide + ' --- TOTAL: ' + opts.slideCount);
			if (opts.nextSlide == 0 && opts.currSlide+1 == opts.slideCount)
			{
				window.location = $('#prev').attr('href');
				return false;
			}
			
			if (opts.currSlide == 0 && opts.nextSlide == opts.slideCount-1)
			{
				window.location = $('#next').attr('href');
				return false;
			}
			
			var slide = parseInt(opts.nextSlide);
			window.location = '#' + (slide + 1);
			
			reloadBanners(window.location + '/#' + opts.currSlide+1);
		}		
	}
});
</script>
