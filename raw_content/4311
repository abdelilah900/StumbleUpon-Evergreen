<script type="text/javascript">
       var s_account = "bizjournalsportfolio";

       var build_site_sections = function() {
           var i=3,sections=["Interactive Features","Foul Ballpark",];  
           s.channel = "Interactive Features";

           for ( var x in sections ) {
               // Don't overwrite prop7 or higher
               if (i < 7 && x.length == 1) { eval("s.prop" + i + " = \"" + sections[x] + "\";"); }
               i++;
           }

           s.hier1 = sections.join(",");
