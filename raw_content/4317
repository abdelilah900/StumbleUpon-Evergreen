  $(function(){
    if($.browser.msie) {
			$('input').keydown(function(e){
	      if (e.keyCode == 13) {
					// don't error out if there's another input on the screen that doesn't have an onSubmit fn defined
          if ($(this).parents('form').attr('onSubmit') != undefined) {
            $(this).parents('form').find('input:image,input:submit').click();
            return false;
					}
	      }
	    });
		}
  });
</script>

