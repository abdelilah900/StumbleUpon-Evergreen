<script type="text/javascript">

function validEmail(){
	var mailId = "";

	if(document.getElementById("newsletter_email")!=null){
		mailId = document.getElementById("newsletter_email").value;
		var status = false;
		if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(mailId)){
			status = true;
		}else{
			alert("Invalid Email Id");
			document.getElementById("newsletter_email").value="";
			status = false;
		}
	}else{
		status = true;
	}
	return status;
}

function subForm(){
	document.newsletter_subscribe.submit();
}
</script>

