							type: 'jsonp',
							url: KIDS.StageVars.momUrl + '/wp-content/themes/nickmom-blog-responsive/inc/partner-rss-feeds.php?rss-feed-url=' + rssFeedUrl + '&rss-image-url=' + rssImageUrl + '&rss-image-link=' + rssImageLink,
