  <div class="subtitle-ad-container">
    <!-- begin ad tag (tile=4) -->
      <iframe src="http://ad.doubleclick.net/adi/cdg.NowPublic.Culture;kw=;ptype=;tile=4;sz=468x60;ord=1302074307?" width="468" height="60" marginwidth="0" marginheight="0" hspace="0" vspace="0" frameborder="0" scrolling=no borderCOLOR="#000000">
      <script language="JavaScript" src="http://ad.doubleclick.net/adj/cdg.NowPublic.Culture;kw=;ptype=;tile=4;sz=468x60;abr=!ie;ord=1302074307?" type="text/javascript"></script>
      <noscript><a href="http://ad.doubleclick.net/jump/cdg.NowPublic.Culture;kw=;ptype=;tile=4;sz=468x60;abr=!ie4;abr=!ie5;abr=!ie6;ord=1302074307?" target="_blank"><img src="http://ad.doubleclick.net/ad/cdg.NowPublic.Culture;kw=;ptype=;tile=4;sz=468x60;abr=!ie4;abr=!ie5;abr=!ie6;ord=1302074307?" width="468" height="60" border="0" alt=""></a></noscript>
