<script type="text/javascript">
(function($) {
  $('body').addClass('js-enabled');
  $('.creative .enhance').hover(function () {
    $(this).addClass('rollover');}, function () {
      $(this).removeClass('rollover');
    });
  $('.creative .fade').FadeOpacity();
  $('.creative .enhance-welcome').EnhanceClick();
  $('.enhance-welcome').hover( function () {
    $(this).find('.rollover').css("color","#fb0240");   }, function () {
      $(this).find('.rollover').css("color","#000000");
    });
  $('.creative .enhance-blank').EnhanceClick({blank:true}).FadeOpacity();

})(jQuery);
