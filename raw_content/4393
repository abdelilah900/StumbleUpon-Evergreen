      <script type="text/javascript">
//<![CDATA[

function getDateStr()
    {
        var today = new Date();
        var year = today.getYear();
        if(year<1000) year+=1900;
        var todayStr = toFullMonth(today.getMonth()) + " " + today.getDate() + ", " + year;
        return todayStr;
    }

//]]>
</script>

