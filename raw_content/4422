			var IE6 = false /*@cc_on || @_jscript_version < 5.7 @*/;
window.onload = function() {
	if  (IE6) {
		$.each($("#surphace-inline img[src$=.png],#surphace-inline img[src$=.PNG]"), function () {
			var img = $(this);
			img.css({"width": img.width(),"height": img.height(), "filter": "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='" + img.attr("src") + "', sizingMethod='scale')"});
			img.attr("src","http://cdn11.surphace.com/images/blank.gif");
		});
	}
}
		</script>
