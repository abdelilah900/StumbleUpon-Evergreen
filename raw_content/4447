<script type="text/javascript">
            jQuery(function(jQuery){
            
            //check window width onload
			if (jQuery(window).width()>1054){
 				jQuery(".largeHorizontal_share_buttons").hide();
				jQuery("#large_share_wrapper").show();
 			} else {
  				jQuery("#large_share_wrapper").hide();
				jQuery(".largeHorizontal_share_buttons").show();
	 		}
	 
	 		//resize function 
			jQuery(window).resize(function () {
		
				if (jQuery(window).width()>1054){
			
				jQuery("#large_share_wrapper").slideDown("fast");
				jQuery(".largeHorizontal_share_buttons").slideUp("fast"); 
	
				} else {
	
				jQuery(".largeHorizontal_share_buttons").slideDown("fast");
				jQuery("#large_share_wrapper").slideUp("fast");
				}
			});
			//resize function ends here
            
                var shareWrap = jQuery("#large_share_wrapper");
                var originalShareTop = shareWrap.offset().top;
                var view = jQuery(window);
                view.bind("scroll resize", function(){
                    var viewTop = view.scrollTop();
                    if ((viewTop > originalShareTop) &&
                    !shareWrap.is(".share_fixed")) {
                    
                        shareWrap.removeClass("share_absolute").addClass("share_fixed");
                        
                    }
                    else 
                        if ((viewTop <= originalShareTop) &&
                        shareWrap.is(".share_fixed")) {
                        
                            shareWrap.removeClass("share_fixed").addClass("share_absolute");
                        }
                });
                
            });
        </script>
		
