<script type="text/javascript">
	function runGA(pageTracker, isTest, chip) {
		try {
			var firstVisit = false;
			pageTracker._setDomainName("mint.com");
			if (isTest) {
				var re = new RegExp(/(?:^|;\s)__utmz=([^;]*)/);
				var utmz = re.exec(document.cookie);
				if(utmz && document.cookie.match("(^|;\\s)__roiz=((?=[^;]*utmcsr=\\(direct\\))|(?=[^;]*utmcsr=[^|]*mint.com)(?=[^;]*utmcmd=referral))") && !utmz[1].match("(?=.*utmcsr=\\(direct\\))|(?=.*utmcsr=[^|]*mint.com)(?=.*utmcmd=referral)")) {
					var exdate=new Date();
					exdate.setTime(exdate.getTime() + 15768000000);
					document.cookie = "__roiz=" + utmz[1] + ";expires=" + exdate.toGMTString() + ";path=/;domain=.mint.com";
				}
			}
			else if (document.cookie.match(/utmcmd=organic/) && !document.cookie.match(/utmctr=[^\|;]*mint/)) {
				var ref = document.referrer;
				re = /(\?|&)(q|p|query|encquery|qt|terms|rdata|qs|wd|text|szukaj|k|searchExpr|search_for|string|search_query|searchfor)=([^&]+)/;
				var searchq = re.exec(ref);
				if(searchq) {
					if(searchq[3].match(/mint/)) {
						pageTracker._addIgnoredOrganic(searchq[3]);
					}
				}
			}
			if(!document.cookie.match("(^|;\\s)__" + chip + "a=")) {
				firstVisit = true;
			}
			else if (isTest) {
				pageTracker._setReferrerOverride("");
				pageTracker._setCampNameKey("zzz");
				pageTracker._setCampMediumKey("zzz");
				pageTracker._setCampSourceKey("zzz");
				pageTracker._setCampTermKey("zzz");
				pageTracker._setCampContentKey("zzz");
				pageTracker._setCampCIdKey("zzz");
			}
      try {
            var findUtm = function (a) {
                        return RegExp("(?:^|;\\s)__" + chip + a + "=36839484");
                  },
                  sourceRegex = /(?:^|;\s)source=([^;]*)/,
                  sourceParamRegex = /[?&]source=/,
                  utmb, utmc, source;

            utmb = findUtm("b").test(document.cookie);
            utmc = findUtm("c").test(document.cookie);
            source = sourceRegex.exec(document.cookie);

           if (source && source[1] && !(utmb && utmc) && !(sourceParamRegex.test(location.search))) {
                  pageTracker._trackPageview(location.pathname + (location.search ? location.search + "&" : "?") + "source=" + source[1]);
            } else {
                  pageTracker._trackPageview();
            }
      } catch(e) {
            pageTracker._trackPageview();
      }

      
			if(document.cookie.match(/(^|;\s)mu=/)){
				var ruRegex = new RegExp("(?:^|;\\s)__" + chip + "v=[0-9]+\\.([^;]+)");
				var ruVal = ruRegex.exec(document.cookie);
				if(ruVal) {
					if(ruVal[1].substring(0,2)!="RU") { 
						pageTracker._setVar("RU_"+unescape(ruVal[1])); 
					}
				} else { 
					pageTracker._setVar("RU"); 
				}
			} else if(!document.cookie.match("(^|;\\s)__" + chip + "v=")) { 
				   if (firstVisit) {
              pageTracker._setVar(location.hostname + location.pathname);
              } else {
        pageTracker._setVar("un_" +
        location.hostname + location.pathname);
       }
			}
		} catch(err) {}
	}

	try {
		var pageTracker = _gat._getTracker('UA-1184728-2'); 
		var testTracker = _roit._getTracker('UA-1184728-11');
		runGA(pageTracker, false, 'utm');
		runGA(testTracker, true, 'roi');
	} catch(err) {}
</script>

