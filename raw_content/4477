<div class="widget search simpleForm widget-editable viziwyg-section-1023 inpage-widget-5054704 superSearchBox"
      >

  <script type="text/javascript" src="http://www.independent.co.uk/resources/js/searchFormHandler.js"></script>

  <div class="simpleSearch" id="simpleSearch-5054704">
    

    <form action="http://www.independent.co.uk/search/simple.do" method="get"
               onsubmit="return validateSimpleSearchForm(this, 'emptySearchExpressionError-5054704');">
      <fieldset>
        <input type="hidden" name="destinationSectionUniqueName" value="search" />
        <input type="hidden" name="publicationName" value="ind" />
        <input type="hidden" name="sortString" value="publishdate" />
        <input type="hidden" name="sortOrder" value="desc" />
        <input type="hidden" name="sectionId" value="506" />
        <input type="hidden" name="articleTypes" value="news" />
        <input type="hidden" name="pageNumber" value="1" />
        <input type="hidden" name="pageLength" value="10" />
        <input type="hidden" name="startDay" value="1" />
        <input type="hidden" name="startMonth" value="1" />
        <input type="hidden" name="startYear" value="2010" />
        <input type="hidden" name="useSectionFilter" value="true" />
        <input type="hidden" name="useHideArticle" value="true" />

        <p class="error" id="emptySearchExpressionError-5054704" style="display:none;">
          Please provide search keyword(s)
        </p>

        <input type="text" name="searchString" value="" class="field" />

        

        

        <input type="submit" value="Go" class="button" title="Click here to search" />
      </fieldset>
    </form>
  </div>

  
    

    
      
      
        
          
        
        
      
    

    

    <script type="text/javascript">
      $(function() {
        $("#simpleSearch-5054704 .field").autocomplete({
          source: ajaxCall ,
          delay: 2000.00,
          close: function(event, ui) {
            $("#simpleSearch-5054704 .field").autocomplete("option", "delay", 500.00);
          },
          select: function(event, ui) {
            var articleId = ui.item.id;
            openLink('http://www.independent.co.uk/eceRedirect?articleId=' + articleId, '_self');
          }
        });
      });

      function ajaxCall(request, response) {
        var iChars = "!@#$%^&*()+=-[]\\\';,./{}|\":<>?~_";
        var term = $.trim(request.term.toLowerCase());
        var wordCount = term.split(" ").length;
        if (term.length > 0 && iChars.indexOf(term.charAt(0)) == -1) {
          var param = "(" + term + "*) publication:(ind) state:(published) contenttype:(news)";
          $.ajax({
            url: "/ind/ewf-servlet/search/",
            data: {q:param , rows:5, wt:'json'},
            cache: true,
            dataType: 'json',
            success: function(json) {
              var responseCount = json.response.docs.length;
              if (json != null && responseCount > 0) {
                response($.map(json.response.docs, function(article) {
                  return {label: article.title, value: article.title, id: article.objectid}
                }));
              }
              else{
                 $("#simpleSearch-5054704 .field").autocomplete("close");
              }
              if (wordCount > 1 && responseCount == 0) {
                param = "(" + term + ") publication:(ind) state:(published) contenttype:(news)";
                $.ajax({
                  url: "/ind/ewf-servlet/search/",
                  data: {q:param , rows:5, wt:'json'},
                  cache: true,
                  dataType: 'json',
                  success: function(json) {
                    var responseCount = json.response.docs.length;
                    if (json != null && responseCount > 0) {
                      response($.map(json.response.docs, function(article) {
                        return {label: article.title, value: article.title, id: article.objectid}
                      }));
                    }
                  }
                })
              }
            }
          })
        }
        else if (term.length > 0 && iChars.indexOf(term.charAt(0)) != -1) {
          $("#simpleSearch-5054704 .field").autocomplete("close");
          alert("Search word should not start with !@#$%^&*()+=-[]\\\';,./{}|\":<>?~_");
        }
      }

    </script>
  
</div>




  

















        
      
    


  
  
  














  

      
    
  
  


        

        
      
    
      
        

        

        

        

        

        
        

        
        

        

        
          
          











  
    
    
      
      
        

















  

  
  

  

  
  
  
  

  

  
    












    
      
      
        
        
          
            
          














  
    
      
      
      
      











































  
    
  
  





  
    
  

  
  

  

















  
  

  
  
  

  
  
  
    
    
    
      
      
      
      

      
        
         
          
        
      

      
      
    
  
    
    
    
      
      
      
      

      
        
         
          
        
      

      
      
    
  
    
    
    
      
      
      
      

      
        
         
          
        
      

      
      
    
  
  
    
    
    
    
    
  

  
    
    
      
      
    
  

  
  
  

  





    

    

    

    

    

    

    

    

    

    

    

    

    

    
















 







  
  
    








