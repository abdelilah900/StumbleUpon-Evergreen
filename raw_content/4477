<script type="text/javascript">
//<![CDATA[
	$(function() {
		var widgetClassList = "widget trailers row widget-editable viziwyg-section-1048 inpage-widget-6138660";
		var widgetClass = widgetClassList.replace(/\s/g, ".");
		var widgetWrapper = $('.'+widgetClass);
		widgetWrapper.find('.article').each(function() {
			var articleWidth = $(this).width();
			addCaptionOverlay(this, articleWidth);
		});
		
		function addCaptionOverlay(article, width) {
			var img = $(article).find('img');
			var imgUrl = "http://www.independent.co.uk/arts-entertainment/music/middle-top-promo-6256477.html";
			if ("A lost Nick Drake recording..." !== "" || "and the lucky 167 people who got to hear it" !== "") {
				$(article).append('<div class="captionOverlay"></div>')
				$(article).append('<div class="captionContent"><a class="oTitle" href="'+ imgUrl +'"></a><a class="oBody" href="'+ imgUrl +'"></a></div>');
				$('.captionContent a.oTitle').html('A lost Nick Drake recording...');
				$('.captionContent a.oBody').html('and the lucky 167 people who got to hear it');
				$('.captionContent').width(width - 10);
				$('.captionOverlay').width(width).height($('.captionContent').height() + 20);
			}			
		}		
	});
//]]>
</script>

