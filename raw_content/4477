  <script type="text/javascript">
    // <![CDATA[
    var api_5054659;
    var obj_5054659;
    var cliked_index_5054659=0;
    var number_items_5054659=1;
    var max_items_perpage_5054659 = 2;
    var totalPage5054659 = Math.ceil(0.5);

    function chicked_5054659(index){
      cliked_index_5054659=index;
      return false;
    }
    // execute your scripts when the DOM is ready. this is a good habit
    jQuery(function() {
      // initialize scrollable
      // clickable
      api_5054659 = jQuery("#carouselScrollableContainer5054659").scrollable({prev:'.prevPage',next:'.nextPage', vertical:false}).navigator({api:true, navi:".indicator"});
      /* set navigaion indicator width so that it is rightly set at middle */
    

      /* set navigation arrows disabled status correctly */
    
      jQuery("#prevCarouselItemButton_5054659").click(function() {
         if(obj_5054659){
          obj_5054659.changeDisplayItemCarousel_5054659(api_5054659, -1);
        }
      });
      jQuery("#nextCarouselItemButton_5054659").click(function() {
         if(obj_5054659){
          obj_5054659.changeDisplayItemCarousel_5054659(api_5054659, 1);
        }
      });

    });

    jQuery(function()
    {
      var tabIndex_5054659 = 0;
      var lastClickedIndex_5054659=-1;
      obj_5054659 = this;
      function displayClickedCarouselItem_5054659(){

        jQuery("#mainImage5054659").css({"display":"block"});
        jQuery("#video5054659").css({"display":"none"});
        var index = cliked_index_5054659;
        jQuery(".item5054659").css({"border":"1px solid #000000"});
        jQuery("#item5054659_"+index).css({"border":"1px solid #00A0EE"});
        if(index!=lastClickedIndex_5054659)
        {
          lastClickedIndex_5054659 = index;
          var mainDisplayContainerId = "mainDisplayContainer5054659";
          var mainImageId = "mainImage5054659";
          var articleAnchorId = "articleAnchor5054659";
          var articleUrlContainerId = "articleUrl_5054659_" + tabIndex_5054659 + "_" + index;
          var imageUrlContainerId = "carouselImgaeUrl_5054659_" + tabIndex_5054659 + "_" + index;

          var mainDisplayContainer = jQuery("#"+mainDisplayContainerId).fadeTo("fast", 0.5);
          var imageUrl = jQuery("#"+imageUrlContainerId)[0].innerHTML;
          var img = new Image();
          // call this function after it's loaded
          img.onload = function() {

            // make wrapper fully visible
            mainDisplayContainer.fadeTo("fast", 1);
            // change the image
            jQuery("#"+mainImageId).attr("src", imageUrl);

          

            // update article ancho url
            jQuery("#" + articleAnchorId).attr("href", jQuery("#"+articleUrlContainerId)[0].innerHTML);

            // update url of a video
            var urlContainerId = "playButton_5054659_" + tabIndex_5054659 + "_" + index;
            jQuery("#playButtonr5054659").attr("href",jQuery("#"+urlContainerId)[0].getAttribute("href"));
            $("#playButtonr5054659").css({"display":$("#"+urlContainerId).css('display')});
          };
          img.src = imageUrl;
        }
      }

      jQuery("#carouselScrollableContainer5054659 .items div").click(function() {
        displayClickedCarouselItem_5054659();
        // when page loads simulate a "click" on the first image
      }).filter(":first").click();

      // change the active item index by pIndexOffset
      this.changeDisplayItemCarousel_5054659 = function(pApi, pIndexOffset){
        if(cliked_index_5054659+ pIndexOffset < 0 ){
          cliked_index_5054659 = cliked_index_5054659 + number_items_5054659;
        }
        cliked_index_5054659 = Math.max(0,(cliked_index_5054659+ pIndexOffset)%number_items_5054659);
        pApi.seekTo(cliked_index_5054659/max_items_perpage_5054659);
        displayClickedCarouselItem_5054659();
      }
    });

    var mouseOverCarousel = false;
    function carouselPlayPause(link){
      if (link.innerHTML == 'Play'){
        link.innerHTML = 'Pause';
        mouseOverCarousel = false;
      }
      else  if (link.innerHTML == 'Pause'){
        link.innerHTML = 'Play';
        mouseOverCarousel = true;
      }
    }

    function carouselMouseOut(){
      mouseOverCarousel = false;
    }

