  <script>
	var isMobileClient = 0;
  
  
  
  		
  		$(function(){
  			
  		$(".sbdiv").click(function(){
  			
  			document.location = '/msearch?key='+$('.searchfield').val();
  			
  		});
  		$(".feedback_link").click(function(){
  			
			$.ajax({
				type : "GET",
				url : "/base/feedback",
				data : "",
				success : function(msg) {
					$.colorbox({html:msg,top:20,width:500});
					// alert( "Data Saved: " + msg );
				}
			});
			
  		});
  		});
		$(".but_register").click(function(){
			
			
			$.ajax({
				type : "GET",
				url : "/security/users/ajaxregform",
				data : "",
				success : function(msg) {
					$.colorbox({html:msg,top:10,height:670});
					// alert( "Data Saved: " + msg );
				}
			});			
			
			
		});	
  
  		$(".but_register").colorbox({width:"600px",height:"600px",top:"50px", inline:true, href:"/security/users/ajaxregform"});
  
		$(".but_enter").click(function(){
			$(".auth_layer").slideToggle("fast", function () {});
		});//auth_layer
		
		$(".but_usermenu").click(function(){
			$(".auth_layer").slideToggle("fast", function () {});
		});

		
		$(".searchfield").focus(function(){
			sfState = true;
			$("#search_results").show();
		});
		$(".searchfield").blur(function(){
			sfState = false;
			if(!sfRState && !sfState){
				$("#search_results").hide();
			}
		});	
		
		if(!isMobileClient){
		
		$("#search_results").hover(function(){
			sfRState = true;
		},function(){
			sfRState = false;
			if(!sfRState && !sfState){
				$("#search_results").hide();
			}
		});
		$(".searchfield").keyup(function() 
			{
			
			sfCurrent = $(".searchfield").val();
			setTimeout(function(){
				
				if(sfCurrent==$(".searchfield").val()){

					//if already searched
					if(sfStop || sfRes==$(".searchfield").val()){
						return;
					}
						
					sfRes = $(".searchfield").val();
					sfStop = true;
					
					var searchbox = $(".searchfield").val();
					var dataString = 'key='+ searchbox;
					if(searchbox!='')
					{
					$.ajax({
					type: "GET",
					url: "/msearch?ajax=1",
					data: dataString,
					cache: false,
					success: function(html)
					{
						sfStop = false;
					//	alert(html);
						if(html=='nsuccess'){
							$("#search_results").html('');
						}else{
							$("#search_results").html(html).show();
						}
					}
					});
					}else{
						sfStop = false;
						$("#search_results").html('');
					}					
					
					
				}
				
			},300);
			
			//	return false; 
			});
		}

	var sfCurrent = '';
	var sfStop;
	var sfRes = '';
	var sfState = false;
	var sfRState = false; 
  
  
  </script>          
           
           
                
