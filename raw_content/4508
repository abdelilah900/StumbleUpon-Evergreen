$(".navigation > li").hover(
	function() {
		$(this).children("ul").clearQueue().stop().fadeTo(150,1);
		$(this).siblings().children("ul").clearQueue().stop().fadeTo(500,0,function(){ $(this).css({ display : "none" }); });
	},
	function() {
		$(this).children("ul").delay(400).fadeTo(500,0,function(){ $(this).css({ display : "none" }); });
	}
);
