<script type="text/javascript">
//<![CDATA[
$(window).load(function() {

	var galleryAdPrefix = 'ST_People_Gallery';
	var overlayAdMgr = $(".modal-window").overlayAdvertManager({
		leaderboard: {
			adPrefix: galleryAdPrefix
		},
		mpu: {
			adPrefix: galleryAdPrefix
, enabled: false		},
		queryOptions: 'AdTitle=coco-chanel-in-her-own-words'
	});

	var slideshowControl = new SlideshowControl();
	$(".modal-window").fancybox({
		"autoScale": false,
		"onStart" : slideshowControl.startSlideshow,
		"onClosed" : function() {
				overlayAdMgr.hideAds();
				slideshowControl.resetSlideshow();
				var newUrl = document.location.pathname;
				if (window.history.replaceState) {
					window.history.replaceState(null, document.title, newUrl);
				} else {
					document.location.hash = "";
				}
			},
		"onComplete": function (event, index) {
			var pageTracker = _gat._getTracker("UA-18349334-1");
			var hash = "#item-" + (index + 1);
			var newUrl = document.location.pathname.replace(/\/gid\/([0-9]+)/, '') + hash;
			if (window.history.replaceState) {
				window.history.replaceState(null, document.title, newUrl);
			} else {
				document.location.hash = hash;
			}
			pageTracker._trackPageview(newUrl);

			overlayAdMgr.updateAds();
			return false;
		},
		"titleShow": false,
		"transitionIn": "none",
		"transitionOut": "none"
	});

	var index = document.location.hash.replace(/[^0-9]*/, "");
	if (index) {
		$($("li > .modal-window")[index -1]).click();
		slideshowControl.pauseSlideshow();
	} else {
		var gidRegex = /gid\/([0-9]+)/;
		gidSearch = gidRegex.exec(document.location);
		if (gidSearch !== null && gidSearch[1] !== '') {
			$('li > .modal-window[href="#content-' + gidSearch[1] + '"]').click();
			slideshowControl.pauseSlideshow();
		}
	}

	window.PinIt = window.PinIt || { loaded:false };
	if (!window.PinIt.loaded) {
		window.PinIt.loaded = true;

		var s = document.createElement("script");
		s.type = "text/javascript";
		s.async = true;
		if (window.location.protocol == "https:")
			s.src = "https://assets.pinterest.com/js/pinit.js";
		else
			s.src = "http://assets.pinterest.com/js/pinit.js";
		var x = document.getElementsByTagName("script")[0];
		x.parentNode.insertBefore(s, x);
	}

	$(".gallery-rate-item a").galleryRateFeature({
		dataId: 1788,
		apiLocation: "http://www.stylist.co.uk/api/poll.php"
});
});
//]]>
</script>
