			if (window.external.msIsSiteMode()&& !(appVersion.indexOf("Windows NT 6.0") !== -1)){
				$('head').append("<script src=\"/template/ver1-0/js/jl/jumpList.js\" type=\"text/javascript\"><\/script>");
			}
			localStorage.setItem('jList', '9575713,9575714,9574892,9574002,9575477,9573822,9575056,9575756,9568946,9575785');
		}
	});

	var g_ext = null;

	function pinifyTeaser(){
 		if (!window.external.msIsSiteMode()) {
 			//$('body').pinable({discStyle: 'topHat'});
 			$('body').prepend('<div class="pinifyNotification"><div id="pinifyTeaser"></div></div>');
 			$('#pinifyTeaser').pinify('pinTeaser', {
 				icon: '/template/ver1-0/i/jumplist/Telegraph_T20x20.png',
 				addStartLink: false,
 	            linkText: 'Click here to add this site to the start menu',
 	            pinText: 'Drag this icon to your Taskbar to get faster access to the latest news.',
 	            backgroundColor: '#8dac5a',
 	            style: {
 				        backgroundImage: "/template/ver1-0/i/jumplist/toolbar_bg.png",
 				        closeButtonImage: "/template/ver1-0/i/jumplist/close.png"
 				    }
 			});
 		}
 	}

 	function pimpMyList(){
		try {
			if (window.external.msIsSiteMode()) {
				g_ext = window.external;
				g_ext.msSiteModeClearJumpList();
				g_ext.msSiteModeCreateJumpList("Latest News - 29 Sep 2012");
				g_ext.msSiteModeAddJumpListItem("Belfast police prepare for Ulster Covenant centenary march", "/news/uknews/northernireland/9575713/Belfast-police-prepare-for-Ulster-Covenant-centenary-march.html", "/template/ver1-0/i/icons/favicon.ico");
			
				g_ext.msSiteModeAddJumpListItem("David Laws back in Government 'too soon', says father", "/news/politics/9575714/David-Laws-back-in-Government-too-soon-says-father.html", "/template/ver1-0/i/icons/favicon.ico");
			
