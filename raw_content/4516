				g_ext.msSiteModeAddJumpListItem("End of the boys in blue as police officers go hi-vis", "/news/uknews/law-and-order/9574002/End-of-the-boys-in-blue-as-police-go-hi-vis.html", "/template/ver1-0/i/icons/favicon.ico");
			
