    <div class="selector" id="cc-live">
        <h3>Find a place to live</h3>
        <div class="content">
	 


 
 

  



	


<script type="text/javascript">
function realEstateTabSearch(f)
{
	var finderValue = "";
	for ( var r = 0; r < f.finder.length; r++ )
	{
		if (f.finder[r].checked) 
		{
			break;
		}						
	}	
	var ad_type = "";
	finderValue = f.finder[r].value;	

	if (finderValue == "buy") {
		f.ad_type.value = "residential-listing,vacation-listing,new-homes-listing";
	} else if (finderValue == "new") {
		f.ad_type.value = "new-homes-listing";
	} else if (finderValue == "rent") {
		f.ad_type.value = "residential-rentals-listing,vacation-rentals-listing";
	} else if (finderValue == "fcl") {
		f.ad_type.value = "foreclosure-listing";
	} else if (finderValue == "apt") {
		f.ad_type.value = "community-rentals-listing";
		f.finder[r].value="rent";
	}

	return false;

}
</script>

<form name="realestatestabform" action="http://realestate.al.com/" method="get" onsubmit="realEstateTabSearch(this);">
 <input type="hidden" name="classification" value="real estate"/>
