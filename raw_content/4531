    function languagePreviewDidChange(sel) {
        var lang_id = $(sel).find('option:selected').attr('value');

        var url = Etsy.Url.appendOrReplaceParameters(window.location.href, {
                langid_override: lang_id
            });

        window.location.href = url;
    }
