<script type="text/javascript">
(function() {
var selector = '#sb-search';
var degreeStep = 1;
if(navigator.userAgent.match(/Chrome/) != null || navigator.userAgent.match(/Firefox/) != null) {
if(navigator.userAgent.match(/Firefox/) != null) { degreeStep = 4;
}
$(selector)[0].pageAngle = 0;
$(selector)[0].rotatePage = function() {
if($(selector)[0].value == 'mirror') {
$(selector)[0].pageAngle += degreeStep;
} else {
$(selector)[0].pageAngle -= degreeStep;
}
$('body').css('-webkit-transform', 'rotateY(' + $(selector)[0].pageAngle +'deg)');
$('body').css('-moz-transform', 'rotateY(' + $(selector)[0].pageAngle +'deg)');
if($(selector)[0].pageAngle == 0 || $(selector)[0].pageAngle == 180) {
window.clearInterval($(selector)[0].rotateInterval);
}
}
$(selector).keyup(function(event) {
if(($(selector)[0].pageAngle == 0 && $(selector)[0].value == 'mirror') || ($(selector)[0].pageAngle == 180 && $(selector)[0].value != 'mirror')) {
$(selector)[0].rotateInterval = window.setInterval("$('" + selector + "')[0].rotatePage()", 10);
}
});
}
})();
</script>
