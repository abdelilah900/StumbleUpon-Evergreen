//$(function(){
	$('a[name="aol-newsletter"]').aolNewsletter({
		verify:true,
		list:'main',
	        template:'welcome',
		fn:{
			revealForm:function($elem){
				$('#CE_follow_us_news').bind('click',function(){
					$(this).addClass('signup-showing');
					$elem.animate({'opacity':'1','height':'155px'},200);
				});
			},
			hideForm:function($elem){
				$('#CE_follow_us_news').removeClass('signup-showing');
				$elem.animate({'opacity':'0','height':'0px'},200);
			}
		}
	});
//});
</script>	<!-- remote: follow-us-init -->	
