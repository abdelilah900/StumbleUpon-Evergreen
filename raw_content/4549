<div id="joinNewsletter">
    <label for="newsletterEmail">Email:</label><input id="newsletterEmail" class="replaceText" type="text" value="Join our Newsletter" /><input type="button" value="Submit" />
    <br />
    <label for="selectAge" class="forselect">Age:</label>
    <select id="selectAge" class="white custom-select" style="width: 120px">
        <option value="">Select Age</option>
        <option value="0-12">0-12</option>
        <option value="13-17">13-17</option>
        <option value="18-34">18-34</option>
