                            $extend(_ctl00_mainContent_VideoBlock_VideoRating_divRatingsClient, {
                                tryToVoteAgain : function() {
                                   this.options.enabled = false;
                                    $('ctl00_mainContent_VideoBlock_VideoRating_divRatings').addClass('iocRatingDisabled');
                                    $('ctl00_mainContent_VideoBlock_VideoRating_divRatings').addEvent('click', function() {
                                        alert('You have already voted');
                                    });
                                }
                            });
                            __SA__.addEvent('voted', function(ratingId) {                        
                                _ctl00_mainContent_VideoBlock_VideoRating_divRatingsClient.tryToVoteAgain();
                            });
                            if (__SA__.hasAlreadyVoted('v94787')) {
                                _ctl00_mainContent_VideoBlock_VideoRating_divRatingsClient.tryToVoteAgain();
                            }
                        });    
