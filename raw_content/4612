        var filters = new OlympicGamesSearchFilters(queryStringAssociations, 'ctl00_mainContent_OlympicGamesSearchBlock_FilterGoButton', 'ctl00_mainContent_OlympicGamesSearchBlock_FilterClearLink', 'ctl00_mainContent_OlympicGamesSearchBlock_SearchButton_hlnkButton', 'PastGames');
        filters.additionalInitialisation(
            carousel, 
            searchData,
            'ctl00_mainContent_OlympicGamesSearchBlock_PastGamesCount');
    });
</script>

