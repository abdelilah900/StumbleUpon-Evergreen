                /*IMPORTANT NOTICE:*/
                // This function has to read the cookies to check whether the #assetId video or picture is already save or not.
                // If not, it has to set a cookie and return false
                //If yes, it has just to return true
                function IsMediaInPlaylist(assetId, IsJS) {

                    var mediaPlaylist = JSON.decode(Cookie.read('mediaPlaylist'));

                    if (!$chk(mediaPlaylist)) {
                        mediaPlaylist = new Array();
                    }

                    if (mediaPlaylist.contains(assetId)) {
                        return true;
                    }

                    if(IsJS){

                    mediaPlaylist.push(assetId);
                    Cookie.write('mediaPlaylist', JSON.encode(mediaPlaylist), { duration: 365, path: '/', domain: 'www.olympic.org' });
                    }

                    return false;
                }
                   
                function RemoveMediaInPlaylist(assetId){
                    var mediaPlaylist = JSON.decode(Cookie.read('mediaPlaylist'));

                    if ($chk(mediaPlaylist) && mediaPlaylist.contains(assetId)) {
                        mediaPlaylist.erase(assetId);
                        Cookie.write('mediaPlaylist', JSON.encode(mediaPlaylist), { duration: 365, path: '/', domain: 'www.olympic.org' });
                    }                
                }
                
                 
