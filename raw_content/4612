			<div id="ctl00_mainContent_VideoBlock_addToMyPlaylist_divAddToPlayList" class="iocAddToPlayList" style="display:none;">
    <script type="text/javascript">
        window.addEvent('load', function() {
            new OnClickPopup('#ctl00_mainContent_VideoBlock_AddToPlaylist', '#ctl00_mainContent_VideoBlock_addToMyPlaylist_closeButton,#ctl00_mainContent_VideoBlock_AddToPlaylist,#ctl00_mainContent_VideoBlock_ShareLink,#ctl00_mainContent_VideoBlock_SendToAFriendLink', 'ctl00_mainContent_VideoBlock_addToMyPlaylist_divAddToPlayList');
            var toggleButton = $('ctl00_mainContent_VideoBlock_AddToPlaylist');
            if ($chk(toggleButton)) {
                toggleButton.addEvent('click', function() {
                    var isInPlaylist = IsMediaInPlaylist('Media94787', false);
                    if (isInPlaylist) {
                        var addToPlaylistCopy = $('divAddToPlayListCopy');
                        var addToPlaylistCopyAdded = $('divAddToPlayListCopyAdded');

                        if ($chk(addToPlaylistCopy)) {
                            addToPlaylistCopy.setStyle('display', 'none');
                        }

                        if ($chk(addToPlaylistCopyAdded)) {
                            addToPlaylistCopyAdded.setStyle('display', 'block');
                        }
                    }
                });
            }
        });
    </script>
    <div class="iocAddToPlayListTitle">
        <span id="ctl00_mainContent_VideoBlock_addToMyPlaylist_lblPopupTitle">Add To Playlist</span>
        <a href="javascript:;" id="ctl00_mainContent_VideoBlock_addToMyPlaylist_closeButton" title="Close">Close</a>
    </div>
    <div class="iocAddToPlayListContent">
        <div id="ctl00_mainContent_VideoBlock_addToMyPlaylist_divAddToPlayListCopy" class="iocAddToPlayListCopy">
            This asset has been added to your playlist.<br />Your playlist is available in 
            <a id="ctl00_mainContent_VideoBlock_addToMyPlaylist_hlnkMediaPlayer" title="the media player section" href="http://www.olympic.org/multimedia-player">the media player section</a>
        </div>
        <div id="ctl00_mainContent_VideoBlock_addToMyPlaylist_divAddToPlayListCopyAdded" class="iocAddToPlayListCopy" style="display: none">
