<script type="text/javascript">
(function($){

if ( ( navigator.userAgent.indexOf( 'iPad') == -1 ) || ( navigator.userAgent.indexOf( 'iPad') > -1 && navigator.userAgent.indexOf( 'Safari') == -1 ) ){
	return;
}

var current_date = new Date();

if( $.cookie('tc_ipad_interstitial_state') === null ) {
	$.cookie( 'tc_ipad_interstitial_state', 'show',  { expires: 1, path: '/' } );
	if( $.cookie('tc_ipad_interstitial_date') === null ) {
		$.cookie( 'tc_ipad_interstitial_date', current_date,  { expires: 14, path: '/' } );
	}
}

var show_state = $.cookie( 'tc_ipad_interstitial_state' );

var created_on =  new Date( $.cookie( 'tc_ipad_interstitial_date' ) );

var difference = Math.round( ( current_date.getTime() / ( 1000*60*60 ) ) - ( created_on.getTime() / ( 1000*60*60 ) ) );

if ( difference >= 72 ){
	if ( show_state == 'show' ){
		display_interstitial();
		$.cookie( 'tc_ipad_interstitial_state', 'disabled',  { expires: 10, path: '/' } );
	}
} else if ( difference >= 48 ){
	if ( show_state == 'show' ){
		display_interstitial();
		$.cookie( 'tc_ipad_interstitial_state', 'hide',  { expires: 1, path: '/' } );
	}
} else if ( difference >= 24 ){
	if ( show_state == 'show' ){
		display_interstitial();
		$.cookie( 'tc_ipad_interstitial_state', 'hide',  { expires: 1, path: '/' } );
	}
}else{
	if ( show_state == 'show' ){
		display_interstitial();
		$.cookie( 'tc_ipad_interstitial_state', 'hide',  { expires: 1, path: '/' } );
	}
}

var body_children;

$( '#ipad-interstitial .download-button' ).on( 'click', function(e){
	$.cookie( 'tc_ipad_interstitial_state', 'disabled',  { expires: 10, path: '/' } );
	window.location ='http://bit.ly/Jy5SHi';
});

function display_interstitial(){
	var interstitial = $('<div id="ipad-interstitial" />');
	var download_button = ('<a class="download-button" href="http://bit.ly/Jy5SHi" />');
	var continue_link = ('<a class="continue-link" href="http://techcrunch.com/2012/06/14/carat-battery/" />');
	body_children = $( 'body' ).children();
	
	$( 'body' ).append( interstitial );
	$( interstitial ).append( download_button ).append( continue_link );
	body_children.hide();
	
	
	$( interstitial ).show();
	setTimeout( remove_interstitial, 15000 );
}

function remove_interstitial(){
	$( '#ipad-interstitial' ).remove();
	body_children.show();
}

})(jQuery);
</script>


</body>
</html>