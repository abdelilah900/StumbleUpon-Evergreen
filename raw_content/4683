    "s.tl(this,'o','Outbound Click');";
  var adClick = "var s=s_gi(s_account); s.events='event5'; s.eVar14='%ad%'; "+
    "s.linkTrackVars='prop17,prop25,eVar14,eVar17,eVar25,events'; s.linkTrackEvents='event5';"+
    "s.tl(this,'o','Ad Click');";
  var providerClick = "var s=s_gi(s_account); s.prop21='in content'; s.events='event13'; s.eVar26=s.prop17; "+
    "s.linkTrackVars='prop21,eVar26,events'; s.linkTrackEvents='event13';"+
    "s.tl(this,'o','Provider Click');";

  $('#adRightTop').attr('onclick', adClick.replace('%ad%', 'ad1'));
  $('#sponsor').attr('onclick', adClick.replace('%ad%', 'logo'));
  $('#adRightMiddle').attr('onclick', adClick.replace('%ad%', 'ad2'));

  var providerUrls = [  ];
  $('a[target=_blank]').not($('#adRightTop a, #sponsor a, #adRightMiddle a')).each(function (i) {
	if (providerUrls.length > 0) {
      for (var j=0; j < providerUrls.length; j++) {
        if ( ($(this).attr('onclick') == undefined) && ($(this).attr('href').toLowerCase().indexOf(providerUrls[j].toLowerCase()) != -1) ) {
          $(this).attr('onclick', providerClick);
        }
        else if ($(this).attr('onclick') == undefined) {
          $(this).attr('onclick', standardClick);
        }
      }
	}
	else {
      $(this).attr('onclick', standardClick);
	}
  });
 });

