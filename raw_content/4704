        $('#show-all-comments').click(function(){scrollToTag('#comments')});

        // share recipe dialog
        $("#share-dialog").dialog({
            autoOpen: false,
            modal: true,
            draggable: false,
            resizable: false,
            width: 600,
            autoResize: true,
            bgiframe: true
        });

        $('.update_default').live('click',function(){
            var key = $(this).attr('key');
            var thiscontainer = $(this);
            $.ajax({
                url:        '/utilities/photodefault/format/json/type/1/id/31438/photo_id/'+key,
                beforeSend: function() {thiscontainer.html('Updating...')},
                success:     function(data) {
                    if(data.success){
                        $('.default_image').removeClass('default_image');
                        $('.photo_'+key).each(function(){$(this).addClass('default_image')});
                        $('.update_default').remove();
                    } else {
                        alert ('Error: Could not convert to main photo.');
                        $('.update_default').remove();
                    }
                }
            });
            return false;
        });

            });

