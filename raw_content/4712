<script type="text/javascript">
	$(document).ready(function() {
		$('#search_find_out').blur(function () {
		   if($('#search_find_out').val() =='')
				$('#search_find_out').val('Try searching for one, here');
		});
		$('#search_find_out').focus(function() {
			if($('#search_find_out').val() =='Try searching for one, here')
				$('#search_find_out').val('');
		});
	});

	function gotoFindOut(){
		var xsearch = document.getElementById("search_find_out").value;
		if(xsearch == "Try searching for one, here"){ xsearch = ""; }
		document.getElementById("search-box").action = "/search/directory/"+xsearch;
		document.getElementById("search-box").submit();
	}

	function fastloading() {
		// Initial first time loading
		var f =  document.forms['frmSearchR24'];

		var p = f.loc;
		if (p) preset_search(p,1);
		if (p) p.onfocus=function(){ preset_search(this,0); };
		if (p) p.onblur=function(){ preset_search(this,1); };

		f.onsubmit=function(){
			if (f.loc.value == '- Search by location -') f.loc.value = "";
			return checkdates(this);
		};
	}


	function preset_search(id ,mode) {
		var prev_value = id.value;
		if (!mode) {  id.value =  ''; id.style.color='#000000'; return prev_value; }
		if (mode>1 || id.value == '' || id.value[0] == '-') switch (id.id) {
			case 'loc': id.value = '- Search by location -';   id.style.color='#CCCCCC'; break;
			}
		return prev_value;
	}


	function checkdates(f) {
	var jsCal = new Object();
	jsCal.startDate   = [0,0,0,null];
	jsCal.endDate   = [0,0,0,null];
	var d = new Date();
	var	d = f.date11.selectedIndex; jsCal.startDate[0] = +f.date11.options[d].value;
		d = f.date12.selectedIndex; jsCal.startDate[1] = +f.date12.options[d].value;
		d = f.date13.selectedIndex; jsCal.startDate[2] = +f.date13.options[d].value;
		d = f.date21.selectedIndex; jsCal.endDate[0] = +f.date21.options[d].value;
		d = f.date22.selectedIndex; jsCal.endDate[1] = +f.date22.options[d].value;
		d = f.date23.selectedIndex; jsCal.endDate[2] = +f.date23.options[d].value;
	var	d1= new Date(jsCal.startDate[2], jsCal.startDate[1]-1, jsCal.startDate[0], 23, 00, 00);
	var	d2= new Date(jsCal.endDate[2], jsCal.endDate[1]-1, jsCal.endDate[0], 23, 00, 00);
	var	d0= new Date();

	if ((d1.getMonth()+1 != jsCal.startDate[1])||(d1.getDate()!= jsCal.startDate[0])||(d1.getFullYear() != jsCal.startDate[2])) d = -1;
	else if ((d2.getMonth()+1 != jsCal.endDate[1])||(d2.getDate()!= jsCal.endDate[0])||(d2.getFullYear() != jsCal.endDate[2])) d = -1;
	else d = (d2 - d1) / 86400000;

	var sd = 0;
		if (d1 > d2) sd = 1;
		if (d1 < d0) d = -1;
	var	p = document.getElementById('dayid');
		if (d> 1) p.innerHTML = "<span style=\"color:#F60\">[" + d + " nights]</span>";
		if (d==1) p.innerHTML = "<span style=\"color:#F60\">[" + d + " night.]</span>";
		if (d< 0 && sd==0) { alert("Error in date input, please verify the validity period."); }
		if (d1 < d0) { alert("Check-in date cannot be in the past, please amend."); }
		if (d<1 && sd==1) alert("Please review validity pariod."); // CHECK
		return (d>0 || d==0) ? true : false;
	}
</script>
