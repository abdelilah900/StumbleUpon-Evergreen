		function handleGotoContribWizard (widgetId, entryId) {
			KalturaModal.openModal("contribution_wizard", "http://www.cosmeticsurg.net/blog/wp-content/plugins/all-in-one-video-pack/page_contribution_wizard_front_end.php?wid=" + widgetId + "&entryId=" + entryId, { width: 680, height: 360 } );
			jQuery("#contribution_wizard").addClass("modalContributionWizard");
		}
	
		function handleGotoEditorWindow (widgetId, entryId) {
			KalturaModal.openModal("simple_editor", "http://www.cosmeticsurg.net/blog/wp-content/plugins/all-in-one-video-pack/page_simple_editor_front_end.php?wid=" + widgetId + "&entryId=" + entryId, { width: 890, height: 546 } );
			jQuery("#simple_editor").addClass("modalSimpleEditor");
		}
		
		function gotoContributorWindow(entryId) {
			handleGotoContribWizard("", entryId);
		}
		
		function gotoEditorWindow(entryId) {
			handleGotoEditorWindow("", entryId);
		}
