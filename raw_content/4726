	image:"/bestrecipes/images/kary_smallvp.jpg"	
	};
var params={
	allowFullScreen:"true",allowScriptAccess:"always",menu:"true",salign:"tl",scale:"noscale",
	bgcolor:"#000000"
	};
var attributes={
	id:"tpSwf",
	name:"tpSwf"
	};
swfobject.embedSWF("/video/swf/UberPlayer.swf", "tpSwf", "373", "275", "9.0.0", "expressInstall.swf", flashvars, params, attributes);


if( typeof(swfobject) == "object" )
{
if( typeof(swfobject.CBC) != "object" ){ swfobject.CBC = {}; }
if( typeof(swfobject.CBC.Modal) != "function" ){
swfobject.CBC.Modal = function (id)
{
	if(!id) return
	this.swf_id = id;
	this.hidden = false;
	if( !swfobject.CBC.Modal.$ref )
	{
		if( typeof( YUI ) == "function" ){ this.yui = swfobject.CBC.Modal.$ref = new YUI (); }
		else
		{
			if( document.domain != "cbc.ca" ) document.domain = "cbc.ca";
			if( typeof( top.YUI ) == "function" ) this.yui = swfobject.CBC.Modal.$ref = new top.YUI ();
		}
	}
	if( this.yui )
	{
		var foo = this;
		this.yui.use('event-custom', function(){ foo.onEvent.apply( foo, arguments) } );
	}
}
swfobject.CBC.Modal.prototype = {
	
	onEvent: function (Y)
	{
		this.yui.Global.on('cbc_modalboxopen', this.onOpen, this );
		this.yui.Global.on('cbc_modalboxclose', this.onClose, this );
	 },
	
	onOpen: function ()
	{
		var swfNode = swfobject.getObjectById( this.swf_id );
		if( swfNode && !this.hidden ) {
			this.hidden = true;
			swfNode.style.visibility = "hidden";
			swfNode.pause();
		}
	},
	
	onClose: function ()
	{
		var swfNode = swfobject.getObjectById( this.swf_id );
		 if( swfNode && this.hidden ) {
			this.hidden = false;
			swfNode.style.visibility = "visible";
			swfNode.resume();
		 }
	}
}}
new swfobject.CBC.Modal (attributes.id);
}


//]]>
</script>



