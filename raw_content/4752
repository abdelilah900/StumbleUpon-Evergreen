        <!-- Subscription Overlay JavaScript -->
        <script type="text/javascript">
            jQuery(function() {
				var liftHeight = -jQuery('#subscribeBox').height()*2+'px';
				var boxHeight = jQuery('#subscribeBox').height()/2;
				var docHeight = jQuery(window).height()/2;
				var dropHeight = docHeight - boxHeight;
				var leftShift = jQuery(window).width()/2-350;
				jQuery('#subscribeBox').css({'left' : leftShift});

                jQuery('#activatorButton').click(function(){
                    jQuery('#subscribeOverlay').fadeIn('fast',function(){
                        jQuery('#subscribeBox').animate({'top':dropHeight},500);
                    });
                });
                jQuery('#subscribeClose').click(function(){ // Close button
                    jQuery('#subscribeBox').animate({'top':liftHeight},500,function(){
                        jQuery('#subscribeOverlay').fadeOut('fast');
                    });
                });

				jQuery('#subscribeOverlay').click(function(){ // Click overlay
                    jQuery('#subscribeBox').animate({'top':liftHeight},500,function(){
                        jQuery('#subscribeOverlay').fadeOut('fast');
                    });
                });

				jQuery(document).keyup(function(e) { // Pres ESC
					if (e.keyCode == 27) { 
						jQuery('#subscribeBox').animate({'top':liftHeight},500,function(){
							jQuery('#subscribeOverlay').fadeOut('fast');
						});
					}   
				});
            });
        </script>



