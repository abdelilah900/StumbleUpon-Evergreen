                                function() {
                                    onMenu = true;
                                    buttonElem.addClass('selected');
                                },
                                function() {
                                    onMenu = false;
                                    setTimeout(function() {
                                        if(!onButton && !onMenu) {
                                            menuElem.hide();
                                            buttonElem.removeClass('selected');
                                        }
                                    }, 250);
                                }
                              );
                          })(this);
