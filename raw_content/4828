// MantaRay
var mantaRayScript = document.createElement('script');
mantaRayScript.type = 'text/javascript';
mantaRayScript.async = true;
mantaRayScript.src = 'http://i.i.com.com/cnwk.1d/Ads/common/manta/adFunctionsD-chow.js';
document.getElementsByTagName("head")[0].appendChild(mantaRayScript);

var cbsiAdGlobal = {
 'SITE' : '103',
 'BRAND' : '26',
 'NCAT' : '12711',
 'PTYPE' : '9910',
 'STAGING' : 0  // set to 1 if on a test environment
};

function startBehavioral(){
   try {
       cbsiRegisterAdGlobals(cbsiAdGlobal);
       cbsiAdBehavioralSetup();
       cbsiAdBehavioral();
   }
   catch(err) {
      setTimeout("startBehavioral()", 500);
   }
}
startBehavioral();
</script>
 
