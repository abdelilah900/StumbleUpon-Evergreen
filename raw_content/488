<script type="text/javascript">
$(function () {
   
  var msie6 = $.browser == 'msie' && $.browser.version < 7;
   
  if (!msie6 && $('.rosside').offset()!=null) {
    var top = $('.rosside').offset().top - parseFloat($('.rosside').css('margin-top').replace(/auto/, 0));
    var height = $('.rosside').height();
    var winHeight = $(window).height();
    var footerTop = $('.site-footer').offset().top - parseFloat($('.site-footer').css('margin-top').replace(/auto/, 0));
    var gap = 10;
    $(window).scroll(function (event) {
      // what the y position of the scroll is
      var y = $(this).scrollTop();
       
      // whether that's below the form
      if (y+winHeight >= top+ height+gap && y+winHeight<=footerTop) {
        // if so, ad the fixed class
        $('.rosside').addClass('rossidefixed').css('top',winHeight-height-gap +'px');
      }
      else if (y+winHeight>footerTop) {
        // if so, ad the fixed class
       $('.rosside').addClass('rossidefixed').css('top',footerTop-height-y-gap+'px');
      }
      else   
      {
        // otherwise remove it
        $('.rosside').removeClass('rossidefixed').css('top','0px');
      }
    });
  } 
});
</script>

