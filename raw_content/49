$(function() {
            var sb_offset = $("#sb").offset().top;
            $(window).scroll(function() {
				// scroll code for social bookmarking buttons starts here
                if (($(window).scrollTop() + 100) > sb_offset) {
					$("#sb").addClass("fixed");
                    $("#sb").stop().animate({
                        marginTop: 'auto'
                    });
                } else {
					$("#sb").removeClass("fixed");
                    $("#sb").stop().animate({
                        marginTop: 0
                    });
                };
				// scroll code for social bookmarking buttons ends here				
            });
});

$(function() {
			var elem = $(".scrollads");
            var ad_offset = elem.offset().top;

            $(window).scroll(function() {
				// scroll code for side ads starts here
                if (($(window).scrollTop() + 100) > ad_offset) {
					elem.addClass("fixed");
                    elem.stop().animate({
                        marginTop: 'auto'
                    });
                } else {
					elem.removeClass("fixed");
                    elem.stop().animate({
                        marginTop: 0
                    });
                };
				// scroll code for side ads ends here
            });
});

$(function() {
			var elem = $("#topbar");
            var ad_offset = elem.offset().top;

            $(window).scroll(function() {
				// scroll code for side ads starts here
                if ($(window).scrollTop() > ad_offset) {
					elem.addClass("fixedtop");
                    elem.stop().animate({
                        marginTop: 'auto'
                    });
                } else {
					elem.removeClass("fixedtop");
                    elem.stop().animate({
                        marginTop: 0
                    });
                };
				// scroll code for side ads ends here
            });
});

$(document).keyup(function(e) 
{
        if (e.keyCode == 39) 
        {  
            window.location.href = $("#next").attr('href');
        }
        if(e.keyCode==37)
        {
            window.location.href = $("#previous").attr('href');
        }

});

