		function footerNavFix() {
		if($$('#footertext li:last-child')[0])
		$$('#footertext li:last-child')[0].addClassName('lastMenuItem');
		$$('#footertext li:last-child')[1].addClassName('lastMenuItem');
		if($$('#footertext li:first-child')[1])
		$$('#footertext li:first-child')[1].addClassName('firstMenuItem');  	
		$$('#footertext li:first-child')[2].addClassName('firstMenuItem');  	
		}
		Event.observe("footertext", 'load', footerNavFix());
		</script>   
