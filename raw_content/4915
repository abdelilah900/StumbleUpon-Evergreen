                  swfobject.removeSWF('defaultDivHD');
                }
		            flashOutPut(1);
		            hideVideoSettingDiv();
		            if(controlBar) {
                  controlBar.style.paddingTop = '18px';
                }
                spacer.style.display = 'none';
                if(metaContainer) {
									metaContainer.style.marginTop = '10px';
								}
                showHPVSDiv();
	            }
	            else if(vidResolution == 1) { // high res player
                hdVidUrl = "http://video1.break.com/dnet/media/2011/5/18/great-fan-escape_2.mp4"; // high res asset
                if(stdPlayer) {
                  swfobject.removeSWF('defaultDiv');
                }
		            hdPlayerWrap.style.display = 'block';
		            hdAdjustWrapper(0,hdVidUrl,sGlobalCMTID);
                if(metaContainer) {
									metaContainer.style.marginTop = '10px';
								}
                hideHPVSDiv();
	            }
	            else { // highest res player
                hdVidUrl = "http://video1.break.com/dnet/media/2011/5/18/great-fan-escape_3.mp4"; // highest res asset
                if(stdPlayer) {
                  swfobject.removeSWF('defaultDiv');
                }
		            hdPlayerWrap.style.display = 'block';
		            hdAdjustWrapper(1,hdVidUrl,sGlobalCMTIDD);
                if(metaContainer) {
									metaContainer.style.marginTop = '260px';
								}
                hideHPVSDiv();
	            }
				      resizeTilingDivs();
            }
          </script>
       <!--NEW end--><!--@@END_MEDIA@@--></td></tr></table></div><div id="sharebarNew" style="width:577px;margin-left:26px;">
