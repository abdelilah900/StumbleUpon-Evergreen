<script type="text/javascript">

    function SetPostIDToHiddenField(postid, hiddenSelectedPostId) {
        var selectedPostId = document.getElementById(hiddenSelectedPostId);
        selectedPostId.value = postid;

        var elementClicked = "#" + postid;
        var ds = $(elementClicked).position().top;

        var scrollVal = $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_hdnScrollValue');
        if (scrollVal != null) {
            scrollVal.val(ds);
        }
    }

    function SetFocusOnComment(postID123) {
        var elementClicked = "#" + postID123;
        var destination = $(elementClicked).offset().top; $('html:not(:animated),body:not(:animated)').animate({
            scrollTop: destination - 20
        }, 500);
        return false;
    }

    function SetGridDisplay(hdnValue) {

        if (hdnValue != "default") {
            if (hdnValue == 'none') {
                $('#CommentSection').show(100);
                $('#CommentSection').attr('display', 'block');

                $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_hdn').val('none');
            }
            else {
                $('#CommentSection').hide(100);
                $('#CommentSection').attr('display', 'none');

                $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_hdn').val('display');
            }
        }

    }

    function DeleteSetTimeOut() {
        var ds = $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_hdnScrollValue').val() - 20;
        window.scrollTo(0, ds);
    }

    function CommentsClick() {

        var zeroCondition = $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_hdnTotalComments').val();

        var attrb = $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_hdn').val();

        if (zeroCondition != "0") {
            if (attrb != "default") {
                if (attrb == 'none') {
                    $('#CommentSection').hide(100);
                    $('#CommentSection').attr('display', 'none');

                    $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_hdn').val('display');
                }
                else {
                    $('#CommentSection').show(100);
                    $('#CommentSection').attr('display', 'block');

                    $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_hdn').val('none');
                }
            }
            else {
                document.getElementById('CommentSection').style.display = "block";

                $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_hdn').val('none');



            }
        }
        else {
            $('#CommentSection').hide(100);
            $('#CommentSection').attr('display', 'none');
            $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_lnkbtn_CommunityComments').val("Comments (0)");
            $('#ctl00_ctl00_ctl00_bhcr_bbcr_bbcr_bbcr_CommentSectionControl_hdn').val('display');
        }

    }
 

</script>



