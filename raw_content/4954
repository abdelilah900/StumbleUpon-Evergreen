// date picker
Date.format = 'mm/dd/yyyy';
$(function() {
    $('.date-pick').datePicker({startDate:'01/01/2010',endDate:new Date().addDays(1).asString(),clickInput:true})
    .bind(
       'dateSelected',
        function(e, selectedDate, $td)
        {
            sdate = document.getElementById('date1').value;
            var sdates = sdate.split("/");
            sdate = sdates[2]+"-"+sdates[0]+"-"+sdates[1];
            edate = document.getElementById('date2').value;
            var edates = edate.split("/");
            edate = edates[2]+"-"+edates[0]+"-"+edates[1];
        }
    );
    $('#date1').bind(
        'dpClosed',
        function(e, selectedDates)
        {
            var d = selectedDates[0];
            if (d) {
                d = new Date(d);
                $('#date2').dpSetStartDate(d.addDays(1).asString());
            }
        }
    );
    $('#date2').bind(
        'dpClosed',
        function(e, selectedDates)
        {
            var d = selectedDates[0];
            if (d) {
                d = new Date(d);
                $('#date1').dpSetEndDate(d.addDays(-1).asString());
            }
        }
    );
});

