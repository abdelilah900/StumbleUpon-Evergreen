var fsbanner = '1';
var hmu_id = '';
var pathToMain = '../';
function initState() {
    state = [];
    state['default_pid'] = '';
    state['geonameid'] = '';
    state['allplace'] = '203';
    state['alldiz'] = '225';
    state['alltag'] = '3';
    state['allfeed'] = '21';
    state['default_time'] = '1 week';
    state['default_feeds'] = '';
    state['default_diseases'] = '';
    state['default_places'] = '';
    state['partner'] = 'hm';
    state['lang'] = 'es';
    state['presets'] = '1,219,216,212,198,127,149,3,4,94,2';
    state['heatscore'] = 1;
    state['omit_notag'] = '';
    state['persistent_kmlpath'] = '';
    state['path_to_main'] = '../';
    state['species'] = '';
    state['sort1'] = 0;
    state['sort2'] = 0;
    state['sort3'] = 0;
    state['sort4'] = 0;
    state['sort5'] = 0;
    state['sort6'] = 0;
    state['sort7'] = 0;

    state['place_id'] = [];
    initCheckboxes('place', true);

    state['disease_id'] = [];
    initCheckboxes('diz', true);

    state['alert_tag_id'] = [];
    initCheckboxes('tag', true);

    state['feed_id'] = [];
    initCheckboxes('feed', true);

    // if disease ids are passed in, treat it like a search
    var dizpi = getParam('disease');
    if (typeof dizpi != "undefined") {
        state['disease_id'] = [];
        presetds();
        var dizpiarr = dizpi.split(",");
        for(var i = 0; i < dizpiarr.length; i++) {
            state['disease_id']['' + dizpiarr[i]] = 1;
        }
        xajax_update_map(state);
        return 1;
    }

        // if a preset is passed into the URL by default, go directly to the preset
    var ps = getParam('ps');
    if (typeof ps != "undefined") {
        if(ps == '127') {
            location.href='http://healthmap.org/haiti';
        }
        state['preset'] = ps;
    } else {
        state['preset'] = '';
        ps = 1;
    }
    // v is lat, lon and zoom
    var v = getParam('v');
    if (typeof v != "undefined") {
        show_map_only();
        var el = v.split(',');
        var pid = Number(el[3]);
        if(pid) {
            state['default_pid'] = pid;
        } else {
            state['geonameid'] = getParam('g');
        }
        state['mapzoom'] = Number(el[2]);
        state['mapcenter'] = Number(el[0]) + ", " + Number(el[1]);
    } 
    xajax_preset(ps, state, '');
    var fs = getParam('fs');
    if(Number(fs) == 1) {
        maxMap();
        return 1;
    }
}
function presetds(id) {
    state['default_pid'] = '';
    state['geonameid'] = '';
    // hide the save this map view option
    if(id) {
        // a preset was clicked on so reset the value of the searchbox
        i$('searchbox').className = 'default';
        i$('searchbox').value = 'disease or location';
        i$('save_current_qv').className='hide';
    } else {
            }
    for(var i = 1; i <= 20; i++) {
        if(i$('preset' + i) != null) {
	        icon_class = i$('preset' + i).className;
            if(i == id) {
                i$('preset' + i).className = icon_class + " active";
            } else {
		    if (icon_class.indexOf('home') != -1) {
			i$('preset' + i).className = 'home';
		    } else {
	            	if (icon_class.indexOf("hm") != -1) {
            	    	i$('preset' + i).className = 'hm';
	            	} else {
            	    	i$('preset' + i).className = 'user';
	            	}
		    }
            }
        }
    }
}
function mSl(elt,id,name) {
    if(!i$(elt + id)) {
        return;  // if id doesnt exist, do nothing
    }
    setAllCheck(elt, 0);
    i$(elt + id).checked = 1;
}
function getSl() {
    var idMap = {'diz':diseaseIds, 'feed':feedIds, 'tag':tagIds, 'place':placeIds};
    var dbidMap  = {'diz':'disease_id', 'feed':'feed_id', 'tag':'alert_tag_id', 'place':'place_id'};
    var ckMap     = {'diz':'diseaseCheck', 'feed':'feedCheck', 'tag':'tagCheck', 'place':'placeCheck'};
    var elts = new Array('diz', 'feed', 'tag', 'place');
    for(var j=0; j< elts.length; j++) {
        var elt = elts[j];
        var ids = idMap[elt];
        var fieldType = dbidMap[elt];
        var ckName = ckMap[elt];
        state[fieldType] = [];

        for(var i = 0; i < ids.length; i++) {
            var f = ids[i];
            var ckName = i$(elt + f);
            if(ckName.checked == true) {
                state[fieldType]['' + f] = 1;
            }
        }

        if(state[fieldType] == "") {
            var dispMap = {'diz':'Disease', 'feed':'Feed', 'tag':'Category', 'place':'Place'};
            i$('aso_error').innerHTML = "Please choose at least one "+dispMap[elt];
            i$('adv_search_options').className='show';
            return;
        }

    }
}
function gSl() {
    getSl();
    if (i$('datechoice1').checked == true) {
        state['start_date'] = i$('time_interval').value;
        state['end_date'] = 'time_interval';
    } else {
        var sdate = i$('date1').value;
        var edate = i$('date2').value;
        var sdates = sdate.split("/");
        state['start_date'] = sdates[2]+"-"+sdates[0]+"-"+sdates[1];
        var edates = edate.split("/");
        state['end_date'] = edates[2]+"-"+edates[0]+"-"+edates[1];
    }
    xajax_update_map(state);
}
function saveView(viewval, viewdesc) {
    getSl();
    xajax_save_view(viewval, viewdesc, state, map.getCenter().lat(), map.getCenter().lng(), map.getZoom(), map.getMapTypeId(), '');
}
function sortElts(evt) {
    var tg = getTarget(evt);
    var othertg = document.getElementById("now_showing_heading").getElementsByTagName("span");
    for (var i=0;i<othertg.length;i++) {
        othertg[i].style.textDecoration = 'underline';
        othertg[i].style.backgroundImage = 'none';
    }
    var s = state[tg.id];
    var sel = tg.id.replace("sort","");
    xajax_buildNowShowing(sel, i$('alert_rows').innerHTML, s, true);
    if(s) {
        state[tg.id] = 0;
        tg.style.backgroundImage = 'url(../images/new/desc_arrow.png)';
    } else {
        state[tg.id] = 1;
        tg.style.backgroundImage = 'url(../images/new/asc_arrow.png)';
    }
    //tg.title = 'Ordene alfabético';
}
function show_comment(alertid) {
    xajax_expand_comment(alertid, '');
}
function rating(rate,alertid) {
    xajax_save_rating(rate, alertid, '');
}
addEvent(i$('sort1'), "click", sortElts);
addEvent(i$('sort2'), "click", sortElts);
addEvent(i$('sort3'), "click", sortElts);
addEvent(i$('sort4'), "click", sortElts);
addEvent(i$('sort5'), "click", sortElts);
addEvent(i$('sort6'), "click", sortElts);
addEvent(i$('sort7'), "click", sortElts);

i$('last-load').innerHTML = '23 Apr 2011 08:00';i$('next-load').innerHTML = '23 Apr 2011 09:00';

</script>

