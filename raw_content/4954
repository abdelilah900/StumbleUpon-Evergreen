// auto-complete search
function quicksearch()
{
    presetds();
    xajax_quicksearch('', '', i$('searchbox').value, state);
}

function show_map_only() {
        $('#gis').show();
        $('#mapds').show();
        $('#outbreak_missing_top').show();
        $('#outbreak_missing').show();
        if (!$('#accordion:empty')) {
            $('#accordionContainer').show();
        }
        if ($('#map_disclaimer').length) {
            $('#map_disclaimer').show();
        }
        // collapse list to nothing
        $('#innertable').height(0);
        $('#now_showing_table').hide(); 
        $('#cp_right').css({"margin-top": 0});
        // increase map height so it fills more of window
        $('#gis').height(510);
        $('#mapds').css({"top": 531});
        // down states
        $('#map_only').addClass("viewdown");
        $('#split').removeClass("viewdown");
        $('#list_only').removeClass("viewdown");
}

function show_split() {
        $('#gis').height(400);
        $('#mapds').css({"top": 421});
        $('#gis').show();
        $('#mapds').show();
        $('#outbreak_missing_top').show();
        $('#outbreak_missing').show();
        $('#now_showing_table').show(); 
        if (!$('#accordion:empty')) {
            $('#accordionContainer').show();
        }
        if ($('#map_disclaimer').length) {
            $('#map_disclaimer').show();
        }
        $('#innertable').height('150px');
        $('#cp_right').css({"margin-top": 0});
        // down states
        $('#split').addClass("viewdown");
        $('#list_only').removeClass("viewdown");
        $('#map_only').removeClass("viewdown");
}

function list_only() {
        $('#gis').hide();
        $('#mapds').hide();
        $('#outbreak_missing_top').hide();
        $('#outbreak_missing').hide();
        if (!$('#accordion:empty')) {
            $('#accordionContainer').hide();
        }
        if ($('#map_disclaimer').length) {
            $('#map_disclaimer').hide();
        }
        $('#now_showing_table').show(); 
        // expand to full height of list
        $('#innertable').height('auto');
        $('#alert_rows').height('auto');
        $('#cp_right').css({"margin-top": 25});
        // down states
        $('#list_only').addClass("viewdown");
        $('#map_only').removeClass("viewdown");
        $('#split').removeClass("viewdown");
}

