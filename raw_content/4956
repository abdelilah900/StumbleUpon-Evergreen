var defaultText = "enter your email here";  

function EmailFocus()
{ 
   if (document.getElementById('email').value == defaultText)
      document.getElementById('email').value = "";
}

function EmailBlur()
{
   var textValue = document.getElementById('email').value;
 
   if (textValue == defaultText || textValue.length == 0)
      document.getElementById('email').value = defaultText;
}
</script> 

