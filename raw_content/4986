<script type="text/javascript">
  (function ($) {
    $('#q').ninja('autocomplete', {
      get: function(q, callback) {
        $.ajax({
          url: '/keywords',
          dataType: 'json',
          data: {
            q: q
          },
          success: function (data) {
            callback(data);
          },
          error: function (request, status, message) {
            $.ninja.error(message);
          }
        });
      },
      select: function () {
        $('#search').submit();
      }
    });
  }(jQuery));
</script>


