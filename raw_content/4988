<script type="text/javascript">

    $(document).ready(function () {
        
        var hash_value = window.location.hash;
        if (hash_value.length > 0) {
            var p_lnk = $("#prev_link").attr("href");
            var n_lnk = $("#next_link").attr("href");
            $("#prev_link").attr("href", p_lnk + hash_value);
            $("#next_link").attr("href", n_lnk + hash_value);
        }
    });

        
	$(function() {
		//smooth scrolling
		 $("a[href^=#]").not("a[href=#page_top]").click(function (event) {
            var href = $(this).attr("href");
			
			//var offset_href = $(href).offset().top - 145;
			var offset_href = $(href).offset().top - 55;
            if (href.length > 1) {
                event.preventDefault();
                $.scrollTo(offset_href, 500);
            }
        });
		 $("a[href=#page_top]").click(function (event) {
            //var href = $(this).attr("href");
			
			//var offset_href = $(href).offset().top - 145;
            //if (href.length > 1) {
                event.preventDefault();
                $.scrollTo(0, 500);
            //}
        });
		//var $grey_top_freeze = $("div#grey_top_freeze");
		var $grey_top_freeze = $("nav.breadcrumb");
		var grey_initial_location = $grey_top_freeze.offset().top + 4;
		
		var $project_tools = $("nav.project-tools");
		var pt_initial_location = $(window).height();
		
		var $title_block = $("div#title_block");
		var $project_content = $("div#project_content");
			   
		var $tools_container = $("div#sidebar_follow");
		var initial_location = $tools_container.offset().top - $grey_top_freeze.outerHeight();
		var offset_margin_bottom = 50;
		var $content_outerHeight = $("div.col720").outerHeight();
		var new_top_setting = $content_outerHeight - $tools_container.outerHeight() - offset_margin_bottom;
		var stopThisTrain = $content_outerHeight - initial_location - $tools_container.outerHeight() - offset_margin_bottom;
		//printf("offset.top();" + initial_location);
		$(document).scroll(function () {
									 
			var document_scrollTop = $(document).scrollTop();
			$content_outerHeight = $("div.col720").outerHeight();
			stopThisTrain = $content_outerHeight +initial_location - $tools_container.outerHeight() - offset_margin_bottom;
			new_top_setting = $content_outerHeight - $tools_container.outerHeight() - offset_margin_bottom;
			
			// left_col
			if (document_scrollTop > grey_initial_location) {
				
				$grey_top_freeze.css({
					position: "fixed",
					top: "-4px",
					"z-index": 50,
				});
			} else {
				
				$grey_top_freeze.css({
					position: "absolute",
					top: 50,
					"z-index": 0,
				});	
			}
			
			// right_col
			
			if (document_scrollTop > initial_location) {
				
				$tools_container.css({
					"position": "fixed",
					"top": ($grey_top_freeze.outerHeight() - 4)
				});
				
				/*$title_block.css({
					"position": "fixed",
					"top": ($grey_top_freeze.outerHeight() - 4)
				});*/
				/*$project_content.css({
					"margin-top": $title_block.outerHeight()
				});*/
				/*$("div#title_block p").slideUp(function() {
					
						$project_content.css({
							"margin-top": $title_block.outerHeight()
						});	
					
				});*/
				
				$project_tools.css({
					position: "fixed",
					bottom: "20px",
					top: "auto"
				});
				
			} else {
				$tools_container.css({
					"position": "absolute",
					"top": "0px"
				});
				/*$title_block.css({
					"position": "absolute",
					"top": "0px"
				});*/
				/*$project_content.css({
					"margin-top": $title_block.outerHeight()
				});*/
				/*$("div#title_block p").slideDown(function() {
					
						$project_content.css({
							"margin-top": $title_block.outerHeight()
						});	
				});*/
				
				$project_tools.css({
					position: "fixed",
					bottom: "-60px",
					top: "auto"
				});
			}
			
			if (document_scrollTop > stopThisTrain) {
		
				$tools_container.css({
					"position": "absolute",
					"top": new_top_setting-4
				});
				/*$title_block.css({
					"position": "absolute",
					"top": new_top_setting-4				
				});*/
				$grey_top_freeze.css({
					position: "absolute",
					top: new_top_setting + initial_location-4,
					"z-index": 50,
				});
			}
		
			if (document_scrollTop > ($content_outerHeight + initial_location + 30 - $(window).height())) {
				$project_tools.css({
					position: "absolute",
					bottom: "auto",
					//top: $content_outerHeight + initial_location-4,
					top: ($content_outerHeight - 80)
				});
			}
			
			
		});
		
		
		
	});
</script>

<script type="text/javascript">
    $(".comments ul li p a#a_delete_comment").live("click", 
        function (event){
            var comment_id = $(this).attr("tag");
            var params = { comment_id: comment_id };
            if (window.confirm("Are you sure you want to delete this comment?"))
                $.ajax
                (
                    {
                        type: 'POST',
                        url: "/people/project_delete_comment",
                        data: params,
                        success: function (data) {
                            if (data.status == "ok")
                            {
                                $("#" + comment_id).detach();
                                $("#" + comment_id).detach();
                                $("aside#comments h2").text("View all " + $("#comment_ul li").length + " comments");
                            }
                            else
                                alert(data.error_message);
                        },
                        error: function (e) {
                            alert(e);
                        },
                        dataType: "json"
                    }
                );
        }
    );

	$(window).resize(function() {
		//printf($("#lb_c").offset().top + " left: " + $("#lb_c").offset().left);
		//printf("window: " + $(window).width()/2);
	});
    $("#add_comment").click(function () {
        var p_comment = $('textarea#comment').val();
        var param = { project_id: id, comment: p_comment, type : 3, specialty : s, user_id_from : uf, user_id_to : ut };
        project_comment_add(param);
        return false;
    });

        function project_comment_add(param) {
            $.ajax({
                type: 'POST',
                url: "/People/project_comment_add",
                data: param,
                success: function (data) {
                    if(data.status=='ok')
                    {
                        var item = data.project_comment;
                        var logged_user_id = ''
                        var _html = '';
                        _html += '<li id="'+ item.comment_id +'">';
                        _html += '<p class="meta">';
                        _html += '<strong><a href="/' + item.url_name + '">' + item.user_name + '</a></strong> <em>' + item.date_posted_string;

                        if(false && (415312 == 0 || item.user_id == 0))
                            _html += '&nbsp' + '<a href="#" onclick="return false;" class="remove" id="a_delete_comment" tag="' + item.comment_id + '">Delete</a>';

                        _html += '</em></p>';
                        _html += '<p>';
                        _html += item.comment + '</p>';
                        _html += '<img src="' + item.avatar_image + '" />';
                        _html += '</li>';
                    
                      
                      $("#comment_ul").prepend(unescape(_html));
                      $("#comment_ul_all_asc").append(unescape(_html));

                      if(!opened)
                        $("aside#comments h2").text("View all " + $("#comment_ul li").length + " comments");

                        $('textarea#comment').val('');
                    }
                    else{
                        alert(data.error_message);
                    }
                },
                dataType: "json"
            });
        }
</script>

    
        
<script type="text/javascript" src="/javascripts/jquery.mpgSlider.js"></script>
<script type="text/javascript">
    var id = 805167;
    var uf = 0;
    var ut = 415312;
    var s = 15;
    
    $(document).ready( function() {
        update_statii(id, 1, s, uf, ut);
    });
    
    var opened = false;

    $(function () 
    {
        var comments_total = "5";
		
        var default_favoritor = {
            total_showing: 18,
            next_btn_selector: "#following_next",
            prev_btn_selector: "#following_prev",
            ul_selector: "favorites_ul",
            show_count_selector: "span#favorites_h1",
            default_avatar_path: "/images/avatar-default.gif",
            root_path: 'http://www.coroflot.com',
            action_name: "/people/get_project_job_seeker_favoritors",
            action_query_string: "project_id=" + '805167',
			left_position: 0
        };
        $("#favorites_cont").mpgSlider(default_favoritor);

		// coments hide/show
		

		var hide_height = 0;
		var full_height = 0;
		$("ul#comment_ul li").each(function(index, value) {
			if (index < 3) {
				hide_height += $(this).outerHeight();
			}
			full_height += $(this).outerHeight();								 
		});
		
		$("ul#comment_ul").css({
			height: hide_height,
			overflow: "hidden"
		});
		$("aside#comments span").hide();
        $("ul#comment_ul_all_asc").hide();

		if (comments_total > 3) {
			$("aside#comments h2").text("View all " + comments_total + " comments");
			$("aside#comments h2").click(function() {	
			
				if (!opened) {
					$("ul#comment_ul").animate({
						height: full_height,
						overflow: "hidden"
					}, function() { 
                        $("ul#comment_ul").hide();
                        $("ul#comment_ul_all_asc").show();
                        
					});
					opened = true;
					$(this).text("Show fewer comments");
					
				} else {
				
					$("ul#comment_ul").animate({
						height: hide_height,
						overflow: "hidden"
					}, function() { 
                        $("ul#comment_ul_all_asc").hide();
                        $("ul#comment_ul").show();                       
					});
					opened = false;
					$(this).text("View all " + $("#comment_ul li").length + " comments");
				}
			
			});
			
		} else if (comments_total == 0) {
			$("aside#comments h2").text("Be the first to comment on this project");
               
	    } else {
			$("aside#comments h2").text("Comments");
		}
			
			//printf($("ul#all_work").get(0));
			if ($("ul#all_work").length == 0) {
				//$("li#has_more_work").hide();
				//$("h2#all_work_title").hide();
				
			}

		
	});

    function update_statii(project_id, activity_type, specialty, user_id_from, user_id_to)
        {
                var param = { project_id: project_id, activity_type: activity_type, specialty: specialty, user_id_from: user_id_from, user_id_to : user_id_to  };
                $.ajax({
                    type: 'POST',
                    url: "/people/project_statii_update",
                    data: param,
                    dataType: "json",
                    success: function (data) {
                    }
                });     //END $.ajax({  
        }

			
</script>


</section>

<script type="text/javascript">
    if('' == '1' && !false)
        window.location = "#comments";

    var is_favorite = false;
    $("#a_favorite").click
    (
        function () {
            if(is_favorite || false)
                return;

//            if(!false)
//            {
//                alert('You are logged out, Please re-login your account and try again.');
//                return;
//            }

//            if(false)
//            {
//                alert('Please complete your profile.');
//                return;
//            }

            var params = { project_id: '805167' };

            $.ajax(
                {
                    type: 'POST',
                    url: "/people/favorite_project",
                    data: params,
                    success: function (data) {
                        if (data.status == "ok")
                        {
                            $("#a_favorite").text("Favorited");
                            is_favorite = true;
                        }
                        else
                            window.location = "http://" + window.location.host + window.location.pathname + "?error=" + data.error_message;
                    },
                    error: function (e) {
                        alert(e);
                    },
                    dataType: "json"
                }
            );
        }
    );
</script>

