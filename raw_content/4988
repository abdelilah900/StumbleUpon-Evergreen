<script type="text/javascript">

    function on_save_portfolio() {
        $("#a_save").text("Saved");
        $("#a_save").attr("href", "#");
        $("#a_save").attr("rel", "");
    }

    var is_featured = false;
    $("#a_feature").click(
        function () {
            if (is_featured)
                return;

            if(!false)
            {
                alert('You are logged out, Please re-login your account and try again.');
                return;
            }

            var url, params;

            if ('project' == "portfolio") {
                url = "/people/feature_job_seeker";
                params = { job_seeker_id: '415312' };
            }
            else if ('project' == "project") {
                url = "/people/feature_project";
                params = { project_id: '805167', project_owner_id: '415312' };
            }

            if (url == null || params == null)
                return;

            $.ajax(
                {
                    type: 'POST',
                    url: url,
                    data: params,
                    success: function (data) {
                        if (data.status == "ok") {
                            $("#a_feature").text("Featured");
                            is_featured = true;
                        }
                        else
                            alert(data.error_message);
                    },
                    error: function (e) {
                        alert(e);
                    },
                    dataType: "json"
                }
            );
        }
    );

    var is_folowed = false;
    $("#a_follow").click(
        function () {
            if (is_folowed || false)
                return;

//            if('415312' == '0')
//            {
//                alert('You cannot follow yourself.');
//                return;
//            }

//            if(!false)
//            {
//                alert('You are logged out, Please re-login your account and try again.');
//                return;
//            }

            var params = { job_seeker_id: '415312' };
            
            $.ajax(
                {
                    type: 'POST',
                    url: "/people/follow_job_seeker",
                    data: params,
                    success: function (data) {
                        if (data.status == "ok") {
                            $("#a_follow").text("Following");
                            is_folowed = true;
                        }
                        else
                            window.location = "http://" + window.location.host + window.location.pathname + "?error=" + data.error_message;
                    },
                    error: function (e) {
                        alert(e);
                    },
                    dataType: "json"
                }
            );
        }
    );
</script>    
