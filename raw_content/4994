				<script type="text/javascript"> 
			//Søkefelt-kode.
			$(document).ready(function() {
				$('#searchBtn').click(doSearch);
				
				$('#searchTxt').keypress(function(e){
					if(e.which == 13){
						doSearch(e);
					}
				});
			});
			
			function doSearch(e){
				var checked = $('input[name=engine]:radio:checked');
				window.location = 'http://www.dagbladet.no/cgi-bin/multisearch/index.cgi?engine=' + checked.val()+'&query=' + $('#searchTxt').val();
				return false;
			}
		</script> 
