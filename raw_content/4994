		document.getElementById("videoplayer_button_over_1612804063001").style.visibility = "hidden";
    });
    $("img#videoplayer_button_normal_1612804063001").fadeTo("slow", 0, function() {    	
		document.getElementById("videoplayer_button_normal_1612804063001").style.visibility = "hidden";
    });	
	
	$("img#videoplayer_splash_1612804063001").fadeTo("slow", 0, function() {    	
		document.getElementById("videoplayer_splash_1612804063001").style.visibility = "hidden";
		document.getElementById("videoplayer_button_over_1612804063001").style.visibility = "hidden";
		document.getElementById("videoplayer_button_normal_1612804063001").style.visibility = "hidden";
		document.getElementById("videoplayer_video_1612804063001").innerHTML = pHtml;

				
		$.fn.videoplaza = function(){
				
			var toppbanner = {
				element: $("#ad1-top"),
				content: $("#ad1-top").html(),
				changed: false
			}
			

			if(1){
				if ( !$("#ad1-right").length ) { // hvis ad1-right ikke finnes
					$("div#header").append('<div id="ad1-right" class="ad adDiv down" style=""></div>');
				}
			}

			var skyscraper = {
				element: $("#ad1-right"),
				content: $("#ad1-right").html(),
				changed: false
			};
			
			/* Finn ut om det kj¿rer en L */
			var l = ( toppbanner.element.find('iframe').width() == 980 ) ? false : true;
			
			var interval = setInterval(function(){
				if( skyscraper.content != skyscraper.element.html() ){
					/* Fjern intervalet nr endringen i dom har skjedd. */
					clearInterval(interval);
					
					
					var vpLTimer = setInterval(function(){
					    /* Sjekk om det kommer L fra videoplaza.*/
						var vl = ( toppbanner.content != toppbanner.element.html() ) ? true : false;
						
						
						if(l){
							if(!vl){
								/* Det kj¿res en L, men det kommer kun skyscraper fra videplaza */
								toppbanner.element.html('');
								skyscraper.element.addClass('down');
								skyscraper.element.addClass('format-l');
							}
						} else {
							if(vl){
								/* Det kj¿res ikke L, men videoplaza leverer dette. */
								skyscraper.element.removeClass('down');
								skyscraper.element.removeClass('format-l');
							}
						}
						clearInterval(vpLTimer);
					}, 100);
					
					
				}
			}, 200);
			
		}
		
		$('#myExperience1612804063001').videoplaza();
		
				
		brightcove.createExperiences('0', 'myExperience1612804063001');
    });
    
    
}

