<script type="text/javascript" src="http://www.dagbladet.no/konto/api/xdr/js/jquery-facebox-1.2.js"></script><script language="javascript" type="text/javascript">

	if(readCookie("css") && readCookie("css") == 'ipad') {

		$('head').append('<link rel="stylesheet" href="http://www.dagbladet.no/css3/i_0.1.css" type="text/css" />');
	}
	
	function readCookie(name) {
		var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return false;
	}
</script>


