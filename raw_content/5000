	<input type="hidden" value="collectionItem" name="_fields"/></form><script type="text/javascript" language="javascript">
<!--
// Code generated to validate collectionpromo form
if (window.FormValidator) {
	document.forms['collectionpromo'].validator = new window.FormValidator(document.forms['collectionpromo']);
	document.forms['collectionpromo'].validator.add('collectionItem', false, null, 2, null, 'collectionItem', null);
}
