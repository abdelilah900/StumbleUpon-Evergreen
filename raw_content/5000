<script type="text/javascript" src="http://www.latimes.com/includes/projects/js/jquery/jquery-1.4.2.min.js"></script>

<script type="text/javascript" src="http://latimesblogs.latimes.com/common/shareify.js"></script>
	
<script type="text/javascript">
jQuery.noConflict();

var RelPath = window.location.href.slice(32).split('/');

    jQuery(document).ready(function(){
        jQuery(".share_button").shareify({
            'image_dir': 'http://www.latimes.com/includes/projects/js/jquery/jquery-shareify/img/'
        });
        
/******************************************
Ad Refresh
******************************************/
		// Grab the container divs for the iframes		
		var bannerCont = document.getElementById('skyContainer');
		var cubeCont = document.getElementById('cubeContainer');
		var cubeCont2 = document.getElementById('cubeContainer2') || false;
		var skyCont = document.getElementById('leaderContainer');
		
		// Now, we have to make a copy of what's in the container divs so we can wipe them on refresh
		// This is necessary because friendly iframes allow the expandable ads to write code in the DOM of the parent document. When we refresh, we have to wipe any extra code outside of the iframes to make way for the new ad
		var bannerHTML = bannerCont.innerHTML;
		var cubeHTML = cubeCont.innerHTML;
		var cubeHTML2 = cubeCont2.innerHTML;
		var skyHTML = skyCont.innerHTML;
		
		// Now, we have a nice to function to reset the Ad HTML to the original version
		function clearAds() {
			bannerCont.innerHTML = bannerHTML;
			cubeCont.innerHTML = cubeHTML;
			skyCont.innerHTML = skyHTML;
			if (cubeCont2 !== false){
				cubeCont2.innerHTML = cubeHTML2;
			}
		}
	
		// And we can refresh the SRC on the ad iframes to load a new ad
		function loadADS() {
			document.getElementById("BannerAd").src = 'http://latimesblogs.latimes.com/' + RelPath[0] + '/leaderboard.html';
			document.getElementById("CubeAd").src = 'http://latimesblogs.latimes.com/' + RelPath[0] + '/cubead.html';
			document.getElementById("SkyAd").src = 'http://latimesblogs.latimes.com/' + RelPath[0] + '/skyad.html';
			if (cubeCont2 !== false){
				document.getElementById("CubeAd2").src = 'http://latimesblogs.latimes.com/' + RelPath[0] + '/cubead2.html';
			}
		}

		// Finally, we can actually wait until document.ready to set the SRC of the ad frames (there is no SRC by default), making the ads the last thing loaded on the page.
		loadADS();
        
    });
</script>

