	var cnnTrackPath;
	var isCanonicalPresent = false;
	var isFortune = false;
	var cnnLinkTags = document.getElementsByTagName("head")[0].getElementsByTagName("link");
	for (var linkTagCount = 0; linkTagCount < cnnLinkTags.length; linkTagCount++) {
		try {
			if (cnnLinkTags[linkTagCount].getAttribute("rel") == "canonical") {
				isCanonicalPresent = true;
				cnnTrackPath = cnnLinkTags[linkTagCount].getAttribute("href");
				break;
			}
		}catch(e){}
	}
	if ( !isCanonicalPresent ) {cnnTrackPath = window.location.href;}
	<!-- set path and title for galleries -->
	var cnnTitle = document.title;
	if (cnnTrackPath.match("galleries")) {
		var galleryUrlMatch = /.*\//;
		cnnTrackPath = cnnTrackPath.match(galleryUrlMatch);
		var hedMatch = /[^\-]*/;
		cnnTitle = cnnTitle.match(hedMatch);
	}
	var cnnSection = '';
	if (typeof jsmd!='undefined') {
		var cnnMeta=jsmd.mdata.business.mny.section;
		cnnSection=cnnMeta[1];
	}
	if (typeof cnnSource != 'undefined') {
		if (cnnSource=="FORT") {
			var isFortune = true;
		}
	}
	if (window.location.href.match(".fortune")||isFortune) {
		cnnSection = cnnSection + ',fortune';
	}
	var cnnByline = '';
	var cnnAuthor;
	if (typeof cnnAuthor != 'undefined') {cnnByline=cnnAuthor;}
	var _sf_async_config={};
	/** CONFIGURATION START **/
	_sf_async_config.uid = 16819;
	_sf_async_config.domain = 'money.cnn.com';
	_sf_async_config.path = cnnTrackPath;
	_sf_async_config.title = cnnTitle;
	_sf_async_config.sections = cnnSection;
	_sf_async_config.authors = cnnByline;
	/** CONFIGURATION END **/
