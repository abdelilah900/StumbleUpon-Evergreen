<input type="hidden" name="domain" value=".cbssports.com">
<input type="hidden" name="poll_arena" value="other">
<input type="hidden" name="poll_title" value="">
<input name="answer" value="Yes" id="pollResponse_1" type="radio" onClick="javascript:selectPoll(this.value);">
<label for="pollResponse_1">Yes</label>
<br />
<input name="answer" value="No" id="pollResponse_2" type="radio" onClick="javascript:selectPoll(this.value);">
<label for="pollResponse_2">No</label>
<br />
<input name="answer" value="Maybe in Canada" id="pollResponse_3" type="radio" onClick="javascript:selectPoll(this.value);">
