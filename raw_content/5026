    DW.customClickHandler=function(e){var target=DW.getEventTarget(e);var linkObj=DW.getLinkObject(target);if (null==linkObj || DW.ignoreClick(linkObj)){return;}var tag=DW.buildTag(target);if (DW.isInternalLink(linkObj)==true) {var params={linkObj: linkObj,target:target,tag:tag};CBSi.util.DW.CookieTrack.setCookie(params);}else{DW.trackClickInBackground(linkObj,tag,target.nodeName,e.type);}};DW.trackClicks(DW.customClickHandler);</script><noscript><img src="http://dw.com.com/clear/c.gif?ts=176641328032198&sid=175" border="0" height="1" width="1"><img src="http://b.scorecardresearch.com/b?c1=2&c2=3000023&c3=&c4=&c5=&c6=&c15=&cv=1.3&cj=1" style="display:none" width="0" height="0" alt="" /></noscript>
