<script type="text/javascript">
var poll_http = new getHTTPObject();
var pollValue;
function submitPoll(){
    var form = document['poll013012_superbowl'];
    var poll_name_override = "";
    var poll_ceid = "";

    if( pollValue ){
        answer = pollValue;
        pollname = form.pollname.value;
        domain = form.domain.value;
        var uri = "answer="+answer+"&pollname="+pollname+"&domain="+domain;
        uri += "&poll_arena="+ form.poll_arena.value;
        uri += "&poll_voted=true";

        if (poll_name_override != 0) uri += "&poll_name_override="+poll_name_override;
        if (poll_ceid != 0) uri += "&poll_ceid="+poll_ceid;

        sndReq({uri:uri});
    }
    else{
        alert('You forgot to vote...');
    }
}
function selectPoll(value){
    pollValue = value;
}
function sndReq(pollArgObj){
            poll_http.open('POST','/poll_submit',false);
            
            poll_http.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
            var now = new Date();
            pollArgObj.uri+='&s='+Math.floor(now.getTime()/1000);
            poll_http.send(pollArgObj.uri);
            handleResponse();
}
function handleResponse(){
    if (poll_http.readyState == 4) {
        if (poll_http.status == 200 || poll_http.status == 304) {
            var pollreply = document.getElementById("pollContent");
            pollreply.innerHTML = poll_http.responseText;
            cbsiSetupAdDiv("cbsiad162_100", cbsiad162_100); cbsiGetDeferredAds(["cbsiad162_100"]);
        }
    }
}
</script><div class="featureTitle">Poll</div><div class="featureComponent stdPad" id="pollContent"><a name="pollTop" id="pollTop"></a><h4>Should the NFL ever consider holding the Super Bowl in a foreign country?</h4><form name="poll013012_superbowl" id="pollForm">
<form name="013012_superbowl" id="pollForm">
