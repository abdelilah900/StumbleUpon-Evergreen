<script>
    var ytplayer = null;

    function init_video()
    {
        var params = { allowScriptAccess: "always", wmode: "transparent" };
        var atts = { id: "myytplayer" };
        var id = '_WN7o1rsKHw';
		var install = "/flash/playerProductInstall.swf";
		var ver = '10.3.0';
        var link = 'http://www.youtube.com/v/' + id + '&enablejsapi=1&playerapiid=ytplayer';
        swfobject.embedSWF( link, "videoContainer", "480", "360", ver, install, null, params, atts);
    }

    init_video();

	function onYouTubePlayerReady(playerId)
	{
		ytplayer = document.getElementById("myytplayer");
		ytplayer.playVideo();
	}


function HandleProfilePanel() {
	$.get('/ws/userinfo/' + ent_name, function(obj) {
		var user_image = '<a href="/' + ent_name + '">' +
			'<img border="0" style="width:27px;height:27px;" src="' + 
				obj.picture_large.replace('_L.', '_SQ.') + '" /></a>';

		var title_text = 'Posts';
		if (ent_type=='weak') title_text = 'Weaks';
		if (ent_type=='wicked') title_text = 'Wicked';
	
		var title = '<span class="wicked" style="font-size:28px;font-weight:bold;">' + title_text + '</span>';

		$('#RightPanelTitleBox').html(user_image + '&nbsp;&nbsp;&nbsp;' + title);
	});

	PopulateProfilePanel();
}

function CreateCrowdUrl(crowd_id) {
	var ret = '/crowd/view/' + crowd_id + '?sort=0&ent=' + ent + '&name=' + ent_name + '&type=' + ent_type;
	return ret;
}

function CreateImgDiv(line, i) {
	var thumb;

	if (line.crowd_type==1)
		thumb = 'http://img.youtube.com/vi/' + line.SIR_path + '/1.jpg';
	else
		thumb = '/sdal/sir/' + line.SIR_path.replace('_L.', '_S.');

	var bg = "background-image: url('" + thumb + "');";
	
	var crowd_url = CreateCrowdUrl(line.id);
	var click_event = 'onclick="location.href=\'' + crowd_url + '\'"';
	var ret = '<div id="InnerImgDiv_' + i + '" ' + click_event + ' style="width:0px;' + bg + '" class="ImgDiv"></div>';

	return ret;
}

//for wicked and weak
function PopulateProfilePanel_Marked() {		
	var url = '/ws/getmarkedcrowdspanel/' + ent_name + '/' + ent_type_id + '/1532/';
	$.get(url, function(obj) {

		PopulatePanel(obj);		
		
		if (obj.one_after_crowd.length>0) {
			var next_item = obj.one_after_crowd[0].id;
			var click_event = 'onclick="location.href=\'' + CreateCrowdUrl(next_item) + '\'"';
			var arr_left = '<img style="cursor:hand;cursor:pointer;" title="Previous" src="/img/site/arr_left.png" ' + click_event + ' />';
			$('#arr_left_cell').html(arr_left);	
			NextCrowd = next_item;
		} else {
			var arr_left = '<img style="opacity:0.4;" title="Previous" src="/img/site/arr_left.png" />';
			$('#arr_left_cell').html(arr_left);
			NextCrowd = 0;
		}

		if (obj.lines.length>0) {
			var pre_item = obj.lines[0].id;
			var click_event = 'onclick="location.href=\'' + CreateCrowdUrl(pre_item) + '\'"';
			var arr_right = '<img style="cursor:hand;cursor:pointer;" title="Next" src="/img/site/arr_right.png" ' + click_event + ' />';
			$('#arr_right_cell').html(arr_right);	
		} else {
			var arr_right = '<img style="opacity:0.4;" title="Next" src="/img/site/arr_right.png" />';
			$('#arr_right_cell').html(arr_right);	
		}
	});
}

function PopulateProfilePanel_Posts() {
	var url = '/ws/getpostscrowdspanel/' + ent_name + '/1532';
	$.get(url, function(obj) {

		PopulatePanel(obj);		
		
		if (obj.one_after_crowd.length>0) {
			var one_after = obj.one_after_crowd[0].id;
			var click_event = 'onclick="location.href=\'' + CreateCrowdUrl(one_after) + '\'"';
			var arr_left = '<img style="cursor:hand;cursor:pointer;" title="Previous" src="/img/site/arr_left.png" ' + click_event + ' />';
			$('#arr_left_cell').html(arr_left);	
			NextCrowd = one_after;
		} else {
			var arr_left = '<img style="opacity:0.4;" title="Previous" src="/img/site/arr_left.png" />';
			$('#arr_left_cell').html(arr_left);	
			NextCrowd = 0;
		}

		if (obj.lines.length>0) {
			var click_event = 'onclick="location.href=\'' + CreateCrowdUrl(obj.lines[0].id) + '\'"';
			var arr_right = '<img style="cursor:hand;cursor:pointer;" title="Next" src="/img/site/arr_right.png" ' + click_event + ' />';
			$('#arr_right_cell').html(arr_right);	
		} else {
			var arr_right = '<img style="opacity:0.4;" title="Next" src="/img/site/arr_right.png" />';
			$('#arr_right_cell').html(arr_right);	
		}
	});
}

function PopulateProfilePanel() {
	if (ent=="profile") {
		if ((ent_type_id==0) || (ent_type_id==1)) {
			PopulateProfilePanel_Marked();
			return;
		}
		PopulateProfilePanel_Posts();
		return;
	}
}

//===================================================================================================

function RunBubbles(id) {
	$('.ImgDiv').HideAllBubblePopups();
	$('.ImgDiv').css('border', '1px solid #000000');
	$('.ImgDiv').css('background-position', 'center');
	$('#InnerImgDiv_' + id).css('background-position', 'left top');
	$('#InnerImgDiv_' + id).css('border', '2px solid #00adef');
	$('#InnerImgDiv_' + id).ShowBubblePopup();
}


function PopulatePanel(obj) {

	PrevCrowd = -1;
	if (obj.lines.length>0){
		PrevCrowd = obj.lines[0].id;
	}
	
	for (var i=obj.lines.length-1; i>=0; i--){
		$('#img'+i).html(CreateImgDiv(obj.lines[i], i));

		$('#InnerImgDiv_' + i).delay(i*300).animate({
		    width: 110
		  }, 300 );
	}
	
	var start = 0;
	var delta = 2000;
	setTimeout(function() {	RunBubbles(0); }, start + 6 * delta);
	setTimeout(function() {	RunBubbles(1); }, start + 5 * delta);
	setTimeout(function() {	RunBubbles(2); }, start + 4 * delta);
	setTimeout(function() {	RunBubbles(3); }, start + 3 * delta);
	setTimeout(function() {	RunBubbles(4); }, start + 2 * delta);
	setTimeout(function() {	RunBubbles(5); }, start + 1 * delta);

	setTimeout(function() {	
			$('.ImgDiv').HideAllBubblePopups(); 
			$('.ImgDiv').css('border', '1px solid #000000');
		}, 
		start + 7 * delta);

	
	
			
	for (var i=obj.lines.length-1; i>=0; i--){
		var title = obj.lines[i].title;
		var text_len = 35;
		if (title.length > text_len) title = title.substr(0, text_len-3) + '...';
		
		$('#InnerImgDiv_' + i).CreateBubblePopup();		
	
		$('#InnerImgDiv_' + i).SetBubblePopupOptions({
			position		: 'top',
			align   		: 'left',
			tail	 		: {align: 'center'},
			innerHtml		: '<div style="width:110px;">' + title + '</div>',
			innerHtmlStyle	: { color:'#000000',  'text-align':'left' },
			themeName		: 'azure',
			themePath		: '/js/jquerybubblepopup-theme',
			alwaysVisible	: false,
			closingDelay	: 200
		});
	}
}


function PopulateCategoryPanel() {
	var url = '/ws/getcategorycrowdspanel/' + ent_name + '/1532/0';

	$.get(url, function(obj) {

		PopulatePanel(obj);		
		
		if (obj.one_after_crowd.length>0) {
			var one_after = obj.one_after_crowd[0].id;
			var click_event = 'onclick="location.href=\'' + CreateCrowdUrl(one_after) + '\'"';
			var arr_left = '<img style="cursor:hand;cursor:pointer;" title="Previous" src="/img/site/arr_left.png" ' + click_event + ' />';
			$('#arr_left_cell').html(arr_left);
			NextCrowd = one_after;
		} else {
			var arr_left = '<img style="opacity:0.4;" title="Previous" src="/img/site/arr_left.png" />';
			$('#arr_left_cell').html(arr_left);	
			NextCrowd = 0;
		}
			
		if (obj.lines.length>0) {
			var click_event = 'onclick="location.href=\'' + CreateCrowdUrl(obj.lines[0].id) + '\'"';
			var arr_right = '<img style="cursor:hand;cursor:pointer;" title="Next" src="/img/site/arr_right.png" ' + click_event + ' />';
			$('#arr_right_cell').html(arr_right);	
		} else {
			var arr_right = '<img style="opacity:0.4;" title="Next" src="/img/site/arr_right.png" />';
			$('#arr_right_cell').html(arr_right);	
		}

		//AnimatePanel(obj);
		
	});
}

function AnimatePanel(obj) {
	for (var i=obj.lines.length-1; i>=0; i--) {
		$('#img'+i).css('border', '1px solid red');
		$('#img'+i).css('border', '1px dashed #aaaaaa');
	}	
	
}

function HandleCategoryPanel() {
	var url = '/ws/categorybylink/' + ent_name;
	$.get(url, function(obj) {
		var name = 'Everything';
		if (obj.length>0) name = obj[0].name;
		var title = '<span class="wicked" style="font-size:28px;font-weight:bold;">' + name + '</span>';
		$('#RightPanelTitleBox').html(title);
	});
	PopulateCategoryPanel();
}

function GetRightPanelInfo() {

	if (ent=='profile') 
		HandleProfilePanel();
	else if (ent=='category') 
		HandleCategoryPanel();
	else
		alert('Error 6001: invalid entity.');
}



function GetCreatorInfo() {
	$.get('/ws/userinfobyid/268', function(obj) {
		var profile_url = (obj.username=='anonymous') ? '#' : '/' + obj.username;
		var creator_link = '<a href="' + profile_url + '">' + obj.username + '</a>';
		$("#created_by").html(creator_link);
		$("#when").html(When('2012-04-06 02:46:27','2012-04-07 09:56:02'));
		$("#crowd_views").html('107 views');
		$("#members_plus").html('5 wicked');
		$("#members_minus").html('1 weak');

		ent = '';
		ent_name = '';
		ent_type = '';

		ent_type_id = 2;	//for posts
		if (ent_type=='weak') ent_type_id = 0;
		if (ent_type=='wicked') ent_type_id = 1;

		if (ent == '') {
			/*
			ent = 'profile';
			ent_name = obj.username;
			ent_type = 'posts';
			*/
			ent = 'category';
			ent_name = 'everything';
			ent_type = '';
		}
		
		GetRightPanelInfo();
		
	});
}

function UpdateCrowdInfo() {
	var is_login = (0 > 0);

	$.get('/ws/crowdinfo/1532', function(obj) {
		$("#members_plus").html(obj.members_plus + ' wicked');
		$("#members_minus").html(obj.members_minus + ' weak');


		
			$('#TwoButtonsDiv').hide();
			$('#UnwickedBtn').hide();
			$('#UnweakBtn').hide();
			$('#SingleButtonDiv').hide();

			if (is_login) {
				if (obj.join_leave=="leave_plus") {
					$('#UnwickedBtn').show();
					$('#SingleButtonDiv').show();
					return; 
				}
				
				if (obj.join_leave=="leave_minus") {
					$('#UnweakBtn').show();
					$('#SingleButtonDiv').show();
					return;
				}
				$('#TwoButtonsDiv').show();
			}

			if (!is_login) {
				if (is_first_update) {
					$('#TwoButtonsDiv').show();
					is_first_update = false;
				}
			}

		

		if (obj.ancestor_crowd_id!=null) {
			var crowd_id = obj.ancestor_crowd_id;
			$.get('/ws/crowdinfo/' + crowd_id, function(obj) {
				var ancestor_img = '<a href="/crowd/view/' + crowd_id + '"><img style="width:200px;" src="/sdal/sir/' + obj.SIR_path.replace('_L.', '_M.') + '" border="0" /></a>';
				$('#AncestorImg').html(ancestor_img);
				$('#AncestorDiv').show();
			});
		}
		
	});
}

function JoinPlusCrowd() {
	/*
			location.href='/account/login';
		*/
	$.get('/ws/join/1532/1', function(obj) {
		UpdateCrowdInfo();
	});
}	

function JoinMinusCrowd() {
	/*
			location.href='/account/login';
		*/
	$.get('/ws/join/1532/0', function(obj) {
		UpdateCrowdInfo();
	});
}

function LeaveCrowd() {
	$.get('/ws/leave/1532', function(obj) {
		UpdateCrowdInfo();
	});
}

</script>

