<script>

//vars will be set after getting creator info inside GetCreatorInfo()
//note: in case of direct link, the creator will be the ent_name by default.
var ent = '', ent_name = '', ent_type = '', ent_type_id=2;	//2 for posts

var NextCrowd = -1;
var PrevCrowd = -1;


var is_first_update = true;

function ChangeCrowd() {
	if (NextCrowd < 1) return;
	location.href = CreateCrowdUrl(NextCrowd);
}

function ChangePrevCrowd() {
	if (PrevCrowd < 1) return;
	location.href = CreateCrowdUrl(PrevCrowd);
}

document.onkeyup = KeyCheck;
function KeyCheck(e) {
   var KeyID = (window.event) ? event.keyCode : e.keyCode;
   switch(KeyID) {
      case 37:
    	  ChangeCrowd();
	      break;
      case 39:
    	  ChangePrevCrowd();
	      break;
   }
}


$(document).ready(function() {

		$('#TheImage, #NextBtn').bind('mousemove', function(e){
		    $('#click_me').css({
		       left:  e.pageX+10,
		       top:   e.pageY+25,
		       display: 'block'
		    });
		}).bind('mouseout', function(e){
		    $('#click_me').css({display: 'none'});
		});

	
	//$('#indiLikeBox').html(MakeLikeBoxCell_IndiPage());

	GetCreatorInfo();

    $("#report-modal").dialog({
    	dialogClass: "flora",
       	autoOpen: false,
        width: 550,
        height: 350,
        modal: true,
		resizable: false,
		draggable: false,
        hide: 'blind',
        show: 'blind',
        create: function (event, ui) {
            //$(".ui-dialog-titlebar").hide();
        },
        open: function (event, ui) {
	        $(this).parents(".ui-dialog:first").find(".ui-widget-header")
            .removeClass("ui-widget-header").addClass("title_class");
            $("#report-body").focus();
        },
        buttons: {
            "Report Post": function () {
                $(this).dialog("close");
				var crowd_id = 1532;
                var reason = $("#report-modal input[type='radio']:checked").val();
				var body =  encodeURIComponent($("#report-body").val());
				var json = {crowd_id: crowd_id, reason: reason, body: body};

				var str = objtostring(json);
				$.post("/ws/report/", str,
					 function(data){
	 					if (data.status==1) 
	 						$( "#thanks" ).dialog( "open" );
	 					else {
	 						alert(data.msg);
	 					}
					 });
            },
            "Cancel": function () {
                $(this).dialog("close");
            }
        },
        close: function () {
        }
    });
    $('.flora.ui-dialog').css({position:"fixed"});  


	$.fx.speeds._default = 1000;
	$(function() {
		$( "#thanks" ).dialog({
			open: function(event, ui){ 
		        $(this).parents(".ui-dialog:first").find(".ui-widget-header")
		            .removeClass("ui-widget-header").addClass("title_class");
		    },
	        buttons: {
	            "Close": function () {
	                $(this).dialog("close");
	            }
	        },
    		autoOpen: false,
			show: 'blind',
			hide: 'blind',
			width: 550,
			height: 250, 
			modal: true,
			resizable: false,
			draggable: false
		});
	});

	UpdateCrowdInfo();

	SetTweakBubble();
});



function SetTweakBubble() {
	var options = {
		position		: 'left',
		selectable		: false,
		align   		: 'center',
		tail	 		: {align: 'center'},
		innerHtml		: 'Edit image',
		innerHtmlStyle	: { color:'#000000',  'text-align':'left' },
		themeName		: 'azure',
		themePath		: '/js/jquerybubblepopup-theme',
		alwaysVisible	: false,
		closingDelay	: 0
	};

	$('#TweakBtn').CreateBubblePopup();		
	$('#TweakBtn').SetBubblePopupOptions(options);
}

</script>


