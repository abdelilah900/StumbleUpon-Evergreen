	});

function SwitchToPage(val) {
	if (val=="home") {
		location.href = "/";
	}
	if (val=="faq") {
		location.href = "/about/faq";
	}
	
	if (val=="settings") {
		location.href = "/user/settings";
	}

	if (val=="logout") {
		location.href = "/index/logout";
	}
}
