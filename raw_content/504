<script type="text/javascript">

	function DoSearch() {
	    var phrase = jQuery.trim($('#Phrase').val());
	    $('#Phrase').val(phrase);
	
	    if ($('#Phrase').val().length < 2) phrase = "ShowAll";
	    $("#msg").html('');
	    
	    var url = '/search/' + phrase;
	    location.href = url;
	}

	function Blinker() {
		var slice = 300;
		setTimeout("$('#RegisterLink').css('color', 'black');",		1 * slice);
		setTimeout("$('#RegisterLink').css('color', '#00adec');",	2 * slice);
		setTimeout("$('#RegisterLink').css('color', 'black');",		3 * slice);
		setTimeout("$('#RegisterLink').css('color', '#00adec');",	4 * slice);
		setTimeout("$('#RegisterLink').css('color', 'black');",		5 * slice);
		setTimeout("$('#RegisterLink').css('color', '#00adec');",	6 * slice);
	}


	$(document).ready(function($){
		
		$('#mega-menu').dcMegaMenu({
			rowItems: '2',
			speed: 'fast',
			effect: 'fade',
			event: 'hover'
		});

		$('#mega-menu-1').dcMegaMenu({
			rowItems: '1',
			speed: 'fast',
			effect: 'fade',
			event: 'hover'
		});
		$('#mega-menu-2').dcMegaMenu({
			rowItems: '2',
			speed: 'fast',
			effect: 'fade',
			event: 'hover'
		});


        $('#Phrase').bind('keypress', function(e){
       	   if ( e.keyCode == 13 ) {
        		  DoSearch();
       	   }
       	 });

