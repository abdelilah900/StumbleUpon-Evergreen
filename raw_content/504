	<script>

    var BackButton = {
	        "width": 27,
	        "height": 99
        }


	


		$(document).ready(function() {
			InitBackButton();
						
			var BackButton = $("#BackButton");

			$(window).scroll(function(){			
				$('#Footer').stop();
				BackButton.stop();
				if ($(document).scrollTop()>350) {
					$('#Footer').show();
					BackButton.show();
				} else {
					$('#Footer').hide();
					BackButton.hide();
				}
			});
			
			TooltipsHandler();
			PopupOpenHandler();
			PopupCloseHandler();

			var left_padding = (document.documentElement.clientWidth - $('#InnerFooter').width())/2;
			$('#InnerFooter').css('left',left_padding);

			
		});

	    function InitBackButton() {
	        var btn = $('#BackButton');
	        var h = ($(window).height() - BackButton.height) / 2;
	        h = (h < 0) ? 0 : h;
	        btn.css('right', '0');
	        btn.css('top', h);
	    }

        $(window).resize(function () {
        	InitBackButton();
        });
		

		
	</script>	
	
