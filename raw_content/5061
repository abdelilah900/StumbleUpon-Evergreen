<script type="text/javascript" charset="utf-8">
	// crummy javascript redirect since we 
	// can't do a real redirect with typepad
	var timeout = setTimeout(function() {
		timeout = null;
		var url = window.location.href;
		
		if (url.indexOf('swiss-miss.com') == -1) {
			url = url.replace('http://', '');
			var pos = url.indexOf('/');
			if (pos != -1) {
				url = url.substr(pos);
				url = url.replace('/weblog', '');
				window.location.href = 'http://www.swiss-miss.com' + url;
			}
		}
	}, 4000);
</script>
	
