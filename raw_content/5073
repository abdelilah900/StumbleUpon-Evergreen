<script type="text/javascript">
    function pageLoad() {

        //Hides hide/show Responses controls for comments with no responses
        $(".div_response_container").each(function() {
            $(".btnHideSingleResponse[title=" + this.title + "]").show();
            $(".show_hide_responses[title=" + this.title + "]").show();
        });

        //Toggles the comment response post div	    
        $(".btnResponse").click(function() {
            var divname = 'div.response_' + this.title;
            $(divname).toggle("slow");
        });

        //Toggles all responses
        $(".hideresponses").click(function() {
            $('.div_response_container').hide("slow");
            $('.response_article_link_icons').hide("slow");
            $(".showResponses").show();
            $(".hideresponses").hide();
            $(".div_response_container").each(function() {
                $(".btnShowSingleResponse[title=" + this.title + "]").show();
            });
            $(".btnHideSingleResponse").hide();
        });
        $(".showResponses").click(function() {
            $('.div_response_container').show("slow");
            $('.response_article_link_icons').show("slow");
            $(".showResponses").hide();
            $(".hideresponses").show();
            $(".div_response_container").each(function() {
                $(".btnHideSingleResponse[title=" + this.title + "]").show();
            });
            $(".btnShowSingleResponse").hide();
        });


        //Toggles individual comment's responses        
        $(".btnHideSingleResponse").click(function() {
            var elms = $("[name=" + this.title + "]")
            $(elms).hide("slow");
            $(this).hide();
            $(".btnShowSingleResponse[title=" + this.title + "]").show();
        });
        $(".btnShowSingleResponse").click(function() {
            var elms = $("[name=" + this.title + "]")
            $(elms).show("slow");
            $(this).hide();
            $(".btnHideSingleResponse[title=" + this.title + "]").show();
        });

    };
</script> 

