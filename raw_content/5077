 <!-- e hearst/collections/blogpromo_homepage.tpl -->		</div>
	
<!-- e src/business/widgets/hearst/collection/widget.tpl -->
	<div class="hst-freeform"><!-- mid:freeform.9621 -->
    	<script type="text/javascript">
var hst_customizable = new Object();
hst_customizable.orderkey = new Array();
hst_customizable.curorderkey = new Array();
hst_customizable.laststate = '';
var hst_customizable_cookie = 'hst_sfg_HpCustom_www';
function initializeCustomizables (customizables,customizableControls,customizableAnchor) {
    var totalCustomizables = customizables.length;
    customizables.each(function(custItemCnt) {  // find each item to include to customizable list
        var customizableParent = $(this); //.parent();
        $(this).find("h2").each(function() {  // assumes only one h2 tag and you know what assuming does!!!
            var customizableTitle = $(this).text();
            var customizableId = customizableTitle.replace(/&/g, "&amp;"); // normalize ampersand because it's not using complant entites
            customizableId = customizableId.replace(/[^\w]/g, "_"); // normalize id
            hst_customizable[customizableId] = customizableParent;  // keep list of objects for resorting on page
            hst_customizable.orderkey.push(customizableId);  // keep ordered list of keys for reset function
            $(customizableControls).append(
                $(document.createElement('li'))  //create lsit to control order of items
                    .addClass('your_content') 
                    .toggleClass('top', custItemCnt == 0)
                    .toggleClass('bottom', custItemCnt == totalCustomizables-1)
                    .append(
                        $(document.createElement('img'))
                            .attr({'src':'http://www.sfgate.com/graphics/home/customize/moveclear.gif'})
                            .addClass('your_content up_clear down')
                    )
                    .append(
                        $(document.createElement('img'))
                            .attr({'src':'http://www.sfgate.com/graphics/home/customize/moveup.gif'})
                            .addClass('your_content up')
                            .click(function () {
                                var litem = $(this).parent();
                                var plitem = litem.prev();
                                plitem.before(litem);
                                litem.css({"background-color":'#e2e5c6'});
                                plitem.css({"background-color":'#f8f8f0'});
                                setCustomizableControls(customizableControls,customizableAnchor);
                            })
                    )
                    .append(
                        $(document.createElement('img'))
                            .attr({'src':'http://www.sfgate.com/graphics/home/customize/movedown.gif'})
                            .addClass('your_content down')
                            .click(function () {
                                var litem = $(this).parent();
                                var nlitem = litem.next();
                                nlitem.after(litem);
                                litem.css({"background-color":'#e2e5c6'});
                                nlitem.css({"background-color":'#f8f8f0'});
                                setCustomizableControls(customizableControls,customizableAnchor);
                            })
                    )
                    .append(
                        $(document.createElement('img'))
                            .attr({'src':'http://www.sfgate.com/graphics/home/customize/moveclear.gif'})
                            .addClass('your_content down_clear up')
                    )
                    .append(
                        $(document.createElement('div'))
                            .addClass('handle')
                            .html(customizableTitle)
                    )
                    .hover(function () {$(this).css({"background-color":'#f8f8f0'})},function () {$(this).css({"background-color":'#e2e5c6'})})
                    .attr('customizableId', customizableId)
            );
        });
    });
}

function orderCustomizableControls (custIdList,customizableControls,customizableAnchor) {
    for (var okey in custIdList) {
        var li = $('li:[customizableId='+custIdList[okey]+']');
        $(customizableControls).append(li);
    }
    setCustomizableControls(customizableControls,customizableAnchor);
}

function setCustomizableControls (customizableControls,customizableAnchor) {
    var custCont = customizableControls+' > li';
    custCont = ($(custCont));
    var custItem = $(customizableAnchor).parent();
    hst_customizable.curorderkey = new Array ();
    custCont.each(function (i) {
        $(this).toggleClass('top', i==0);
        $(this).toggleClass('bottom', i==(custCont.length-1));
        custItem.after(hst_customizable[$(this).attr('customizableId')]);
        custItem = hst_customizable[$(this).attr('customizableId')];
        hst_customizable.curorderkey.push($(this).attr('customizableId'));
    });
}

$(document).ready(
    function () {
        // first take care of workaround (using separate freeform for stock ticker instead of in footer ob hp module) 
        // we must also strip the script tag to avoid it running a second time (after document.ready fires) 
        var hst_stockTicker = $("#stockticker").parent();
        hst_stockTicker.prev(".hide-rss-link").append(hst_stockTicker.find("script").remove().end());
        // now lets initialize our list by passing a list of elements and the controller target divs
        initializeCustomizables($("#hst_hp_custom").parent().nextAll('div[.$="-rss-link"]'),'#your_content','#hst_hp_custom');
        hst_customizable.cookeys = GetCookie(hst_customizable_cookie);
        if (hst_customizable.cookeys) {
            hst_customizable.curorderkey = hst_customizable.cookeys.split(',');
            orderCustomizableControls(hst_customizable.curorderkey,'#your_content','#hst_hp_custom');
        } else {
            hst_customizable.curorderkey = hst_customizable.orderkey;
        }
        hst_customizable.laststate = hst_customizable.curorderkey.join(',');
        $('#sfg_toggle_a').click( function () {
            var tlink = $('#sfg_toggle_img');
            var src = tlink.attr('src');
            if (src.indexOf('/graphics/home/customize/closed.gif') > 0) {
                tlink.attr('src', 'http://www.sfgate.com/graphics/home/customize/open.gif');
                $('#your_content').slideDown(500);
                $('#console_form').slideDown(500);
            } else {
                tlink.attr('src', 'http://www.sfgate.com/graphics/home/customize/closed.gif');
                $('#your_content').slideUp(500);
                $('#console_form').slideUp(500);  
            }
            tlink.get(0).blur();
            return false;
        });
    }
);
</script>	

