			/* Custom gaready event - no need to check if ga is ready. Use it around any reference to the ga object
			*  Methods attached after gaready has been fired will execute immediately.
			*/
			(function(){
				var gareadyfired = false;
				if(MooTools.version == '1.11') {
					Element.Events.extend({
						'gaready': {
							add: function(fn){
								if(gareadyfired){
									fn.call(this);
								}
							}
						}
					});
				} else {
					Element.Events.gaready = {
						onAdd: function(fn){
							if(gareadyfired){
								fn.call(this);
							}
						}
					};
				}

				if(typeof ga != 'undefined'){
					window.fireEvent('gaready');
					gareadyfired = true;
				}
			})();
