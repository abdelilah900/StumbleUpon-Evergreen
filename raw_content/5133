        var tool;
        //alert(tool);
        $(function() {
            // Don't hide content when paging through recipes
            SetContentAnimations("#ctl00_cphMain_clStep1");
            SetContentAnimations("#ctl00_cphMain_clStep2");
            SetContentAnimations("#ctl00_cphMain_clStep3");
            tool = new FiveOrLess();
        });

        function Initialize() { tool = new FiveOrLess(); }
        function SelectCategory(lnkClicked, catId) { tool.SelectCategory(lnkClicked, catId); }        
        function SelectRecipe(lnkClicked, recipeId) { tool.SelectRecipe(lnkClicked, recipeId); }        
        function CheckIfRecipesSelected() { tool.CheckIfRecipesSelected(); }
        function CheckIfRecipeInList(lnkAddToListId, recipeId) { tool.CheckIfRecipeInList(lnkAddToListId, recipeId);}
        function UpdateAddToRecipeListLink(lnk, bIsAdded) { tool.UpdateAddToRecipeListLink(lnk, bIsAdded);}
        function GetRecipes(page) { tool.GetRecipes('ctl00_cphMain_clStep2', page); }
        function GetShoppingList() { tool.GetShoppingList('ctl00_cphMain_clStep3'); }
        function GetRecipeDetails(divDetailsId, divLoadingId, recipeId) { tool.GetRecipeDetails('ctl00_cphMain_clRecipeDetails', divDetailsId, divLoadingId, recipeId); }
        function ShowRecipeDetails() { tool.ShowRecipeDetails("ctl00_cphMain_clRecipeDetails_divContent"); }
        function EmailList() { tool.EmailList("ctl00_cphMain_hfRecipeIds"); }

    </script>



        

