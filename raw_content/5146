                        var html = '<a href="http://www.g4tv.com/videos/58758/kevin-pereiras-last-walk-through-attack-of-the-show/#add-comment">1 Comment</a>'
                        //    console.log('got one comment yesss ');
                    } else if (comments > 1) {
                        var html = '<a href="http://www.g4tv.com/videos/58758/kevin-pereiras-last-walk-through-attack-of-the-show/#add-comment">' + comments + ' Comments</a>'
                    }

                    jQuery('.fbCommentCount58758').html(html);

                });

            }

        });
        //]]>
    </script>

