<script type="text/javascript">
	function zebraStripe() {        
		$('#comments li:even').addClass('odd');
		$('#comments li:odd').addClass('even');
		$('#comments-no-update li:even').addClass('odd');
		$('#comments-no-update li:odd').addClass('even');
	}

	function applyCuteTime() {
		$('#comments time').cuteTime({ refresh: 60 * 1000 });
		$('#comments-no-update time').cuteTime({ refresh: 60 * 1000 });
	}

	function formatComments() {
		zebraStripe();
		applyCuteTime();
	}

	jQuery(function($) {
		formatComments();
	});
</script>

