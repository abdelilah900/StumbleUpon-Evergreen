<script type="text/javascript">
	function processClick(thisId, url)
	{
		$.ajax({
			type: "GET",
			url: url,
			cache: false,
			success: function(html) {
				$("#main_list_menu li").removeClass("selected");
				$("#" + thisId).closest("#main_list_menu li").addClass("selected");
				$("#main_list").html(html);
			}
		});
	}
	
	jQuery(function($) {

		$("#newest").click(function () {
			var thisId = this.id;
			var url = "/home/newest"
			processClick(thisId, url)

			return false;
		});

		$("#new_and_interesting").click(function () {
			var thisId = this.id;
			var url = "/home/newandinteresting"
			processClick(thisId, url)

			return false;
		});

		$("#most_viewed").click(function () {
			var thisId = this.id;
			var url = "/home/mostviewed"
			processClick(thisId, url)

			return false;
		});

	});
</script>

					
		
