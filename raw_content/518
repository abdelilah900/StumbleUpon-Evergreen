<script language="JavaScript" src="/cm/shared/scripts/s_code.js"></script>
<script language="JavaScript"><!--
/* You may give each page an identifying name, server, and channel on
the next lines. */
    s.pageName = "Woman's Day: Roadside Attractions - Weird Roadside Attractions at WomansDay.com";
    s.pageType = "";

s.server = "WDY"
s.channel="Lifestyle"

s.prop1="Lifestyle"
s.prop3="Article"
s.prop4="Lifestyle"
s.prop5=""
s.prop9="6569692"
s.prop10="15 Must-See Roadside Attractions"
s.prop11="Amanda Greene"
s.prop12=""
s.prop26="wellness"
s.prop29="Woman's Day"
s.hier1="Lifestyle"
/* E-commerce Variables */
s.linkInternalFilters="javascript:,womansday.com"

s.visitorNamespace="hearstmagazines"



var rootSection = "life";
var userName = "";
s.eVar33 = "";

/* Deal with user data */
if( typeof(mag_user) != "undefined"){
    userName = mag_user.user_name;
    if( mag_user.logged_in == 1 && mag_user.initial_login == 1){
	s.eVar33 = "Logged In";
    } else {
	s.eVar33 = "";
    }
}

if( typeof(hearst_user) != "undefined" && !userName){
    userName = hearst_user.user_name 
    if( hearst_user.logged_in == 1 && hearst_user.initial_login == 1){
        s.eVar33 = "Logged In";
    } else {
        s.eVar33 = "";
    }
}

if( userName && rootSection ){
    s.eVar23 = userName + "|" + rootSection;
} else {
    s.eVar23 = "";
}

s.prop44 = userName;
s.eVar21 = userName;

/* Deal with cookie'd data */
try{
    if(readCookie("redirected_from") != null){
    	s.prop17 = "http://www.womansday.com" + unescape(readCookie("redirected_from"))
    	eraseCookie("redirected_from");
    } else {
    	s.prop17 = "";
    }
} catch (err){
    s.prop17 = "";
}

try {
    if(readCookie("original_referrer") != null){
        s.original_referrer=unescape(readCookie("original_referrer"))
    } else {
        createCookie("original_referrer", document.referrer, 0)
        s.original_referrer=document.referrer
    }
} catch(err){
    s.original_referrer = "";
}

s.campaign=""
s.state=""
s.zip=""
s.events=""
s.products=""
s.purchaseID=""
s.eVar1=""
s.eVar2=s.pageName
s.eVar3=s.channel
s.eVar4=s.hier1
s.eVar5=""
