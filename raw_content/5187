<div class="popup-content cfix">
<div class="<%= width_class %>">
<div class="popup-description">
<%= description %>
</div>
<div class="popup-form-controls cfix" id="popup-btn-wrap">
<% if (confirm_btn) { %>
<div id="popup-confirm-container" class="form-item form-item-button">
<input type="<%=confirm_type %>" value="Confirm" id="popup-confirm" class="form-button form-button-small form-button-confirm" name="popup-confirm" />
</div>
<% } %>
<% if (dont_save_btn) { %>
<div id="popup-dont-save-container" class="form-item form-item-button">
<input type="<%=confirm_type %>" value="Don't Save" id="popup-dont-save" class="form-button form-button-small form-button-dont-save" name="popup-dont-save" />
</div>
<% } %>
<% if (save_btn) { %>
<div id="popup-save-container" class="form-item form-item-button">
<input type="button" value="Save" id="popup-save" class="form-button form-button-small form-button-save" name="popup-save" />
</div>
<% } %>
<% if (close_btn) { %>
<div id="popup-force-close-container" class="form-item form-item-button">
<input type="button" value="Close" id="popup-force-close" class="form-button form-button-small form-button-close" name="popup-close" />
</div>
<% } %>
<% if (cancel_btn) { %>
<div id="popup-cancel-container" class="form-item form-item-button">
<input type="button" value="Cancel" id="popup-cancel" class="form-button form-button-small form-button-cancel" name="popup-cancel" />
</div>
<% } %>
</div>
</div></div>
</form>
</script>
