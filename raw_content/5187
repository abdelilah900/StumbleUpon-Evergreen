<% if (typeof(confirm_btn) == 'undefined') { confirm_btn = false; } %>
<% if (typeof(save_btn) == 'undefined') { save_btn = false; } %>
<% if (typeof(dont_save_btn) == 'undefined') { dont_save_btn = false; } %>
<% if (typeof(cancel_btn) == 'undefined') { cancel_btn = false; } %>
<% if (typeof(width_class) == 'undefined') { width_class = 'grid-14'; } %>
<% if (typeof(confirm_type) == 'undefined') { confirm_type = 'button'; } %>
<% if (typeof(form_id) == 'undefined') { form_id = 'popup-form'; } %>

<form id="<%=form_id%>" class="popup-form" action="#" onsubmit="return false;">
