        <script type="text/javascript">
            $(document).ready(function(){

                $(".relish_report .product .product-name-prod-img").live('mouseenter', function(){
                    var popup = $(this).parents('.product').find('.popup');
                    popup.fadeIn('fast');
                  });

                $(".relish_report .product .product-name-prod-img").live('mouseleave', function(){
                    var popup = $(this).parents('.product').find('.popup');
                    popup.hide();
                  }
                ); 

                $(".relish_report .product .product-name-prod-img").live('mousemove', function(e){

                    var popup = $(this).parents('.product').find('.popup');

                    if((e.pageX- $(window).scrollLeft()) < popup.outerWidth()) {
                        popup.css({left: e.pageX + 10 , top: e.pageY - $(window).scrollTop() + 10}); 
                    } else {
                        popup.css({left: e.pageX + 10 , top: e.pageY- $(window).scrollTop() + 10}); 
                    }
                });


                $('#subscribe_form').submit(function(){

                    if($('#email').attr('value') == '') {
                        alert('Input email plese.');
                        return false;
                    } 

                    $('#subscribe_submit').hide();
                    $('#loading').show();

                    $.get($(this).attr('action'), $(this).serialize(), function(json) {


                        $('#subscribe_submit').show();
                        $('#loading').hide();
                        $('#email').attr('value', '');

                        if(json.status == 'ok') {
                            alert('You have successfully subscribed to our newsletter!');
                        } else {
                            alert("Oops... I just don't know what went wrong...");
                        }
                    }, 'json');
                    return false;
                });


                
                $('img').error(function(){
                    $(this).hide(); 
                });

                $('img').each(function() { this.src = this.src; });

            });
        </script>
    


