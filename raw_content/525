						</form>
						<div class="clear"></div>
					</div>
				</div>	                   
            <script type="text/javascript">
            	//indenting list options
            	  $(".gog.left-rail .ideasForm select option.indent").each(function(){
            	      	$(this).html('&nbsp;&nbsp;&nbsp;'+$(this).text());
