        			var centerPlace= new Object();
        			centerPlace.lat = 38.899033;
        			centerPlace.lon = -77.26834;
				</script>
				<script type="text/cjs">
  					//start: gog-left-rail-map.js
/*by yadong*/
	//google map api v3 

var google_map_api_v3_url='http://maps.google.com/maps/api/js?client=gme-washingtonpost&sensor=false&v=3.7&channel=gog';
//document.write('<'+'script src="'+google_map_api_v3_url+'" type="text/javascript">' + '<'+'/script>');
//document.write('<'+'script src="http://maps.google.com/maps/api/js?sensor=false" type="text/javascript">' + '<'+'/script>');


function initialize(){
    var config={
        centerPlace:centerPlace,
        onMapLoadCallback:null
     };
     var gogMapUtil=new GogLeftRailMapUtil(config);
     gogMapUtil.init();
}

GogLeftRailMapUtil=function(config){
	var defaults={
		mapDivId:"cgMap",
		mapOptions:{
		      zoom: 15,
		     // center: new google.maps.LatLng(38.889813, -77.008067),
			  mapTypeControl:false,
			  streetViewControl:false,
			  zoomControl:false,
			  scaleControl: false,
			  panControl: false
		     // mapTypeId: google.maps.MapTypeId.ROADMAP			
		},
		centerPlace:null,
		places:[],
		centerMarkerImage:{
			pinUrl:"http://www.washingtonpost.com/wp-srv/gog/images/maps/main-pin-orange.png",
			shadowUrl:"http://www.washingtonpost.com/wp-srv/gog/images/maps/main-pin-shadow.png",
			anchor:{
				x:9,
				y:25
			}
		}
	}
	this.settings=$.extend(defaults,config);
};
//static methods
GogLeftRailMapUtil.MarkersEqual=function(marker1,marker2){
	return (marker1.getPosition().equals(marker2.getPosition())&&marker1.getTitle()==marker2.getTitle());
};
//instance methods
GogLeftRailMapUtil.prototype={
	mapInstance:null,
	markerBounds:null,
	centerMarker:null,
	gmarkers:[],
	init:function(){
		var self = this, settings = this.settings;
		settings.mapOptions.center=new google.maps.LatLng(settings.centerPlace.lat,settings.centerPlace.lon);
		settings.mapOptions.mapTypeId=google.maps.MapTypeId.ROADMAP;
		
		self.mapInstance=new google.maps.Map(document.getElementById(settings.mapDivId), settings.mapOptions);
		self.markerBounds=new google.maps.LatLngBounds();
		settings.centerPlace.markerImage=new google.maps.MarkerImage(
										    settings.centerMarkerImage.pinUrl,
										    null,
										    null,
										    new google.maps.Point(settings.centerMarkerImage.anchor.x,settings.centerMarkerImage.anchor.y)
										  );
		settings.centerPlace.shadowImage=new google.maps.MarkerImage(
										    settings.centerMarkerImage.shadowUrl,
										    null,
										    null,
										    new google.maps.Point(settings.centerMarkerImage.anchor.x,settings.centerMarkerImage.anchor.y)
										  );
		settings.centerPlace.hoverImage=settings.centerPlace.markerImage
		centerMarker=self.addMarker(self.mapInstance,settings.centerPlace);
		self.markerBounds.extend(new google.maps.LatLng(settings.centerPlace.lat,settings.centerPlace.lon))
		if (typeof settings.onMapLoadCallback=='function'){
				settings.onMapLoadCallback.call();
			}		
	},
	addMarker:function(map,place){
		var self = this;
		var point =new google.maps.LatLng(place.lat,place.lon);
		var marker = new google.maps.Marker({
			position:point,
			map:map,
			shadow:place.shadowImage,
			icon:place.markerImage
		});	
		
		self.gmarkers.push(marker);
		return marker;
	}
	
}
       
$(".gog.left-rail .mapbox-container, .gog.left-rail .address").hover(
		function(){$(".gog.left-rail .mapbox-container .darken-overlay-container").show();},
		function(){$(".gog.left-rail .mapbox-container .darken-overlay-container").hide();}
);

function loadGoogleMapApi() {
	  var script = document.createElement("script");
	  script.type = "text/javascript";
	  script.src = google_map_api_v3_url+"&callback=initialize";
	  document.body.appendChild(script);
}

//prevent google map api gets loaded mutiple times, due to ajax replace
if (typeof google === 'object' && typeof google.maps === 'object') {
	initialize();
} else {
	loadGoogleMapApi();
}

//end: gog-left-rail-map.js
				</script>
