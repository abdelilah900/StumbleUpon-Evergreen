<script type="text/javascript">
$(window).load(
    function() {
        
    var wrapper = $('.previewSlidesWrapper'), slider = $('#previewSlidesSlider'), slides = slider.find('.previewSlide'),
        currentSlide = Math.floor(2/4)*4,
        leftArrow = wrapper.find('.leftArrow'), rightArrow = wrapper.find('.rightArrow');

    function toggleArrows() {
        if (currentSlide == 0)
            leftArrow.hide();
        else
            leftArrow.show();
            
        if (currentSlide + 4 > slides.length)
            rightArrow.hide();
        else
            rightArrow.show();
        
    }
    function slidePreviews(offset, ev) {
        currentSlide+=offset;
        slider.animate({left:(0-currentSlide*155)});
        toggleArrows()
    }

    slider.css({'width':(slides.length*155),'left':(0-(currentSlide*155))});
    toggleArrows();
    wrapper.bind('click', function(ev){
        var t = $(ev.target), changed = false;
        function isArrow(dir) {
            var co = (dir == 'left' ? leftArrow : rightArrow);
            return t.attr('class') == co.attr('class') || t.parent().attr('class') == co.attr('class');
        }
        if (currentSlide < slides.length-1 && isArrow('right') && slides[currentSlide+4]) {
            ev.stopPropagation();
            slidePreviews(4);
        }
        else if (currentSlide > 0 && isArrow('left') && slides[currentSlide-4]) {
            ev.stopPropagation();
            slidePreviews(-4);
        }
    });
    }
);
</script>
