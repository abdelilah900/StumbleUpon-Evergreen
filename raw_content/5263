<div class="widget code html widget-editable viziwyg-section-70 inpage-widget-1491913" >
<script type="text/javascript">
/*
This code checks to see if the useFastArticles URL variable is set to true,
if it is it preloads all the mirror articles and hence allows each article to be loaded
much faster (about 100 times faster) into the browser.
It's only a demo, but works well in most scenarios. It currently only works on article pages, and doesn't reload all the parts of the page that we would probably want to reload.
It works best in Chrome and Firefox, because the URL updates there too.
To use it just add ?useFastArticles=true to the URL of an article
- ARB
*/
tmArticleHTMLMap = {};
function tmLinkClicked(linkHref) {
var articleHtml = tmArticleHTMLMap[linkHref];
var articleElement = $(articleHtml)[0];
$('.article.styleGroup').empty().append(articleElement);
if(history.pushState != undefined) {
history.pushState(null, null, linkHref);
}
window.scrollTo(0, 300);
}
(function() {
var queryArguments = window.location.search.replace(/\?/, '').split('&');
var queryArgumentsMap = {};
for(queryArgumentsIndex in queryArguments) {
var queryArgument = queryArguments[queryArgumentsIndex];
var queryArgumentKeyValue = queryArgument.split('=');
if(queryArgumentKeyValue.length > 1) {
var key = queryArgumentKeyValue[0];
var value = queryArgumentKeyValue[1];
queryArgumentsMap[key] = value;
} else {
var key = queryArgumentKeyValue[0];
var value = '';
queryArgumentsMap[key] = value;
}
}
if(queryArgumentsMap['useFastArticles'] != undefined && queryArgumentsMap['useFastArticles'] == 'true') {
$('a[href ^= "http://www.mirror.co.uk"]').filter(
function(index) {
var matches = this.href.match(/(\d+)$/);
if(matches != null) {
return matches.length > 0
} else {
return false;
}
}
).each(function() {
var articleHref = this.href;
this.href = 'javascript:tmLinkClicked(\'' + articleHref + '\');';
if(tmArticleHTMLMap[articleHref] == undefined) {
$.ajax({
url: articleHref + '?widgetName=master&widgetId=1490451',
success: function (html) {
tmArticleHTMLMap[articleHref] = html;
}
});
}
});
}
})();
</script>
</div>
