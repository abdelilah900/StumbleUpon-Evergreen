<script language="javscript" type="text/javascript">
$(document).ready(function(){
	$.ajax({
		type: "GET",
		url: "/api_static/delishenca_lifestyle_flashinfopane_resized.xml",///api_static/msn-lifestyle-feed.xml
		success:function(parsedData){
		    var xml = parsedData;
				toutItemList = $(xml).find('item'),
				html = '';
			$(toutItemList).each(function(i){
				var itemTitle = $(this).find('title').text(),
					itemLink = $(this).find('link').text();
				if(i == 0){
					var xnwImage = $(this).find('enclosure').attr('url'),
						xnwTitle = $(this).find('title').text(),
						xnwText  = $(this).find('description').text();
					xnwText = (xnwText.length > 49) ? xnwText.substring(0,46) + '&hellip;' : xnwText ;
					xnwTitle = (xnwTitle.length > 40) ? xnwTitle.substring(0,37) + '&hellip;' : xnwTitle ;
					html += "<div class='headline'><a href=\"" + itemLink + "\"><img src=\"" + xnwImage + "\"></a><p><a href=\"" + itemLink + "\">" + xnwTitle + "</a></p></div><div class='clear'></div>";
				} else {
					if (i == toutItemList.length - 1){
						html += "<div class='li_links last'><a target=\"_blank\" href=\"" + itemLink + "\">" + itemTitle + "</a></div>";
					} else {
						html += "<div class='li_links'><a target=\"_blank\" href=\"" + itemLink + "\">" + itemTitle + "</a></div>";
					}
				}
			});
			$('#msn_tout').html(html);
		}

	});	
});
</script>


