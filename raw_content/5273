				<script>
				if (!fb_adInterval){
					var fb_adInterval = 5;
				}
				function runme(){
					fb.registerAdInterval(fb_adInterval);
					fb.populateThumbbar();
					fb.selectIndex(0);
				}
				$(document).ready(function(){runme();});
				
				//Image Swap Script
				function img_swap(img_ID,nameoffile){
					document.getElementById(img_ID).setAttribute('src',nameoffile);
				}
			</script>
