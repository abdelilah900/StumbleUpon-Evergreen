    // Default Most Commented shows all sections on nymag.com
    var activeMC = "DailyIntel,TheCut,TheSportsSection,NewYorkMagazine,GrubStreetBoston,GrubStreetChicago,GrubStreetLosAngeles,GrubStreetNewYork,GrubStreetPhiladelphia,GrubStreetSanFrancisco";
    
    var mcTimeFrame = "1 day ago";
    
    // Show only Sports section and show last 3 days
    if ($("body").hasClass("sports")) activeMC = "TheSportsSection"; mcTimeFrame = "3 days ago";

    NYM.echo.loadMostCommented({
        timeframe: mcTimeFrame,
        elementId: "tab-commented ul.nymag",
        section: activeMC,
        items: 4
    });
    
    NYM.echo.loadMostCommented({
	   timeframe: "1 day ago",
 	   elementId: "tab-commented ul.vulture",
	   section: "Vulture",
	   items: 4
	});
});

