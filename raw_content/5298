            holder.currentPostId = 8752667170819264016
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/656432641681112781/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/656432641681112781/comments/default";
          
          holder.currentBlogUrl = "http://www.seasaltwithfood.com/";
          holder.currentBlogId = "656432641681112781";
        
        return holder;
      });
    }
  </script>
