<script type="text/javascript">
    function submitNLSignUp() {
        var email = document.getElementById('footer_ctl00_tbxEmailAddress').value;
        if (validate(email)) {
            var ids = '';
            for (var i = 1; i < 7; i++) {
                var oInputId = 'subscriptionID' + i;
                if (document.getElementById(oInputId).checked)
                    ids += document.getElementById(oInputId).value + ',';
            }
            if (ids != '') {
                ids += '3';
                SubmitNewsletterOverlay(ids, email);
            }
            else {
                CloseIt();
            }
        }
    }

    function validate(email) {
        var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
        if (reg.test(email) == false) {
            document.getElementById('divError2').className = "error show";
            return false;
        }
        else {
            document.getElementById('divError2').className = "eror hide";
            return true;
        }
    }

    function fnSubscription(e, obj) {
        var key;
        if (window.event)
            key = window.event.keyCode; //IE
        else
            key = e.which; //firefox      

        if (key == 13) {
            var btnId = GetClientId("btnSignUpNewsLetter");
            $LS('#' + btnId).click();
        }
        return (key != 13);

    }
    
</script>

