<script type="text/javascript">
    $LS(document).ready(function () {

        $LS('#disqus_thread_box').css('display', 'none');

        if (pageCount != 1) {
            LS_Utility.Hide(".pnlSignoff");
            LS_Utility.Hide(".quizSignoff");
        }
        else {
            LS_Utility.Show(".pnlSignoff");
            LS_Utility.Show(".quizSignoff");
        }

        if (showNewRelatedLinksLook == "True") {
            LS_Utility.Toggle(".relatedHeader", "relatedHeader", "relatedHeaderNew");
            LS_Utility.Toggle(".FCLink", "FCLink", "rlLink");
            LS_Utility.Hide(".hide4NewLook");
        }

//        $LS(".centerFrameContent").addClass("centerFrameContentForArticles");
//        $LS(".headerTabs").addClass("headerTabsForArticles");
//        $LS(".headerSubNav").addClass("headerSubNavForArticles");
        LS_Utility.Hide(".articleBorder");

        $LS(".centerFrameContent").css("border", "none");
        $LS(".centerFrameContent").css("background-color", "#FFFDE4");
        $LS(".headerTabs").css("width", "474px");
        $LS(".headerTabs").css("border-top", "1px solid #CCCACB");
        $LS(".headerTabs").css("border-left", "1px solid #CCCACB");
        $LS(".headerTabs").css("border-right", "1px solid #CCCACB");
        $LS(".headerSubNav").css("border-left", "1px solid #CCCACB");
        $LS(".headerSubNav").css("border-right", "1px solid #CCCACB");
        $LS(".headerSubNav").css("width", "474px");
        $LS(".headerSubNav").css("background-color", "#FFFFFF");
        $LS(".headerSubNav").css("text-align", "center");
//        $LS(".articleBorder").css("display", "none");


        $LS("#postCommentId").attr("href", "javascript:showPage(" + (parseInt(pageCount, 10) + 1) + ", " + pageCount + ")");
        $LS("#Article-Toolbar-Container").css("line-height", "15px");
        $LS("#emailSection").css("margin", "-55px 0 0 -350px");

        var allQueryStrings = window.location.search;
        if (allQueryStrings != "") {
            var queryString = allQueryStrings.substring(1).split("&");
            for (var i = 0; i < queryString.length; i++) {
                var pair = queryString[i].split("=");
                if (pair[0] == "page") {
                    curPage = 1;
                    showPage(pair[1], pageCount);
                }
            }
        }

        $LS(".pnlImage a").mouseover(function () {
            if ($LS.browser.msie && $LS.browser.version == "7.0") {
                if ($LS(".pnlImage a").attr("onclick") != undefined) {
                    $LS(".pnlImage a img").css("cursor", "pointer");
                }
            }
        });

        $LS(".pnlImage a").click(function () {
            if ($LS.browser.msie && $LS.browser.version == "7.0") {
                if ($LS(".pnlImage a").attr("onclick") != undefined) {
                    $LS(".pnlImage a img").css("border", "3px solid #d82954");
                }
            }
            else {
                $LS(".pnlImage a[onclick] img").css("border", "3px solid #d82954");
            }
        });

        var contentWidth = $LS("#mid-col-1").width();
        var imageWidth = $LS(".headerImg").width();
        if (imageWidth == null) {
            $LS("#artcileHeaderId").width(contentWidth);
        }
        else {
            $LS("#artcileHeaderId").width(contentWidth - imageWidth - 10);
        }

        var imageHeight = $LS(".headerImg").height();
        if (imageHeight != null) {
            var contentHeight = $LS("#artcileHeaderId").height();
            if (imageHeight > contentHeight) {
                $LS("#artcileHeaderId").css('padding-top', (imageHeight - contentHeight) / 2);
            }
            else {
                $LS(".headerImg").css('padding-top', (contentHeight - imageHeight) / 2);
            }
        }

        $LS('.post-comment').mouseover(function () {
            $LS('.view-comment').css('color', '#ff6633');
            $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/Toolbar/comments-hover.gif)');
            $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
        });
        $LS('.post-comment').mouseout(function () {
            $LS('.view-comment').css('color', '');
            $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/healthcenter/comments2.png)');
            $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
        });
        $LS('.post-comment').click(function () {
            onClickComments();
        });

        $LS('.view-comment').mouseover(function () {
            $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/Toolbar/comments-hover.gif)');
            $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
        });
        $LS('.view-comment').mouseout(function () {
            $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/healthcenter/comments2.png)');
            $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
        });
        $LS('.view-comment').click(function () {
            onClickComments();
        });

    });

    function onClickComments() {
        var commentBox = $LS('#disqus_thread_box').css('display');
        $LS('#disqus_thread_box').css('display', 'block');
        $LS("#genericArticleCommentId").removeClass("hide").addClass("show");
        gIsUgc = true;
        RefreshAllAds();
        TrackPageView();
    }

    function onHoverComments() {
        $LS('.view-comment').css('color', '#ff6633');
        $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/Toolbar/comments-hover.gif)');
        $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
    }
    function onNonHoverComments() {
        $LS('.view-comment').css('color', '');
        $LS('#bottomPostComment a.post-comment').css('background', 'url(http://images.lifescript.com/images/healthcenter/comments2.png)');
        $LS('#bottomPostComment a.post-comment').css('background-repeat', 'no-repeat');
    }
</script>
