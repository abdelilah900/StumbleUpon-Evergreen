    $LS(document).ready(function () {
        $LS(this).find(".subMenuListing .subMenuListingItem ul.subMenuSubMenuItem").each(function () {
            var subSubItem = $LS.trim($LS(this).html()).length;
            if ($LS.browser.msie && $LS.browser.version == "7.0" && subSubItem == 0) {
                $LS(this).remove();
            }
        });
    });
</script>
