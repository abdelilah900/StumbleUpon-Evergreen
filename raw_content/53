<script type="text/javascript">
    function fnSubscription(e, obj) {
        var key;
        if (window.event)
            key = window.event.keyCode; //IE
        else
            key = e.which; //firefox      

        if (key == 13) {
            var btnId = document.getElementById("lsImageSubmitButton");
            $LS('#' + btnId).click();
        }
        return (key != 13);
    }

    function SubmitNLSignupModule() {
        if (!validateFormData) {
            return false;
        }
        var email = document.getElementById("txtEmailAddress").value;
        var subscriptionIds = "9";
        //temporary translations between SC and DB
        subscriptionIds = subscriptionIds.replace("9", "6");
        subscriptionIds = subscriptionIds.replace("10", "7");
        subscriptionIds = subscriptionIds.replace("11", "9");
        subscriptionIds = subscriptionIds.replace("12", "10");
        window.location = "/Subscriptions.aspx?Mode=3&SubscriptionIDs=" + subscriptionIds + "&NewEmailAddress=" + email;
    }

    function validateFormData() {
        return true;
    }
</script>

    
    
    
