<script type="text/javascript">
    $LS(document).ready(function() {
        if ($LS.browser.msie) {
            if ($LS.browser.version == "7.0") {
                $LS(".sectionTopLeftNav .headerLeftNavListItemContainer .leftTopNavFirstLevelText").each(function() {
                    $LS(this).children().each(function() {
                        var height = $LS(this).height();
                        if (height < 19) {
                            $LS(this).css("position", "relative");
                            $LS(this).css("top", "7px");
                        }
                    });
                });
            }
        }

        $LS(".firstLevelMenuItemNoSubItemsSpacing .navImgHolder a img").each(function () {
            var iconOnSrc = $LS(this).attr("src");
            if (iconOnSrc == "") {
                $LS(this).closest(".firstLevelMenuItemNoSubItemsSpacing").addClass("hide");
                $LS(this).closest(".sectionTopLeftNav").children(".collectionLeftNavListItems").css("margin-top", "-12px");
            }
        });
    });
</script>

