<META HTTP-EQUIV="Pragma" CONTENT="no-cache"> 
<div class="clear fL" id="pushdownAdWrapper" style="width:970px; padding-left:10px;">
    <input type="hidden" value="" id="quizPushDownAd" />
    <input type="hidden" value="1" id="articlePushDownAd" />
    <input type="hidden" value="true" id="matchItem" />
    <input type="hidden" value="true" id="isValidDate" />
    
    <script type="text/javascript" language="javascript">
      function ShowPushDownAd(page) {
        var isValidDate = true;
        if (!isValidDate)
            return false;

        var matchItem = true;
        if (!matchItem)
            return false;

        var isArticlePage = false;
        var articlePages = "1";
        var isQuizPage = false;
        var quizPages = "";

        var maxViewPerVisit = "2147483647";
        maxViewPerVisit = maxViewPerVisit * 1;

        var visitDuration = "2147483647";

        var isChannelPage = !(isArticlePage || isQuizPage);

        var cookieVal = fnGetAlphaCookie("lsPushDownAd");

        if (!fnCookieThere("lsPushDownAd") || (cookieVal != null && cookieVal > 0)) {
            if (cookieVal == null)
                cookieVal = maxViewPerVisit;

            if (isChannelPage) {
                fnCreateNewCookieV2("lsPushDownAd", cookieVal - 1, visitDuration);
                return true;
            }
            else if (isArticlePage) {
                var articlePages = articlePages.split(",");
                for (var i = 0; i < articlePages.length; i++) {
                    if (articlePages[i] == page) {
                        fnCreateNewCookieV2("lsPushDownAd", cookieVal - 1, visitDuration);
                        return true;
                    }
                }
               
            }
            else if (isQuizPage) {
            var quizPages = quizPages.split(",");
                for (var i = 0; i < quizPages.length; i++) {
                    if (quizPages[i] == page) {
                        fnCreateNewCookieV2("lsPushDownAd", cookieVal - 1, visitDuration);
                        return true;
                    }
                }
            }
        }
        return false;
    }
 </script>
    <div id="pushdownAdContainer" class="fL" style="padding-bottom: 30px; *padding-bottom: 10px;">
      <script type="text/javascript" language="javascript">
          var page;
          if ($LS.query.get('page') != "")
              page = $LS.query.get('page');
          else if ($LS.query.get('Page') != "")
              page = $LS.query.get('Page');
          else
              page = 1;

          if (ShowPushDownAd(page)) {
              try {
                  var javascriptSrc = GetAd('15', 'pushdown', gDartTag, gContexts, gCampaigns, gAdFlag, '', '970', '90', 'pushdownAdContainer', '1x1');
                  document.write('<scr' + 'ipt type="text/javascript" language="javascript" src="' + javascriptSrc + '"></scr' + 'ipt>');
              } catch (e) { }
              LS_Utility.Show("#pushdownAdWrapper");
          }
          else {
              LS_Utility.Hide("#pushdownAdWrapper");
          }
      </script>
    </div>
</div>

<script type="text/javascript">
    $LS(document).ready(function () {
        var tagNameOfLastChildOfPushAdContainer = $LS("#pushdownAdContainer").children(":last").get(0).tagName;
        if (tagNameOfLastChildOfPushAdContainer.toLowerCase() == "a") {
            LS_Utility.Show("#pushdownAdWrapper");
            return;
        }
        else {
            var adContainer = $LS("#pushdownAdContainer");
            var embedNoScriptExist = checkChildrenFormEmbedOrNoScriptTag(adContainer);
            if (embedNoScriptExist) {
                LS_Utility.Show("#pushdownAdWrapper");
                return;
            }
            else {
                var isShowing = false;

                adContainer.children().each(function () {
                    var tagAdContainer = $LS(this).get(0).tagName;
                    if (tagAdContainer.toLowerCase() != "script") {
                        var embedNoScriptExistOneLevel = checkChildrenFormEmbedOrNoScriptTag($LS(this));
                        if (embedNoScriptExistOneLevel) {
                            LS_Utility.Show("#pushdownAdWrapper");
                            isShowing = true;
                            return;
                        }
                    }
                });

                // not sure if the retun inside the each function returns all the way or just from inside the each :)
                if (isShowing)
                    return;
            }

            LS_Utility.Hide("#pushdownAdWrapper");
        }

        function checkChildrenFormEmbedOrNoScriptTag(container) {
            var tagChildrenHasEmbedNoScript = false;
            container.children().each(function () {
                var tagInsideAdContainer = $LS(this).get(0).tagName;
                if (tagInsideAdContainer.toLowerCase() == "noscript" || tagInsideAdContainer.toLowerCase() == "embed" || tagInsideAdContainer.toLowerCase() == "div") {
                    tagChildrenHasEmbedNoScript = true;
                }
                else if (tagInsideAdContainer.toLowerCase() == "a") {
                    if ($LS(this).children("img").length > 0) {
                        tagChildrenHasEmbedNoScript = true;
                    }
                }
            });
            return tagChildrenHasEmbedNoScript;
        }
    });
</script>
               </div>
         </div>
    </div>
</div>
