 <script type="text/javascript">
     $LS(document).ready(function () {
        var PinterestJson = receiveCount({"count": 1829, "url": "http://www.lifescript.com/diet-fitness/articles/b/boob_lift_or_bust_3_best_chest_exercises.aspx"});
         $LS.globalEval(PinterestJson);
        RepositionSocialMediaBar();
    });

    function RepositionSocialMediaBar() {
    	var cwOffset = $LS('.contentwrapper').offset();
    	var topOffset = ($LS(this).height() - $LS('#socialMediaBar').height()) / 2;
    	if (topOffset < 0) topOffset = 0;
    	topOffset += + $LS(document).scrollTop();
    	$LS('#socialMediaBar').offset({ top: topOffset, left: cwOffset.left-86});
    }

    var resizeTimer;
    $LS(window).resize(function() {
        clearTimeout(resizeTimer);
        resizeTimer = setTimeout(RepositionSocialMediaBar, 100);
    });

    function receiveCount(pinterestJsonData) {
        if (pinterestJsonData.count === undefined) { }
        else {
            $LS(".st_pinterest_vcount .stBubble_count").text(pinterestJsonData.count);
        }
    }
     var FBShareCount = '';
     if (!isNaN(FBShareCount) && $LS.trim($LS(".fb_share_count_inner").text()) == "") {
         $LS(".fb_share_count_inner").text(FBShareCount);
     }
 </script>
