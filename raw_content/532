<script type="text/javascript" src="http://extras.denverpost.com/cache/redirect_lookup.js"></script>
<script type="text/javascript">
    /*
    This chunk of javascript initializes the article comments variables, the article recommendation variables, 
    and downloads the comments and the other dynamic var data.
    */
    
    var section = document.location.pathname.split('/');
    var article_id;
    //var sectiontmp = document.location.pathname.split('/');
    var tmp = section[section.length-1].split('ci_');
    article_id = tmp[1].substr(0,8);
    var article_id_tmp = redirect_lookup(article_id);
    if ( article_id_tmp != article_id && article_id_tmp != '' ) article_id = article_id_tmp;

    //The section comes from the URL -- we split it at the slashes to get each part.
    //DEFINED ABOVE: var section = document.location.pathname.split('/');
    //If there is more than one part then we know we've got a section value
	var section_str = '';
	var section_name = '';
	var section_path = '';
    if ( section.length >  2 ) 
	{
		section_name = section[1];
		section_str = '&section=' + section_name;
		section_path = section_name + '/';
	}

    document.write('<script type="text\/javascript" src="http:\/\/extras.denverpost.com\/cache\/neighbors\/comments\/feed_comments_' + article_id + '.js"><\/script>');
    
    function load_js(file_name)
    {
        var article_votes = new Number(document.getElementById('article_votes').innerHTML);
        document.getElementById('article_votes').innerHTML = article_votes + 1;
    }
    function refresh_votes()
    {
        setTimeout("load_js();",3000);
    }
</script>
