<script type="text/javascript">
/*
 * timeago: a jQuery plugin, version: 0.9.2 (2010-09-14)
 * @requires jQuery v1.2.3 or later
 *
 * Timeago is a jQuery plugin that makes it easy to support automatically
 * updating fuzzy timestamps (e.g. "4 minutes ago" or "about 1 day ago").
 *
 * For usage and examples, visit:
 * http://timeago.yarp.com/
 *
 * Licensed under the MIT:
 * http://www.opensource.org/licenses/mit-license.php
 *
 * Copyright (c) 2008-2010, Ryan McGeary (ryanonjavascript -[at]- mcgeary [*dot*] org)
 */
(function($) {
  $.timeago = function(timestamp) {
    if (timestamp instanceof Date) return inWords(timestamp);
    else if (typeof timestamp == "string") return inWords($.timeago.parse(timestamp));
    else return inWords($.timeago.datetime(timestamp));
  };
  var $t = $.timeago;

  $.extend($.timeago, {
    settings: {
      refreshMillis: 60000,
      allowFuture: false,
      strings: {
        prefixAgo: null,
        prefixFromNow: null,
        suffixAgo: "ago",
        suffixFromNow: "from now",
        seconds: "less than a minute",
        minute: "about a minute",
        minutes: "%d minutes",
        hour: "about an hour",
        hours: "about %d hours",
        day: "a day",
        days: "%d days",
        month: "about a month",
        months: "%d months",
        year: "about a year",
        years: "%d years",
        numbers: []
      }
    },
    inWords: function(distanceMillis) {
      var $l = this.settings.strings;
      var prefix = $l.prefixAgo;
      var suffix = $l.suffixAgo;
      if (this.settings.allowFuture) {
        if (distanceMillis < 0) {
          prefix = $l.prefixFromNow;
          suffix = $l.suffixFromNow;
        }
        distanceMillis = Math.abs(distanceMillis);
      }

      var seconds = distanceMillis / 1000;
      var minutes = seconds / 60;
      var hours = minutes / 60;
      var days = hours / 24;
      var years = days / 365;

      function substitute(stringOrFunction, number) {
        var string = $.isFunction(stringOrFunction) ? stringOrFunction(number, distanceMillis) : stringOrFunction;
        var value = ($l.numbers && $l.numbers[number]) || number;
        return string.replace(/%d/i, value);
      }

      var words = seconds < 45 && substitute($l.seconds, Math.round(seconds)) ||
        seconds < 90 && substitute($l.minute, 1) ||
        minutes < 100 && substitute($l.minutes, Math.round(minutes)) ||
        minutes < 120 && substitute($l.hours, 2) ||
        hours < 24 && substitute($l.hours, Math.round(hours)) ||
        hours < 48 && substitute($l.day, 1) ||
        days < 30 && substitute($l.days, Math.floor(days)) ||
        days < 60 && substitute($l.month, 1) ||
        days < 365 && substitute($l.months, Math.floor(days / 30)) ||
        years < 2 && substitute($l.year, 1) ||
        substitute($l.years, Math.floor(years));

      return $.trim([prefix, words, suffix].join(" "));
    },
    parse: function(iso8601) {
      var s = $.trim(iso8601);
      s = s.replace(/\.\d\d\d+/,""); // remove milliseconds
      s = s.replace(/-/,"/").replace(/-/,"/");
      s = s.replace(/T/," ").replace(/Z/," UTC");
      s = s.replace(/([\+-]\d\d)\:?(\d\d)/," $1$2"); // -04:00 -> -0400
      return new Date(s);
    },
    datetime: function(elem) {
      // jQuery's `is()` doesn't play well with HTML5 in IE
      var isTime = $(elem).get(0).tagName.toLowerCase() == "time"; // $(elem).is("time");
      var iso8601 = isTime ? $(elem).attr("datetime") : $(elem).attr("title");
      return $t.parse(iso8601);
    }
  });

  $.fn.timeago = function() {
    var self = this;
    self.each(refresh);

    var $s = $t.settings;
    if ($s.refreshMillis > 0) {
      setInterval(function() { self.each(refresh); }, $s.refreshMillis);
    }
    return self;
  };

  function refresh() {
    var data = prepareData(this);
    if (!isNaN(data.datetime)) {
      $(this).text(inWords(data.datetime));
    }
    return this;
  }

  function prepareData(element) {
    element = $(element);
    if (!element.data("timeago")) {
      element.data("timeago", { datetime: $t.datetime(element) });
      var text = $.trim(element.text());
      if (text.length > 0) element.attr("title", text);
    }
    return element.data("timeago");
  }

  function inWords(date) {
    return $t.inWords(distance(date));
  }

  function distance(date) {
    return (new Date().getTime() - date.getTime());
  }

  // fix for IE6 suckage
  document.createElement("abbr");
  document.createElement("time");
})(jQuery);

</script>

<script type="text/javascript">
    // This loops through the divs on a page, looking for the "articleoptions" divs.
    // We're rewriting those divs to add comment-number information and share-this info.
    // 
    // 
    // We don't want to run this on the home page or section fronts.
    if ( ArticleTitle != "null" )
    {
        var tags = document.getElementsByTagName("DIV");
        var y=0;
        //var dp_buzzrewrite = document.getElementById('buzzholder').innerHTML;
        for(x=0; x<tags.length; x++)
        {
            /*
            if( tags[x].className == "listingItemTitle" && tags[x].innerHTML.indexOf("Sorry, no items are currently available.") != -1)
            {
                var clb = tags[x].parentNode;
                //Traverse the DOM tree backwards until we hit a table with a class name of "complexListingBox"
                while(clb.className != "complexListingBox")
                {
                    clb = clb.parentNode;
                }
                clb.style.display="none";
            }
            else 
            */
            
            if ( tags[x].className == "articleOptions" )
            {
                var commenttitle = "View comments on this article";
                if ( commentnumber == 1 ) 
                {
                    var commentlabel = "Comment";
                }
                else 
                {
                    if ( commentnumber == 0 ) var commenttitle = "Add your comments to this article";
                    var commentlabel = "Comments";
                }
            

                //var defaultlinks = '<a onclick="return addthis_sendto()" onmouseout="addthis_close()" onmouseover="return addthis_open(this, \'\', \'[URL]\', \'[TITLE]\')" href="http://www.addthis.com/bookmark.php" class="articleOptions"><img src="http://extras.denverpost.com/images/icon-share.gif" alt="" border="0"></a><a onclick="return addthis_sendto()" onmouseout="addthis_close()" onmouseover="return addthis_open(this, \'\', \'[URL]\', \'[TITLE]\')" href="http://www.addthis.com/bookmark.php" class="articleOptions">Share</a> '
                
                var defaultlinks = '';
				var facebook_link = '<br><a name="fb_share" type="button_count" href="http://www.facebook.com/sharer.php" share_url="http://www.denverpost.com/ci_' + article_id + '?source=sb-facebook" style="display:block; margin-top:2px;">Share</a>';
				facebook_link = '';
				// var facebook_link = '<br><iframe src="http://www.facebook.com/plugins/like.php?layout=button_count&amp;show_faces=false&amp;width=200&amp;action=like&amp;colorscheme=light&amp;height=21&amp;href=' + article_id + '" id="facebook-like-top" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:100px; height:21px;" allowTransparency="true"></iframe>';
				var pop_link = '<br><a href="#most-popular"  class="articleOptions"><img src="http://extras.mnginteractive.com/live/media/site36/2009/0209/20090209_043502_li_data.gif" width="14" height="11" border="0" alt="" style="margin-bottom:0;">Story Stats</a>';
				if ( pop_flag == false ) pop_link = '';
                var t = tags[x].innerHTML;
        
                //Link list at the top of the article, next to the headline:
                if ( y == 0 ) 
                {
                    tags[x].innerHTML = t + defaultlinks + '<br><a href="#comments" title="View comments on this article" class="articleOptions"><img src="http://extras.mnginteractive.com/live/media/site36/2008/0409/20080409_060550_comment_article.gif" alt="Comments" width="16" height="16" border="0"></a><a href="#comments" title="' + commenttitle + '" class="articleOptions">' + commentnumber + ' ' + commentlabel + '</a>' + pop_link + facebook_link;
                    
                    if ( typeof(article_flag) != "undefined" )
                    {
                        tags[x].innerHTML = tags[x].innerHTML + article_flag;
                    }
                }
        
                //Link list at the bottom of the article, below the last graf:
                else if ( y == 1 ) 
                {
                    tags[x].innerHTML = t + defaultlinks + '&nbsp;&nbsp;&nbsp;<strong>&raquo;</strong>&nbsp;<a href="https://www.denverpostcustomercare.com/ZipCheck.asp?PageName=newstartstep1.asp&Pid=3&PageDisplay=P" title="Subscribe" class="articleOptions">Get Home Delivery</a>&nbsp;&nbsp;&nbsp;<strong>&raquo;</strong>&nbsp;<a href="/termsofuse" title="Terms of Use" class="articleOptions">Terms of Use</a><div id="share_this_bottom" class="social_bookmarks" style="display:none;">&nbsp;</div>';
                }

                y++;
            }
            else if ( tags[x].className == "social_bookmarks" && y == 0 )
            {
                tags[x].id = 'share_this';
            }
        }
    }
</script>



<script type="text/javascript" src="http://media.denverpost.com/cache/comment_quantity_lookup.js"></script>
<script type="text/javascript">
//var firstcomment = '&nbsp;comments';
function comment_numberer(id)
{
    var chunk = document.getElementById(id);
    if ( chunk != null )
    {
        var chunklink = chunk.getElementsByTagName("A");
        var chunklinklength = chunklink.length;
        var chunklinklast = chunklink.length - 1;
        for(x=0; x<chunklinklength; x++)
        {
            //Make sure we're dealing with the correct type of link
            if ( chunklink[x].className == 'complexListingLink' || chunklink[x].className == 'listingItemTitle' )
            {
                var lenid = chunklink[x].href.length;
                var linkid = chunklink[x].href.substr((lenid-8), lenid);
                //alert(g);
                //See if the article has any comments on it
                for (y=0; y<comment_quantity.length; y++)
                {
                    //if ( chunklink[x].href == 'http://www.denverpost.com/ci_' + comment_id[y] || chunklink[x].href == 'http://www.denverpost.com/news/ci_' + comment_id[y] || chunklink[x].href == 'http://www.denverpost.com/sports/ci_' + comment_id[y] || chunklink[x].href == 'http://www.denverpost.com/business/ci_' + comment_id[y] ) 
                    if ( linkid == comment_id[y] ) 
                    {
                        chunklink[x].innerHTML += '&nbsp;&nbsp;<span class="commentnumber"><nobr>&nbsp;' + comment_quantity[y] + '</nobr></span>';
                        //else if ( chunklink[x].className == 'listingArticleLinkTo' && chunklink[x].innerHTML == '&raquo;&nbsp;MORE ' ) chunklink[x].innerHTML = '(' + comment_quantity[y] + '&nbsp;comments)&nbsp;' + chunklink[x].innerHTML;
                    }
                }
            }
        }
    }
}



// 
// If we're not on an article, we run the comment-numberer for the linklists on the page.
// We also rewrite the datestamps wherever we can.
//
// If we're on an article, we load the facebook javascript we use to publish the "Share" button.
if ( ArticleTitle == 'null' || SectionName == "Denver Colorado Breaking News" )
{
    comment_numberer('redesign_complexListing_layout5');

    if ( SectionName == "Denver Colorado Breaking News" || PageName == "DENVER & THE WEST" || PageName == "BUSINESS" || PageName == "SPORTS" )
    {
        // We have different requirements for timestamps on the breaking news section. 
        if ( SectionName == "Denver Colorado Breaking News" ) var timeago_regex = /(([8-9]+|[0-9]{2}) hours|day|year)/gi;
        else var timeago_regex = /(days|year)/gi;
        
        $('.listingItemDate').each(function(i){
            var timeago = jQuery.timeago($(this).html());
            
            // Chrome, Safari don't play nice with this extension --
            // they display the timeago as a year. This piece of js
            // makes sure we don't show timestamps for that -- and if 
            // we're not showing timeagos, then we do some clean-up on the 
            // existing timestamp.
            if ( timeago.search(timeago_regex) == -1 ) $(this).html('&nbsp;&nbsp;&nbsp;&ndash; ' + timeago)
            else
            {
                var ts = $(this).html().replace(/ (MST|MDT)/gi, '');
	            ts = ts.replace(/ 0([0-9]{1})/gi, ' $1');
	            ts = ts.replace(/ /gi, '&nbsp;');
	            ts = ts.replace(/&nbsp;-&nbsp;/gi, ' &nbsp;&nbsp;&nbsp;&ndash; ');
                ts = ts.replace(/\/2011/gi, '');
                $(this).html(ts)
            }
            
            // If the timeago is in the minutes, we turn the timestamp red.
            if ( timeago.indexOf("minute") > -1 ) $(this).css("color", "red")
        });
        
        $('.listingItemDateWithBlurb').each(function(i){
            var strip_tags = $(this).html().replace(/(<([^>]+)>)/ig,""); 
            var timeago = jQuery.timeago(strip_tags);
            
            if ( timeago.search(timeago_regex) == -1 ) $(this).html(timeago + ' -')
            else
            {
                var ts = $(this).html().replace(/ (MST|MDT)/gi, '');
	            ts = ts.replace(/ 0([0-9]{1})/gi, ' $1');
	            ts = ts.replace(/ /gi, '&nbsp;');
	            //ts = ts.replace(/&nbsp;-&nbsp;/gi, ' &nbsp;&nbsp;&nbsp;&ndash; ');
                ts = ts.replace(/\/2011/gi, '');
                $(this).html(ts)
            }
            
            
            if ( timeago.indexOf("minute") > -1 ) $(this).css("color", "red")
        });
    }
    else
    {
    	var element_ar = document.getElementsByTagName('span');
    	var i;
    	for(i in element_ar)
    	{
    	    if ( typeof(element_ar[i]) != 'undefined' )
    	    {
    	        if ( element_ar[i].className == 'listingItemDate' )
    	        {
    	            var ts = element_ar[i].innerHTML.replace(/ (MST|MDT)/gi, '');
    	            ts = ts.replace(/ /gi, '&nbsp;');
    	            ts = ts.replace(/&nbsp;-&nbsp;/gi, ' &nbsp;&nbsp;&nbsp;&ndash; ');
    	            element_ar[i].innerHTML = ts.replace(/\/2011/gi, '');
    	        }
    	    }
    	}
	}
}
else
{
	// Facebook javascript used to enhance the facebook share widget at the top of our articles.
	var script = document.createElement('script');
	script.src = 'http://static.ak.fbcdn.net/connect.php/js/FB.Share';
	script.type = 'text/javascript';
	var head = document.getElementsByTagName('head').item(0);
	head.appendChild(script);

	// Tynt
	if(document.location.protocol=='http:'){
	 var Tynt=Tynt||[];Tynt.push('drJey4bd8r3PGFab7jrHtB');Tynt.i={"st":true,"as":"Read The Denver Post's Terms of Use of its content: http://www.denverpost.com/termsofuse"};
	 (function(){var s=document.createElement('script');s.async="async";s.type="text/javascript";s.src='http://tcr.tynt.com/ti.js';var h=document.getElementsByTagName('script')[0];h.parentNode.insertBefore(s,h);})();
	}

}


