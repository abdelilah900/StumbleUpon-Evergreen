 <!-- 
#############
Begin Most-Popular addons
#############
 -->
<div id="most-popular" class="boxwhite" style="margin-left:20px; margin-top:10px;">
<script type="text/javascript" src="http://extras.denverpost.com/cache/pop_lookup.js"></script>
<script type="text/javascript">
// The Most-Popular addon puts a little blurb of text 
// at the bottom of an article that lets readers know
// where this article is in the most-pop rankings,
// and directs them to other most-pop content.
//
// To do this, we look up the article id against an array defined in http://extras.denverpost.com/cache/pop_lookup.js

function to_ordinal(num)
{
	var n = num % 100;
	var suff = ["th", "st", "nd", "rd", "th"]; // suff for suffix
	var ord= n<21?(n<4 ? suff[n]:suff[0]): (n%10>4 ? suff[0] : suff[n%10]);
	if ( num == 1 ) return '<strong>#1</strong>';
	return num + ord;
}


var pop_section_lookup = isNaN(pop_section[article_id]);
var pop_full_lookup = isNaN(pop_full[article_id]);
var pop_hour_lookup = isNaN(pop_hour[article_id]);
var pop_sanscolo_lookup = isNaN(pop_sanscolo[article_id]);
var pop_sanscolo_hour_lookup = isNaN(pop_sanscolo_hour[article_id]);
var pop_colo_lookup = isNaN(pop_colo[article_id]);
var pop_colo_hour_lookup = isNaN(pop_colo_hour[article_id]);
var pop_den_lookup = isNaN(pop_den[article_id]);

var pop_flag = false;
// If we have a number on the section-based lookup or the full lookup or the hour lookup,
// we know we've got something to publish.
//
// The challenge here is creating a legible sentence with the six possible combinations.
//if ( pop_section_lookup == false || pop_full_lookup == false || pop_hour_lookup == false || pop_colo_lookup == false || pop_colo_hour_lookup == false || pop_sanscolo_lookup == false || pop_sanscolo_hour_lookup == false || pop_den_lookup == false )
if ( pop_section_lookup == false || pop_full_lookup == false || pop_hour_lookup == false ) 
{
	document.write('<p>This article is the:</p><ul>');
	if ( pop_hour_lookup == false )
	{
		document.write('<li><a href="/popular#popularhour">' + to_ordinal(pop_hour[article_id]) + ' most-clicked article of the hour</a></li>');
	}
	if ( pop_full_lookup == false )
	{
		document.write('<li><a href="/popular">' + to_ordinal(pop_full[article_id]) + ' most-clicked article of the day</a></li>');
	}
	if ( pop_colo_lookup == false )
	{
		document.write('<li><a href="/popular#popularcolorado">' + to_ordinal(pop_colo[article_id]) + ' most-clicked article among Colorado readers of the day</a></li>');
	}
	if ( pop_colo_hour_lookup == false )
	{
		document.write('<li><a href="/popular#popularcolorado">' + to_ordinal(pop_colo_hour[article_id]) + ' most-clicked article among Colorado readers of the hour</a></li>');
	}
	if ( pop_sanscolo_lookup == false )
	{
		document.write('<li><a href="/popular">' + to_ordinal(pop_sanscolo[article_id]) + ' most-clicked article among readers outside Colorado of the day</a></li>');
	}
	if ( pop_sanscolo_hour_lookup == false )
	{
		document.write('<li><a href="/popular">' + to_ordinal(pop_sanscolo_hour[article_id]) + ' most-clicked article among readers outside Colorado of the hour</a></li>');
	}
	if ( pop_den_lookup == false )
	{
		document.write('<li><a href="/popular#popularcolorado">' + to_ordinal(pop_den[article_id]) + ' most-clicked article among Denver readers</a></li>');
	}
	if ( pop_section_lookup == false ) 
	{
		document.write('<li><a href="/popular#pop-' + pop_section_name[article_id] + '">' + to_ordinal(pop_section[article_id]) + ' most-clicked article among ' + pop_section_name[article_id] + ' articles today</a></li>');
	}
	
	document.write('<li><em><a href="#top">Back to the top</a></em></li></ul>');
	pop_flag = true;
}
</script>
</div>
