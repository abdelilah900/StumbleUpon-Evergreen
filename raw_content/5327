<script type="text/javascript">

	var overlay_api;

	$(document).ready( function(){

		// Initalize the overlay
		$("#mies1").overlay({
			top: 120,
			mask: {
				color: '#000',
				zIndex: '100014',
				loadSpeed: 200,
				opacity: 0.5
			},
			closeOnClick: false,
			onClose: function() { 
				window.setTimeout(function(){
					process_badges();
				}, 1000);
			}
		});

		// Save a handler to the overlay
		overlay_api = $("#mies1").data("overlay");
		process_badges();

	});

	function process_badges() {
		if(typeof badges_unnotified == "undefined") {
			return;
		}

		// Base Case (checks if there is a badge to be shown)
		if(badges_unnotified.length <= 0) {
			return;
		}

		var badge = badges_unnotified.pop();
		show_badge(badge);
	}

	function show_badge(badge) {
		if ((typeof badges_suppress_notification != 'undefined') && badges_suppress_notification) {
                        return;
                }
		
		badge.name = badge.name.replace("LIVESTRONG", "LIVE<strong>STRONG</strong>");
		badge.description = badge.description.replace("LIVESTRONG", "LIVE<strong>STRONG</strong>");
		var postHTML = $.template('popup_badge', badge);		
		var html = $(".simple_overlay").html(postHTML);
		
		$(".facebook_share").click(function(){
			// close the popup
			overlay_api.close();

			// Show the facebook share popup
			Facebook.ui({ 
				method: 'feed',
				name: badge.name +" Badge",
				link: "http://www.livestrong.com/prof/badges/",
				description:  " just achieved the "+ badge.name +" Badge: "+ badge.description.replace("!", "") + " on LIVESTRONG.COM",
				picture: "http://www.livestrong.com/media/images/badges/" +badge.image+ "_100x100.png"
			}, function(response){
			});

		});

		$(html).find(".close").click(function(){
			overlay_api.close();
		});

		$(html).find("#popup_checkbox").change(function(){
			if(this.checked === true) {

				//
				// Fire off ajax to not show any more badge popus
				//
				badges_suppress_notification = true;
				$.get('/api/badges/hide_notifications/', { value: 1 });

				badges_unnotified.length = 0; 
				overlay_api.close();
			} 
		});
		
		overlay_api.load();


		// remove a badge from the unnotified list 	
		$.post('/api/badges/awarded/'+badge.badge_awarded_id+'/', { notified_web: 1 });
	
	}

</script>








<div class="simple_overlay" id="mies1"></div>

<script type="text/html" id="popup_badge">	
	<div class="close_button close"></div>
	<div class="congrats">Congratulations!</div>
	<div class="earned">You have achieved the [!=name!] badge</div>
	<div class="badge_container">
		<div class="badge_image">
			<img class="reflect rheight20 ropacity20" src="http://i.lsimg.net/media/images/badges/[!=image!]_130x130.png" alt="[!=description!]" title="[!=description!]"/>
		</div>
		<p class="badge_compliment" >[!=description!]</p>
	</div>
	
	<div class="cta_button">
		<a class="css3_button blue_button_large facebook_share" href="#">Share</a>
	</div>

	<form id="no_badge_popups">
		<input type="checkbox" id="popup_checkbox" />
		<label for="popup_checkbox" > Don't show these badge pop-ups anymore.</label>
	</form>
</script>


<script type="text/javascript">

function testSiteMode() {
	try {
        if (window.external.msIsSiteMode()) {
			$.get('/api/ie9pinning/', { pinned: 1 });
        } else {
			$.get('/api/ie9pinning/', { pinned: 0 });
		}
    }
    catch(ex) {
        return;
    }
}

$(document).ready(function(){
	testSiteMode();
});

</script>

</body>
</html>