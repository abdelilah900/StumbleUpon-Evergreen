<script type="text/javascript" language="JavaScript">

$(document).ready(function(){
	$(".changer").click(function(){
		var $mainText = $('.resizeable');
		if($mainText.length != 0) {
			var currentSize = $mainText.css('font-size');
			var num = parseFloat(currentSize, 10);
			var unit = currentSize.slice(-2);
			if (this.id == 'font_bigger'){
				num = num * 1.25;
			} else if (this.id == 'font_smaller'){
				num = num / 1.25;
			}
			$mainText.css('font-size', num + unit);
		}
		return false;
	});
});


$(document).ready(function() {
	$("#comments").onVisible({ callback: function(){
		$("#comments .delayed_image").lazyImage({ errorImg: 'http://i.ehowcdn.com/images/thumbnail_unavailable_article.gif'});
	}});
	$(".comment").comments();
	$(".comment_form").comment_form({
		callback: function(comment_text){
			if($("#postToFacebook").attr("checked") == true) {

				var data = {};
				data.attachment = {}
				data.attachment.name = $("h1")[0].innerHTML.replace(/(<([^>]+)>)/ig,"");
				data.attachment.description = comment_text;
				data.attachment.href = document.location.href;

				var articleImageURL = $("link[rel=image_src]").attr("href");
				if(articleImageURL) {
					data.attachment.media = [{'type': 'image',
											  'src': articleImageURL,
											  'href': document.location.href }]
				}

				Facebook.publish(data);
			}
		}
	});
});

