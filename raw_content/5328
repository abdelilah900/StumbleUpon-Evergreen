        <script>
		jQuery('#top10ContentLinks a').click(function()
		{
			var voteTypeLink = this;
			var voteType = voteTypeLink.rel;
			var currentVoteType = jQuery('#top10ContentLinks .selectedType').attr('rel');
	
			if (voteType != currentVoteType)
			{
				// Get the new listing. 
				var interstUrl = '';
				if (MPORA.globalVars.interest_id)
				{
					interstUrl = MPORA.globalVars.interest_id+'/';
				}
				
				if (MPORA.globalVars.uid > 0 && MPORA.globalVars.uri_segment_one == 'feed' && (MPORA.globalVars.uri_segment_three == 'interests' || MPORA.globalVars.uri_segment_three == ''))
				{
					interstUrl = 'userInterests/';
				}
				
				jQuery('#top10ContentContainer').load('/ajax/top10content/'+voteType+'/'+interstUrl, function()
				{
					jQuery('#top10ContentLinks .selectedType').removeClass('selectedType');	
					jQuery(voteTypeLink).addClass('selectedType');

					initImageLoad();
				});
			}
			
			return false;
		});
		</script>
	</div>
