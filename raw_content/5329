	var max_age_for_time_kicker = 1000 * 60 * 60 * 3;
	var now    = new Date();
	var now_ts = now.getTime();

	jQuery( 'span.timeKicker').each( function( key )
	{
		var article_ts = jQuery( this ).attr( 'rel' ) * 1000;
		
		if( ( now_ts - article_ts ) <= max_age_for_time_kicker )
		{
			jQuery( this ).show();
			//console.log( now_ts - article_ts );
		}	
		//console.log( now.toUTCString() );
		//test = new Date( article_ts );
		//console.log( test.toUTCString() );
	});

