<script type="text/javascript">
    document.observe('dom:loaded', function(){
        $('frm_signup_newsletter_dialog').observe('keypress', function(event){
            if (event.keyCode == 13)
            {
                signupNewsletterDialog();
            }
        });

        $('frm_signup_newsletter_dialog').observe('keydown', function(event){
            HideErrorMessage();
        });
    });
    function signupNewsletterDialogWomen()
    {
        $('newsletter_type_dialog').value = '21';
        signupNewsletterDialog();
    }
    function signupNewsletterDialogMen()
    {
        $('newsletter_type_dialog').value = '2';
        signupNewsletterDialog();
    }
    function checkSignupNewsletterDialog()
    {
        activeEle = 'frm_signup_newsletter_dialog';
        if(!isEmpty($('newsletter_dialog[email]')))
        {
            ShowNoticeMessage("Please enter your email", '');
            return false;
        }
        else if(!validateEmailByElement($('newsletter_dialog[email]')))
        {
            ShowNoticeMessage("Invalid email", '');
            return false;
        }
        return true;
    }
    function signupNewsletterDialog()
    {
        if(checkSignupNewsletterDialog('frm_signup_newsletter_dialog'))
        {
            jQuery.fancybox.showActivity();
            jQuery.cookie('newsletter_show_modal_dialog', '1', { expires: 30, path: '/' });
            jQuery.post(
                "http://www.80spurple.com/shop/newsletter/save",
                jQuery("#frm_signup_newsletter_dialog").serialize(),
                function(data){
                    jQuery.fancybox.hideActivity();

                    jQuery.fancybox('<p style="width: 370px; text-align: center;">Thank you! Your email address ' +$('newsletter_dialog[email]').value+ ', is now signed up to recieve 80&#39;s Purple email updates.</p><br /><h1 id="newsletter-dialog-counter" style="text-align: center;">5</h1>', {
                        'scrolling'		: 'no',
                        'padding'		: 20,
                        'transitionIn'	: 'none',
                        'transitionOut'	: 'none'
                    });
                    var newsletterDialogCounter = 5;
                    var newsletterDialogInvertal = setInterval(
                        function(){
                            newsletterDialogCounter --;
                            jQuery('#newsletter-dialog-counter').html(newsletterDialogCounter);
                            if (newsletterDialogCounter < 1) {
                                jQuery.fancybox.close();
                                clearInterval(newsletterDialogInvertal);
                            }
                        },
                        1000
                    );
                }
            );
        }
        return false;
    }


    var isShowNewsletterDialog = false;
    function showNewsletterDialog() {
        if (isShowNewsletterDialog == false) {
            jQuery.fancybox({
                'href'          : '#newsletter-dialog',
                'titlePosition' : 'inside',
                'transitionIn'  : 'none',
                'transitionOut' : 'none',
                'width'         : 352,
                'height'        : 252,

                onClosed        : function() {
                    jQuery.cookie('newsletter_show_modal_dialog', '1', { expires: 30, path: '/' });
                }
            });
        }
        isShowNewsletterDialog = true;
    }
    jQuery(document).ready(
        function (){
            var _allowDialog = jQuery.cookie('newsletter_show_modal_dialog');
            if (null == _allowDialog || _allowDialog == '') {
                var _waitingTime = parseInt("45") * 1000;
                setTimeout("showNewsletterDialog()", _waitingTime);
            }
        }
    );
	function login(){
		jQuery.cookie('newsletter_show_modal_dialog', '1', { expires: 30, path: '/' });
		location.href="/shop/customer/login";
	}
</script>
    </div>
</div>
