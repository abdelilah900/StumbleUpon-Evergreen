<script type="text/javascript">
    var conv = LF({
        domain: 'myvoicenation.fyre.co',
        site_id: '304539',
        article_id: 'Mzc4NzQyNjUtMQ==',
        conv_meta: {
            article_url: "http://www.sfweekly.com/slideshow/the-food-of-eat-real-festival-37874265/1/",
            tags: "san,slideshow,dining,san_slideshow,san_dining,slideshow_dining,san_slideshow_dining",
            title: "The Food of Eat Real Festival - San Francisco - Slideshows",
            sig: "c631f98fca10370fd9a3ff7e9edf34fa"
        }
    }).ready(function(){
        LF.CommentCount({
            replacer: function(element, count) {
                element.innerHTML = count;
            }
        });
        MVN.queue(function(){
            MVN.LF.login();
            LF.Dispatcher.addListener(MVN.LF.binds());
            MVN.LF.clean();
        });
    });

    /*
    ** LiveFyre Component
    */
    var LF_Component = function(controller){
        var me = this;
        me.MVN = controller;
        me.version = 'LiveFyre for MVN 1.0';
        me.autoInit = false;
    }
    LF_Component.prototype = {
        login: function(){
            var me = this;
            if(me.MVN.user||VVM.getCookie('MVN[anon]')){
                me.MVN.sso.getLfToken(function(o){
                    if(o&&o.status=='success'){
                        conv.login(o.token);
                        if(o.anon && o.anon == true){
                            me.clean();
                        }
                    }
                });
            }
            return false;
        },
        clean: function(){
            $('#livefyre .lf_comment_container').not('.lf_deleted').each(function(){
                var usr = $('.lf_comment_user', this).attr('jid');
                if(typeof usr == 'string' && usr.substr(0,5) == 'anon_'){
                    $('.hastooltip', this).removeClass('hastooltip');
                    $('a.lf_comment_user_nick', this).replaceWith(
                        $('<span style="font-weight:bold" title="Anonymous User"/>').text($('a.lf_comment_user_nick', this).text())
                    );
                    $('img.lf_main_avatar', this).css('cursor', 'default');
                    $('a.lf_user', this).replaceWith('<div class="lf_user" title="Anonymous User">'+$('a.lf_user', this).html()+'</div>');
                }
            });
        },
        binds: function(){
            var me = this;
            return {
                handle_auth_login: function(){
                    if(!me.MVN.user){
                                                var src_desc = '';
                        if(window.OAS_query_category||false){
                            switch(OAS_query_category.toLowerCase().replace(/\s/,'')){
                                case 'film':
                                case 'movies':
                                    src_desc = 1034;
                                    break;
                                case 'music':
                                    src_desc = 1035;
                                    break;
                                case 'news':
                                    src_desc = 1036;
                                    break;
                                case 'restaurants':
                                case 'dining':
                                case 'food':
                                    src_desc = 1037;
                                    break;
                                case 'bestof':
                                    src_desc = 1038;
                                    break;
                            }
                        }
                        me.MVN.omni.actionSrc = 'lf';
                        me.MVN.omni.action(me.MVN.omni.buildAction('login', {sid: '8|53|'+src_desc}));
                                            }
                },
                handle_auth_logout: function(){
                    if(me.MVN.user||VVM.getCookie('MVN[anon]')){
                                                me.MVN.omni.action('logout');
                                            }
                },
                handle_comment_shown: function(){
                    me.clean();
                }
            }
        }
    }
    
    MVN.onReady(function(){
        MVN.load({
            'LF':{
                component: LF_Component,
                params: {}
            }
        });
    });
</script>
