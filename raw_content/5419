<script type="text/haml" id="_pagination">
.page_container
  :if shotsCount > window.pageSize
    %ul#pages
      %li.previous{style: 'display: none;'}
        %a{href: '#'} Previous
      :each page in pages
        %li{class: 'page_num ' + page.cssClass}
          %a{href: '#'}= page.number.toString()
      %li.next
        %a{href: '#'} Next
    %a.view_all{href: '#'}= "View all " + shotsCount.toString() + " looks"
  :if shotsCount <= window.pageSize
    %span.already_viewing_all= "Viewing all " + shotsCount.toString() + " looks"

.all_container{style: 'display: none;'}
  %a.view_per_page{href: '#'}= "View " + window.pageSize.toString() + " per page"
</script>

<script type="text/html" id="__shots_by_product">
  <% var index = 0; %>
  <% for(var productIndex = 0; productIndex < collectionProducts.length; productIndex++) { %>
    <% var collectionProduct = collectionProducts[productIndex]; %>
    <% var shot = controller.findShot(collectionProduct.id.toString()); %>
    <% if(shot) { %>
      <li class="<%= index % 3 == 0 ? 'clear' : '' %>" id="shot_li_<%= shot._id %>">
        <div class="image">
          <div class="image_container">
            <div class="main_image" presrc="<%= controller.convertToCloudFront(shot.image) %>" src="" title="<%= collectionProduct.title %>"></div>
          </div>
          <div class="actions_container">
            <div class="actions">
              <img src="http://cdn.shopify.com/s/files/1/0053/6742/assets/ajax-loader-trans.gif?1288110192" class="loader" style="display: none;" />
              <span class="product_price <%= collectionProduct.variants[0].compare_at_price != null ? 'has_compare_price' : '' %>">
                <% if(collectionProduct.variants[0].compare_at_price != null) { %>
                  <span class="on_sale"><%= formatMoneyWithoutMultiplying(collectionProduct.price) %></span>
                  <br/>
                  <span class="compare_price"><%= formatMoneyWithoutMultiplying(collectionProduct.variants[0].compare_at_price) %></span>
                <% } else { %>
                  <%= formatMoneyWithoutMultiplying(collectionProduct.price) %>
                <% } %>
              </span>
              <a class="buy_shot" href="#" shotid="<%= shot._id %>">Buy this Look!</a>
            </div>
          </div>
        </div>
        <h3 class="product_description"><%= collectionProduct.title %></h3>
        <h3><%= collectionProduct.vendor %></h3>
      </li>
      <% index += 1; %>
    <% } %>
  <% } %>
</script>

<script type="text/html" id="__shots_by_shot">
  <% var index = 0; %>
  <% for(var shotIndex = 0; shotIndex < controller.shots.length; shotIndex++) { %>
    <% var shot = controller.shots[shotIndex]; %>
    <% if(shot) { %>
      <li class="<%= index % 3 == 0 ? 'clear' : '' %>" id="shot_li_<%= shot._id %>">
        <div class="image">
          <div class="image_container">
            <div class="main_image" presrc="<%= controller.convertToCloudFront(shot.image) %>" src="" title="<%= shot.title %>"></div>
          </div>
          <div class="actions_container">
            <div class="actions">
              <img src="http://cdn.shopify.com/s/files/1/0053/6742/assets/ajax-loader-trans.gif?1288110192" class="loader" style="display: none;" />
              <a class="buy_shot" href="#" shotid="<%= shot._id %>">Buy this Look!</a>
            </div>
          </div>
        </div>
        <h3><%= shot.title %></h3>     
      </li>
      <% index += 1; %>
    <% } %>
  <% } %>
</script>

<script type="text/html" id="__associations_by_product">
  <% if(shot.associations != undefined) { %>
    <% var uniqAssocs = uniqueAssociations(shot.associations); %>
    <% for(var i = 0; i < uniqAssocs.length; i++) { %>
      <% var association = uniqAssocs[i]; %>
      <% var product = association.product; %>
      <% if(product.images.length > 0) { %>
        <div class="hot_area" style="left: <%= association.x %>px; top: <%= association.y %>px;">
          <div class="spot" />
          <div class="product_popup_container">
            <div class="product_popup">
              <div class="thumb">
                <img src="<%= formatImage(product.images[0].src, 'thumb') %>" alt="<%= product.title %>" />
              </div>
              <div class="description">
                <h4><b><%= product.title %></b> by <%= product.vendor %></h4>
                <p class="price">
                  <span <% if(product.variants[0].compare_at_price != null) { %>class="on_sale"<% } %>><%= formatMoney(product.variants[0].price) %></span>
                  <% if(product.variants[0].compare_at_price != null) { %><span class="compare_price"><%= formatMoney(product.variants[0].compare_at_price) %></span><% } %>
                </p>
                <a class="buy" productid="<%= product.id %>" href="http://shopmarkethq.com/products/<%= product.handle %>">View Item</a>
              </div>
            </div>
          </div>
        </div>
      <% } %>
    <% } %>
  <% } %>
</script>

<script type="text/haml" id="_product">
  .buy_popup{productid: product.id}
    .images
      :if product.images.length > 0
        %img.large{src: formatImage(product.images[0], 'large')}
        :each image in product.images
          %a.thumb{href: '#'}
            %img{src: formatImage(image, 'thumb')}
    .description
      %h3
        %b= product.title
         by 
        = product.vendor
      .desc
        %p.price
          :if product.variants[0].compare_at_price != null
            %span.on_sale= formatMoneyWithoutMultiplying(product.variants[0].price)
            &nbsp;
            %span.compare_price= formatMoneyWithoutMultiplying(product.variants[0].compare_at_price)
          :if product.variants[0].compare_at_price == null
            = formatMoneyWithoutMultiplying(product.variants[0].price)
        = product.description
        %p
          %a{href: '/pages/size-chart', target: '_blank'} Size Chart
      :if product.available == true
        %ul.form_area
          :each option in product.options
            %li
              %label= option.name
              :if this.optionKey = __key__
                %select{class: option.name.toLowerCase()}
                  :each variantOption in uniqVariantForIndex(product, this.optionKey)
                    %option= variantOption
          %li.submit
            %input{type: 'submit', value: 'Add to Bag'}
      :if product.available == false
        %p
          %strong TEMPORARILY UNAVAILABLE
      .recommendations_container
        %h4 Also, You might like...
        %ul#recommendations
        %script{type: "text/javascript", src: "http://werecommendapp.com/recommendations/product/" + product.id + ".json?shop=marketstore.myshopify.com&callback=displayRecommendations"}
</script>

<script type="text/haml" id="_recommendation">
  :if recommendation.images.length > 0
    %li
      %a{href: '/products/' + recommendation.handle, target: '_blank'}
        %img{src: formatImage(recommendation.images[0].src, 'small')}
</script>

<script type="text/haml" id="_products">
  .buy_products_popup
    .shot
      %h3= shot.title
      .image
        %img{src: shot.image, title: shot.title}
    .products_area
      %a.size_chart{href: '/pages/size-chart', target: '_blank'} View our Sizing Chart
      %table.products{cellpadding: 0, cellspacing: 0}
        %tr
          %th{colspan: 2} Item Description
          %th &nbsp;
          %th &nbsp;
          %th Price
          %th &nbsp;
        :each association in uniqueAssociations(shot.associations)
          :if this.product = association.product
            %tr.product{productid: this.product.id, handle: this.product.handle}
              %td{colspan: 5}
                %img{src: "http://cdn.shopify.com/s/files/1/0053/6742/assets/ajax-loader.gif?134694"}
        %tr.no_border
          %td{colspan: 4, align: 'right'}
            %strong Total
          %td.total &nbsp;
        %tr.no_border
          %td{colspan: 4, align: 'right'} &nbsp;
          %td.submit
            %input{type: 'submit', value: 'Add all to Bag!'}
</script>

<script type="text/haml" id="_product_details">
  %td.image
    :if product.images.length > 0
      %img{src: formatImage(product.images[0], 'thumb')}
  %td.title
    %a{href: 'http://shopmarkethq.com/products/'+ product.handle, target: '_blank'}
      %b= product.title 
       by 
      = product.vendor
  :if product.available
    :each option in product.options
      %td.option
        %label= option.name
        %br/
        :if this.optionKey = __key__
          %select{class: option.name.toLowerCase()}
            :each variantOption in uniqVariantForIndex(product, this.optionKey)
              %option= variantOption
    :if product.options.length < 2
      %td &nbsp;
  :if !product.available
    %td.option{colspan: 2} TEMPORARILY UNAVAILABLE
  %td.price
    :if product.variants[0].compare_at_price != null
      %span.on_sale= formatMoneyWithoutMultiplying(product.variants[0].price)
      &nbsp;
      %span.compare_price= formatMoneyWithoutMultiplying(product.variants[0].compare_at_price)
    :if product.variants[0].compare_at_price == null
      = formatMoneyWithoutMultiplying(product.variants[0].price)
  %td.remove
    %a{href: '#'} Remove
</script>

<script type="text/javascript">
// Simple JavaScript Templating
// John Resig - http://ejohn.org/ - MIT Licensed
// See: http://ejohn.org/blog/javascript-micro-templating/
(function(){
  var cache = {};
  
  this.tmpl = function tmpl(str, data){
    // Figure out if we're getting a template, or if we need to
    // load the template - and be sure to cache the result.
    var fn = !/\W/.test(str) ?
      cache[str] = cache[str] ||
        tmpl(document.getElementById(str).innerHTML) :
      
      // Generate a reusable function that will serve as a template
      // generator (and which will be cached).
      new Function("obj",
        "var p=[],print=function(){p.push.apply(p,arguments);};" +
        
        // Introduce the data as local variables using with(){}
        "with(obj){p.push('" +
        
        // Convert the template into pure JavaScript
        str
          .replace(/[\r\t\n]/g, " ")
          .split("<%").join("\t")
          .replace(/((^|%>)[^\t]*)'/g, "$1\r")
          .replace(/\t=(.*?)%>/g, "',$1,'")
          .split("\t").join("');")
          .split("%>").join("p.push('")
          .split("\r").join("\\'")
      + "');}return p.join('');");
    
    // Provide some basic currying to the user
    return data ? fn( data ) : fn;
  };
})();
</script>

