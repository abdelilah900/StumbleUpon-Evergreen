  <script type="text/javascript" charset="utf-8">
    $(document).ready(function() {
      $('#nav ul.primary > li').hover(function(){$(this).addClass('hovering');}, function(){$(this).removeClass('hovering');});
      $('#search_input').focus(function() {
        if($(this).val() == 'Search') $(this).val('');
      }).blur(function() {
        if($(this).val() == '') $(this).val('Search');
      });
    });
  </script>

  <script type="text/javascript">
    $(function() {
      var sortedAlphaByColumn = function(array, groupCount, groupLength) {
        var results = [];
        for(var i = 0; i < groupCount; i++) {
          results.push([]);
        }
        var groupIndex = 0;
        var inGroupIndex = 0;
        $(array).each(function(arrayIndex) {
          var anchor = $(this).find('a').get(0);
          results[groupIndex].push({href: anchor.href, html: $(anchor).html()});
          if(inGroupIndex < groupLength - 1) {
            inGroupIndex += 1;
          }
          else {
            groupIndex += 1;
            inGroupIndex = 0;
          }
        });
        var flattened = [];
        var tooLongIndex = 0;
        while(results.length > 0 && tooLongIndex < 200) {
          for(var groupIndex = 0; groupIndex < groupCount; groupIndex++) {
            if(results[groupIndex] == undefined) {
              break;
            }
            else if(results[groupIndex].length == 0) {
              results.splice(groupIndex, 1);
            }
            else {
              element = results[groupIndex].splice(0, 1);
              flattened.push(element[0]);
            }
          }
          tooLongIndex += 1;
        }
        return flattened;
      }
      var alignNav = function(ul, lis, numberColumns, colWidth, sizeMultiplier) {
        $(ul).width($(ul).width() * sizeMultiplier);
        var groupLength = lis.length / numberColumns;
        results = sortedAlphaByColumn(lis, numberColumns, groupLength)
        $(ul).html('');
        $(results).each(function() {
          $(ul).append('<li class="second_link"><a href="' + this.href + '">' + this.html + '</a>');
        });
        $(ul).find('li').css({float: 'left', width: colWidth}).each(function(index) {
          if(index % numberColumns == 0) {
            $(this).css('clear', 'both');
          }
        });
      }

      $('ul.secondary').each(function() {
        var lis = $(this).find('li');
        var length = $(lis).length;
        if(length > 10 && length < 20) {
          alignNav(this, lis, 2, '45%', 2);
        }
        else if(length > 20) {
          alignNav(this, lis, 3, '33%', 3.3);
        }
      });
    });
  </script>

