  function isiPhone() {
    var agent = navigator.userAgent.toLowerCase();
    return agent.match(/iPhone/i) || agent.match(/iPad/i);
  }
  
  function formatMoney(money) {
    return formatMoneyWithoutMultiplying(money * 100);
  }
  
  function formatMoneyWithoutMultiplying(money) {
    try {
      return Shopify.formatMoney(money, "${{amount_no_decimals}} AUD");
    }
    catch(err) {
      return '';
    }
  }
  
  function uniqueAssociations(associations) {
    var results = [];
    var alreadyAdded = [];
    $.each(associations, function() {
      var test = this.x.toString() + this.y.toString();
      if(alreadyAdded.indexOf(test) == -1) {
        results.push(this);
        alreadyAdded.push(test);
      }
    });
    return results;
  }
  
  function colliding(element, event) {
    var bounds = $(element).bounds();
    var x = event.pageX;
    var y = event.pageY;
    return (x > bounds.left && x < bounds.right && y > bounds.top && y < bounds.bottom);
  }
  
  function uniqVariantForIndex(product, index) {
    var results = Shopify.map(product.variants, function(e) {
      var option_col = "option" + (parseInt(index)+1);
      return (e[option_col] == undefined) ? null : e[option_col];
    });
    return (results[0] == null ? null : Shopify.uniq(results));
  }
  
  function setupZoom(largeImage) {
    $('.buy_popup img.large').addpowerzoom({largeimage: largeImage, magnifiersize: [200, 200]});
  }
  
  function displayRecommendations(recommendations) {
    $.each(recommendations, function() {
      $('ul#recommendations').append(Haml.render($('#_recommendation').html(), {locals: {recommendation: this}}));
    });
  }
  
  function watchForThumbClick() {
    $('a.thumb').live('click', function() {
      var index = $(this.parentNode).find('a.thumb').index(this);
      var image = $(this).find('img').get(0).src.replace(/_thumb/, '');
      $('.buy_popup img.large').bind('load', function() {
        setupZoom(formatImage(image, 'original'));
        $(this).unbind('load');
      });
      $('.buy_popup img.large').attr('src', formatImage(image, 'large'));
      return false;
    });
  }
  
  function addOneCartCount() {
    window.cartCount += 1;
    $('a#cart').html(window.cartCount + ' ' + (window.cartCount == 1 ? 'item' : 'items') + ' in <em>your bag</em>');
  }
  
  function range(arr, start, finish) {
    var result = [];
    for(var i = start; i < finish; i++) {
      if(i < arr.length) result.push(arr[i]);
    }
    return result;
  }
  
  function ProductsController() {
    this.shots = [];
    this.currentPage = 1;
    this.pageSize = 18;
    this.viewingAll = false;
  }
  
  ProductsController.prototype.setupEvents = function() {
    var self = this;
    $(document).bind('shotsLoaded', function() { self.render(); });
    $(document).bind('setPage', function(ev, pageNumber) { self.currentPage = pageNumber; self.showByPage(); });
    $(document).bind('movePage', function(ev, direction) { self.currentPage += direction; self.showByPage(); });
    $(document).bind('viewAll', function(ev, direction) { self.viewingAll = true; self.showByPage(); });
    $(document).bind('viewFirst', function(ev, direction) { self.viewingAll = false; self.showByPage(); });
    $(document).bind('loadVisibleImages', function(ev) { self.loadVisibleImages(); });
  }
  
  ProductsController.prototype.loadShots = function() {
    var self = this;
    $.getJSON('http://coordinatrapp.com/shots-only/marketstore.myshopify.com/brands-minkpink.json?callback=?', function(data) {
      $.each(data, function() {
        self.shots.push(this.attributes);
      });
      $(document).trigger('shotsLoaded');
    });
  };
  
  ProductsController.prototype.findShot = function(productId) {
    var shot = null;
    $.each(this.shots, function() {
      if(this.product_ids.indexOf(productId.toString()) != -1) shot = this;
    });
    return shot;
  };
  
  ProductsController.prototype.render = function() {
    var self = this;
    $('#shot_area').append('<ul id="shots"></ul>');
    var productsLength = self.activeShots(collectionProducts).length;
    if(window.location.href.toString().match(/\&whatsnew=true/) || window.location.href.toString().match(/\?whatsnew=true/)) {
      productsLength = this.shots.length;
      $('#shot_area ul#shots').append(tmpl("__shots_by_shot", {controller: this}));
    }
    else if(window.location.href.toString().match(/\&looks=true/) || window.location.href.toString().match(/\?looks=true/)) {
        $('#shot_area ul#shots').append(tmpl("__shots_by_product", {controller: this, collectionProducts: collectionProducts}));
    }
    else {
      $('#collection').show();
      $('.pagination_area').hide();
    }
    
    if(isiPhone()) {
      $('#shot_area ul#shots li').click(function() { self.hoverOnShot(this); });
    }
    else {
      $('#shot_area ul#shots li').hover(function() { self.hoverOnShot(this) }, function() { self.hoverOffShot(this) });
    }
    self.showByPage();
    $(document).trigger('shotsRendered', [productsLength, self.pageSize]);
  };
  
  ProductsController.prototype.activeShots = function(colProducts) {
    var result = [];
    for(var i = 0; i < colProducts.length; i++) {
      if(this.findShot(colProducts[i].id.toString())) {
        result.push(colProducts[i]);
      }
    }
    return result;
  }
  
  ProductsController.prototype.hoverOnShot = function(li) {
    var self = this;
    if($(li).find('.processed').length == 0) {
      $(li).find('.actions').addClass('processed');
      var shotId = li.id.replace(/shot_li_/, '');
      $(li).find('.loader').show();
      $.getJSON('http://coordinatrapp.com/shots-only-by-id/marketstore.myshopify.com/' + shotId + '.json?callback=?', function(shot) {
        var html = $(li).find('.actions').html();
        html = html + tmpl("__associations_by_product", {shot: shot.attributes});
        $(li).find('.actions').html(html); // .append wasn't working in ie8.
        $(document).trigger('hotAreaLoaded', [li]);
        self.addHotAreaEvents(li);
        $(li).find('.loader').hide();
      });
    }
    $(li).addClass('show_spot');
  };
  
  ProductsController.prototype.hoverOffShot = function(li) {
    $(li).removeClass('show_spot');
  };
  
  ProductsController.prototype.addHotAreaEvents = function(li) {
    $(li).find('.hot_area').each(function() {
      if(isiPhone()) {
        $(this).hover(function() {
          $('ul#shots li .hot_area').removeClass('hover');
          $(this).addClass('hover');
        },
        function() {
          $('ul#shots li .hot_area').removeClass('hover');
        });
      }
      else {
        $(this).hover(
          function() {
            $(this).addClass('hover');
            var hot_area = this;
            $('ul#shots li .hot_area').each(function() {
              if(this != hot_area) {
                $(this).removeClass('hover');
              }
            });
            setTimeout(function() {
              $(document).mousemove(function(event) {
                if(!colliding(hot_area, event)) $(hot_area).removeClass('hover');
                return false;
              });
            }, 10);
          }, 
          function() {}
        );
      }
    });
  }
  
  ProductsController.prototype.convertToCloudFront = function(img) {
    return img.replace('assets.coordinatorapp.com.s3.amazonaws.com', 'dcuzfdkf5yz37.cloudfront.net');
  }
  
  ProductsController.prototype.start = function() {
    return (this.currentPage - 1) * this.pageSize;
  }
  
  ProductsController.prototype.finish = function() {
    return this.start() + this.pageSize;
  }
  
  ProductsController.prototype.showByPage = function() {
    $('#shot_area #shots li').hide();
    var arr = this.viewingAll ? $('#shot_area #shots li') : range($('#shot_area #shots li'), this.start(), this.finish());
    $.each(arr, function() {
      $(this).show();
    });
    $(document).trigger('loadVisibleImages');
  }
  
  ProductsController.prototype.loadVisibleImages = function() {
    $('#shot_area #shots li:visible').each(function() {
      var image = $(this).find('.main_image');
      if(image.attr('src') != image.attr('presrc')) {
        image.attr('src', image.attr('presrc'));
        image.replaceWith('<img src="' + image.attr('presrc') + '" title="' + image.attr('title') + '" />');
      }
    });
    $('#body').css('height', ($('#body .container').height() + 81) + 'px'); // ie8 fix
  }
  
  function ProductCache() {
    var self = this;
    this.products = [];
    $(document).bind('productRetrieved', function(ev, product) { self.products.push(product); });
  }
  
  ProductCache.prototype.findProduct = function(id) {
    var product = null;
    $.each(this.products, function() {
      if(this.id.toString() == id.toString()) product = this;
    });
    return product;
  }
  
  ProductCache.prototype.findVariantWithValues = function(product, values) {
    var selectedVariant = null;
    $.each(product.variants, function() {
      var isCorrectVariant = true;
      var variant = this;
      $.each(values, function(index) {
        if(variant['option' + (index + 1).toString()] != this) isCorrectVariant = false;
      });
      if(isCorrectVariant) selectedVariant = variant;
    });
    return selectedVariant;
  }
  
  ProductCache.prototype.watchForBuySubmit = function() {
    var self = this;
    $('li.submit input').live('click', function() {
      var buyPopup = $(this).parents('.buy_popup');
      var product = self.findProduct($(buyPopup).attr('productid'));
      var formArea = $(buyPopup).find('.form_area');
      var values = [];
      $(formArea).find('select').each(function() {
        values.push($(this).val());
      });
      var variant = self.findVariantWithValues(product, values);
      $.post('/cart/add.js', {quantity: 1, id: variant.id}, function() {
        addOneCartCount();
        $(formArea).find('li.submit').html('<p><a href="/cart">Proceed to checkout</a></p>');
      }, "json");
      
      return false;
    });
  }
  
  ProductCache.prototype.watchForIndividualSoldOutChange = function(product, currentSelect, allSelects) {
    var currentSelectIndex = allSelects.index(currentSelect);
    var variantsWithChangedOption = [];
    $.each(product.variants, function() { // Select variants that have the option that the user selected.
      if(this['option' + (currentSelectIndex + 1).toString()] == $(currentSelect).val()) variantsWithChangedOption.push(this);
    });
    // TODO: Refactor.
    if(allSelects.length == 1) { // This is different because you aren't watching the other selects.
      var oldValue = $(currentSelect).val();
      var selectToUpdate = currentSelect;
      $(currentSelect).html('');
      $.each(product.variants, function() {
        var value = this.option1;
        var isSelected = value == oldValue ? ' selected' : '';
        if(this.available) {
          $(selectToUpdate).append('<option' + isSelected + '>' + value + '</option>');
        }
        else {
          $(selectToUpdate).append('<option value="' + value + '"' + isSelected + '>' + value + ' (Sold Out)</option>');
        }
      });
    }
    else {
      allSelects.each(function(index) {
        if(this != currentSelect) {
          var selectToUpdate = this;
          var oldValue = $(this).val();
          $(this).html('');
          $.each(variantsWithChangedOption, function() {
            var value = this['option' + (index + 1).toString()];
            var isSelected = value == oldValue ? ' selected' : '';
            if(this.available) {
              $(selectToUpdate).append('<option' + isSelected + '>' + value + '</option>');
            }
            else {
              $(selectToUpdate).append('<option value="' + value + '"' + isSelected + '>' + value + ' (Sold Out)</option>');
            }
          });
        }
      });
    }
  }
  
  ProductCache.prototype.watchForSoldOutChange = function() {
    var self = this;
    $('body').delegate('ul.form_area select', 'change', function() {
      var select = this;
      var buyPopup = $(this).parents('.buy_popup');
      var product = self.findProduct($(buyPopup).attr('productid'));
      self.watchForIndividualSoldOutChange(product, select, $('ul.form_area select'));
      self.checkForSoldOut();
    });
  }
  
  ProductCache.prototype.watchForBuyShotSoldOutChange = function() {
    var self = this;
    $('body').delegate('.buy_products_popup tr.product select', 'change', function() {
      var select = this;
      var allSelects = $(this).parents('tr').find('select');
      var product = self.findProduct($(this).parents('tr').attr('productid'));
      self.watchForIndividualSoldOutChange(product, select, allSelects);
    });
  }
  
  ProductCache.prototype.checkForSoldOut = function() {
    var soldOut = false;
    $('ul.form_area select').each(function() {
      if($(this).find('option:selected').html().indexOf('Sold Out') != -1) soldOut = true;
    });
    if(soldOut) {
      $('li.submit input').attr('disabled', 'disabled');
    }
    else {
      $('li.submit input').removeAttr('disabled');
    }
  }
  
  function SingleProductController() {
  }
  
  SingleProductController.prototype.setupEvents = function() {
    var self = this;
    $(document).bind('hotAreaLoaded', function(ev, li) { self.listenForClick(li) });
  }
  
  SingleProductController.prototype.listenForClick = function(li) {
    $(li).find('.hot_area a.buy').click(function() {;
      $.getJSON(this.href + '.js', function(product) {
        $(document).trigger('productRetrieved', [product]);
        var popup = Haml.render($('#_product').html(), {locals: {product: product}});
        $.fancybox(popup, {'autoDimensions': false, 'autoScale': false, 'width': 700, 'height': 570, 'transitionIn': 'none', 'transitionOut': 'none', 'scrolling': 'no'});
        setupZoom(formatImage(product.images[0], 'original'));
        $('ul.form_area select:first').trigger('change');
      });
      return false;
    });
  }
  
  function ShotController(productCache) {
    this.productCache = productCache;
  }
  
  ShotController.prototype.setupEvents = function() {
    this.listenForClick();
    this.listenForRemoveClick();
    this.listenForBuyShotClick();
  }
  
  ShotController.prototype.listenForClick = function(li) {
    var self = this;
    $('a.buy_shot').live('click', function() {
      $.getJSON('http://coordinatrapp.com/shots-only-by-id/marketstore.myshopify.com/' + $(this).attr('shotid') + '.json?callback=?', function(shot) {
        shot = shot.attributes;
        var popup = Haml.render($('#_products').html(), {locals: {shot: shot}});
        $.fancybox(popup, {'autoDimensions': false, 'autoScale': false, 'width': 860, 'height': 570, 'transitionIn': 'none', 'transitionOut': 'none', 'scrolling': 'no'});
        self.updateShotDetails();
      });
      return false;
    });
  }
  
  ShotController.prototype.listenForRemoveClick = function() {
    var self = this;
    $('td.remove a').live('click', function() {
      $(this).parents('tr.product').remove();
      self.updateShotPrice();
      return false;
    });
  }
  
  ShotController.prototype.updateShotDetails = function() {
    var self = this;
    $('tr.product').each(function() {
      var tr = this;
      $.getJSON('http://shopmarkethq.com/products/' + $(this).attr('handle') + '.js', function(product) {
        $(tr).html(Haml.render($('#_product_details').html(), {locals: {product: product}}));
        self.updateShotPrice();
        $(document).trigger('productRetrieved', [product]);
        $(tr).find('select:first').trigger('change');
      });
    });
  }
  
  ShotController.prototype.updateShotPrice = function() {
    var sum = 0;
    $('table.products tr.product td.price').each(function() {
      var value = $(this).html();
      if($(this).find('.on_sale').length > 0) {
        value = $(this).find('.on_sale').html();
      }
      sum += parseFloat(value.replace('$', '').replace(' AUD', ''));
    });
    $('table.products td.total').html(formatMoney(sum));
  }
  
  ShotController.prototype.listenForBuyShotClick = function() {
    var self = this;
    $('.buy_products_popup td.submit input').live('click', function() {
      var popup = $(this).parents('.buy_products_popup');
      var productTrs = $(popup).find('tr.product');
      $(productTrs).each(function(index) {
        var tr = this;
        setTimeout(function() {
          var product = self.productCache.findProduct($(tr).attr('productid'));
          var values = [];
          $(tr).find('select').each(function() {
            values.push($(this).val());
          });
          var variant = self.productCache.findVariantWithValues(product, values);
          if(variant.inventory_quantity > 0) {
            $.post('/cart/add.js', {quantity: 1, id: variant.id}, function() {
              addOneCartCount();
              $(tr).addClass('in_bag');
              if(index + 1 == productTrs.length) {
                $(popup).find('td.submit').html('<a href="/cart">Proceed to checkout</a>');
              }
            }, "json");
          }
        }, index * 250);
      });
    });
  }
  
  function PaginationController() {
    var self = this;
    $(document).bind('shotsRendered', function(ev, shotsCount, pageSize) { self.render(shotsCount, pageSize); });
    $(document).bind('setPage', function(ev, currentPage) { self.highlightPage(currentPage); });
    $(document).bind('movePage', function(ev, direction) { self.highlightMovedPage(direction); });
    $(document).bind('viewAll', function(ev) { self.togglePaginationView(); });
    $(document).bind('viewFirst', function(ev) { self.togglePaginationView(); });
  }
  
  PaginationController.prototype.render = function(shotsCount, pageSize) {
    var pages = [];
    var pageCount = parseInt(shotsCount / pageSize);
    if(shotsCount % pageSize > 0) pageCount += 1;
    for(var i = 1; i <= pageCount; i++) { 
      pages.push({'number': i, 'cssClass': (i == window.currentPage ? 'current' : '')}); 
    };
    $('.pagination_area').html(Haml.render($('#_pagination').html(), {locals: {pages: pages, shotsCount: shotsCount}}));
    this.listenForChangeOfPage();
    this.listenForViewChange();
  }
  
  PaginationController.prototype.listenForChangeOfPage = function() {
    $('.pagination_area ul li.page_num a').click(function() {
      $(document).trigger('setPage', [parseInt($.trim($(this).html()))]);
      return false;
    });
    $('.pagination_area ul li.previous a').click(function() {
      $(document).trigger('movePage', [-1]);
      return false;
    });
    $('.pagination_area ul li.next a').click(function() {
      $(document).trigger('movePage', [1]);
      return false;
    });
  }
  
  PaginationController.prototype.highlightPage = function(currentPage) {
    var pageCount = $('.pagination_area ul li.page_num').length;
    $('.pagination_area ul li').removeClass('current');
    $('.pagination_area ul li a').each(function() {
      if(currentPage == parseInt($.trim($(this).html()))) {
        $(this.parentNode).addClass('current');
      }
    });
    if(currentPage < pageCount) { $('.pagination_area ul li.next').show(); }
    else { $('.pagination_area ul li.next').hide(); }
    
    if(currentPage > 1) { $('.pagination_area ul li.previous').show(); }
    else { $('.pagination_area ul li.previous').hide(); }
  }
  
  PaginationController.prototype.highlightMovedPage = function(direction) {
    var currentPage = parseInt($.trim($('.pagination_area ul li.current a').html())) + direction;
    this.highlightPage(currentPage);
  }
  
  PaginationController.prototype.listenForViewChange = function() {
    $('.pagination_area .page_container a.view_all').click(function() {
      $(document).trigger('viewAll');
      return false;
    });
    $('.pagination_area .all_container a.view_per_page').click(function() {
      $(document).trigger('viewFirst');
      return false;
    });
  }
  
  PaginationController.prototype.togglePaginationView = function() {
    $('.pagination_area .page_container').toggle();
    $('.pagination_area .all_container').toggle();
  }
  
  $(document).ready(function() {
    var singleProductController = new SingleProductController();
    singleProductController.setupEvents();
    
    var productsController = new ProductsController();
    productsController.setupEvents();
    
    var productCache = new ProductCache();
    productCache.watchForBuySubmit();
    productCache.watchForSoldOutChange();
    productCache.watchForBuyShotSoldOutChange();
    
    var shotController = new ShotController(productCache);
    shotController.setupEvents();
    
    var paginationController = new PaginationController();
    
    productsController.loadShots();
    
    watchForThumbClick();
  });
</script>
      </div>
    </div>
    
  </div>
  
