      <script type="text/javascript" charset="utf-8">
        function getTweets () {

          var user = "dezeen";
          var count = 10;

          var url = "http://api.twitter.com/statuses/user_timeline.json";
          url += "?screen_name="+user;
          url += "&count="+count;

          $.ajax({
            url:url,
            type: 'GET',
            dataType: 'jsonp',
            timeout: 1000,
            error: tweetError,
            success: tweetSuccess
          });
        }

        function tweetSuccess (e, xml, opt) {
          if (e.length > 0) {
            $(e).each(function() {
              var tweet = makeTweet(this);
              $("#twit-container").append(tweet);
            });
          } else {
            $("#tweet-load").remove();
          }
        }

        function tweetError (e, xml, opt) {
          var tweet = makeTweet(e);
          $("#twit-container").append(tweet);
        }

        function makeTweet(obj) {
          var html = "<li class=\"tweet\" id=\"tweet-"+obj.id+"\">";
          html += "<a href=\"http://twitter.com/dezeen/statuses/"+obj.id+"\">"+obj.text+"</a></li>";
          return html;
        }

        getTweets();
      </script>

