<script language="Javascript">
var cornerOfficeOpen = false;
function ToggleCornerOffice() {
	if (!cornerOfficeOpen) {
		// open it
		$('#corneroffice_content').css('display','block');
		// switch the image
		$("#corneroffice_arrow").attr('src', 'http://img.deusm.com/internetevolution/arrow_open.gif')
		// set boolean
		cornerOfficeOpen = true;
	} else {
		// close it
		$('#corneroffice_content').css('display','none');
		// switch the image
		$("#corneroffice_arrow").attr('src', 'http://img.deusm.com/internetevolution/arrow_closed.gif')
		// set boolean
		cornerOfficeOpen = false;
	}
}
</script>

