<script type="text/javascript">
var strReviewerUserID = 'fa351109608d06a8af846c6b4247d712';

function callback(responseBatch)
{
	var user = responseBatch.Responses[0].User;
	var strURL;
	strURL = user.AvatarPhotoUrl;
	var objReviewerAvatar;
	objReviewerAvatar = document.getElementById('imgReviewerAvatar');
	if (strURL != '')
	{
		objReviewerAvatar.src = strURL;
		objReviewerAvatar.style.display = 'block';
	}
	
}

if (strReviewerUserID != '')
{
	var requestBatch = new RequestBatch();

	var reviewerKey = new UserKey(strReviewerUserID);

	requestBatch.AddToRequest(reviewerKey);   
	requestBatch.BeginRequest(serverUrl, callback);
}


</script>

