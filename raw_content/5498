var description=document.title;
description=description.replace(" at Kraftcanada.com", "");
description=description.replace(" sur kraftcanada.com", "");
description=description.replace("recette de ", "");
description=description.replace("&","and");

var tagurl=baseurl + 'url=' + shareurl + '&media=' + media + '&description=' + description;

tagurl=encodeURI(tagurl);

// Creating an new anchor with an Object
