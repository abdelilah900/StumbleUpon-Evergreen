  <div class="block-content">
    <div class="dart-ad dart-ad-300x252" id="dart-ad-13-13-129682270">
    <noscript class="darts-ad-noscript">
    <a href="http://ad.doubleclick.net/jump/cdg.examiner2.national/;tt=technology;plc=national;chn=gadgets%20%26%20tech;subc=tech%20gear;sect=tech%20gear;nid=33343711;top=gadgets%20%26%20tech;top=tech%20gear;ed=national;uid=2059686;etid=142431;pgtp=article;tile=13;pos=13;sz=300x252;kw=;ord=129682270?" target="_blank"><img src="http://ad.doubleclick.net/ad/cdg.examiner2.national/;tt=technology;plc=national;chn=gadgets%20%26%20tech;subc=tech%20gear;sect=tech%20gear;nid=33343711;top=gadgets%20%26%20tech;top=tech%20gear;ed=national;uid=2059686;etid=142431;pgtp=article;tile=13;pos=13;sz=300x252;kw=;ord=129682270?" width="300" height="252" alt="" /></a>
