<script type="text/javascript">

  function handleEmailFocus () {
    $('email').setStyle({color: '#000'});
    if ($F('email') == 'Enter email address') {
      $('email').setValue('');
    }
  }

  function handlePasswordFocus () {
    if ($F('password') == 'Enter password') {
      $('password_input').update(new Element('input', {'type': 'password', 'id': 'password', 'name': 'password', 'style': 'display=block', 'tabindex': 2}))
      $('password').focus();
    }
  }

</script>

