            (function($) {
        		
        		  var accountType  = 'Member';
        		  var accountTypeName  = 'gt';
        		  var accountSpecialty = '';
        		  var accountTypeAbbr = '';

        		  if( accountType == 'Expert' ) {
        			accountSpecialty = '';
        			accountTypeName  = 'pt';
        		  }

        		 if( accountType == 'Member') {
        			accountTypeAbbr = "con";
        	     } else if( accountType == 'Expert') {
        			accountTypeAbbr = "exp";
        		 } else if ( accountType == 'Hospital' ) {
        			accountTypeAbbr = "hsp";
        		 } else if ( accountType == 'KnowledgePartner'   ) {
        			accountType = 'KnowledgePartner';
        			accountTypeAbbr = "knp";
        		 } else if ( accountType == 'ContentProvider') {
        			accountTypeAbbr = "cpr";
        		 } else if (accountType == 'PharmaKnowledgePartner' ) {
        			accountType = 'Pharma';
        			accountTypeAbbr = "php";
        		 }

