// Create player instance
var cbsiPlayer = new CBSi.app.VideoPlayer.UVP(global);

// Set up UVP external interface calls
var onCanPlayerReady    = function()        { cbsiPlayer.onCBSIPlayerReady(); }
var isJSReady           = function()        { return cbsiPlayer.isJSReady(); }
var getUVPVersion       = function()        { return cbsiPlayer.getUVPVersion(); }
var getAdsVersion       = function()        { return cbsiPlayer.getAdsVersion(); }
var getServerTime       = function()        { return cbsiPlayer.clock.time; }
var getShowAd           = function()        { return cbsiPlayer.getShowAd(); }
var setShowAd           = function(showAd)  { cbsiPlayer.setShowAd(showAd); }
var getChannelList      = function()        { return cbsiPlayer.global.state.channelList; }
var getChannelData      = function()        { return cbsiPlayer.global.state.channelData; }
var getVideoData        = function()        { return cbsiPlayer.global.state.videoData; }
var getTodaysGroupsData = function()        { return cbsiPlayer.global.state.todaysGroupsData; }
var setChannel          = function(channel) { cbsiPlayer.global.state.currentChannel = channel; }
var getChannel          = function()        { return cbsiPlayer.global.state.currentChannel; }
var playVideo           = function(pid)     { cbsiPlayer.playVideo(pid); }
var getClockTime        = function()        { return cbsiPlayer.clock.time; }
var getTournamentInfo   = function()        { return cbsiPlayer.global.state.golfData.tournamentInfo; }
var getScoringData      = function()        { return cbsiPlayer.global.state.golfData.scoring; }
var getBufferTime       = function()        { return cbsiPlayer.global.params.bufferTime*600*1000; }
var setDisplayState     = function(state)   { cbsiPlayer.global.params.fullscreen = state; }

cbsiPlayer.currentRound = '';

// Set up error slates here
cbsiPlayer.onStateChange = function(e) {
    cbsiPlayer.playState = e;
    var states = {
        "-1"    : "unstarted",
        "0"     : "stopped",
        "1"     : "playing",
        "2"     : "paused",
        "3"     : "buffering",
        "5"     : "thumbnail image cued",
        "100"   : "domain blacklisted",
        "110"   : "video not available",
        "120"   : "video geo-blocked",
        "130"   : "data load error",
        "140"   : "ad blocked"
    };

    cbsiPlayer.logger.log('onStateChange',e+': '+states[e]);
    if (e >= 100) {
        // hide any other overlays
        cbsiPlayer.hideAllOverlays();
        cbsiPlayer.api.activateOverlay('error'+e,'on');
    }
    else {
        for (var i in states) {
            if (parseFloat(i) >= 100) { cbsiPlayer.api.activateOverlay('error'+i,'off'); }
        }
    }
}

cbsiPlayer.onPlaylistStart = function (e) {}
cbsiPlayer.onPlayListEnd = function (e) {
    // push vod clips to history
    if (!cbsiPlayer.global.state.currentVideo.is_live) {
        cbsiPlayer.global.state.history.push(cbsiPlayer.global.state.currentVideo.pid);
    }

    // remove this pid from playlist
    if (cbsiPlayer.getArrayIndex(cbsiPlayer.global.state.playlist,cbsiPlayer.global.state.currentVideo.pid) != -1) {
        cbsiPlayer.global.state.playlist.splice(cbsiPlayer.getArrayIndex(cbsiPlayer.global.state.playlist,cbsiPlayer.global.state.currentVideo.pid), 1);
    }

    // stop video if playlist is empty
    if (!cbsiPlayer.global.state.playlist.length)
        cbsiPlayer.stopVideo();
    else { // or play next video if it's not
        cbsiPlayer.playVideo(cbsiPlayer.global.state.playlist[0]);
        cbsiPlayer.global.state.currentVideo = cbsiPlayer.global.state.videoData[cbsiPlayer.global.state.playlist[0]][0];
    }
}

cbsiPlayer.onPlayerLoaded = function() {
    $('#progressHolder').fadeOut('slow', function(){
        $('#videoHolder').css('top', '0').fadeIn('slow');
    });
}

cbsiPlayer.onAdEnd = function(e) {
    if (cbsiPlayer.global.params.fullscreen == 'normal') {
        cbsiPlayer.api.activateOverlay('breakingnews','on');
    } else {
        cbsiPlayer.api.activateOverlay('nowoncourse','off');
    }
}

cbsiPlayer.onContentStart = function(e) {
    if (cbsiPlayer.global.params.fullscreen == 'normal') {
        cbsiPlayer.api.activateOverlay('breakingnews','on');
    } else {
        cbsiPlayer.api.activateOverlay('nowoncourse','off');
    }
}

cbsiPlayer.cuePointHandler = function(cp) {
    cbsiPlayer.logger.log('cuePointHandler', cp);

    // default cue point handler (MLBAM format)
    if (cp && cp != null && cp.parameters && cp.parameters.URL && cp.parameters.URL != '0') {
        var ar = cp.parameters.URL.split('=');
        if (ar.length == 2) {
            cp.parameters.URL = {};
            cp.parameters.URL[ar[0]] = ar[1];
        }
        var adIdAr = cp.parameters.URL.AD_ID.split(':');
        var adId = adIdAr[0];

        if (adId) {
            if (cbsiPlayer.global.params.showCompAd && cp.name == 'URL') {
                switch (adId)
                {
                    case 'start':
                        break;
                    case 'end':
                        cbsiPlayer.playListEnd();
                        break;
                }
            }
        }
    }
}

cbsiPlayer.updateData = function(data) {
    clearTimeout(cbsiPlayer.dataIntId);
    data = eval( '(' + decodeURIComponent(data) + ')' );
    var time = cbsiPlayer.clock.time;
    var playlist = cbsiPlayer.global.state.playlist;

    var dataTemp = {};
    var livePids = [];
    var livePlaylist = [];

    // (re)build video data
    for (var vidRef in data) {
        var v = data[vidRef];

        var channel;
        for (var i in cbsiPlayer.global.state.channelData) {
            if (v.channel == i) {
                channel = cbsiPlayer.global.state.channelData[i];
                break;
            }
        }

        dataTemp[v.pid] = (dataTemp[v.pid]) ? dataTemp[v.pid] : [];

        v.is_live = parseInt(v.is_live);

        if (v.is_live) {
            // need to convert s to ms and add buffer
            var buffer = cbsiPlayer.global.params.bufferTime*600000;
            v.start_time = (v.start_time*1000)-buffer;
            v.end_time = (v.end_time*1000)+buffer;

            // skip if its over
            if (time > v.end_time) {
                v.live_state = 1;
                if (cbsiPlayer.getArrayIndex(cbsiPlayer.global.state.playlist, v.pid) != -1) {
                    cbsiPlayer.global.state.playlist.splice(cbsiPlayer.getArrayIndex(cbsiPlayer.global.state.playlist, v.pid),1);
                }
                continue;
            }
            else if (time >= v.start_time && time <= v.end_time) {
                v.live_state = 0;
            }
            else if (time < v.start_time) {
                v.live_state = -1;
                if (cbsiPlayer.getArrayIndex(cbsiPlayer.global.state.playlist, v.pid) != -1) {
                    cbsiPlayer.global.state.playlist.splice(cbsiPlayer.getArrayIndex(cbsiPlayer.global.state.playlist, v.pid),1);
                }
            }

            // add to data
            dataTemp[v.pid].push(v);

            // add to channel
            if (cbsiPlayer.getArrayIndex(channel.data,v.pid) == -1) {
                channel.data.push(v.pid);
            }

            // store the live pids for date sorting
            if (cbsiPlayer.getArrayIndex(livePids,v.pid) == -1)
                livePids.push(v.pid);

            // add live items to temp playlist
            if (cbsiPlayer.getArrayIndex(playlist,v.pid) == -1 && time <= v.end_time && time >= v.start_time) {
                livePlaylist.push(v.pid);
            }
        }
        else {
            // add to data
            dataTemp[v.pid].push(v);

            // add to channel
            if (cbsiPlayer.getArrayIndex(channel.data,v.pid) == -1) {
                channel.data.push(v.pid);
            }

            // add all vod to playlist
            if (cbsiPlayer.getArrayIndex(playlist,v.pid) == -1 && cbsiPlayer.getArrayIndex(cbsiPlayer.global.state.history,v.pid) == -1)
                playlist.push(v.pid);

        }
    }

    // add live pids to top of playlist
    cbsiPlayer.global.state.playlist = livePlaylist.concat(playlist);

    // sort live series
    for (var lP in livePids) {
        var vA = dataTemp[livePids[lP]];
        if (vA && vA.length > 1) //only sort if it's a series
            vA.sort(function (a, b) { return a.start_time - b.start_time });
    }

    // update data storage
    cbsiPlayer.global.state.videoData = dataTemp;

    cbsiPlayer.updateTodaysGroups();

    // if this is the first load, set the pid
    if (!cbsiPlayer.global.state.isPlayerDrawn) {
        if (playlist.length) {
            cbsiPlayer.toggleSlate('none');
            // if passed a pid, add to top of playlist and reset default channel
            if ('' != '' && cbsiPlayer.getArrayIndex(cbsiPlayer.global.state.playlist,'') != -1) {
                cbsiPlayer.global.state.playlist.unshift('');
                // if passed a timecode, set the flashvar
                if ('0' != '') {
                    cbsiPlayer.global.swfobject.flashVars.tc = '0';
                }
            }
            cbsiPlayer.global.swfobject.flashVars.pid = cbsiPlayer.global.state.playlist[0];
            cbsiPlayer.global.state.currentVideo = cbsiPlayer.global.state.videoData[cbsiPlayer.global.swfobject.flashVars.pid][0];
            cbsiPlayer.global.state.currentChannel = cbsiPlayer.global.state.currentVideo.channel;
            cbsiPlayer.draw();
            cbsiPlayer.global.state.isPlayerDrawn = true;
        }
        else {
            cbsiPlayer.toggleSlate('block');
        }
    }

    cbsiPlayer.dataIntId = setTimeout('cbsiPlayer.getData(\'http://'+cbsiPlayer.global.params.host+'/data/video/player/get-videos/general/'+cbsiPlayer.global.params.defaults.CHANNEL_LIST.join(',')+'?as=json\', cbsiPlayer.updateData)', cbsiPlayer.global.params.defaults.CAROUSEL_REFRESH*1000);
}

cbsiPlayer.onDefaultsLoaded = function(data) {
    var isInitLoad = (cbsiPlayer.defaultsDataIntId) ? false : true;
    clearTimeout(cbsiPlayer.defaultsDataIntId);
    cbsiPlayer.global.params.defaults = data.content;
    var defs = cbsiPlayer.global.params.defaults;

    // parse channel list
    defs.CHANNEL_LIST = defs.CHANNEL_LIST.split(',');
    defs.CHANNEL_LIST_LABELS = defs.CHANNEL_LIST_LABELS.split(',');

    // move default channel to front of list
    var defChannelIndex = cbsiPlayer.getArrayIndex(defs.CHANNEL_LIST,defs.DEFAULT_CHANNEL);
    defs.CHANNEL_LIST.unshift(defs.CHANNEL_LIST.splice(defChannelIndex,1)[0]);
    defs.CHANNEL_LIST_LABELS.unshift(defs.CHANNEL_LIST_LABELS.splice(defChannelIndex,1)[0]);

    //if (!isInitLoad) { cbsiPlayer.global.state.channelList = []; }

    // normalize booleans and numbers
    for (var i in defs) {
        if (defs[i] == 'true') { defs[i] = true; }
        else if (defs[i] == 'false') { defs[i] = false; }
        else if (!isNaN(parseFloat(defs[i]))) { defs[i] = parseFloat(defs[i]); }
    }

    // parse any player groups
    defs.TODAYS_GROUPS = (defs.TODAYS_GROUPS) ?  eval( '(' + defs.TODAYS_GROUPS + ')' ) : {};
    var playerList = [];
    for (var pid in defs.TODAYS_GROUPS) {
        playerList.push(defs.TODAYS_GROUPS[pid]);
    }
    defs.TODAYS_GROUPS_PLAYERLIST = playerList.join(',');

    if (isInitLoad) {
        for (var i in defs.CHANNEL_LIST) {
            cbsiPlayer.global.state.channelList.push(defs.CHANNEL_LIST[i]);
            cbsiPlayer.global.state.channelData[defs.CHANNEL_LIST[i]] = {
                label   : defs.CHANNEL_LIST_LABELS[i],
                data    : []
            };
        }

        // load golf data
        cbsiPlayer.getData('http://'+cbsiPlayer.global.params.host+'/data/golf/course?series=pga&year=2012&tournament=292758&round='+cbsiPlayer.currentRound+'&as=json&r='+(new Date().getMinutes()),cbsiPlayer.onGolfCourseDataLoaded,true);
        cbsiPlayer.getData('http://'+cbsiPlayer.global.params.host+'/data/golf/scoring?series=pga&year=2012&tournament=292758&round='+cbsiPlayer.currentRound+'&as=json&r='+(new Date().getMinutes()),cbsiPlayer.onGolfScoringDataLoaded,true);
        cbsiPlayer.getData('http://'+cbsiPlayer.global.params.host+'/data/golf/leaders?series=pga&year=2012&tournament=292758&round='+cbsiPlayer.currentRound+'&as=json&r='+(new Date().getMinutes()),cbsiPlayer.onGolfLeadersDataLoaded,true);

        // init clock
        defs.STIME = 1333822207.4807;

        // preload channels
        cbsiPlayer.global.state.currentChannel = defs.DEFAULT_CHANNEL;
        cbsiPlayer.getData('http://'+cbsiPlayer.global.params.host+'/data/video/player/get-videos/general/'+defs.CHANNEL_LIST.join(',')+'?as=json',cbsiPlayer.updateData);
    }

    // poll defaults for changes
    cbsiPlayer.defaultsDataIntId = setTimeout('cbsiPlayer.getData(\'http://'+cbsiPlayer.global.params.host+'/data/video/player/get-dat-file?as=json&dat_file_path=video/player/masters-live/defaults\',cbsiPlayer.onDefaultsLoaded,true)', defs.DEFAULTS_REFRESH*1000);
}

cbsiPlayer.onGolfCourseDataLoaded = function(data) {
    cbsiPlayer.global.state.golfData.course = data;
    clearTimeout(cbsiPlayer.golfCourseDataIntId);
    cbsiPlayer.golfCourseDataIntId = setTimeout('cbsiPlayer.getData(\'http://'+cbsiPlayer.global.params.host+'/data/golf/course?series=pga&year=2012&tournament=292758&round='+cbsiPlayer.currentRound+'&as=json\',cbsiPlayer.onGolfCourseDataLoaded, true)', cbsiPlayer.global.params.defaults.GOLF_COURSE_DATA_REFRESH*1000);
}

cbsiPlayer.onGolfScoringDataLoaded = function(data) {
    cbsiPlayer.global.state.golfData.scoring = data;
    clearTimeout(cbsiPlayer.golfScoringDataIntId);
    cbsiPlayer.updateTodaysGroups();
    cbsiPlayer.golfScoringDataIntId = setTimeout('cbsiPlayer.getData(\'http://'+cbsiPlayer.global.params.host+'/data/golf/scoring?series=pga&year=2012&tournament=292758&round='+cbsiPlayer.currentRound+'&as=json\',cbsiPlayer.onGolfScoringDataLoaded, true)', cbsiPlayer.global.params.defaults.GOLF_SCORING_DATA_REFRESH*1000);
}

cbsiPlayer.onGolfLeadersDataLoaded = function(data) {
    cbsiPlayer.global.state.golfData.leaders = data;
    clearTimeout(cbsiPlayer.golfLeadersDataIntId);
    cbsiPlayer.golfLeadersDataIntId = setTimeout('cbsiPlayer.getData(\'http://'+cbsiPlayer.global.params.host+'/data/golf/leaders?series=pga&year=2012&tournament=292758&round='+cbsiPlayer.currentRound+'&as=json\',cbsiPlayer.onGolfLeadersDataLoaded, true)', cbsiPlayer.global.params.defaults.GOLF_LEADERS_DATA_REFRESH*1000);
}

cbsiPlayer.updateTodaysGroups = function() {
    // add scorecard data to video data
    for (var pid in cbsiPlayer.global.params.defaults.TODAYS_GROUPS) {
        if (!cbsiPlayer.global.state.todaysGroupsData[pid]) { cbsiPlayer.global.state.todaysGroupsData[pid] = []; }
        for (var i in cbsiPlayer.global.params.defaults.TODAYS_GROUPS[pid]) {
            if (cbsiPlayer.global.state.golfData.scoring) {
                for (var player in cbsiPlayer.global.state.golfData.scoring.players) {
                    if (cbsiPlayer.global.state.golfData.scoring.players[player].id == cbsiPlayer.global.params.defaults.TODAYS_GROUPS[pid][i]) {
                        cbsiPlayer.global.state.todaysGroupsData[pid][i] = { 'label': cbsiPlayer.global.state.golfData.scoring.players[player].firstname+' '+cbsiPlayer.global.state.golfData.scoring.players[player].lastname, 'id': player.id, 'score': cbsiPlayer.global.state.golfData.scoring.players[player].trnparrelative };
                    }
                }
            }
        }
    }
}

cbsiPlayer.toggleSlate = function(display) {
    var slate = document.getElementById('slateCnt');
    if (slate) slate.style.display = display;
}

cbsiPlayer.onGolfTournamentDataLoaded = function() {

    var delay = (isNaN(cbsiPlayer.global.params.defaults.GOLF_TOURNAMENT_DATA_REFRESH*1000)) ? 1 : cbsiPlayer.global.params.defaults.GOLF_TOURNAMENT_DATA_REFRESH*1000;

    clearTimeout(cbsiPlayer.golfTournamentDataIntId);
    cbsiPlayer.golfTournamentDataIntId = setTimeout(function() {
        $.getJSON(CBSi.mediaHttpServer + '/data/golf/info?league_code=p&as=json&r='+(new Date().getMinutes()),
            function(data) {
                cbsiPlayer.global.state.golfData.tournamentInfo = data;
                cbsiPlayer.currentRound = (cbsiPlayer.global.state.golfData.tournamentInfo.tournamentId == '292758')
                    ? cbsiPlayer.global.state.golfData.tournamentInfo.currentRound : 4;

                // load defaults
                if (!cbsiPlayer.defaultsLoaded) {
                    cbsiPlayer.onDefaultsLoaded(cbsiPlayer.global.params.defaults);
                    cbsiPlayer.defaultsLoaded = true;
                }
                cbsiPlayer.onGolfTournamentDataLoaded();


            }
        );
    }, delay);
};



// Load/Unload functions
var loader = function() {
    // Init player
    cbsiPlayer.load();

    //load tournament info
    cbsiPlayer.defaultsLoaded = false;
    cbsiPlayer.onGolfTournamentDataLoaded();

}

if (window.addEventListener) window.addEventListener("load", loader, false);
else if (window.attachment) window.attachment("onload", loader);
else window.onload = loader;

var unloader = function() {
    // Uninit player
}
if (window.addEventListener) window.addEventListener("unload", unloader, false);
else if (window.attachment) window.attachment("onunload", unloader);
else window.onunload = unloader;

</script>

