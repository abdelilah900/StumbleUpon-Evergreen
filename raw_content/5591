 {
		var now=new Date();
        var expiresDate=new Date();
        expiresDate.setDate(now.getDate() + 14);
		$.cookie('ipadCookieCntr', 1,  { expires: expiresDate, path: '/' });
			 // alert('You have set the cookie: '+ $.cookie('ipadCookieCntr'))
	}	
	else
	{	
		var now=new Date();
        var expiresDate=new Date();
        expiresDate.setDate(now.getDate() + 14);
		ipadCookieCntrValue = $.cookie('ipadCookieCntr');
		ipadCookieCntrValue = parseInt(ipadCookieCntrValue) +1;
		$.cookie('ipadCookieCntr', ipadCookieCntrValue,  { expires: expiresDate, path: '/' });
          // alert('You have set the cookie: '+ $.cookie('ipadCookieCntr'));
		  
	}	   
       });
	
