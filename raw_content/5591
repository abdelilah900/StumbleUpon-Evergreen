window.onorientationchange = function() {
	var orientation = window.orientation;
    if(orientation == 0) {
 window.scrollTo(0, 0);

$("#fancybox-overlay").width(1024);
$("#fancybox-outer").width(768);
$("#fancybox-content").width(768);
$("#fancybox-wrap").css('top','41');
$("#fancybox-wrap").css('right','10');

//alert('Portrait');
    } else {

   window.scrollTo(0, 0);

	
	$("#fancybox-overlay").width(1024);
	$("#fancybox-outer").width(1024);
	$("#fancybox-content").width(1024);
	$("#fancybox-wrap").css('top','123');
$("#fancybox-wrap").css('right','10');
  //alert('Landscape');

    }

}
	 
$(document).ready(function()
{ 
		
var isiPad = navigator.userAgent.match(/iPad/i) != null;
			
				
if (isiPad == 1) {	
		
