<script type="text/javascript">

function showDaysArticles(whichUL) {
    var ULswitcher = $("#day" + whichUL);
    var arrowSwitcher = $("#day_arrow_" + whichUL);
    if ($(ULswitcher).css("display") == "none") {
        $(ULswitcher).show();
        $(arrowSwitcher).removeClass("sl-arrow-closed");
        $(arrowSwitcher).addClass("sl-toc-day-icon sl-arrow-opened sl-sprite-tops-tap2 sl-block-item");    
		$(ULswitcher).find('img').trigger("appear");
    } else {
        $(ULswitcher).hide();
        $(arrowSwitcher).removeClass("sl-arrow-opened");
        $(arrowSwitcher).addClass("sl-toc-day-icon sl-arrow-closed sl-sprite-tops-tap2 sl-block-item");
    }
}

if (!sl) {
	var sl = {};
	sl.setLeftRail = function(){
		if($('.homepage_leftrail').length){
			$('.sl-toc-lr-list').css('height', 'auto');
		}
		else{
			if($('.sl-toc-lr-list').height() < $('.sl-toc-cntr').height()){
				$('.sl-toc-lr-list').height($('.sl-toc-cntr').height() - $('.sl-toc-lr-head').height() - 15);
			}
		}
	}
}
$('.sl-toc-nav-link').click(function() {
    var name = ($(this).attr("name"));
    var id = ($(this).attr("id"));
    $('.showAndHide').css("display","none");
    $("#sl-toc-nav-mrec").removeClass("sl-toc-nav-mrec-selected");
    $("#sl-toc-nav-mrec").addClass("sl-toc-nav-mrec");
    
    $("#sl-toc-nav-mrea").removeClass("sl-toc-nav-mrea-selected");
    $("#sl-toc-nav-mrea").addClass("sl-toc-nav-mrea");
    
    $("#sl-toc-nav-mcom").removeClass("sl-toc-nav-mcom-selected");
    $("#sl-toc-nav-mcom").addClass("sl-toc-nav-mcom");
    
    $("#sl-toc-nav-mlik").removeClass("sl-toc-nav-mlik-selected");
    $("#sl-toc-nav-mlik").addClass("sl-toc-nav-mlik");
    
    $("#sl-toc-nav-hot").removeClass("sl-toc-nav-hot-selected");
    $("#sl-toc-nav-hot").addClass("sl-toc-nav-hot");
    $("#"+id).removeClass(id);
    $("#"+id).addClass(id+"-selected");
    $("#"+name).css("display","block");
	$("#"+name).find('img').trigger('appear');
    
    sl.setLeftRail();
});
$(document).ready(function() {
    $(".sl-toc-art-illo").lazyload({delay:250});
});
</script>
