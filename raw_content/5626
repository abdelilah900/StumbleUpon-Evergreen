		<script>
			OAS_url = 'http://oas.guardian.co.uk/RealMedia/ads/';
			OAS_sitepage = "www.guardian.co.uk/world/interactive/2011/mar/22/middle-east-protest-interactive-timeline/oas.html";
			OAS_listpos = 'Top,Middle1';

						OAS_query = 'k=tunisia&k=arab-and-middle-east-protests&k=libya&k=bahrain&k=egypt&k=middleeast&cf=global+%28temp+commercial%29&pid=&ct=interactive&pt=interactive&';
			OAS_target = '_blank';

			OAS_rn = '001234567890';
			OAS_rns = '1234567890';
			OAS_rn = new String (Math.random());
			OAS_rns = OAS_rn.substring (2, 11);

            // Add segments from rev_sci cookie to OAS_query.
            OAS_query += (function() {
                var maxSegs = 100;
                var rsi_segs = [];
                var cookies = document.cookie;
                var start = cookies.indexOf('rsi_segs=');
                var end;

                if (start >= 0) {
                    start = cookies.indexOf('=', start) + 1;
                    if (start > 0) {
                        end = cookies.indexOf(';', start);
                        if (end == -1) { end = cookies.length; }
                    }

                    var tmp_segs = cookies.substring(start, end).split(/E0/);
                    for (var i = 0, j = tmp_segs.length; i < j; i++) {
                        if (tmp_segs[i]) { rsi_segs.push('E0' + tmp_segs[i]); }
                    }
                    rsi_segs = rsi_segs.slice(0, maxSegs);
                }

                return 'a=' + rsi_segs.join('&a=') + '&';
            })();
		</script>
            	    





		
										

        		
		
			            						
										
					
                                                                                                                                                                                                                            
