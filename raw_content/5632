<script>

if (showAlternateSlideshow) {

	var imageArray = new Array();
	var imageCounter = 1;
	var cnt=0;
		
	function loadPhotos(gid, did) {
		var photoXML = "/templates/photoGalleryXML?galleryid=" + gid;
		jQuery.get(photoXML, {}, function(xml) {

			jQuery("#"+did).html(""); // empty out container
			imageArray = Array(); // empty out the image array
			imageCounter = 1; // reset image counter to first
			// read each item and append to container
			jQuery('photo', xml).each(function(i) {
				var fotoUrl = jQuery(this).attr('name');
				var slideNumber = jQuery(this).find('slideNumber').text();
				var fotoCaption = jQuery(this).find('caption').text();
				var copyright = jQuery(this).find('copyright').text();
				var fotoCredit = jQuery(this).find('photoCredit').text();
				var html = "<div class='fotoContainer'><img src='"+fotoUrl+"'/>";
				html += "<div class='fotoCreditContainer'>" + fotoCredit + "</div>";
				html += "<div class='fotoCaptionContainer'>" + fotoCaption + "</div>";
				html += "</div>";
				jQuery("#"+did).append(html);
				imageArray[i] = fotoUrl;
			});
			jQuery("#"+did).width(imageArray.length*(600)+"px");
			jQuery("#"+did).css({'left':'0px','position':'relative'});
			jQuery("#imageCounter").html(imageCounter +" of "+ imageArray.length);
		});
	}
	
	
		jQuery("#navi-next").click(function(){
			if (imageCounter<imageArray.length) {
				jQuery("#imageCounter").html(++imageCounter +" of "+ imageArray.length);
				var newPos = jQuery("#slider").position().left - jQuery("#slider").position().left - ((imageCounter-1)*600);
				jQuery("#slider").animate( {left:newPos}, 500 );
				jQuery("#navi-prev").css({opacity:1});
			}
			if (imageCounter==imageArray.length) { 
				jQuery(this).css({opacity:0.3});
			} else {
				jQuery(this).css({opacity:1});
			}
		});
		jQuery("#navi-prev").click(function(){
			if (imageCounter>1) {
				jQuery("#imageCounter").html(--imageCounter +" of "+ imageArray.length);			
				var newPos = jQuery("#slider").position().left  - jQuery("#slider").position().left - ((imageCounter-1)*600);
				jQuery("#slider").animate( {left:newPos}, 500 );
				jQuery("#navi-next").css({opacity:1});
			}
			if (imageCounter==1) {
				jQuery(this).css({opacity:0.3});
			} else {
				jQuery(this).css({opacity:1});
			}
		});
		jQuery('#photos').swipe({
		     swipeLeft: function() { jQuery('#navi-next').click() },
		     swipeRight: function() { jQuery('#navi-prev').click() },
		})
		var gid=119059404;
		if (gid!="undefined") {
			loadPhotos(gid,'slider');
		} else {
			loadPhotos('119059404','slider');
		}

}
</script>


