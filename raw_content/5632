<script> 
if (showAlternateSlideshow) {

document.write('<scr'+'ipt type="text/javascript" language="javascript" src="http://www.philly.com/includes/jQuery.js"></scr'+'ipt>');	
document.write('<scr'+'ipt type="text/javascript" language="javascript" src="http://www.philly.com/includes/jquery.swipe.js"></scr'+'ipt>');	

