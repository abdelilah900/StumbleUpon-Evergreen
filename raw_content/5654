<script type="text/javascript">
folha.information.check();

if( folha.ads.expanded ){
    $('#fourth_column').css( { display: 'block' } ) ;
}
else{
    var element = "" ;
    $('#fourth_column div').each( function(){
        element = "#" + $(this).attr( 'id' ) + "-content" ;
        $(element).remove() ;
        $(this).remove() ;
    } ) ;
    if($(window).width() < 1024 && $.browser.msie && $.browser.version < 7) { $('#aligner').css('margin', '0'); }
}

//Top of mind - Galery Carousel
var gal = $('#galery_carousel');
if(gal.length > 0){
    gal.carousel({ display: 3, step: 1});
}

folha.preload() ;
folha.unread.init() ;
folha.ticker.start() ;
//folha.timer.start();
folha.media.init() ;
folha.shop.mount() ;
folha.anchors.fix() ;
folha.refresh.start() ;

