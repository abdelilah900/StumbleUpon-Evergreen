    width: 120,
    height: 45,
    url: 'http://media.folha.uol.com.br/folha/furniture/ads/creatives/2009/09/11/relogio-itau.swf',
    params: {menu: false},
    vars: {clickTag: escape('http://www.itau.com.br')}
});

folha.searchInput = '';
$('input.clean-out').focus(function(){
    var that = $(this);
    folha.searchInput = that.val();
    that.val('');
});
$('input.clean-out').blur(function(){
    var that = $(this);
    if( that.val() == '' ){
        that.val(folha.searchInput);
    } else {
        folha.searchInput = '';
    }
});
</script>
