<script type="text/javascript" >
function vote(categoryId, categoryName) {
    var userType = "U";
    var userId = 0;
    if (userId == 0) {
        userType = "V";
        userId = 161056092;
    }
    var promptShare = false;
    var autoShare = false;
    var neverShare = false;
    var showShare = !promptShare &&  !autoShare && !neverShare;
    if (promptShare){
        
        void(0);
    }
    var storydesc = jQuery("meta[name=description]").attr("content");
    var url = '/controlpage.aspx?control=StoryCategory&storyid=' + storyId + '&storycategoryid=' + categoryId + '&PageTitle=' + document.title + '&storyheadline=' + storyHeadline + '&userid='+userId+'&type='+userType + "&showshare=" + showShare + "&autoshare=" + autoShare + "&storydescription=" + storydesc;
    AjaxReplace(url, jQuery('#voteDivContent'));
    
}
</script>


