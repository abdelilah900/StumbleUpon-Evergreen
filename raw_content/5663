									var E = Event,
									    Dom = YAHOO.util.Dom,
									    O = o.responseText;
								    Dom.get("wx-saved-locations").innerHTML = O;
								}
							}
						});
					};
			YAHOO.util.Connect.asyncRequest(_reqType, _pageletLoc, {
				success:function(o) {
					if(o.status.toString().match("2[0-9][0-9]")!=null) {
						var E = Event,
						    Dom = YAHOO.util.Dom,
						    O = o.responseText;
						    E.onAvailable("wx-saved-locations",function(){
						      Dom.get("wx-saved-locations").innerHTML = O;
						    });
/*
							// Use GeoLocation (if available) if default location was returned
							if(O.match("defaultLocation")){ 
								_pageletLoc = "";
								if(navigator.geolocation){
									var getLocation = function(obj){
