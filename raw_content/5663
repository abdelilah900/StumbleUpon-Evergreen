<script type="text/javascript">
(function() { var loader = new YAHOO.util.YUILoader({ base:"http://s.imwx.com/js/2.8.0r4/", loadOptional:false, combine:false, filter:"MIN", allowRollup:true,
require: ["animation","button","connection","container","containercore","cookie","dom","event-mouseenter","event-delegate","selector","tabview","yahoo","datasource","autocomplete","element","event"], onSuccess: function() {

/* Inserted by wx-bind */
try {
YAHOO.namespace("weather");
} catch (err) { try { console.log(err); } catch(error) {} }

/* Inserted by $empty1 */
try {
YAHOO.util.Event.on("twc-google-site-search", "submit", function(e){
					    var siteSearchForm = document.forms["twc-google-site-search"];
						var siteSearchTextBox = siteSearchForm.elements["q"];

						if (siteSearchTextBox.value == siteSearchTextBox.title || siteSearchTextBox.value=="") {
							YAHOO.util.Dom.addClass(siteSearchTextBox,'twc-error-state');
						} else {
							YAHOO.weather.intelliTrak.isLinkClicked = true;
							YAHOO.weather.intelliTrak({'href': wx.config.page.pageURL,'from':'hdr_sitesearch'});
							siteSearchForm.submit();
						}

					});
} catch (err) { try { console.log(err); } catch(error) {} }

/* Inserted by $empty2 */
try {
var handleSearchForm = function(e){
		  		YAHOO.util.Event.stopEvent(e);							
				var webSearchForm = document.forms["twc-google-web-search"],
					webSearchTextBox = webSearchForm.elements["q"],
					radioGrp = webSearchForm.search,
					radioGrplength = radioGrp ? radioGrp.length : 0;
		
				if (webSearchTextBox.value == webSearchTextBox.title || webSearchTextBox.value=="") {
					YAHOO.util.Dom.addClass(webSearchTextBox,'twc-error-state');
				} else {										
					YAHOO.weather.intelliTrak.isLinkClicked = true;
					for(var i = 0;i < radioGrplength;i++){
						if(radioGrp[i].checked){
							switch(radioGrp[i].value){
								case 'site':
									YAHOO.weather.intelliTrak({'href': wx.config.page.pageURL,'from':'footer_sitesearch'});
									webSearchForm.action = '/info/sitesearch/';
									break;
								case 'web':
									YAHOO.weather.intelliTrak({'href': wx.config.page.pageURL,'from':'footer_websearch'});
									webSearchForm.action = '/info/websearch/';
							}
						}
					}
					webSearchForm.submit();
				}				
			}
		  YAHOO.util.Event.on("twc-google-web-search", "submit",handleSearchForm); 
		  YAHOO.util.Event.delegate("wx-footer-search","click",handleSearchForm,".wx-searchButton");
} catch (err) { try { console.log(err); } catch(error) {} }
var loader = new YAHOO.util.YUILoader({ combine:true, comboBase:"http://d.imwx.com/js/", require: ["wx-bind","wx-panels","wx-savedlocations","wx-dropdown","wx-locationeditor","global","wx-appspromo","wx-weathersearch","wx-pageoptions","wx-survey","wx-video-embeddedplayer"], onSuccess: function() {

/* Inserted by wx-savedlocations */
try {
// Setup tab views
            var tabContainers = YAHOO.util.Selector.query(".twc-tabview");
            for (var i = 0; tabContainers && i < tabContainers.length; i++) {
                new YAHOO.widget.TabView(tabContainers[i]);
            }
            var body = new YAHOO.util.Element(YAHOO.util.Selector.query('body')[0]);
            body.addClass("has-js");
            body.removeClass("no-js");


	
	// Setup textboxes
                var textboxes = YAHOO.util.Selector.query("input[type=text]"),
					dummyInputElement = document.createElement("input"),
					supportsPlaceholder = ("placeholder" in dummyInputElement) ? true : false;
                for (var i = 0; textboxes && i < textboxes.length; i++) {
                  if(textboxes[i].id !== "typeaheadBox" && textboxes[i].id !== "searchLocationTypeaheadBox" &&
                  	 !(supportsPlaceholder && textboxes[i].placeholder)){
                    new YAHOO.weather.TextBox(textboxes[i]);
                   }
                 }
                    // Setup weather search module
                 //new YAHOO.weather.SavedLocations('wx-weather-search');
                 new YAHOO.weather.SavedLocations('wx-saved-locations');
		    addCloseicon('container-close');
} catch (err) { try { console.log(err); } catch(error) {} }

/* Inserted by wx-dropdown */
try {
