	    <script type="text/javascript">
	        jQuery(document).ready(function(){
                 if (jQuery('.ts-main_article2_image').width()<=350) {
                     var newFakeDivHeight =  600;
                  }
                  else
                  {
                     var newFakeDivHeight =  250;
