<script language="JavaScript">
function cnnad_rendergalleryad( default_ad, location ) {
		if( cnnPage.path.indexOf('/multimedia/photo_gallery') !== -1 ){
				var folder = cnnPage.path.replace(/\/multimedia\/photo_gallery/,'').replace(/[^\/]+$/,'');
				if( location == "promo300" ) {
					for (var i=0; i<sipg.adConfig.sponsored_list.length;i++){
						var profile = sipg.adConfig.sponsored_list[i];
						if ( cnnPage.path.indexOf(profile.url)>=0 ) {
							if( profile[ location ] ) {
								default_ad = '<div class="_promo300x50"><div class="inner"><a href="'+profile[ location ].url+'">'+'<img src="'+profile[ location ].image+'"/></a></div></div>';
							}
						}
					}
					if( default_ad != '' ) document.write( default_ad );
				} else {
					for (var i=0; i<sipg.adConfig.sponsored_list.length;i++){
						var profile = sipg.adConfig.sponsored_list[i];
						if ( cnnPage.path.indexOf(profile.url)>=0 ) {
							if( profile[ location ] ) {
								default_ad = profile[ location ];
							}
							if( location == 'top' ) {
								document.getElementById('gallPollTop').style.padding = '15px 0';
							}
						}
					}
					if( default_ad != '' )
						document.write( '<scr'+'ipt type="text/javascript" language="JavaScript">adFactory.getAd(200, 66).write();</scr'+'ipt>' );
				}
		}
}

sipg.adConfig = {
	"ad"	: function (url, params) {
		if (url); else return;
		/* GROUP DEFAULTS */
		var group = {
			"url"	:	url,
			"rgt" 	:	"",
			"skin"	:	"",
			"spon"	:	"",
			"inter" :	""
		};
		if (params) {
			params = params.split('|');
			for (i=0;i<params.length;i++){ //>
				var data = params[i].split(':');
				if (data.length != 2) { continue; }
				var key = data[0];
				var val = data[1];
				if (val == '') { 
					group[key] = '';
				} else {
					group[key] = val;
				}
			}
		}
		this.sponsored_list.push(group);
	},
