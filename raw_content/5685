    'ams_ads_script_src': 'http://subscribe.epicurious.com/ams/page-ads.js?ad_category_prefix=WEB-INF&browser_path=%2FWEB-INF%2Finterface%2Ftemplates%2Ftemplate_a.jsp&cat_prefixes=%2CWEB-INF%2Cinterface%2Ctemplates%2Ctemplate_a.jsp&keywords='+metaKeyWords+'&position_list=AMS_EPI_HOMEPAGE_FOOTER_SUBSCRIBE_BNA%2CAMS_EPI_HOMEPAGE_FOOTER_SUBSCRIBE_SLF%2CAMS_EPI_HOMEPAGE_RIGHTRAIL_A%2CAMS_EPI_GLOBAL_HEADER_SUBSCRIBELINK%2CAMS_EPI_GLOBAL_NAVBAR&site_prefix=epicurious&subdomain=www&url_name=template_a'
