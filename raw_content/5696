						document.headerSearchForm.hidden05.name="safe";
						document.headerSearchForm.hidden05.value="active";
						document.headerSearchForm.hidden06.name="cof";
						document.headerSearchForm.hidden06.value="GALT:#008000;GL:1;DIV:#336699;VLC:663399;AH:center;BGC:FFFFFF;LBGC:336699;ALC:0000FF;LC:0000FF;T:000000;GFNT:0000FF;GIMP:0000FF;FORID:1;";
						document.headerSearchForm.hidden07.name="hl";
						document.headerSearchForm.hidden07.value="en";
						document.headerSearchForm.method='get';
					} else if (rads[i].value == 'google_archive') {
						document.headerSearchForm.action='/google_archive/search.php';
						document.headerSearchForm.method='POST';
					} else {
						document.headerSearchForm.action='/google/search.php';
						document.headerSearchForm.cx.name="cx";
						document.headerSearchForm.cx.value="005271800417660358352:dubhaachbpe";
