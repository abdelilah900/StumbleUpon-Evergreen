<script type="text/javascript">
	var list_block_shown = false;
	function loadNextItems()
	{
		list_block_shown = false;
		$(".list_block").each(function() {
			if (!$(this).is(":visible") && !list_block_shown) {
				$(this).show();
				list_block_shown = true;
			}
		});
		var hideButton = true;
		$(".list_block").each(function() {
			if (!$(this).is(":visible")) hideButton = false;
		});
		if (hideButton) $(".load_button").hide();
	}
</script>



