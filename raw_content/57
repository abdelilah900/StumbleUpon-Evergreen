<script type="text/javascript">

	//Added blogroll.js here instead of separate file, since it was not working in webkit browsers ()
	//As of Jquery 1.7 live will be depreciated (use .on() )
	$("ul.blogroll li span").live("click", function() {
		// decode blogroll item URL
		var reg_exp = new RegExp("\\|+", "g");
		var url = jQuery('span', this).text().replace(reg_exp, ".");
		url = 'http://' + url;

		window.open(url); 
	});

</script>

