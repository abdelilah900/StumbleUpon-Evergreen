<script type="text/javascript">
// For blogs under Serious Eats, we want to use their blog ID for search
if ($('#IncludeBlogs').val() == '31') {
   if (document.location.href.indexOf('/talk/')>-1) {
      $('#IncludeBlogs').val('32');
   }else if (document.location.href.indexOf('/recipes/')>-1) {
      $('#IncludeBlogs').val('34');
   }else {
      $('#IncludeBlogs').val('30');
   }
}

</script>



        </div>
        <div class="clearer"></div>
        
    </div><!-- close navBar -->

</div><!-- close header -->

