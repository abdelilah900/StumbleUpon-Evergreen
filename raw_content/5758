<script language="JavaScript" type="text/javascript">
//<![CDATA[
	cmSetProduction();
	
	$(function() {
		if(!readCookie('hasVisited')) {
			createCookie('hasVisited', true, 1);
			cmCreateTechPropsTag("yarn bombing | Free People Clothing Boutique Blog", "Blog Pages");
		}
		else {
			cmCreatePageviewTag("yarn bombing | Free People Clothing Boutique Blog", "Blog Pages", null, null);
		}		
		
		function createCookie(name,value,days) {
			var cookie = [name + "=" + value];
		
			if (days) {
				var date = new Date();
				date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
				
				cookie.push('expires=' + date.toGMTString());
			}
			
			cookie.push('path=/');
			document.cookie = cookie.join(';');
		}

		function readCookie(name) {
			var nameEq = name + "=",
				ca = document.cookie.split(';');
			
			for(var i = 0; i < ca.length; i++) {
				var c = ca[i];
				
				while (c.charAt(0) == ' ')
					c = c.substring(1,c.length);
					
				if (c.indexOf(nameEq) == 0) 
					return c.substring(nameEq.length, c.length);
			}
			
			return null;
		}

	});
//]]>
</script>


