<script type="text/javascript">

CN.features.render({
    features : (function() {
        var
            i,
            feature = null,
            render = [],
            featuresOnPage = "centeralign whitebg divider rotator   narrowrotator  blackbg rightalign leftalign centeralign whitebg  divider3 centeralign whitebg  divider3 centeralign whitebg  divider centeralign whitebg  divider3 centeralign whitebg  divider centeralign centeralign centeralign   divider centeralign centeralign mostviewed  whitebg  mostpopularfeatures  whitebg leftalign   alignleftgeorgia16  alignleftgeorgia16 centeralign   divider3 thumbnailgrid".replace(/\s{2,}/g, ' ').split(' '),
            len = featuresOnPage.length,
            cssClass;

        featuresOnPage = jQuery.grep(featuresOnPage,function(el,idx){
            return jQuery.inArray(el, featuresOnPage) === idx;
        });
        for(i = 0; i < len; i++){
            cssClass = featuresOnPage[i];
            if (CN.features.types.hasOwnProperty(cssClass)) {
                render.push(cssClass);
            }
        }
        return render;
    }())
});


