  <script type="text/javascript">
    Stats.get({
      action    : 's',
      callback  : 'Stats.insert',
      entity    : 'projects',
      type      : 'all',
      time      : 'all',
      ids       : '2795039|2416390|2318404'
    });

    Stats.insert = function ( stats ) {
      
    	for ( var id in stats ) {
    	  $project = $('#project-cover-' + id);
    	  $project.find( '.cover-stat-appreciations .stat-value').html( stats[id].apps_all );
    	  $project.find( '.cover-stat-comments .stat-value' ).html( stats[id].cmts_all );
    	  $project.find( '.cover-stat-views .stat-value' ).html( stats[id].views_all );
      }
    }
  </script>
  
