	<script type="text/javascript" language="javascript">
	hash = location.hash;
	var lastHash = hash;
	
	function checkForMessages()
	{
		hash = location.hash;
		if(hash != lastHash)
		{
			var oldHashIndex;
			var oldUrl;
			var reloadIndex;
			
			lastHash = hash;
			
			reloadIndex = hash.indexOf('&reload');
			if (reloadIndex != -1)
			{
			
				oldHashIndex = document.URL.indexOf('#');
				if (oldHashIndex != -1)
				{
					oldUrl = document.URL.substring(0,oldHashIndex);
				}
				else
				{
					oldUrl = document.URL;
				}
				
				
				newUrl = oldUrl + hash.substr(0, hash.length - '&reload'.length);
				document.location.replace(newUrl);
				document.location.reload(true);
				
			}
		}
	}
	setInterval(checkForMessages, 200);
	</script>
	<!--[if IE]>
	<script type="text/javascript" language="javascript">
	// IE hash reload fix
	if (location.hash != "")
		location.href=location.hash; 
	</script>	
	<![endif]-->
