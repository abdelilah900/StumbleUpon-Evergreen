       "/wp-content/mu-plugins/w4l/w4l_i_like_it_ajax.php" );    

  mysack.execute = 1;
  mysack.method = 'POST';
  mysack.setVar( "website_channel_article_id", website_channel_article_id );
  mysack.setVar( "blog_id", blog_id );
  mysack.setVar( "post_id", post_id );
  mysack.setVar( "results_div_id", "iLikeItNumber" );
