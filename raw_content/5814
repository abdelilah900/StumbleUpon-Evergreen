function doILikeIt( website_channel_article_id,blog_id,post_id )
{
   document.getElementById('iLikeItLinkDiv').innerHTML = '';
   document.getElementById('iLikeItNumber').innerHTML = '<img src="http://trifter.com/wp-content/themes/trifter/images/i-like-it-load.gif" />';
