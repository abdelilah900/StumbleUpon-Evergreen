<div id="clasif2_partials"></div>
<script type='text/javascript'>
var displayOpt = {
'clasif2_eye_autocom_a' : null
,'clasif2_eye_autocom_b' : null
,'clasif2_eye_autocom_c' : null
,'clasif2_eye_autocom_d' : null
};
var howManyOpt = 0;
var selectedOptId = null;
var selectedWas = false;
function processClick(selId) {
document.getElementById('clasif2_eye_donde').value = showProv(displayOpt[selId]);
selectedWas = displayOpt[selId];
hideAutocom();
}
function processKeyPress(e) {
if(! e) { var e = window.event; }
if(e.keyCode == 13) {
if(selectedOptId == null) {
return true;
}
else {
document.getElementById('clasif2_eye_donde').value = showProv(displayOpt[selectedOptId]);
selectedWas = displayOpt[selectedOptId];
hideAutocom();
return false;
}
}
}
function processKeyUp(e) {
if(! e) { var e = window.event; }
if(e.keyCode == 38) {
if(selectedOptId == 'clasif2_eye_autocom_b') { selectOpt('clasif2_eye_autocom_a'); }
if(selectedOptId == 'clasif2_eye_autocom_c') { selectOpt('clasif2_eye_autocom_b'); }
if(selectedOptId == 'clasif2_eye_autocom_d') { selectOpt('clasif2_eye_autocom_c'); }
}
else if(e.keyCode == 40) {
if((selectedOptId == 'clasif2_eye_autocom_c') && (howManyOpt > 3)) { selectOpt('clasif2_eye_autocom_d'); }
if((selectedOptId == 'clasif2_eye_autocom_b') && (howManyOpt > 2)) { selectOpt('clasif2_eye_autocom_c'); }
if((selectedOptId == 'clasif2_eye_autocom_a') && (howManyOpt > 1)) { selectOpt('clasif2_eye_autocom_b'); }
}
else if(e.keyCode == 13) {
return false;
}
else {
var val = document.getElementById("clasif2_eye_donde").value;
if(val.length == 0) {
selectedWas = false;
hideAutocom();
}
else {
for(somePrefix in asky) {
if(val.match(new RegExp(("^" + somePrefix), "i"))) {
if(asky[somePrefix] == 0) {
asky[somePrefix] = 1;
var cP = somePrefix;
var fSuff = ((cP=='Á')?'Aacute':((cP=='À')?'Agrave':((cP=='É')?'Eacute':((cP=='Í')?'Iacute':((cP=='Ó')?'Oacute':((cP=='Ò')?'Ograve':((cP=='Ú')?'Uacute':cP)))))));
document.getElementById('clasif2_partials').appendChild(createElement('script', {src:'http://estaticos.elmundo.es/clasificados/cajas/js/muni'+fSuff+'.js','type':'text/javascript','language':'javascript'}));
}
var ringing = setInterval(function() { if(asky[somePrefix] != 1) { clearInterval(ringing); showAutocom(); } }, 100);
break;
}
}
}
}
}
function showAutocom() {
var seenPrefix = null;
var askyf = [];
var val = document.getElementById("clasif2_eye_donde").value;
for(somePrefix in asky) {
if(val.match(new RegExp(("^" + somePrefix), "i"))) {
seenPrefix = somePrefix;
var valPrefix = new RegExp(("^" + val), "i");
for(iX = 0; iX < asky[seenPrefix].length; iX++) {
var target = (hasSpecials(val) ? asky[seenPrefix][iX] : killSpecials(asky[seenPrefix][iX]));
if(target.match(valPrefix)) {
askyf.push(asky[seenPrefix][iX]);
}
}
break;
}
}
howManyOpt = askyf.length;
if((seenPrefix == null) || (howManyOpt == 0)) {
hideAutocom();
}
else {
var sugg = document.getElementById('clasif2_eye_autocom');
var sugg_a = document.getElementById('clasif2_eye_autocom_a');
var sugg_b = document.getElementById('clasif2_eye_autocom_b');
var sugg_c = document.getElementById('clasif2_eye_autocom_c');
var sugg_d = document.getElementById('clasif2_eye_autocom_d');
sugg.className = "clasif2_show";
sugg_a.className = ((howManyOpt > 0) ? "clasif2_show" : "clasif2_hide");
sugg_a.innerHTML = "<div>" + ((howManyOpt > 0) ? showProv(askyf[0]) : '') + "</div>";
displayOpt['clasif2_eye_autocom_a'] = ((howManyOpt > 0) ? askyf[0] : null);
selectOpt('clasif2_eye_autocom_a');
sugg_b.className = ((howManyOpt > 1) ? "clasif2_show" : "clasif2_hide");
sugg_b.innerHTML = "<div>" + ((howManyOpt > 1) ? showProv(askyf[1]) : '') + "</div>";
displayOpt['clasif2_eye_autocom_b'] = ((howManyOpt > 1) ? askyf[1] : null);
sugg_c.className = ((howManyOpt > 2) ? "clasif2_show" : "clasif2_hide");
sugg_c.innerHTML = "<div>" + ((howManyOpt > 2) ? showProv(askyf[2]) : '') + "</div>";
displayOpt['clasif2_eye_autocom_c'] = ((howManyOpt > 2) ? askyf[2] : null);
sugg_d.className = ((howManyOpt > 3) ? "clasif2_show" : "clasif2_hide");
sugg_d.innerHTML = "<div>" + ((howManyOpt > 3) ? showProv(askyf[3]) : '') + "</div>";
displayOpt['clasif2_eye_autocom_d'] = ((howManyOpt > 3) ? askyf[3] : null);
}
return;
}
function hideAutocom() {
document.getElementById('clasif2_eye_autocom' ).className = "clasif2_hide";
howManyOpt = 0;
selectedOptId = null;
}
function showProv(val) {
return val.replace(/,(P\d\d)/, function(a, f) { return ", " + provinces[f]; });
}
function hasSpecials(source) {
return ((source.match(new RegExp("[àÀáÁâÂäÄèÈéÉêÊëËìÌíÍîÎïÏòÒóÓôÔöÖùÙúÚûÛüÜñÑçÇ]", ""))) ? true : false);
}
function killSpecials(source) {
source = source.replace(new RegExp("[àÀáÁâÂäÄ]", "g"), "a");
source = source.replace(new RegExp("[èÈéÉêÊëË]", "g"), "e");
source = source.replace(new RegExp("[ìÌíÍîÎïÏ]", "g"), "i");
source = source.replace(new RegExp("[òÒóÓôÔöÖ]", "g"), "o");
source = source.replace(new RegExp("[ùÙúÚûÛüÜ]", "g"), "u");
source = source.replace(new RegExp("[ñÑ]", "g"), "n");
source = source.replace(new RegExp("[çÇ]", "g"), "c");
return source;
}
function selectOpt(optId) {
selectedOptId = optId;
opt = document.getElementById(optId);
opt.style.color = 'white';
opt.style.backgroundColor = '#426FD9';
if(optId == 'clasif2_eye_autocom_a') { deselectOpt('clasif2_eye_autocom_b'); deselectOpt('clasif2_eye_autocom_c'); deselectOpt('clasif2_eye_autocom_d'); }
if(optId == 'clasif2_eye_autocom_b') { deselectOpt('clasif2_eye_autocom_a'); deselectOpt('clasif2_eye_autocom_c'); deselectOpt('clasif2_eye_autocom_d'); }
if(optId == 'clasif2_eye_autocom_c') { deselectOpt('clasif2_eye_autocom_a'); deselectOpt('clasif2_eye_autocom_b'); deselectOpt('clasif2_eye_autocom_d'); }
if(optId == 'clasif2_eye_autocom_d') { deselectOpt('clasif2_eye_autocom_a'); deselectOpt('clasif2_eye_autocom_b'); deselectOpt('clasif2_eye_autocom_c'); }
}
function deselectOpt(optId) {
opt = document.getElementById(optId);
opt.style.color = 'black';
opt.style.backgroundColor = 'white';
}
function clasif2_eye_busca() {
var que = document.clasif2_eye_form.clasif2_eye_que.value;
if(que == '¿Qué buscas?') { que = ''; }
if(que != null) { que = que.replace(/ /g, '+'); }
var donde = ((selectedWas == false) ? 'www' : selectedWas);
selectedWas = false;
donde = donde.replace(new RegExp(" - Provincia", ""), "-provincia");
donde = donde.replace(new RegExp("\/", ""), "");
donde = donde.replace(new RegExp("'", ""), "");
donde = donde.replace(new RegExp(",.*", ""), "");
donde = donde.replace(new RegExp("[áÁàÀä]", "g"), "a");
donde = donde.replace(new RegExp("[éÉèÈë]", "g"), "e");
donde = donde.replace(new RegExp("[íÍìÌï]", "g"), "i");
donde = donde.replace(new RegExp("[óÓòÒö]", "g"), "o");
donde = donde.replace(new RegExp("[úÚùÙü]", "g"), "u");
donde = donde.replace(new RegExp("[ñÑ]", "g"), "n");
donde = donde.replace(new RegExp("[çÇ]", "g"), "c");
donde = donde.replace(new RegExp(" ", "g"), "-");
donde = donde.toLowerCase();
return ('http://' + ((donde) ? donde : 'www') + '.expansionyempleo.com/busqueda/' + que);
}
</script>
<form name="clasif2_eye_form" action="#" autocomplete="off" onsubmit="location.href = clasif2_eye_busca(); return false;">
<div style="height:37px; width:218px; position:absolute; left:0px; top:0px;"><input name="clasif2_eye_que" type="text" style="font: 16px Arial,Helvetica,sans-serif; border: 0 none; outline: none; height:27px; width:198px; position: absolute; left: 11px; top: 6px;" value="¿Qué buscas?" onfocus="if(this.value == '¿Qué buscas?') { this.value=''; }"></div>
<div style="text-align: left; height:37px; width:218px; position:absolute; left:0px; top:59px;">
<input name="clasif2_eye_donde" id="clasif2_eye_donde" type="text" style="font: 16px Arial,Helvetica,sans-serif; border: 0 none; outline: none; height:27px; width:198px; position: absolute; left: 11px; top: 5px;" value="¿Dónde?" onfocus="if(this.value == '¿Dónde?') { this.value=''; selectedWas = false; }" onkeyup="return processKeyUp(event);" onkeypress="return processKeyPress(event);"></div>
<div style="height:32px; width:108px; position:absolute; left:105px; top:111px;"><input type="submit" value="" style="background: none repeat scroll 0 0 transparent; border: 0 none; margin: 0px; padding: 0px; cursor: pointer; height:32px; width:108px;"></div>
<style type="text/css">
