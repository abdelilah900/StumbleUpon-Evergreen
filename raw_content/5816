<script language="javascript" type="text/javascript">
function clasif2_mot_busca() {
var marca = document.clasif2_mot_form.clasif2_mot_marca.value;
var prov = document.clasif2_mot_form.clasif2_mot_prov.value;
var precio = document.clasif2_mot_form.clasif2_mot_precio.value;
var queryStr = '';
if(marca != null) {
queryStr += (queryStr.length ? '&' : '?') + "marca=" + marca.replace(/ /g, '+');
}
if(prov != null) {
queryStr += (queryStr.length ? '&' : '?') + "provincia=" + prov.replace(/ /g, '+');
}
if(precio != null) {
queryStr += (queryStr.length ? '&' : '?');
if(precio == 1) { queryStr += "precio_desde=0&precio_hasta=1000"; }
else if(precio == 2) { queryStr += "precio_desde=0&precio_hasta=5000"; }
else if(precio == 3) { queryStr += "precio_desde=0&precio_hasta=10000"; }
else if(precio == 4) { queryStr += "precio_desde=0&precio_hasta=15000"; }
else if(precio == 5) { queryStr += "precio_desde=0&precio_hasta=20000"; }
else if(precio == 6) { queryStr += "precio_desde=0&precio_hasta=25000"; }
else if(precio == 7) { queryStr += "precio_desde=0&precio_hasta=30000"; }
else if(precio == 8) { queryStr += "precio_desde=0&precio_hasta=-1"; }
else { queryStr += "precio=-1"; }
}
return 'http://www.marcamotoranuncios.com/resultados.php' + queryStr;
}
</script>
