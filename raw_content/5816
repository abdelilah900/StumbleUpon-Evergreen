<script language="javascript" type="text/javascript">
function clasif2_sv_busca() {
nomProv = new Array();
nomProv[1] = "albacete";
nomProv[2] = "alicante";
nomProv[3] = "almeria";
nomProv[4] = "avila";
nomProv[5] = "badajoz";
nomProv[6] = "barcelona";
nomProv[7] = "vizcaya";
nomProv[8] = "burgos";
nomProv[9] = "caceres";
nomProv[10] = "cadiz";
nomProv[11] = "castellon";
nomProv[12] = "ciudadReal";
nomProv[13] = "cordoba";
nomProv[14] = "coruna";
nomProv[15] = "cuenca";
nomProv[16] = "girona";
nomProv[17] = "granada";
nomProv[18] = "guadalajara";
nomProv[19] = "huelva";
nomProv[20] = "huesca";
nomProv[21] = "jaen";
nomProv[22] = "leon";
nomProv[23] = "rioja";
nomProv[24] = "lugo";
nomProv[25] = "lleida";
nomProv[26] = "madrid";
nomProv[27] = "malaga";
nomProv[28] = "murcia";
nomProv[29] = "orense";
nomProv[30] = "asturias";
nomProv[31] = "palencia";
nomProv[32] = "navarra";
nomProv[33] = "pontevedra";
nomProv[34] = "salamanca";
nomProv[35] = "guipuzcoa";
nomProv[36] = "cantabria";
nomProv[37] = "segovia";
nomProv[38] = "sevilla";
nomProv[39] = "soria";
nomProv[40] = "tarragona";
nomProv[41] = "teruel";
nomProv[42] = "toledo";
nomProv[43] = "valencia";
nomProv[44] = "valladolid";
nomProv[45] = "alava";
nomProv[46] = "zamora";
nomProv[47] = "zaragoza";
nomProv[48] = "baleares";
nomProv[49] = "ceuta";
nomProv[50] = "melilla";
nomProv[51] = "lasPalmas";
nomProv[52] = "tenerife";
var provId = document.clasif2_sv_form.clasif2_sv_provid.value;
var prov = nomProv[provId];
var tipoInm = document.clasif2_sv_form.clasif2_sv_tipoinm.value;
var comprar = document.clasif2_sv_form.clasif2_sv_compalq[0];
var alquilar = document.clasif2_sv_form.clasif2_sv_compalq[1];
if(tipoInm == 7) {
return ("http://www.globaliza.com/www/es/buscador/promociones.jsp?nombreProvincia=" + prov + "&origenApp=35");
}
else if(tipoInm == 99) {
return ("http://www.globaliza.com/www/es/buscador/" + ((alquilar.checked) ? "alquileres" : "inmuebles") + ".jsp?nombreProvincia=" + prov + "&origenApp=35");
}
else {
return ("http://www.globaliza.com/www/es/buscador/otrosInmuebles/listadoInmuebles.jsp?buscar=10&idProvincia=" + provId + "&idTipoInmueble=" + tipoInm + "&idSituacion=" + ((comprar.checked) ? '1' : ((alquilar.checked) ? '2' : '')) + "&origenApp=35");
}
}
</script>
