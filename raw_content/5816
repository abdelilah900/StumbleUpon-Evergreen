<li class="clasif2_hide" id="clasif2_eye_autocom_b" onmouseover="selectOpt(this.id);" onmouseout="deselectOpt(this.id);" onclick="processClick(this.id);"><div></div></li>
<li class="clasif2_hide" id="clasif2_eye_autocom_c" onmouseover="selectOpt(this.id);" onmouseout="deselectOpt(this.id);" onclick="processClick(this.id);"><div></div></li>
<li class="clasif2_hide" id="clasif2_eye_autocom_d" onmouseover="selectOpt(this.id);" onmouseout="deselectOpt(this.id);" onclick="processClick(this.id);"><div></div></li>
</ul>
