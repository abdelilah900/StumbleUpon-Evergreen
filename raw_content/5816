<script type="text/javascript">
function clasif2_selPesta(cur) {
document.getElementById("clasif2_pesta_sv_sel").className = ((cur == 1) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_pesta_sv_nosel").className = ((cur != 1) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_pesta_sv_inf").className = ((cur == 1) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_sv").className = ((cur == 1) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_pesta_eye_sel").className = ((cur == 2) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_pesta_eye_nosel").className = ((cur != 2) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_pesta_eye_inf").className = ((cur == 2) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_eye").className = ((cur == 2) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_pesta_mot_sel").className = ((cur == 3) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_pesta_mot_nosel").className = ((cur != 3) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_pesta_mot_inf").className = ((cur == 3) ? "clasif2_show" : "clasif2_hide");
document.getElementById("clasif2_mot").className = ((cur == 3) ? "clasif2_show" : "clasif2_hide");
return false;
}
function newB(ow,r,t) {
ow.style.background = "url('http://estaticos.elmundo.es/clasificados/cajas/img/ccom_spr_e.png') no-repeat scroll -" + r + "px -" + t + "px";
}
</script>
