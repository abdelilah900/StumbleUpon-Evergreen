                        <script type="text/javascript">
	var mh_flyout_callState1 = 0;
	
	function displayMenu1(e)
	{
		hideMenuImmed();
		var gmnuObj = this;
		jQuery(this).attr('ON','1');
		jQuery(this).removeAttr('OFF');
		setTimeout(function() {
			var mnuObj = gmnuObj;
			if(jQuery(mnuObj).attr('ON') == '1')
			{
				jQuery('.mh_flyout_menu1').css('display','none');
				jQuery('#mh_header_flyout_lean').css('display','block');
				jQuery(mnuObj).removeAttr('ON');
			}
			},
			500
		);
	}
	function hideMenu1(e)
	{
		mh_flyout_callState1=0;
		var gmnuObj = this;
		jQuery(this).attr('OFF','1');
		jQuery(this).removeAttr('ON');
		setTimeout(function() {
			var mnuObj = gmnuObj;
			if((mh_flyout_callState1==0) && (jQuery(mnuObj).attr('OFF') == '1'))
			{
				jQuery('#mh_header_flyout_lean').css('display','none');
				jQuery(mnuObj).removeAttr('OFF');
			}
			},
			500
		);
	}
	
	function setCallOnState1(e)
	{
		mh_flyout_callState1 = 1;
	}
	function setCallOffState1(e)
	{
		mh_flyout_callState1 = 0;
		var mnuId = "#mh_header_subnav_lean";
		jQuery(mnuId).trigger("mouseout");
	}
	
	function hideMenuImmed1(e)
	{
		mh_flyout_callState1=0;
		jQuery('#mh_header_flyout_lean').css('display','none');
	}


$(document).ready( function() {
	$('.mh_flyout_menu1').hover(
		setCallOnState1,
		setCallOffState1
	);
	$('#mh_header_subnav_lean').hover(
 		 displayMenu1,
 		 hideMenu1
 	);
	 
 	$('.main-menu').hover(
	hideMenuImmed1,
	function(){}
	);
});

</script>
