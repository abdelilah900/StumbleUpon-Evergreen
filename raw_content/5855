      if (!window.registeredBloggerCallbacks) {
        window.registeredBloggerCallbacks = true;

        

        
        gadgets.rpc.register('requestReload', function() {
          document.location.reload();
        });

        
        gadgets.rpc.register('requestSignOut', function(siteId) {
          
          google.friendconnect.container.openSocialSiteId = siteId;
          google.friendconnect.requestSignOut();
        });
      }
    </script>
<script type="text/javascript">
    
    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
            
            
              holder.currentPost = "http://www.blogger.com/feeds/2597735463379728991/posts/default/6612153996174668526";
            
            
            
              holder.currentComments = "http://www.blogger.com/feeds/2597735463379728991/6612153996174668526/comments/default";
            
            holder.currentPostUrl = "";
            holder.currentPostId = 6612153996174668526
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/2597735463379728991/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/2597735463379728991/comments/default";
          
          holder.currentBlogUrl = "http://jest4laughs.blogspot.com/";
          holder.currentBlogId = "2597735463379728991";
        
        return holder;
      });
    }
  </script>
