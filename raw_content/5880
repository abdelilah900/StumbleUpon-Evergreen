<script>
//apply a border to the longer element
$(document).ready(function() {
    var main = $(".sl-main-section");
    var rail = $(".sl-right-rail");

    if(main.height() > rail.height())
    {
        main.css('border-right', 'solid 1px #e6e6e6')
    }
    else
    {
        rail.css('border-left', 'solid 1px #e6e6e6')
    }

  });
</script>
