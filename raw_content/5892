<script type="text/javascript">

	var imgCount = 13;
	var scrollIndex = +1;
	var scrollCount = imgCount/6;
	/* If scrollCount is 0 or 1 - you have no need for a scrollable */
	$(document).ready(function(){
		if (imgCount < 6) {
			$(".thumbs.scrollable .prev, .thumbs.scrollable .next").hide();
		}
		var api = $(".scrollable").data("scrollable");
		if (scrollIndex > 6) {
			var whereMI = scrollIndex/6; 
			if (whereMI%1 === 0) {
				var whereToGo = whereMI-1;
			} else {
				var whereToGo = Math.floor(whereMI);
			}
			api.seekTo(whereToGo);
		}
	});


function personalizationClose() {
	$("#personalizationOverlay .close_button").click();
}
$(document).ready(function(){
	$("a[rel=#personalizationOverlay]").overlay({
		top: 200,
		mask: {
			color: '#000',
			loadSpeed: 200,
			opacity: 0.2
		},
		fixed: false,
		load: false,
		onBeforeLoad: function() {
			if (this.getTrigger().attr("rel") == '#personalizationOverlay') {
				var wrap = this.getOverlay().find(".contentWrap");
				wrap.html("");
				$.get(this.getTrigger().attr("href"), function(data){
					wrap.html(data);
					loadPersonalizationScrollable();
				});
			}
		},
		onClose: function() {
			if (this.getTrigger().attr("rel") == '#personalizationOverlay' && personalizationReloadOnComplete) {
				$.get('/just-for-you/personalize//-1/?forceContent=1', function(data){
					if ($.browser.msie) {
						$(".JustForYou").replaceWith(innerShiv(data,false));
					} else {
						$(".JustForYou").replaceWith(data);
					}
					$(".userArticles").scrollable({ circular: false }).navigator({ navi: '.paginator'});
				});
				personalizationReloadOnComplete = false;
			}
		}
	});
	skipCompletionPage = true;
});


