<script type="text/javascript">

    function hideAjaxControls(behaviorID, updateProgressDivId) {
        hidePopup(behaviorID);
        hideUpdateProgressDiv(updateProgressDivId);
    }

    function hidePopup(behaviorID) {
        $find(behaviorID).hide();        
        $('select').css("visibility", "visible");
    }

    function showSignupPopup() {
        $get("ctl00_uctlFullSignupControl_pnlSignupPopupMyMcCormick").style.zIndex = '10001';
        document.getElementById("ctl00_uctlFullSignupControl_pnlSignupPopupMyMcCormick").style.display = 'block';
        $find("bSignupPopupMyMcCormick").show();

    }


    function hideUpdateProgressDiv(id) {
        document.getElementById(id).style.visibility = 'hidden';
        return false;
    }

    function cmdSendMobile_ClientClick() {
        document.getElementById('divProgSignupPopup').style.visibility = 'visible';
    }

    function clickButton(e, buttonid) {
        var bt = document.getElementById(buttonid);
        if (typeof bt == 'object') {
            if (navigator.appName.indexOf("Netscape") > (-1)) {
                if (e.keyCode == 13) {
                    bt.click();
                    return false;
                }
            }
            if (navigator.appName.indexOf("Microsoft Internet Explorer") > (-1)) {
                if (event.keyCode == 13) {
                    bt.click();
                    return false;
                }
            }
        }
    }

</script>

       
      

<script type="text/javascript">
    function controlLoad(object, eventArgs) {
        var name = getQueryStringValue("name");
        name = unescape(name);
        
        if ('undefined' !== name) {
            var txtName = $get('ctl00_uctlNewsletterSignup_txtname');
            txtName.value = name;
        }    
    
        if (window.location.search.substring(1).toLowerCase().indexOf('showsignup=1') != -1)
            showDineInDineOutPopup();

}

Sys.Application.add_load(controlLoad);
</script>

