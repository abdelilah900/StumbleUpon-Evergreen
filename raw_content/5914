  var show = true;

  function hideSlideDown() {
      show = false;
      setCookies("MccSessionTrackingCookie", "PageViews", 0);
      $('#ctl00_uctlNewsLetterSignupControl_pnlSignupSlideDown').slideUp(700);
  }

  function hideThanksPanel() {
      $('#ctl00_uctlNewsLetterSignupControl_pnlThanksPanel').slideUp(700);
  }
    

  function showSignupSlideDownPopup() {
    if (show)
        $('#ctl00_uctlNewsLetterSignupControl_pnlSignupSlideDown').slideDown(700);
  }

</script>

	            
