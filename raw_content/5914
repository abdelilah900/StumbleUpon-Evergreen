<script type="text/javascript">
    function controlLoad(object, eventArgs) {
        showSignupSlideDownPopup();       
    }
    function setCookies(name, subName,value, days) {
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                var expires = date;        
            }
            else var expires = "";
            subCookieUtil.setCookie(name, subName, value, expires);
    }

Sys.Application.add_load(controlLoad);
</script>

