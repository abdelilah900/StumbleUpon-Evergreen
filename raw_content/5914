<script type="text/javascript">

SitecoreExtranetDomainUserId = '';

  function cmdNewsletterSignupContinue_ClientClick() 
  {
    if (!Page_ClientValidate("NewsletterSignup")) return false;

    if (SitecoreExtranetDomainUserId=='')
    {
        tagNewsletterSignupRegistration();
    }
    else
    {
        tagNewsletterSignupMemberThankYou();
    }    
  }


  function hideAjaxControls(behaviorID, updateProgressDivId)
  {
    hidePopup(behaviorID);
    hideUpdateProgressDiv(updateProgressDivId);
  }
  
  function hidePopup(behaviorID)
  {
    document.getElementById("ctl00_uctlNewsletterSignup_txtname").value = '';
    document.getElementById("ctl00_uctlNewsletterSignup_txtemail").value = '';
    $find(behaviorID).hide(); 
  }
  
  function hideUpdateProgressDiv(id)
  {
    document.getElementById(id).style.visibility = 'hidden';
    return false;
  }
  
  function cmdSendMobile_ClientClick()
  {
    document.getElementById('divProgNewsletterSignup').style.visibility = 'visible';
  }
  
  function showNewsletterSignupPopup()
  {
      $get("ctl00_uctlNewsletterSignup_pnlDineInDineOut").style.zIndex = '10001';
      document.getElementById('ctl00_uctlNewsletterSignup_pnlDineInDineOut').style.display = 'block';
      $find("bNewsletterSignup").show();
     
  }

  function showNewsletterSignupPopupHoliday(tag)
  {
        var s=s_gi("studiocommccormickcom");

        s.eVar1="NEWSLETTER_SIGNUP_HOLIDAYS";
        s.pageName="Holidays: " + tag;
        s.channel="Holidays";
        s.events="event27";
        s.linkTrackVars="eVar1,events";
        s.linkTrackEvents="event27";
        s.customLinkTrackingCall = true;  
        s.tl("link URL","o","Internal Promotion Click");

        $get("ctl00_uctlNewsletterSignup_pnlDineInDineOut").style.zIndex = '10001';
        document.getElementById('ctl00_uctlNewsletterSignup_pnlDineInDineOut').style.display = 'block';
        $find("bNewsletterSignup").show();
  }

  function showConfirmation()
  {      
      $find('bNewsletterSignup').hide();
      $find('bConfirmNewsletteration').show();
  }

  function clickButton(e, buttonid)
  { 
  	var bt = document.getElementById(buttonid); 
  	if (typeof bt == 'object'){ 
  		if(navigator.appName.indexOf("Netscape")>(-1)){ 
  		    if (e.keyCode == 13){ 
  			    bt.click(); 
  			    return false; 
  		    } 
  		} 
  		if (navigator.appName.indexOf("Microsoft Internet Explorer")>(-1)){ 
  			if (event.keyCode == 13){ 
  				bt.click(); 
  				return false; 
  			} 
  		} 
  	}
  }
</script>      
      </noindex>
    
