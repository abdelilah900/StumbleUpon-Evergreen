<script type="text/javascript">
  function deselectSearchArticles(article)
  {
  }

function divcontentSearch(rollover) {
    if (rollover) {
        document.getElementById('contentSearch').style.background = "url('/mcc/images/commons/box_01.gif')";
        document.getElementById('contentSearch').style.backgroundRepeat = "no-repeat";
        document.getElementById('cntr_header_search_bigbox').style.background = "url('/mcc/images/commons/box_02.gif')";
        document.getElementById('cntr_header_search_bigbox').style.backgroundRepeat = "no-repeat";
        document.getElementById('cntr_header_search_bigbox').style.display = "block";

    }
    else {
        document.getElementById('contentSearch').style.background = "";
        document.getElementById('cntr_header_search_bigbox').style.background = "";
        document.getElementById('cntr_header_search_bigbox').style.display = "none";
    }
}
</script>
