			ImageSpots.show( 'spot40670', 'F1FDY6KGQB4XRUQ' );
		    });
		
		var next_row = $("spot40670_spots_next_row");
		var opt = {};
		opt.rowsize = 7;
		if (next_row) {
		    next_row.setStyle({display: "block"});
		    next_row.observe("click", function (evt) {
			evt.preventDefault();
			spot.handleNextRow("spot40670_spots", opt);
		    });
		}
		var prev_row = $("spot40670_spots_prev_row");
		if (prev_row) {
		    prev_row.observe("click", function (evt) {
			evt.preventDefault();
			spot.handlePrevRow(opt);
		    });
		}

            });
          </script><div class="txt mt15"><div>
