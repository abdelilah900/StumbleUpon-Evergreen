		    });
		
		    $("imgThumb_spot40670_F6JBN3SGQB4XRJB").observe("click", function() {
			ImageSpots.show( 'spot40670', 'F6JBN3SGQB4XRJB' );
		    });
		
		    $("imgThumb_spot40670_FTSEI5JGQB4XRKF").observe("click", function() {
			ImageSpots.show( 'spot40670', 'FTSEI5JGQB4XRKF' );
		    });
		
		    $("imgThumb_spot40670_FIK3S36GQB4XRLG").observe("click", function() {
			ImageSpots.show( 'spot40670', 'FIK3S36GQB4XRLG' );
		    });
		
		    $("imgThumb_spot40670_F2H8IA9GQB4XRMI").observe("click", function() {
			ImageSpots.show( 'spot40670', 'F2H8IA9GQB4XRMI' );
