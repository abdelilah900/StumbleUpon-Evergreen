<script type="text/javascript">
Backplane.init({
	"serverBaseURL" : "http://api.js-kit.com/v1",
	"busName": "nymag.com"
});
Backplane.expectMessagesWithin(4);


$(document).ready(function() {
	NYM.echo.loadCommentTouts({displayLink: true});

if (NYM.config.membership.service == "nym") {
        var activeMC = $("#most-commented ul.tabs li.ui-state-active").attr("id");
        loadMC("btn-grubny");
        $("#most-commented ul.tabs li a").bind("click.mc", function() {
                $(this).unbind("click.mc");
                var section = $(this).parent().attr("id");
                loadMC(section);
                $(this).addClass("active");
        });
        $("#most-commented ul.tabs li.ui-state-active a").unbind("click.mc");
}
});

function loadMC(section) {
        var tab = section.substring(4);
        var scope;
        if (section == "btn-intel") { scope = "DailyIntel"; }
        if (section == "btn-vulture") { scope = "Vulture"; }
        if (section == "btn-grubny") { scope = "GrubStreetNewYork"; }
        if (section == "btn-cut") { scope = "TheCut"; }
        NYM.echo.loadMostCommented({
            timeframe: "3 days ago",
            elementId: "tab-"+tab+" ul",
            section: "source:" + scope
        });
}


var blogTypeReferrer = null;

</script>


		
	
	
		
	
	
	
