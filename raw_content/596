<script type="text/javascript" src="/skin/frontend/shopruche/default/js/jquery.fancybox.20.js" ></script>
 
 
<script type="text/javascript">
 jQuery(window).load(function(){
 
jQuery('#mc-footer-embedded-subscribe, #mc-embedded-subscribe').click(function() {

		var $mcemail = jQuery(this).closest('form').find('.mcinput');
		
		var mcemail = $mcemail.val();
		var atpos=mcemail.indexOf("@");
		var dotpos=mcemail.lastIndexOf(".");
		var $mcerror = jQuery(this).closest('form').find('.mc-error-msg');
		
		if (mcemail==null || mcemail=="")
		{
			$mcerror.html('No email address provided.');
			$mcemail.addClass('validation-failed');
			return false;
		}else{
			$mcerror.html('');
			$mcemail.removeClass('validation-failed');
		}
		
		if (atpos<1 || dotpos<atpos+2 || dotpos+2>=mcemail.length)
		{
			$mcerror.html('Email address is invalid.');
			$mcemail.addClass('validation-failed');
			return false;
		}else {
			$mcerror.html('');
			$mcemail.removeClass('validation-failed');
		}
		
		//var test = jQuery.fancybox();
		
		var pageModal = jQuery.fancybox({minWidth:340, minHeight:120, modal:true, helpers : {overlay:{opacity:0.4} }, content:'<div id="mc-msg"> Please wait while we process your subscription.<br><img src="/skin/frontend/shopruche/default/css/fancybox20/loading.gif" /> </div>' });

		jQuery.ajax({
			url: '/ruche_scripts/mc/mailing-list.php',
			data: 'ajax=true&email=' + escape($mcemail.val()),
			success: function(msg) {
				//If msg is success
				if (msg.indexOf('Welcome') > -1){
					jQuery('#mc-msg').html(msg);
					$mcemail.val('');
				} else {
					jQuery('#mc-msg').html(msg);
				}
			}
		});
			
		return false;

	});
	
});
</script>

