    <script>

function openShareBox(tpl, dataObj) {
		tpl = tpl.replace("##URL##",	encodeURIComponent(dataObj.theLink()));
		tpl = tpl.replace("##TITLE##",	encodeURIComponent(dataObj.theTitle()));
		tpl = tpl.replace("##IMAGE##",	encodeURIComponent(dataObj.theImage()));
		tpl = tpl.replace("##DESC##",	encodeURIComponent(dataObj.theDesc()));
        window.open(tpl,"sharewindow"+tpl.substr(6,15),"width=640,height=480");	
	
}

var mporaPhotoShareURLS = {
    fb:function() 		{ return 'http://www.facebook.com/sharer.php?u=##URL##&t=##TITLE##'; },
    tw:function() 		{ return 'http://twitter.com/home?status=##URL##+##TITLE##';},
    su:function()		{ return 'http://www.stumbleupon.com/submit?url=##URL##';},
    reddit:function()	{ return 'http://www.reddit.com/submit?url=##URL##';},
    digg:function()		{ return 'http://digg.com/submit?phase=2&amp;url=##URL##&amp;title=##TITLE##'; },
	pintrest:function() { return 'http://pinterest.com/pin/create/button/?url=##URL##&media=##IMAGE##&description=##DESC##'; },
	tumbler:function() 	{ return 'http://www.tumblr.com/share/photo?source="##IMAGE##&caption=##TITLE##&clickthru=##URL##';
	}
};
</script>
	
