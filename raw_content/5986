    $(document).ready(function(){
		$(".submenu").hoverIntent(function(){
			$(this).find(".subnav").fadeIn('fast');
			$(this).find("a:first").addClass("active");
		},function(){
			$(this).find(".subnav").delay(200).fadeOut('slow');
			$(this).find("a").removeClass("active");
		});
        
