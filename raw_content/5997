<script type="text/javascript">
    //** adding JS to parse and present messages **//
    var vMessageCookieName = 'bbox_msgs';
    var aMessageCookies = document.cookie.match('(^|;) ?' + vMessageCookieName + '=([^;]*)(;|$)');
    if ( aMessageCookies ){
        var vMessageCookie = decodeURIComponent(aMessageCookies[2]);
        if (vMessageCookie != '') {
            var aMessages = JSON.parse(vMessageCookie);
            var eMessagesDiv = jQuery('#product-detail-global-message');
            var eMessagesUl = eMessagesDiv.append('<ul class="alert alert-success"></ul>');
            aMessages.each(function(vMessage){
                eMessagesUl.find('ul').prepend('<li class="success-message"><ul><li></li></ul></li>');
                eMessagesUl.find('ul li').html(vMessage + " <a href='/shop/checkout/cart'>Check Out Now</a>");
            })
             //now delete the cookie by setting expiry date in past
            document.cookie = escape(vMessageCookieName) + '=;expires=' + new Date(0) 
                +';path=/; domain=.www.birchbox.com';
        }
    }
</script>    

