var loggedInUIdOfUser = '';
var loginDomain = '';


window.fbAsyncInit = function() {
	FB.init({appId  : '4a27a904c492f128d46163d02575765c', status : true, cookie : true, xfbml  : true });

	if(typeof kFacebookConnectObj == 'undefined'){
		var kfc = document.createElement('script'); kfc.type = 'text/javascript'; kfc.src = 'http://cn1.kaboodle.com/ht/scripts/facebookConnect.js?v=r81_56137-1';
		document.getElementById('fb-root').appendChild(kfc);
	}
	checkForFBSesion();
};
function checkForFBSesion(){
	if(typeof kFacebookConnectObj == 'undefined'){
		window.setTimeout('checkForFBSesion()', 200);
	}else{
		FB.Event.subscribe('auth.sessionChange', function(response) {
			if (!response.session && loggedInUIdOfUser.length > 0 && loginDomain == 'FACEBOOK') {
				kFacebookConnectObj.loggedOutFromFB();
			}
		});
	}
}
(function() {
	var e = document.createElement('script');e.type = 'text/javascript';e.async = true;e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';document.getElementById('fb-root').appendChild(e);
}());
</script>
