<script type="text/javascript" >
function on_state_change()
{
	var dropdown = document.getElementById('selected_state');
	var myindex  = dropdown.selectedIndex;
	var SelValue = dropdown.options[myindex].value;
	if (SelValue != 'united-states')
	{
	var baseURL  = 'http://www.worldlifeexpectancy.com/usa/'+SelValue+'-health-profile';
	top.location.href = baseURL;
	}
	else
	{
	var baseURL  = 'http://www.worldlifeexpectancy.com/usa-health-rankings';
	top.location.href = baseURL;		
	}
	return true;
}
</script> 

