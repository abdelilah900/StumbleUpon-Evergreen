<script type="text/javascript">
function switchInfoTab(tab, page_type) {
    updateInfoTab(tab,
        '100000001',
        '4558',
        page_type,
        null );
}

function updateInfoTab(tab, entity_id, media_id, page_type, page, mcid) {
  var page_number = '',
      container = $('container'),
      url = "http://myown.oprah.com/common/info_tab_container.inc",
      base_url = '';

  page_number = '';

  var myAjax = new Ajax.Request(url, {
    method: 'get',
    parameters: {
      type:   tab,
      entity_id:  entity_id,
      media_id:   media_id,
      page_type:  page_type || '',
      page:  page_number,
      base_url: base_url,
      mcid: mcid || '',
      content_src: 'ocom',
      star_image: 'http://www.oprah.com/images/video/ratings_own_39b54a_v1.png',
      is_article: 1 },
    onComplete: function(req) {
      if (req.status != 200) return;

      container.update(req.responseText);
      $('comments_tab').fire('comments:loaded', tab);
      var input = container.down('input[name=total_entries]');
      if (input) {
        var value = input.value;
        if (tab == 'comments') {
          $$('span.comment_count').invoke('update', value);
        }
        else {
          $$('span.history_count').invoke('update', value);
        }
      }
    }
  });
}

document.observe("dom:loaded", function() {
  switchInfoTab('comments','text');
});


</script>

