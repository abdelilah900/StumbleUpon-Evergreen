<script>
  function module_167_subscribe_to_newsletter() {
    if (!loginCheck({
      onLogin: module_167_subscribe_to_newsletter,
      login_req: 1
    })) return;

    var url = Global.SERVER_NAME + '/cgi-bin/ajax_processor.cgi';
    new Ajax.Request(url, {
      method: 'post',
      parameters: {
        op: 'subscribe_newsletter',
        news_options: "8"
      },
      onComplete: function(request) {
        var response = request.responseXML;
        if (response) {
          var errcode
            = response.getElementsByTagName('errcode')[0].firstChild.nodeValue;
          alert('Thank you for subscribing to the Searching for Answers?')
        }
      }
    });
  }
</script>

