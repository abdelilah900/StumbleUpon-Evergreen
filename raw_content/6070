      $('.nc_footer_tout_carousel > .jcarousel-prev').after('<div class="nc_position_footer_tout_carousel">Displaying <span class="display_range">1-n</span> of ' + count + ' galleries</div>');
      $('.nc_footer_tout_carousel > .jcarousel-prev').before('<div class="gallery_controls"></div>');
      $('.nc_footer_tout_carousel > .gallery_controls').append($('.nc_footer_tout_carousel > .jcarousel-prev'));
      $('.nc_footer_tout_carousel > .gallery_controls').append($('.nc_position_footer_tout_carousel'));
      $('.nc_footer_tout_carousel > .gallery_controls').append($('.nc_footer_tout_carousel > .jcarousel-next'));
    }
    if (footer_displayed == 0) {
      var count = $('#nc_footer_tout_carousel div.total').get(0).innerHTML;
      if (count > 0) {
        var width = parseInt($('ul#nc_footer_tout_carousel').css('width'));
        var itemWidth = width / ($('ul#nc_footer_tout_carousel > li').length);
        footer_displayed = Math.ceil(parseInt($('div.nc_clip_footer_tout_carousel').css('width')) / itemWidth);
      }
    }
    $('.nc_position_footer_tout_carousel span.display_range').text(index + '-' + (index+footer_displayed-1));
    //alert('displayed: ' + footer_displayed);
  }</script>
<script type="text/javascript">var related_displayed = 0;
function related_gallery_scroll(carousel, index, direction) {
  if (direction == 'init') {
    var count = $('#nc_related_photo_galleries div.total').get(0).innerHTML;
    $('.nc_related_photo_galleries > .jcarousel-prev').after('<div class="nc_position_related_photo_galleries">Displaying <span class="display_range">1-n</span> of ' + count + ' galleries</div>');
    $('.nc_related_photo_galleries > .jcarousel-prev').before('<div class="gallery_controls"></div>');
    $('.nc_related_photo_galleries > .gallery_controls').append($('.nc_related_photo_galleries > .jcarousel-prev'));
    $('.nc_related_photo_galleries > .gallery_controls').append($('.nc_position_related_photo_galleries'));
    $('.nc_related_photo_galleries > .gallery_controls').append($('.nc_related_photo_galleries > .jcarousel-next'));
  }
  if (related_displayed == 0) {
    var count = $('#nc_related_photo_galleries div.total').get(0).innerHTML;
    if (count > 0) {
      var width = parseInt($('ul#nc_related_photo_galleries').css('width'));
      var itemWidth = width / ($('ul#nc_related_photo_galleries > li').length);
      related_displayed = Math.ceil(parseInt($('div.nc_clip_related_photo_galleries').css('width')) / itemWidth);
    }
  }
  $('.nc_position_related_photo_galleries span.display_range').text(index + '-' + (index+related_displayed-1));

}
//debugger;
related_gallery_scroll('nc_related_photo_galleries', 0, 'init');</script>
<script type="text/javascript">
  /* Reload the page impression to omniture so that it's counted. */
  function image_gallery_scroll(carousel, index, direction) {
    if (direction != 'init') {
      void(s.t());
    }
  }
</script>
