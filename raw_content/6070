jQuery('ul').removeClass('hidden');
jQuery('div.jcarousel_waiting').hide();
  jQuery('ul#nc_main_feature_touts').jcarousel({
    visible: 1,
    scroll: 1,
    vertical: false,
    wrap: 'both',
    auto: 9,
    animation: 'fast',
    start: 1,
    buttonNextHTML: '<div></div>',
