<script type="text/javascript">
    (function ($) {
        $(function() {
            $( '#signup_losangeles' ).data( 'cleared', false );
            
            $( '#signup_losangeles' ).focus(function() {
                var s = $( this );
                if ( false === s.data( 'cleared' ) )
                {
                    s.val( '' );
                    s.data( 'cleared', true );
                }
            });
            
            $( 'form.lnewsletter.losangeles' ).submit( function() {
                $.ajax( $( this ).attr( 'action' ), 
                    {
                        'data': $( this ).serializeArray(),
                        'method': 'GET',
                        'success': function(data) 
                            {
                                var cnt = $( '#sign-up-container.losangeles' );
                                cnt.data( 'form', cnt.html() );
                                $( '#sign-up-container.losangeles' ).html( data );
                            }
                    }
                );
                return false;
            });
            
            $( '#mlback_losangeles' ).live('click', function()
                {
                    var cnt = $( '#sign-up-container.losangeles' );
                    cnt.html( cnt.data( 'form' ) );
                    return false;
                } 
            );
        });
    })(jQuery);
</script>
</div><div class="tabGroup followUs roundBox module">
		
		
		    <div class="inner">
	        	            <h4 class="facebook">
	                	            </h4>
	
	            <div class="roundBox facebook_container">
	            			                		                    <iframe style="border: none; overflow: hidden; width: 240px; height: 274px;" src="http://www.facebook.com/plugins/likebox.php?href=https%3A%2F%2Fwww.facebook.com%2FTimeOutLA&amp;width=240&amp;height=274&amp;colorscheme=light&amp;show_faces=true&amp;border_color&amp;stream=false&amp;header=false" frameborder="0" scrolling="no"></iframe>		                		        		            </div>
	        	            <h4 class="twitter">
	                	            </h4>
	
	            <div class="roundBox twitter_container">
	            			                		                <script type="text/javascript">//<![CDATA[
			                (function($){
				                $(document).ready(function() {
				                	$('.tabTriggers h4.twitter').click(function(event){
				                		$(this).unbind(event); //load tweet only first time
					                        
				                		$.getJSON('https://api.twitter.com/1/statuses/user_timeline.json?screen_name=timeoutla&count=3&callback=?', function(tweetsJson){
					                        var tweets = [];
					                        $(tweetsJson).each(function(i, v){
					                            tweets.push('<li>');
				                            	tweets.push('<img src="' + v.user.profile_image_url_https + '" alt="twitter icon" />');
				                            	tweets.push('<p><span class="user">');
				                            	tweets.push('<a href="http://www.twitter.com/' + v.user.screen_name + '" target="_blank">@' + v.user.screen_name + '</a> ');
				                            	tweets.push('</span>' + processTweetLinks(v.text) + '</p>');
				                            	tweets.push('<span class="more">');
				                            	tweets.push('<span class="time">' + niceTime(v.created_at) + '</span>');
				                            	tweets.push('</span>');
					                            tweets.push('</li>');
					                        });
				                            
					                        $('#last_three_tweets > ul').empty().html(tweets.join(''));
					                    });
				                	});
				                    
				                });
				                
				                //add anchor to usenames, links and ashtags
				                function processTweetLinks(text) {
								    var exp = /(\b(https?|ftp|file):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/i;
								    text = text.replace(exp, "<a href='$1' target='_blank'>$1</a>");
								    exp = /(^|\s)#(\w+)/g;
								    text = text.replace(exp, "$1<a href='http://search.twitter.com/search?q=%23$2' target='_blank'>#$2</a>");
								    exp = /(^|\s)@(\w+)/g;
								    text = text.replace(exp, "$1<a href='http://www.twitter.com/$2' target='_blank'>@$2</a>");
								    return text;
								}
								
								var niceTime = (function() {
	 
								    var ints = {
								        second : 1,
								        minute : 60,
								        hour   : 3600,
								        day    : 86400,
								        week   : 604800,
								        month  : 2592000,
								        year   : 31536000
								    };
								    
								 	var plurals = {
								 		second : 'seconds',
								        minute : 'minutes',
								        hour : 'hours',
								        day : 'days' ,
								        week : 'weeks',
								        month : 'months',
								        year : 'years'
								 	}
								 	
								    return function(time) {
								        time = +new Date(time);
								 
								        var gap = ((+new Date()) - time) / 1000,
								            amount, measure;
								 
								        for (var i in ints) {
								            if (gap > ints[i]) { measure = i; }
								        }
								 
								        amount = gap / ints[measure];
								        amount = gap > ints.day ? Math.round(Math.round(amount * 100) / 100) : Math.round(amount);
								        amount += ' ' + ( amount == 1 ? measure : plurals[measure] ) + ' ago';
								 
								        return amount;
								    };
								 
								})();
				            })(jQuery);    
		                //]]></script> 
		                <div id="last_three_tweets">
		                	<ul></ul>
		                	<div class="twitter-follow-button-container"><a href="https://twitter.com/timeoutla" title="Follow on Twitter" target="_blank" class="goto-twitter-follow-button">Follow on Twitter</a></div>
		                </div>
		                		        		            </div>
	        	    </div>
	
</div>


