<script language="javascript" type="text/javascript">

  

  function submitSearchPagination(url) {

    var searchKeyword = encodeURIComponent(encodeURIComponent(document.getElementById("searchKeyword").value));
    var currentPageNum = document.getElementById("currentPageNum").value;
    if(currentPageNum == null || currentPageNum == "") {
      currentPageNum = 1;
    }

    window.location="http://family.go.com/" + url + "/" + searchKeyword + "/" + currentPageNum;
  }

  function submitLocalSearchPagination() {
    var searchKeyword = encodeURIComponent(encodeURIComponent(document.getElementById("localSearchKeyword").value));
    var zipCode = document.getElementById("zipCode").value;
    var currentPageNum = document.getElementById("localSearchCurrentPageNum").value;

    if(currentPageNum == null || currentPageNum == ""){
      currentPageNum = 1;
    }

    window.location="http://family.go.com/localsearch/" + searchKeyword + "/" + zipCode + "/" + currentPageNum;
  }

  function submitLocalSearch() {
    var searchKeyword = encodeURIComponent(encodeURIComponent(document.getElementById("localSearchKeyword").value));
    var zipCode = document.getElementById("zipCode").value;
    window.location="http://family.go.com/localsearch/" + searchKeyword + "/" + zipCode;
  }

  function submitLogin() {
    var locationStr = document.location.href;
    var secureUrl = "";
    if (document.location.href.indexOf("9080") == -1){
        
          secureUrl = "https://" + document.location.host;
        
    }
    if (locationStr.lastIndexOf("http") == 0) {
      window.location = secureUrl + "/accounts/login/?appName=family&redirectUrl=" + document.location;
    }else {
      window.location = secureUrl + "/accounts/login/?appName=family&redirectUrl=" + locationStr.substring(locationStr.lastIndexOf("http"));
    }
  }

  function submitLogout() {
        var locationStr = document.location.href;
  
     
     
           if (locationStr.lastIndexOf("http") == 0) {
              window.location = "http://family.go.com/logout/?redirectUrl=" + document.location;
           }else {
              window.location = "http://family.go.com/logout/?redirectUrl=" + locationStr.substring(locationStr.lastIndexOf("http"));
           }
           
  
  }


  function submitRegister() {
    var locationStr = document.location.href;
    var secureUrl = "";
    if (document.location.href.indexOf("9080") == -1){
        
          secureUrl = "https://" + document.location.host;
      
    }
    if (locationStr.lastIndexOf("http") == 0) {
      window.location = secureUrl + "/accounts/registration/?appName=family&redirectUrl=" + document.location;
    }else {
      window.location = secureUrl + "/accounts/registration/?appName=family&redirectUrl=" + locationStr.substring(locationStr.lastIndexOf("http"));
    }
  }

       function submitBecomeAMember() {
            var locationStr = document.location.href;
            var secureUrl = "";
            if (document.location.href.indexOf("9080") == -1){
            
                secureUrl = "https://" + document.location.host;
            
            }
            if (locationStr.lastIndexOf("http") == 0) {
                window.location = secureUrl + "/accounts/registration/?appName=family&redirectUrl=" + document.location;
            }else {
                window.location = secureUrl + "/accounts/registration/?appName=family&redirectUrl=" + locationStr.substring(locationStr.lastIndexOf("http"));
            }
        }

  function submitEditRegistration() {
    var locationStr = document.location.toString();
    var secureUrl = "";
    if (document.location.href.indexOf("9080") == -1){
          
          secureUrl = "https://" + document.location.host;
        
    }
    window.location = secureUrl + "/accounts/my-account/";
  }

  function submitEditProfile() {
    var locationStr = document.location.toString();
    var secureUrl = "";
    if (document.location.href.indexOf("9080") == -1){
          
          secureUrl = "https://" + document.location.host;
        
    }
    window.location = secureUrl + "/accounts/my-account/";
  }

  function submitEditSubscriptions() {
    var locationStr = document.location.toString();
    var secureUrl = "";
    if (document.location.href.indexOf("9080") == -1){
          
          secureUrl = "https://" + document.location.host;
        
    }
    window.location = secureUrl + "/accounts/edit-subscriptions/";
  }

  function urlToEmail(suffix, title) {
    document.emailForm.url.value = window.location.href;
    document.emailForm.title.value = title;
    document.emailForm.suffix.value = suffix;
    document.emailForm.submit();
  }

    function launch(newURL, newName, newFeatures)
  {
    window.open(newURL, newName, newFeatures);
  }

  YAHOO.util.Event.onDOMReady(function() {
  FormUtil.setDefaultText("searchKeyword", "Search");
  FormUtil._defaultTextColor = "#CCCCCC";
  FormUtil._typingTextColor = "#666666";
  });  

</script>

