<script type="text/javascript">	
/* To disable a clickable element from a blocked user and trigger the popup, 
insert the JSP tag to generate the user status code in the class declaration. All users 
with status code 'B', 'S2', and 'S3' will be blocked. 

(Currently nothing happens for 'S1' users, hence functionality pertaining to 'S1' 
has been commented out but kept in place for when specifications change.)
*/
	function BlockedUserTrigger(clickElement,iframeOffsetX,iframeOffsetY,statusCode) {
		this.clickElement = clickElement;
		
		if(statusCode == 'B') {
			this.loginBox = YAHOO.util.Dom.get('block_pu');
			var offsetX = -30;
			var offsetY = -115;
		} else {
			this.loginBox = YAHOO.util.Dom.get('account_warning_pu');
			var offsetX = -30;
			var offsetY = -120;
			
			if(this.clickElement.tagName == "INPUT" || this.clickElement.tagName == "TEXTAREA") {
				var offsetX = -20;
			}
			
		}
		
		if (iframeOffsetX) {
			offsetX = iframeOffsetX;
			offsetY = iframeOffsetY;
		}
		
		// strip events and clear href
		if(statusCode != 'S1') {
			YAHOO.util.Event.removeListener(this.clickElement, 'click');
			if (this.clickElement.href) {
				this.clickElement.href = 'javascript:void(0);'
			}
			
			if (this.clickElement.onclick) {
				this.clickElement.onclick = 'javascript:void(0);'
			}
		}
		
		
		YAHOO.util.Event.addListener(this.clickElement, 'click', function() {
				var clickX = YAHOO.util.Dom.getX(this.clickElement) + offsetX;
				var clickY = YAHOO.util.Dom.getY(this.clickElement) + offsetY;

				if(statusCode == 'S2' || statusCode == 'S3') {
					var warningMessage = YAHOO.util.Dom.get('account_warning_message');
					
					switch(statusCode) {
						/*
						case "S1":
						warningMessage.innerHTML = 'You have been blocked from contributing to this site. Please contact Customer Service for any questions at <a href="mailto:community@disneyfamily.com">community@disneyfamily.com</a>.'; 
						break;
						*/

						case "S2":
						warningMessage.innerHTML = 'You will be prevented from uploading to the site until you complete this online course about understanding copyright laws.  Please complete this course before the next time you log in. <a href="https://register.go.com/global/legalsystem/copyrightSchool?appRedirect=http://family.go.com">https://register.go.com/global/legalsystem/copyrightSchool</a>'; 
						break;
						
						case "S3":
						warningMessage.innerHTML = 'Due to repeated violations of copyright law, your upload account has been terminated for all Disney sites.'; 
						break;
					}
				}
				
				
				
				YAHOO.util.Dom.setStyle(this.loginBox, 'display', 'block');
				
				//reposition pop-up if it's hanging off the page
				if (!iframeOffsetX) {
				var pageWidthOffset = ((YAHOO.util.Dom.getViewportWidth() - 980)/2) + 600;
				if(clickX > pageWidthOffset) {clickX = pageWidthOffset};
				}
				
				YAHOO.util.Dom.setXY(this.loginBox, [clickX,clickY]);
				if(this.clickElement.tagName == "INPUT" || this.clickElement.tagName == "TEXTAREA") {
					if(statusCode != 'S1') {
						this.clickElement.onfocus = this.clickElement.blur();
					}
				}
				return false;
			}, this, true);
			
	}
	
	//add event listeners for different restricted status codes
	YAHOO.util.Event.onDOMReady(function() {
			var clickElementsBlocked = YAHOO.util.Dom.getElementsByClassName('B');
			for (var i=0; i<clickElementsBlocked.length; i++) {
				clickElementsBlocked[i] = new BlockedUserTrigger(clickElementsBlocked[i],false,false,'B');
			}
			
			/* 
			var clickElementsWarned = YAHOO.util.Dom.getElementsByClassName('S1');
			for (var i=0; i<clickElementsWarned.length; i++) {
				clickElementsWarned[i] = new BlockedUserTrigger(clickElementsWarned[i],false,false,'warned');
			}
			*/
			
			var clickElementsRevoked = YAHOO.util.Dom.getElementsByClassName('S2');
			for (var i=0; i<clickElementsRevoked.length; i++) {
				clickElementsRevoked[i] = new BlockedUserTrigger(clickElementsRevoked[i],false,false,'S2');
			}
		
			var clickElementsBanned = YAHOO.util.Dom.getElementsByClassName('S3');
			for (var i=0; i<clickElementsBanned.length; i++) {
				clickElementsBanned[i] = new BlockedUserTrigger(clickElementsBanned[i],false,false,'S3');
			}
		
		
		});
	
</script>
<!-- begin blocked pop up -->
<div class="popup_223_delete_wrap" id="block_pu" style="display:none;">
    <div class="popup_223_top"></div>
    <div class="popup_223_mid">
You have been blocked from contributing to this site. Please contact Customer Service for any questions at <a href="mailto:community@disneyfamily.com">community@disneyfamily.com</a>.	            <div class="popup_302_links">
            <span>
            <a href="javascript:showHide('block_pu');" title="Cancel">Close</a>
            </span>
            </div>

    </div>
    <div class="popup_223_btm"></div>
</div>
<!-- end blocked pop up -->

<!--begin warning pop-up -->
<div class="topics_account_warning_wrapper" id="account_warning_pu" style="display:none;">
    <a id="btn_close_account" href="javascript:void(0)" onclick="document.getElementById('account_warning_pu').style.display='none';">Close</a>
    <div id="topics_account_warning_top"></div>
    <div id="topics_account_warning_mid">
        <h3>Account Warning</h3>
        <p id="account_warning_message">
            You have been blocked from contributing to this site. Please contact Customer Service for any questions at <a href="mailto:community@disneyfamily.com">community@disneyfamily.com</a>.								
        </p>							
    </div>
    <div id="topics_account_warning_bot"></div>
</div>
<!--end warning pop-up -->


    
  
</div>

