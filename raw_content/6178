			var $dropnav = null,
				contentTimeout = null,
				contentRequested = false;

			$('#other_cats')
				.mouseenter(function() {
					var $this = $(this);
					// If the flyout exists then we show it, otherwise construct it now
					if($dropnav && $dropnav.length)
						$dropnav.show();
					else
						$dropnav = $('<div class="dropnav_container"><div class="dropnav"></div></div>')
							.appendTo($this);

					if(!contentRequested) {
						// We delay slighly in case the user was not stopping over this item
						contentTimeout = setTimeout(function() {
							contentRequested = true;
							$this.find('.dropnav').load('/nav.shtml', {cat:14});
						}, 400);
					}
				})
				.mouseleave(function() {
					if($dropnav) $dropnav.hide();
					clearTimeout(contentTimeout);
				});
		})(jQuery);
	</script>
