			categories[i] = new Option("-------", ""); i++; 
			categories[i] = new Option("Bars", "3"); i++; 
			categories[i] = new Option("Cakes", "5"); i++; 
			categories[i] = new Option("Candy", "4"); i++; 
			categories[i] = new Option("Cheesecakes", "7"); i++; 
			categories[i] = new Option("Cookies", "2"); i++; 
			categories[i] = new Option("Cupcakes", "1"); i++; 
			categories[i] = new Option("Parties", "11"); i++; 
			categories[i] = new Option("Pastries", "8"); i++; 
			categories[i] = new Option("Pies", "9"); i++; 
			categories[i] = new Option("Tortes", "6"); i++; 

