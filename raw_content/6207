<script type="text/javascript">
	initFacebook();

	function setLikeButtonReporting() {
		FB.Event.subscribe('edge.create', function(href, widget) {
			var forecastPageName = '';
			var url = href.replace(/http:\/\/[a-zA-Z\.\-]*/, "");
			$.getJSON(repoForecastURL+'?url=' + url + '&callback=?', function(data){
				
				forecastPageName = data.pageName;
				
				mtvn.btg.Controller.sendLinkEvent({
					linkName: 'share',
					linkType: 'o',
					eVar9: forecastPageName,
					eVar19: 'share_fblike',
					events: 'event9'
				});
			});
		});
		window.indexOfLikeSubscribe = FB.Event._subscribersMap["edge.create"].length -1;

	}

	runAfterFacebookInit(setLikeButtonReporting);
</script>
