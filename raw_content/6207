	$("#twitterPosts,.followHolder .twitter a.button").hover(
		function(){
			$(".followHolder .twitter a.button").css("background-color","white");
			$(".followHolder .twitter a.button").css("border-left","1px solid gray");
			$(".followHolder .twitter a.button").css("border-right","1px solid gray");
			$(".followHolder .twitter a.button").css("border-top","1px solid gray");
			$(".followHolder .twitter #twitterPosts").show();
		},
		function(){
			$(".followHolder .twitter a.button").css("background-color","transparent");
			$(".followHolder .twitter a.button").css("border","none");
			$(".followHolder .twitter #twitterPosts").hide();
			$(".followHolder .twitter a.button").css("background-position","0px -149px");
			$("#twitterPostCount").hide();
		}
	)
</script>


<script type="text/javascript">
	var displayCount = ("3" != "") ? 3 : 3;
	var nDisplayed = 0;
	$.ajaxSetup({ scriptCharset: "utf-8" });
	var tweetUrl = "http://search.twitter.com/search.json?callback=?&q=%23danieltosh";
		tweetUrl = "http://tweetriver.com/tomgreenpoint/danieltosh.json";

		if ( ( matches = tweetUrl.match(/^http:\/\/(www\.)?tweetriver\.com\/([a-zA-Z0-9_-]+)\/([a-zA-Z0-9_-]+)(\.json|\.xml)?/) ) ) {
			tweetUrl = "http://social-proxy.mtvnservices.com/feeds/twitter/v1/" + matches[2]+'/'+matches[3] + "?callback=?";
		}		
		$.ajaxSetup({ scriptCharset: "utf-8", jsonpCallback: 'jsonp1027' });
    $.getJSON(tweetUrl, function(response){
        if (response.results != undefined){
			$("#twitterPosts").prepend('<li class="label">Most Recent Tweets</li>');
			$.each(response.results, function(index,data){
	           	if (nDisplayed < displayCount){
					if (data.from_user && data.text && data.profile_image_url && data.created_at) {
						var name = data.from_user;
						var description = data.text;
						var picture = data.profile_image_url;
						var time = data.created_at;
						var link = '<a href="http://twitter.com/'+name+'" target="_blank" style="color:#95181A">';
						if (index == 0 ) checkTweetCookie (time);
						var newhtml = '<li><div class="tweetLeft">'+link+'<img class="image" src="' + picture + '" height="28" width="28"/></a></div> <div class="text tweetRight">' + link + name + '</a> ' + description + '<br/>' + relative_time(time) + '</div><div class="clearAll"></div></li>';
					 	$(newhtml).insertBefore( $('#lastli') );
					 	nDisplayed++;
					}
			 	}
			});
        }
	});

	function checkTweetCookie(time){
		var currentTweetTime = ($.cookie("lastTweet")) ?  new Date($.cookie("lastTweet")) : "";
		var newTweetTime = new Date (time);
		if  (newTweetTime  -  currentTweetTime > 0 || currentTweetTime == "") {
			$(".followHolder .twitter a.button").css("background-position","0px -50px");
			$("#twitterPostCount").html("!");
		}
		$.cookie("lastTweet", time, { expires:365 });
	}

	
</script>
<script type="text/javascript">
$(document).ready(function(){
	twttr.events.bind('follow', function(event){
		if (event && event.data && event.data.screen_name) {
			sendLinkEvent('twitterfollow_'+event.data.screen_name);
		}
	});
});
</script>



										
