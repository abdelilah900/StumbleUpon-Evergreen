<script>
	$("#facebookPosts,.followHolder .facebook a.button").hover(
		function(){
			$(".followHolder .facebook a.button").css("background-color","white");
			$(".followHolder .facebook a.button").css("border-left","1px solid gray");
			$(".followHolder .facebook a.button").css("border-right","1px solid gray");
			$(".followHolder .facebook a.button").css("border-top","1px solid gray");
			$(".followHolder .facebook #facebookPosts").show();
		},
		function(){
			$(".followHolder .facebook a.button").css("background-color","transparent");
			$(".followHolder .facebook a.button").css("border","none");
			$(".followHolder .facebook #facebookPosts").hide();
			$(".followHolder .facebook a.button").css("background-position","0px -100px");
			$("#facebookPostCount").hide();
		}
	)
</script>



																






						


