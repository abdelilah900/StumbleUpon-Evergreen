<!-- TIPSY JS -->
<script type="text/javascript">
  $(function() {
  	$('[rel=tipsy_north]').tipsy({title: 'tipsy', gravity: 'n', fade:true});
  	$('[rel=tipsy_top_banner]').tipsy({title: 'tipsy', gravity: 'n', fade:true, offset: 4});
  	$('#goto [title]').tipsy({trigger: 'focus', gravity: 's', offset:7, fade:true});
  });
</script>

<!-- SOME JS THAT IS MAGICAL -->
<script type="text/javascript">
	function typeImg(str) {
		if (str=="") {
  			return;
  		} 
		if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
  			xmlhttp=new XMLHttpRequest();
  		}
		else { // code for IE6, IE5
  			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  		}
		xmlhttp.onreadystatechange=function() {
  		if (xmlhttp.readyState==4 && xmlhttp.status==200) {
    		document.getElementById("type_select").innerHTML=xmlhttp.responseText;
    		
    		//binding js to dynamically loaded content
    		//show_error();
    	}
  	}
	
	xmlhttp.open("GET","http://iwastesomuchtime.com/do.php?typeImg",true);
	xmlhttp.send();
	
	}
	function typeVid(str) {
		if (str=="") {
  			return;
  		} 
		if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
  			xmlhttp=new XMLHttpRequest();
  		}
		else { // code for IE6, IE5
  			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  		}
		xmlhttp.onreadystatechange=function() {
  		if (xmlhttp.readyState==4 && xmlhttp.status==200) {
    		document.getElementById("type_select").innerHTML=xmlhttp.responseText;
    		
    		//binding js to dynamically loaded content
    		//show_error();
    	}
  	}
	
	xmlhttp.open("GET","http://iwastesomuchtime.com/do.php?typeVid",true);
	xmlhttp.send();
	
	}
	function typeImgl(str) {
		if (str=="") {
  			return;
  		} 
		if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
  			xmlhttp=new XMLHttpRequest();
  		}
		else { // code for IE6, IE5
  			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  		}
		xmlhttp.onreadystatechange=function() {
  		if (xmlhttp.readyState==4 && xmlhttp.status==200) {
    		document.getElementById("type_select").innerHTML=xmlhttp.responseText;
    		
    		//binding js to dynamically loaded content
    		//show_error();
    	}
  	}
	
	xmlhttp.open("GET","http://iwastesomuchtime.com/do.php?typeImgl",true);
	xmlhttp.send();
	
	}
	function typeReset(str) {
		if (str=="") {
  			return;
  		} 
		if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
  			xmlhttp=new XMLHttpRequest();
  		}
		else { // code for IE6, IE5
  			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  		}
		xmlhttp.onreadystatechange=function() {
  		if (xmlhttp.readyState==4 && xmlhttp.status==200) {
    		document.getElementById("type_select").innerHTML=xmlhttp.responseText;
    		
    		//binding js to dynamically loaded content
    		//show_error();
    	}
  	}
	
	xmlhttp.open("GET","http://iwastesomuchtime.com/do.php?reset",true);
	xmlhttp.send();
	
	}

</script>

<!-- FB LIKE/SEND JS -->
<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) {return;}
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=170669086347267";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>

<!-- DISQUS -->
<script type="text/javascript">
    var disqus_shortname = 'iwsmt';

    (function () {
        var s = document.createElement('script'); s.async = true;
        s.type = 'text/javascript';
        s.src = 'http://' + disqus_shortname + '.disqus.com/count.js';
        (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
    }());
</script>		
</body>
</html>
