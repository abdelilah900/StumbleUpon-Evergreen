            <script type="text/javascript">

                var $bm_recent_videos_ix = 0;
                var $bm_recent_videos_total = 5;

                function bm_recent_videos_scroll($d) {
                    $bm_recent_videos_ix += $d;
                    if ($bm_recent_videos_ix < 0) $bm_recent_videos_ix = $bm_recent_videos_total - 1;
                    if ($bm_recent_videos_ix >= $bm_recent_videos_total) $bm_recent_videos_ix = 0;
                    var $widget = jQuery("#bm-sidebar-recent-videos");
                    $widget.find(".video, .title").hide();
                    $widget.find(".video-" + $bm_recent_videos_ix + ", .title-" + $bm_recent_videos_ix).show();
                }

                jQuery(document).ready(function() {
                    bm_recent_videos_scroll(0);
                });

            </script>

