            <script type="text/javascript">

                var $bm_popular_blog = "1";
                var $bm_popular_time = "week";

                function bm_on_popular_blog($blog) {
                    $bm_popular_blog = $blog;
                    $bm_popular_time = "week";
                    bm_switch_populars();
                }
                function bm_on_popular_time($time) {
                    $bm_popular_time = $time;
                    bm_switch_populars();
                }
                function bm_switch_populars() {
                    jQuery("#bm-sidebar-populars-tabs-blogs .active,#bm-sidebar-populars-tabs-times .active,#bm-sidebar-populars-container .active").removeClass("active");
                    jQuery("#bm-sidebar-populars-tabs-blogs .tab-blog-" + $bm_popular_blog + ",#bm-sidebar-populars-tabs-times .tab-time-" + $bm_popular_time + ",#bm-sidebar-populars-" + $bm_popular_blog + "-"+$bm_popular_time).addClass("active");
                }
            </script>

