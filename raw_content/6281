function resize_performiframe()
{
var performiframe = $('perform_video');
if (!performiframe) return 0;
var iframebody_arr = performiframe.contentDocument.getElementsByTagName("body");
if (!iframebody_arr || !iframebody_arr[0]) return 0;
var iframebody = iframebody_arr[0];
var dim = Element.getDimensions(iframebody);
if (dim.height < 200 || dim.height > 700) return;
performiframe.height = dim.height;
if (dim.height > 600) return 1;
return 0;
}
tryRunAsync(function() { return resize_performiframe(); }, 20, 500);
