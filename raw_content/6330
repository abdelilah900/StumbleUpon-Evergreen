					<script type="text/javascript">


					function setCookie(c_name,value,exdays)
					{
					var exdate=new Date();
					exdate.setDate(exdate.getDate() + exdays);
					var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
					document.cookie=c_name + "=" + c_value;
					}

					 function getCookie(c_name)
					 {
						 var i,x,y,ARRcookies=document.cookie.split(";");
						 for (i=0;i<ARRcookies.length;i++)
						 {
						   x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
						   y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
						   x=x.replace(/^\s+|\s+$/g,"");
						   if (x==c_name)
						     {
						     return unescape(y);
						     }
						   }
					 }

					 jQuery(document).ready(function($) { 
					 var username=getCookie("animation");
					   if (username!=null && username!="")
					   {

						    $('#lightbulb').css("margin-top","-56px");
						    $('#twitterBell').css("top","0px");
						    $('#pinterestBell').css("top","0px");
						    $('#facebookBell').css("top","0px");
						    $('#siteBell').css("margin-top","-56px");

						   
					   }
					 else
					   {
					     setCookie("animation","1");

							var initialDelay = 2000;			
								
								$('#lightbulb').delay(initialDelay).animate({ "marginTop": -56 }, {duration: 3000, easing: 'easeOutBounce'});
								$('#twitterBell').delay(initialDelay+1000).animate({ "top": 0 }, {duration: 3000, easing: 'easeOutBounce'});
								$('#pinterestBell').delay(initialDelay+3000).animate({ "top": 0 }, {duration: 2500, easing: 'easeOutBounce'});
								$('#facebookBell').delay(initialDelay+2000).animate({ "top": 0 }, {duration: 3200, easing: 'easeOutBounce'});
								$('#siteBell').delay(initialDelay+4000).animate({ "marginTop": -56 }, {duration: 2000, easing: 'easeOutBounce'});

						

					     
					   }
					 
					   });


					</script>
					
