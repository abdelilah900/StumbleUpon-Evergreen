        $(document).ready(function() {
           var voteuid = new Date().getTime();
           $.get('http://www.iol.co.za:80/lifestyle/food-drink/recipes/7.8239?ot=inmsa.AjaxPageLayout.ot&r=http://www.iol.co.za:80/lifestyle/food-drink/recipes/the-cupcake-is-king-1.1267781&view=right&voteuid='+voteuid, loadPoll7_8239HTML);
        });
        function loadPoll7_8239HTML(pollHTML) {            $('div#poll7_8239').html(pollHTML);
        }
    </script>

