<script type="text/javascript">
	window.addEvent("domready", function() {
	$('watch-related-videos-list').getElements('span.video-title').each(function(el) { ellipsisizeVideosTitles(el);  });
	
	});

	function ellipsisizeVideosTitles(videoSpan) {

		if (videoSpan.scrollHeight > videoSpan.clientHeight) {
			var offsetHeight = videoSpan.clientHeight;
			var title = videoSpan.get("text");
			var tilteParts = title.split(/\s+/g);
			var lastGoodIndex = tilteParts.length;
			var currentParts = [];

			while (videoSpan.scrollHeight > offsetHeight && lastGoodIndex > 0) {
				currentParts = tilteParts.slice(0, --lastGoodIndex);
				videoSpan.set("text", currentParts.join(" ") + "...");
			}
		}
	};

	
</script>
			
