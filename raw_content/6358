		<script language="javascript" type="text/javascript" src="/inc/jquery.validate/1.8.1/jquery.validate.min.js"></script>
		<script type="text/javascript" language="javascript">
		jQuery.noConflict();
		jQuery(document).ready(function(){
	
			jQuery('.ghost-text').each(function(){
				var d = jQuery(this).val();
		 
				jQuery(this).focus(function(){
					if (jQuery(this).val() == d){
						jQuery(this).val('').removeClass('ghost-text');
					}
				});
		 
				jQuery(this).blur(function(){
					if (jQuery(this).val() == ''){
						jQuery(this).val(d).addClass('ghost-text');
					}
				});
			});

			// some validation to the email field + form submit handler
			jQuery.validator.setDefaults({
				submitHandler: function(form) { 
					form.submit();
				} 
				,errorPlacement: function(error, element) {
		   	        error.insertAfter(element); 
			   	    }
			});
	
			jQuery("#newsletter_signup_form").validate({
		   		rules: {
		   	 		signup_email: {
		   	   			required: true,
		   	   			email: true
		   	 		}
   				}
				,messages: {
					signup_email: "<div class='error_msg'>Please provide a valid e-mail address</div>"
				}
			});
		});
		</script>

