				<div id="video-hero-player"></div>
				<script type="text/javascript">
					nfl.use('nfl-video','event-custom', function (Y) {
						var Video = Y.NFL.Video, vp, swf;
						
						// create a video player instance
						vp = new Video.Player({
							size: 'detail',
							companionSize: "300x250",
							dartURL: "http://ad.doubleclick.net/pfadx/nfl.videogallery/videohome;s1=videohome;slot=;url=videohome;nfl=ad;!category=;kw=;tile=10;test=;sz=640x360;ord=" + (window.adRandom || nfl.constants.AD_RANDOM) + "?",
							adSetting: 17 // pre-roll
						});
						vp.render('#video-hero-player');
						swf = vp.get('player');
						
						// force video player events to be global
						Y.Object.each(Video.PlayerEvent, function (type, constName) {
							swf.getEvent(type).broadcast = 2;
						});
						
						Y.local.player = vp;
					});
				</script>
			</div>
