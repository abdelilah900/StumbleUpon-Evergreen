<script type="text/javascript">
var randarray = new Array();
var l=0;
var flag;
var numofpost=1;
var randomPostIndex = Math.floor(Math.random()*495);
console.log("randomPostIndex " + randomPostIndex);

function randomposts(json){

for(i=0;i < numofpost;)   {
    flag=0;
     randarray.length=numofpost;
      
     l =1;

     for(j in randarray){if(l==randarray[j]){ flag=1;}}
      if(flag==0&&l!=0){randarray[i++]=l;}}
       for(n in randarray){ var p=randarray[n];var entry=json.feed.entry[p-1];
            for(k=0; k < entry.link.length; k++){
              if (entry.link[k].rel=='alternate'){var item = "" + "<a           class='randomPostLink' href=" + entry.link[k].href + ">" + 'surprise me! ' +   "</a> ";
document.write(item);}}
}}
</script>
<script>
randomUrl = "http://kblog.lunchboxbunch.com/feeds/posts/default?alt=json-in-script&start-index=" + randomPostIndex + "&max-results=2&callback=randomposts";
console.log(randomUrl);

document.write('<' + 'script type="text/javascript" src="' + randomUrl + '"></' + 'script>'); 
</script>
