	{
		mh_flyout_callState3=0;
		var gmnuObj = this;
		jQuery(this).attr('OFF','1');
		jQuery(this).removeAttr('ON');
		setTimeout(function() {
			var mnuObj = gmnuObj;
			if((mh_flyout_callState3==0) && (jQuery(mnuObj).attr('OFF') == '1'))
			{
				jQuery('#mh_header_flyout_ipad').css('display','none');
				jQuery(mnuObj).removeAttr('OFF');
			}
			},
			500
		);
	}
	
