	<script type="text/javascript">
	// LOCAL APPID =   163044127081320
	// STAGING APPID =    201470423196690
	// PROD APPID =    129993933737955
  (function() {
     var e = document.createElement('script');
     e.type = 'text/javascript';
     e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
     e.async = true;
     document.getElementById('fb-root').appendChild(e);
   } ());
   window.fbAsyncInit = function () {
   FB.init({
     appId: '129993933737955',
     status: true, // check login status
     cookie: true, // enable cookies to allow the server to access the session
     xfbml: true  // parse XFBML
   });
   FB.Event.subscribe('auth.sessionChange', function (response) {
   if (response.session) {
     //A user has logged in, and a new cookie has been saved
     //alert('response 3 ' +response.session.uid);
     setTimeout(function() {update_user_box(response.session.uid)},1500);
    }
   });
   FB.getLoginStatus(function (response) {
   if (response.session) {
     // The user is already logged into Facebook
     //alert('response 2 ' +response.session.uid);
     setTimeout(function() {update_user_box(response.session.uid)},1500);
     //FB.XFBML.parse();
   } else {
     FB.Event.subscribe('auth.login', function (response) {
     // Listen if the user logs into Facebook through this application
     //alert('response 1 ' +response.session.uid);
     setTimeout(function() {update_user_box(response.session.uid)},1500);
     //FB.XFBML.parse();
   });
   }
   });
   }
  </script>

  <script type="text/javascript">
  function update_user_box(x) {
    if (document.getElementById("fb_user_box")) {
    var fb_user_box = document.getElementById("fb_user_box");
    fb_user_box.innerHTML =
       "<span style='position:relative;left:15px;top:10px;'>"
       + "Welcome, <fb:name uid='" +x+ "' useyou='false'></fb:name>."
       + "You are signed in with your Facebook account."
       + "</span>";
       if (document.getElementById("fb_comments")){
     document.getElementById("fb_comments").innerHTML =
         "<fb:comments numposts='10' width='630' publish_feed='true'></fb:comments>";
       }
       if (document.getElementById("overview-home-all")){ //we are on the homepage
     document.getElementById("overview-home-all").style.display="none";
       }
       if (document.getElementById("recipe_comments")){ //we are on a recipe page
     document.getElementById("recipe_comments").style.display="none";
       }
       if (document.getElementById("normal-login")) {
     document.getElementById("normal-login").style.display="none";
       }
       if(document.getElementById("or-span")){
     document.getElementById("or-span").style.display="none";
       }
       FB.XFBML.parse();
    }
  }
  </script>
