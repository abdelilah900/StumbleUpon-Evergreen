                        function addEvent(obj, type, fn) {
                            var obj = ( obj.constructor === String ) ? document.getElementById(obj) : obj;
                            if (obj.attachEvent) {
                                obj[ 'e' + type + fn ] = fn;
                                obj[ type + fn ] = function() {
                                    obj[ 'e' + type + fn ](window.event)
                                };
                                obj.attachEvent('on' + type, obj[ type + fn ]);
                            } else obj.addEventListener(type, fn, false);
                        }

                        function updateKeywordsfromSegs() {
                            if (typeof rsi_segs == "object" && rsi_segs.length > 0) {
                                var iskw = "";
                                for (var segs in rsi_segs) {
                                    var segId = rsi_segs[segs];
                                    iskw += segId + ",";
                                }
                                if (iskw.length > 0)
                                    InSkinParams['srv_Keywords'] = iskw + InSkinParams['srv_Keywords'];
                            }
                        }


                        if ((navigator.platform.indexOf("Mac") == -1) && (navigator.platform.indexOf("Linux") == -1)) {
