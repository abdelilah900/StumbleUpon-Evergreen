	pluckAppProxy.loadPlugin("pluck/pluck.js", function(){
		(function($, jQuery) {
			if (typeof(pluckAppProxy.pluck_util_share_dialog_display) === 'undefined') {
				pluckAppProxy.pluck_util_share_dialog_display = function(dialog, obj) {
					var offsets = "";
					if (obj.attr("dialogdisplay") == "baseline") {
						offsets = { top: 9, left: (obj.width() + 10) * -1 };
					} else {
						offsets = { top: dialog.outerHeight(), left: Math.round((dialog.width() - obj.width()) / 2) };
					}
					if ($.pluckDisplayCorners() && !dialog.hasClass("pluckShown")) {
						dialog.addClass("pluckShown");
						var allCorners = {tl: { radius: 4 }, tr: { radius: 4 }, bl: { radius: 4 }, br: { radius: 4 }, antiAlias: true, autoPad: false, validTags: ["div"] };
						$("#" + dialog.attr("id") + " > div.pluck-dialog-wrapper").pluckCorner(allCorners);
					}
					pluckAppProxy.displayDialog(dialog, obj, offsets, function() {
						setTimeout(function() { $(".pluck-dialog-middle .pluck-share-col1 a:visible:first", dialog).focus(); }, 200);
						if (obj.attr("includeclipboard") != "true") return;
						var loadClip = function() {
							var clip = new ZeroClipboard.Client();
							pluckAppProxy.pluck_clipboard_register(dialog.attr("id"), clip);
							clip.setHandCursor(true);
							clip.glue($('.pluck-share-link-clipboard', dialog).parent().get(0));
							clip.addEventListener('onComplete', function() { pluckAppProxy.fadeOut(dialog); clip.hide(); dialog.attr("permalink", "");});
							clip.setText(dialog.attr("permalink"));
							clip.show();
						};
						if (!pluckAppProxy.pluck_find_clipboard(dialog.attr("id"))) {
							if (typeof(ZeroClipboard) == 'undefined') {
								pluckAppProxy.loadLibrary(pluckAppProxy.baseUrl + "Content/ua/scripts/ZeroClipboard.js", function(){
									ZeroClipboard.setMoviePath(pluckAppProxy.baseUrl + "/Content/ua/swf/ZeroClipboard.swf");
									loadClip();
								});
							} else {
								loadClip();
							}
						} else {
							var clip = pluckAppProxy.pluck_find_clipboard(dialog.attr("id"));
							if (clip) {
								clip.show();
								clip.setText(dialog.attr("permalink"));
							}
						}
					});
				};
			}
		})(dmJQuery, dmJQuery);
		pluckAppProxy.pluck_util_share_dialog('#pluck_util_share_dialog_933112', '#pluck_comments_933098', { });
	}, 'MetroFromScratch');
