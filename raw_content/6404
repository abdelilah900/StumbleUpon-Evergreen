function tiiQuigoSetEnabled(b) {
	_tiiQuigoEnabled = b;
}

function tiiQuigoIsEnabled() {
	if (typeof(_tiiQuigoEnabled) == "boolean") {
		return _tiiQuigoEnabled;
	}
	return true;
}

function tiiQuigoWriteAd(pid, placementId, zw, zh, ps) {
	if (tiiQuigoIsEnabled()) {
		qas_writeAd(placementId, pid, ps, zw, zh, 'ads.adsonar.com');
	}
}


		
		
	
		function mrFormatAdTagValue(ad_tag_value) {
			var ret_val = ad_tag_value;
			var character_set = ["'","&","/","-","!"];
			for (var i in character_set) {
				ret_val = ret_val.split(character_set[i]);
				ret_val = ret_val.join("");
			} 
			return ret_val;
		}
		function mrSetAdParamAndSnoothQueryString(key, value) {
			var splitValues = value.split(',');
			adFactory.setParam(key, splitValues);
				__sn_qs += "|" + key + ":" + value;
		}
		
		var ka_pub = mrFormatAdTagValue("Oxmoor House");
		var ka_cookbook = mrFormatAdTagValue("");
		var ka_course = mrFormatAdTagValue("Desserts,Cakes/Frostings");
		var ka_cuisine = mrFormatAdTagValue("");
		var ka_mainIng = mrFormatAdTagValue("");
		var ka_excludeIng = mrFormatAdTagValue("");
		var ka_diet = mrFormatAdTagValue("");
		var ka_prep = mrFormatAdTagValue("");
		var ka_convenience = mrFormatAdTagValue("");
		var ka_season = mrFormatAdTagValue("");
		var ka_costperserving = "";
		var ka_cooktime = "";
		
		var __sn_qs = '';
		adFactory.setArticleId(ka_recipeID);
		mrSetAdParamAndSnoothQueryString('pub',ka_pub);
		mrSetAdParamAndSnoothQueryString('cbook',ka_cookbook);
		mrSetAdParamAndSnoothQueryString('ingr',ka_mainIng);
		mrSetAdParamAndSnoothQueryString('cours',ka_course);
		mrSetAdParamAndSnoothQueryString('diet',ka_diet);
		mrSetAdParamAndSnoothQueryString('occ',ka_season);
		mrSetAdParamAndSnoothQueryString('cuis',ka_cuisine);	
		mrSetAdParamAndSnoothQueryString('prep',ka_prep);	
		mrSetAdParamAndSnoothQueryString('conv',ka_convenience);

		var adsqs = getQueryStringValue('adsqs');
		var is_franchise_set = 0;
		if (adsqs != null && adsqs != '') {
			var ad_key_value_list = adsqs.split('|');
			for (var ad_key_value in ad_key_value_list) {
				var k_v = ad_key_value_list[ad_key_value].split(':');
				k_v[1] = mrFormatAdTagValue(k_v[1]);
				mrSetAdParamAndSnoothQueryString(k_v[0], k_v[1]);
				if (k_v[0] == 'frnch') {
					is_franchise_set = 1;
				}
			}
		}
		if (is_franchise_set == 0) {
			var recipe_franchise = mrFormatAdTagValue("unknown");
			mrSetAdParamAndSnoothQueryString('frnch',recipe_franchise);
		}
		var top5 = getQueryStringValue('top5');
		if (top5 == 'yes') {
			mrSetAdParamAndSnoothQueryString('top5','yes')	
		}
	
		var __sn_rid = ka_recipeID;
		var __sn_rname = ka_recipeName;
		var __sn_pid = 1;
		var __sn_iurl = 'http://www.myrecipes.com/wines/wine-finder/wine-details/?wine_id=';
		var __sn_turl = 'http://edge-tyrant.snooth.com';
		var __sn_live = '1';
		var __sn_version = 2;
		if (__sn_qs != '') {
			__sn_qs = '&qs=' + __sn_qs.substring(1);  
		}
	

	if (typeof(aoluser) == 'undefined') { 
		var aoluser = 'no';
	} 



	function showdiv(name) {
		if (document.getElementById) { // DOM3 = IE5, NS6
			document.getElementById(name).style.display = 'block';
		} else {
			if (document.layers) { // Netscape 4
			document.layers[name].visibility = 'block';
			} else { // IE 4
			document.all[name].style.visibility = 'block';
			}
		}
	} 
	
	function showAOL() {
		if (aoluser == 'yes') {
			showdiv('didyk');
		} 
	}
	
	

