<script>

  // set up DealSaver

  if ( typeof( mi.DealSaver ) !== 'undefined' ) {
    if ( typeof( mi.dealSaver ) == 'undefined' ) {
      mi.dealSaver = new mi.DealSaver();
      jQuery(document).ready( function() { mi.dealSaver.executeDs(); } );
    }
  }

  

  // set up Find n Save

  if ( typeof( mi.FindnSave ) !== 'undefined' ) {
    mi.findnSave = new mi.FindnSave();
  }

  if ( mi.findnSave.isFirstWidget() ) {
    //jQuery(document).ready( function() { mi.findnSave.executeFns(); } );

    mi.findnSave.setConf( "fns_domain", "bradenton.com" );
    mi.findnSave.setConf( "enabled", true );
  }

  jQuery(document).ready( function() {
    var domain = "http://" + mi.findnSave.getConf( "fns_subdomain" )
      + mi.findnSave.getConf( "fns_domain" );
    console.info( "FindnSave domain is " + domain );
    jQuery("div.dsSearch form").attr( 'action', domain + '/Search' + '#wgt=fnscombo' );
    var randNum = Math.ceil( Math.random() * 2 );
    //randNum = 1;//TAKE OUT!!!
    if ( randNum == 1 ) {
      jQuery.getJSON( domain + "/api/aacoupons.json?callback=?",
        function(json) { AACoupons(json) } );
    } else {
      jQuery.getJSON( domain + "/api/printadvertiser.json?n=2&callback=?",
        function(json) { printadvertiser(json) } );
    }
  } );

  function printadvertiser(data) {
    var paAdvertiser = data.result.advertisers.advertiser;
    var num_ads = paAdvertiser.length;
    if(  num_ads == 0 ) return;

    jQuery(".dsROP").css( 'display', 'block' );

    jQuery("h3.dsROP_title span.dsROP_title_text").text( 'Featured Advertisers' );
    jQuery("h3.dsROP_title span.dsROP_viewall_link a").attr( 'href',
      '/Local-Ads' + '#wgt=fnscombo' );
    mi.findnSave.insertFnSDomain();

    var content = jQuery("div.dsROP_content div.fnsPrintAd");

    //for (var i = 0; i < num_ads; i++) {
    //  content.eq(i).find("a").attr( 'href', paAdvertiser[i].adUrl + '#wgt=fnscombo' );
    //  content.eq(i).find("h5.paBusiness a").text( paAdvertiser[i].name );
    //  content.eq(i).find("a img").attr( 'src', paAdvertiser[i].adimageUrl );
    //  content.eq(i).css( 'display', 'inline' );
    //}
    

    
    jQuery("div.dsROP_content").each(function(index) {
      var content = $(this).find("div.fnsPrintAd");
      for (var i = 0; i < num_ads; i++) {
        content.eq(i).find("a").attr( 'href', paAdvertiser[i].adUrl + '#wgt=fnscombo' );
        content.eq(i).find("h5.paBusiness a").text( paAdvertiser[i].name );
        content.eq(i).find("a img").attr( 'src', paAdvertiser[i].adimageUrl );
        content.eq(i).css( 'display', 'inline' );
      }
    });
    
    
    
    
  }
  
  

  function AACoupons(data) {
    var num_coups = data.result.coupon.length;
    var startPos = 0;
    if (num_coups > 3) {
      startPos = Math.floor(Math.random() * (num_coups - 2));
    }
    num_coups = Math.min( num_coups, 3 );
    if( num_coups == 0 ) return;

    jQuery(".dsROP").css( 'display', 'block' );

    jQuery("h3.dsROP_title span.dsROP_title_text").text( 'Coupons' );
    jQuery("h3.dsROP_title span.dsROP_viewall_link a").attr( 'href',
      '/Coupons' + '#wgt=fnscombo' );
    mi.findnSave.insertFnSDomain();

    jQuery("ul.analog_coupons").css( 'display', 'block' );

    var content = jQuery("ul.analog_coupons li");
    var subcontent;

    //var i = 0;
    //for (var index = startPos; index < startPos + num_coups; index++) {
    //  var cc = data.result.coupon[index];
    //  subcontent = content.eq(i).find( "div.ds_image" );
    //  subcontent.find("a").attr( 'href', cc.couponUrl + '#wgt=fnscombo' );
    //  subcontent.find("img").attr( 'src', cc.couponPhotoUrl );
    //  subcontent = content.eq(i).find( "div.ds_text" );
    //  subcontent.find("a").attr( 'href', cc.couponUrl + '#wgt=fnscombo' );
    //  subcontent.find("h5 a").text( cc.advertiserName );
    //  subcontent.find("p").text( cc.title );
    //  content.eq(i).css( 'display', 'block' );
    //  i++;
    //}
    
    
    
    jQuery("ul.analog_coupons").each(function(index) {
      var content = $(this).find("li");
      var subcontent;
  
      var i = 0;
      for (var index = startPos; index < startPos + num_coups; index++) {
        var cc = data.result.coupon[index];
        subcontent = content.eq(i).find( "div.ds_image" );
        subcontent.find("a").attr( 'href', cc.couponUrl + '#wgt=fnscombo' );
        subcontent.find("img").attr( 'src', cc.couponPhotoUrl );
        subcontent = content.eq(i).find( "div.ds_text" );
        subcontent.find("a").attr( 'href', cc.couponUrl + '#wgt=fnscombo' );
        subcontent.find("h5 a").text( cc.advertiserName );
        subcontent.find("p").text( cc.title );
        content.eq(i).css( 'display', 'block' );
        i++;
      }
    });
    
    
    
    
  };

</script>

<!-- *** Find n Save Combo Traffic Driver *** -->

