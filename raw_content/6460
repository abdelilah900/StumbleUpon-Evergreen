//<![CDATA[
    function facebook_connect_check_permissions_helper() {
        window.SUGAR = window.SUGAR || {};

        if (SUGAR.FB_CONNECT) {
            return;
        }

        SUGAR.FB_CONNECT = {
            loggedInStatus: false
        };
        FB.getLoginStatus(function (response) {
            if (response.session) {
              if (typeof response.perms === 'string') {
                response.perms = eval('(' + response.perms + ')');
                var perms = response.perms.extended;
                for (var i = 0; i < perms.length; i++) {
                    if (perms[i].indexOf('publish_stream') >= 0) {
                        SUGAR.FB_CONNECT.loggedInStatus = true;
                        return;
                    }
                }
              }
            }
        }, true);
    };

    function facebook_connect_check_permissions() {
      if (document.getElementById('edit-facebook_post').checked === false) {
        // They don't want to post to Facebook, so just submit the form.
        return true;
      }

      if (SUGAR.FB_CONNECT.loggedInStatus === true) {
        document.getElementById('comment_form').submit();
        return true;
      }
      else {
        FB.login(function(response) {
          if (response.session) {
            if (response.perms) {
              // user is logged in and granted some permissions.
              document.getElementById('comment_form').submit();
            }
            else {
              // user is logged in, but did not grant any permissions
              alert('You must grant permission to publish to your Facebook feed.');
            }
          }
          else {
            // user is not logged in
            alert('You must login to publish to your Facebook feed.');
          }
        }, {perms:'publish_stream'});
      }
      return false;
    }
    //]]>
    </script>
