    document.location = "http://m.refinery29.com" + location.pathname.replace(/^(.*)\/slideshow/, "$1");
  } else if (R29.url.param.getVar('mobile') == 'false') {
    R29.cookie.set('force_web', 1);  
  }
  
  //Check if we got us a mobile device    
  if((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPod/i)) || (navigator.userAgent.match(/android/gi))) { 
    
    //See if we're overriding the sensed web version
    if (!R29.cookie.get('force_web') || R29.cookie.get('force_web') == "0") {
      
      //We know we're going to redirect, but we have to load the GA code so the referrers aren't messed up.
      _mobileRedirect = true;
