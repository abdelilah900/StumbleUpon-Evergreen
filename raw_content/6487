        $('form#header-search div.search-icon, form#footer-search div.search-icon').live('click', function(){
            $(this).parent().parent().submit();
        }).live('mouseover mouseout', function(event){
            if (event.type == 'mouseover') {
                $(this).addClass('on');
            } else {
                $(this).removeClass('on');
            }
        });
    </script>
