/* ONLY USE THIS WORKAROUND FOR THE NBCNEWS.COM COVER */

//add the Workbench ID of your slideshow
//next to the gssSlideshowID variable below
var gssSlideshowID = 52063731; // <--CHANGE THIS VALUE
</script>

<!-- add jquery script call if page loads in wb preview -->
<script type = "text/javascript"charset = "utf-8" >
 if ( window.location.host.indexOf("wbpreview") != -1 ) document.write('<s' + 'cript src="http://www.msnbc.msn.com/id/23149822/" type="text/javascript" charset="utf-8"></s' + 'cript>');
</script>
<script type="text/javascript" charset="utf-8">
(function(){
 var gssAIMSSize, gssImageWidth, gssImageHeight, gssSlot;
 var gssBeacon = "gridSlideshowBeacon";

 function init(){
  gssSlot = getSlot();
  renderFrame();
 }

 function renderFrame(){
  populateFromSlot();

  if( !gssImageWidth || !gssImageHeight ){
   setTimeout(renderFrame,50);
   return;
  }

  var frameUrl = getFrameUrl();
  var iframe = $('<iframe/>')
   .attr({
    src: frameUrl,
    scrolling: "no",
    frameborder: 0
   })
   .width(gssImageWidth)
   .height(gssImageHeight+20);

  gssSlot.empty().append(iframe);
 }

 function populateFromSlot(){
  var slotIMG = $( gssSlot ).find( "img:first" );
  var slotIMGSrc = slotIMG.attr( "src" ); //src for the slot image
  var slotIMGSrcSplit = slotIMGSrc.split( "." );

  gssAIMSSize = slotIMGSrcSplit[ ( slotIMGSrcSplit.length - 2 ) ]; //AIMS sizing based on AIMS size of original slot image
  gssImageWidth = slotIMG.attr( "clientWidth" ); //width of each slideshow image, in pixels
  gssImageHeight = slotIMG.attr( "clientHeight" ); //height of each slideshow image, in pixels
 }

 function getFrameUrl(){
  return 'http://www.msnbc.msn.com/id/48258290?' + $.param({
   aims:gssAIMSSize,
   w:gssImageWidth,
   h:gssImageHeight,
   wbid:gssSlideshowID
  });
 }

 function getSlot(){
  var gssSlot = $( '#' + gssBeacon ).parent().children( 'div.force-inline' ).children( '.img:first' );

  if ( $( gssSlot ).length < 1 ){
   gssSlot = $( '#' + gssBeacon ).parent().children( 'div.story' ).children( '.img:first' );
  }

  return gssSlot;
 }

 $(function(){
  init();
 });
})();
</script>
