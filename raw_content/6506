<script type="text/javascript">
    //Like button
    window.fbAsyncInit = function () {
        FB.Event.subscribe('edge.create', function (href, widget) {
            /* Trigger custom event on the FacebookWrapper object */
            $(AR.FacebookWrapper).trigger("FacebookLikeButton_onClick", href);
            AR.ClientService.LogFacebookLike(AR.Ads.TypeSpecificID, AR.ServerSideData.Data.ItemType, AR.Ads.VisitorID, function (data) { });
        });
        FB.Event.subscribe('edge.remove', function (href, widget) {
            /* Trigger custom event on the FacebookWrapper object */
            $(AR.FacebookWrapper).trigger("FacebookUnLikeButton_onClick", href);
        });
    };
    $(function () {
        if (document.getElementById('facebook-jssdk')) { return; }
        var like = document.createElement('script'); like.type = 'text/javascript'; like.async = true;
        like.src = 'https://connect.facebook.net/en_US/all.js#xfbml=1'; like.id = 'facebook-jssdk';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(like, s);
    });
    //+1 button
    $(function () {
        if (document.getElementById('google-plusone-sdk')) { return; }
        var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
        po.src = 'https://apis.google.com/js/plusone.js'; po.id = 'google-plusone-sdk';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
    });
    //Pin It
    $(function () {
        if (document.getElementById('pin-it-sdk')) { return; }
        var pinit = document.createElement('script'); pinit.type = 'text/javascript'; pinit.async = true;
        pinit.src = 'https://assets.pinterest.com/js/pinit.js'; pinit.id = 'pin-it-sdk';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(pinit, s);
    });
    $(function () {
        if (document.getElementById('tweet-sdk')) { return; }
        var tweet = document.createElement('script'); tweet.type = 'text/javascript'; tweet.async = true;
        tweet.src = 'https://platform.twitter.com/widgets.js'; tweet.id = 'tweet-sdk';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(tweet, s);
    });
</script>


