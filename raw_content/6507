<script type="text/javascript" charset="utf-8">

	jQuery(function(){
		
		jQuery('.gallery_image a').click(function() { 
			var href = jQuery(this).attr('href');
			var matches = /image_id=([0-9]*)/.exec(href)
			if(matches[1]) {
				// If we have a correctly formatted image URL, generate the new image URL
				var new_id = matches[1];
				var big_size = "rect540";
				if(jQuery(".gallery_image img:first").attr('src').match(/rect480/)) {
					big_size="rect480";
				}
				var new_src = jQuery(this).children('img')[0].src.replace(/square72/,big_size);
				jQuery(".gallery_image img:first").attr('src', new_src);

				// Look for a caption
				var title=jQuery(this).children('img').attr('title');
				if(title.length > 0) {
					jQuery('#image_caption').html(title).show();
				}
				else {
					jQuery('#image_caption').html("").hide();
				}

				return false;
			}
			return true;
		});
		
	}); // DOM-ready
	
</script>

