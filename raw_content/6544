<!-- algemeen -->
<div class="section algemeen">

    
  <div class="leadarticle " data-vr-zone="AlgemeenHoofd">
          <a title="Negentien EHEC- besmettingen in Belgi&euml; door vlees" onclick="NU.AT.internalLink(this, event);" class="imagelink" href="/buitenland/2835557/negentien-ehec--besmettingen-in-belgie-vlees.html" data-vr-contentbox="AlgemeenFoto">
                  <img  xtclib="position1_image_article_1" src="http://media.nu.nl/m/m1gz6qta4brm.jpg" />
              </a>

