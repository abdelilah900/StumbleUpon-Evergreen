	<script type="text/javascript">
		var connectUserId = 0;
		var serverUserId = 0;
		var voted = false;
		
		function voteUp() {
            trackEvent('Toolbar', 'Vote', 'b4bcF');
		    FB.Connect.requireSession();
		    FB.Facebook.get_sessionState().waitUntilReady(function() {
		    	// Enable the unvote button
		    	enableUnvoteButton();

                trackEvent('Toolbar', 'VoteRecord', 'b4bcF');

				// Send the vote to the server.
				$.ajax({
					type: "POST",
					url: "/getposts.php?m=vote&postid=b4bcF&blogid=583360",
					success: function (response) {
					},
					error: function (xhr) {}
				});
				return false;
			});
		}

		function unvote() {
            trackEvent('Toolbar', 'Unvote', 'b4bcF');
		    FB.Connect.requireSession();
		    FB.Facebook.get_sessionState().waitUntilReady(function() {
		    	// Enable the vote button
		    	enableVoteButton();

                trackEvent('Toolbar', 'VoteRecord', 'b4bcF');

				// Send the vote to the server.
				$.ajax({
					type: "POST",
					url: "/getposts.php?m=unvote&postid=b4bcF&blogid=583360",
					success: function (response) {
					},
					error: function (xhr) {}
				});
				return false;
			});
		}	
		
		function closeToolbar() {
	    	// Track closing toolbar 
            trackEvent('Toolbar', 'Close', 'b4bcF');
	    	window.location = "http://nutmegnanny.wordpress.com/2010/11/26/double-nut-maple-bars/";
		}

		function tweet() {
	    	// Track tweeting event
	    	trackEvent('Toolbar', 'Tweet', 'b4bcF');
	    	window.location = "http://twitter.com?status=Double-Nut%20Maple%20Bars%20http%3A%2F%2Fnetworkedblogs.com%2Fb4bcF";
		}
		
		function enableVoteButton() {
			$("#votebutton")
			.unbind()
			.click(function() { voteUp(); return false; })
			.removeClass("off");

			$("#votebutton span").text("Vote");
		}
		
		function enableUnvoteButton() {
			$("#votebutton")
			.addClass("off")
			.unbind()
			.click(function() { unvote(); return false; });
			
			$("#votebutton span").text("Voted");
		}
		
		function sharePost() {
            trackEvent('Toolbar', 'Share', 'b4bcF');
		    FB.Connect.requireSession();
		    FB.Facebook.get_sessionState().waitUntilReady(function() {
		    	// Track share event 
		    	trackEvent('Toolbar', 'ShareOpen', 'b4bcF');
			    
                FB.Connect.streamPublish("", postAttachment, postActionLinks, null, "", function(post_id, exception){}, false, null);
			});
		}
		
		function onConnected(userId) {
			connectUserId = userId;
			if (connectUserId == serverUserId) {
				if (voted == false) {
					enableVoteButton(); 
				}
				else {
					enableUnvoteButton();
				}
			}
			else {
                // Since the server didn't have the session, so we need to check if the user has voted or is an author here.
                if (voterIds.search(userId.toString()) > -1){
                    voted = true;
                    enableVoteButton();
                }
                else {
                    voted = false;
                    enableUnvoteButton();
                }
			}
			fbReady();
		}
		
		function onNotConnected(userId) {
			connectUserId = 0;
			enableVoteButton();  // clicking with trigger FB Connect to get user to authorize the app
			fbReady();
		}
		
		function fbReady()
		{
            var loginStatus;
            FB.Connect.get_status().waitUntilReady( function( status ) {
                switch ( status ) {
                    case FB.ConnectState.connected:
                        loginStatus = 'Connected'
                        break;

                    case FB.ConnectState.appNotAuthorized:
                        loginStatus = 'NotAuthorized'
                        break;

                    case FB.ConnectState.userNotLoggedIn:
                        loginStatus = 'NotLoggedIn'
                        break;
                }
            
                // Google Analytics tracking.
                try {
                    // Record whether the user is logged in or not (page-level custom variable).
                    pageTracker._setCustomVar(1, "Login Status", loginStatus, 3); 
                    pageTracker._trackPageview();

                    // TODO: Until we get custom segments activated, we're reporting user segments with an event.
                    trackEvent('Toolbar', 'Visit', loginStatus);
                } catch(err) {}

                            });
		}

		$(document).ready(function(){
			FB.init("f7f71c355e1dcca1ba0d472eef124184", "/xd_receiver.htm", 
				{"ifUserConnected":onConnected,
		          "ifUserNotConnected":onNotConnected});
		});
	</script>
	<script type="text/javascript">
	var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
	document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
	</script>
	<script type="text/javascript">
	try {
		var pageTracker = _gat._getTracker("UA-11094983-3");
	} catch(err) {}
	</script>
