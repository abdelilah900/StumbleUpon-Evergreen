	<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.3.2/jquery.min.js"></script>
	<script type="text/javascript">
        var postAttachment = {"name":"Double-Nut Maple Bars","href":"http:\/\/networkedblogs.com\/b4bcF","caption":"","description":"Happy Black Friday! Did everyone go out and shop? I went to the mall this morning and managed to score a front row, right by the door parking spot. Sadly I did not find anything to buy. Well that\u2019s...","comments_xid":"p583360_bab4cd3185203585a353af317c91544b_","media":[{"type":"image","src":"http:\/\/nutmegnanny.files.wordpress.com\/2010\/11\/img_5651-small.jpg?w=500&h=333","href":"http:\/\/networkedblogs.com\/b4bcF"}],"properties":{"link":{"text":"Full Article...","href":"http:\/\/networkedblogs.com\/b4bcF"}}};
        var postActionLinks = [{"text":"Share","href":"http:\/\/networkedblogs.com\/b4bcF?a=share"}];
        var voterIds = "";

		        
    function trackEvent(category, action, label, value) 
    {
    	var prefix = 'Experiment 2A: ';
        try {
            pageTracker._trackEvent(prefix + category, action, label, value);
        }
        catch (err) {}
    }
    </script>
