<script type="text/javascript" charset="utf-8">
	//sponge stats client widget
	GawkerClientside.pushWidget('spongestatsclient', jQuery('.gawkerwidget.spongestatsclient_area').spongestatsclient(sponge_client).data('spongestatsclient'));

	// gallery widget
	GawkerClientside.pushWidget('gallery', jQuery('.mainContent').GalleryWidget().data('GalleryWidget'));
	// navigation widget
	var gnw = GawkerClientside.pushWidget('navigation', jQuery('.mainContent').NavigationWidget().data('NavigationWidget'));
	// main content loader
	GawkerClientside.pushWidget('maincontent', jQuery('.mainContent').MainContentWidget({'interstitial_frequency' : 4 }).data('MainContentWidget'));
	// comment related widgets
	GawkerClientside.pushWidget('thread', jQuery('.threads_area').ThreadWidget( {} ).data('ThreadWidget'));
	GawkerClientside.pushWidget('threadadmin', jQuery('.gawkerwidget.threadadmin_area').ThreadAdminWidget({}).data('ThreadAdminWidget'));
