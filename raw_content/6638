(function() {

	$('.sect_galleries').each(function(){	if( this.handled ) return;
	
/*		var href;
		$(this).click(function(){
				window.open( href, 'popup', 'height=600,width=800' );
				return false;
				}); 
*/	
		// the switching content needs to know its alternative image sources
		$('.sectbody li',this).each(function(){
			var a = $('a.objlink',this);
			a[0].topImg = $('.topImg',this).attr('href');
			a[0].botImg = $('.botImg',this).attr('href');
			a.click(function(e){
				e.preventDefault();
				window.open( a.attr('href'), 'popup', 'height=642,width=800' );
				return false;
				});
			});

		var top = $('.sectbody li:first',this);
		var bot = $('.sectbody li:gt(0)',this);
	
		// mouseover content switching
		bot.hover(function(){	if( this.hovered ) return; this.hovered = true;
			
			var topA = $('a.objlink',top );
			var botA = $('a.objlink',this); 

			$('img',top).replaceWith('<img src="'+ topA[0].botImg+ '"/>');

			var    upA = botA.clone(true);        upA[0].topImg = botA[0].topImg;        upA[0].botImg = botA[0].botImg;
			this.downA = topA.clone(true); this.downA[0].topImg = topA[0].topImg; this.downA[0].botImg = topA[0].botImg;

			top.empty().append( upA );

			$('img',top).replaceWith('<img src="'+  upA[0].topImg+ '"/>');

			}, function(){

			$(this).empty().append( this.downA );
			this.hovered = false;

			});
		
	
		this.handled = true;	// mark this done
		});
	

  })(); 
	//</script>


    

