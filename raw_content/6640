<script type=text/javascript>

  window.fbAsyncInit = function() {
  
    FB.init({ appId: '155023867867753', status: true, cookie: true, xfbml: true });
    
    /* Check if the user is NOT logged into R29, but IS logged into Facebook.  Fix it */
    if (R29.url.param.getVar('fb') && !R29.user.token() && FB.getSession()) { //We have some sort of session, we *may* be logged in.
      FB.getLoginStatus(function(response) {
        if(response.status == 'connected') {
          return document.location = 'https://welcome.refinery29.com/register?from=http%3A%2F%2Fwww.refinery29.com%2Fyolanda-dominguez-modeling-poses-video&sig=5fcb1&app=blog&fb=1';
        }    
      });      
    }
        
    //FACEBOOK LOGIN
    $('a.fb_button').live('click', function() {
      R29.FBLoginPopup();
      return false;
    });
    
    if (typeof window.fbAsyncOnInit == "function") {
      window.fbAsyncOnInit();
    }
       
  };
  
  
  R29.FBLoginPopup = function () {
    console.log("Login Popup");
    FB.login(function(response) {
    
      if (response.session) {
        //They *may* have given us perms, but who knows.
        return document.location = 'https://welcome.refinery29.com/connect/fb?from=http%3A%2F%2Fwww.refinery29.com%2Fyolanda-dominguez-modeling-poses-video&sig=5fcb1&app=blog&';
      } else {
        //Oh well, something is wrong, let's try the normal login
        return document.location = 'https://welcome.refinery29.com/register?from=http%3A%2F%2Fwww.refinery29.com%2Fyolanda-dominguez-modeling-poses-video&sig=5fcb1&app=blog&';
      }
      
     },{ perms: 'email,offline_access,publish_stream' } );
  }
</script>

