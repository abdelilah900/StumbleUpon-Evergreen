      <script>
      $('#header ul.categories > li').hover(
        function() {
          $(this).addClass('active');
          $('ul',this).show();
        },
        function() {
          if (!$(this).hasClass('selected')) {
            $(this).removeClass('active');
          };
          $('ul',this).hide();
        }
      );
      $('.categories').hover(function() {
        this.style.zIndex = '1000003';
      }, function() {
        this.style.zIndex = '5';
      });
    </script>
  

