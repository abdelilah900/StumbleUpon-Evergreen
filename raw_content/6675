<script type="text/javascript">
    
    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
            
            
              holder.currentPost = "http://www.blogger.com/feeds/3118544728735187059/posts/default/2284243091184834207";
            
            
            
              holder.currentComments = "http://www.blogger.com/feeds/3118544728735187059/2284243091184834207/comments/default";
            
            holder.currentPostUrl = "";
            holder.currentPostId = 2284243091184834207
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/3118544728735187059/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/3118544728735187059/comments/default";
          
          holder.currentBlogUrl = "http://www.utahdealdiva.com/";
          holder.currentBlogId = "3118544728735187059";
        
        return holder;
      });
    }
  </script>
<script type="text/javascript">
  if (!window.registeredCommonBloggerCallbacks) {
    window.registeredCommonBloggerCallbacks = true;

    gadgets.rpc.register('resize_iframe', function(height) {
      var el = document.getElementById(this['f']);
      if (el) {
        el.style.height = height + 'px';
      }
    });

    
    gadgets.rpc.register('set_pref', function() {});

    registerGetBlogUrls();
  }
  </script>
