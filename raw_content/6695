<script type="text/javascript" language="javascript">

    function trackLink(linkLocation, linkName, pageName, leavePage) {
        var s = s_gi(s_account);
        s.linkTrackVars = 'prop11,prop12,prop34,eVar12,eVar13,eVar14';
        s.linkTrackEvents = "None";
        s.prop34 = linkName;
        s.prop11 = linkLocation;
        s.prop12 = pageName;
        s.eVar12 = linkName;
        s.eVar13 = linkLocation;
        s.eVar14 = pageName;

        if (leavePage == true) {
            s.tl(this, 'o', linkName);
        }
        else {
            setTimeout('s.tl(this,"o","' + linkName + '")', 1000);
        }
    }

</script>
