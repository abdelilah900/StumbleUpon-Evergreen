<script type="text/javascript">
	var cartProducts = eval([]);
	var message = '';
	
	if (!Object.isUndefined(screenManager)) {
		screenManager.getCart().setCartItems(cartProducts);
		if (message) {
			var messageObj = screenManager.getMessage();
			messageObj.setMessage(message, "error");
			messageObj.showMessage();
			screenManager.getCart().unregisterCartChangeCallback();
		}
		else {
			try {
				if (checkoutCart) {  // only register callback on cart page
					screenManager.getCart().registerCartChangeCallback(function () {
						document.location.reload();
					});
				}
			} catch (e) {
				; // ignore
			}
		}
	}		
</script>
