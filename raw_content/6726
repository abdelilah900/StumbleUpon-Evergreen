	<script type="text/javascript">
		function openFeaturedProductModal() {
			var modal = screenManager.getProductModal();
			modal.setId(this.getAttribute("product_id"));
			modal.setUrl(this.getAttribute("url"));
			modal.load();
		}
		function toggleFeaturedProductClass (event)	{
			var elm = Event.element(event).up(null, 1);
			if (elm.hasClassName("product-name-hover"))
				elm.removeClassName("product-name-hover");
			else
				elm.addClassName("product-name-hover");
		}
		/*
		function toggleFeaturedProductImage (event)	{
			var elm = Event.element(event);
			if (elm.hasClassName("mini-featured-product-image-hover"))
				elm.removeClassName("mini-featured-product-image-hover");
			else
				elm.addClassName("mini-featured-product-image-hover");
		}
		*/
		Event.observe(window, 'load', function() {
			var elms = $("mini-week").select(".mini-featured-product");			
			Event.observe(elms[0], "click", openFeaturedProductModal);
			/*
			var imgs = $("mini-week").select(".mini-featured-product-image");
			Event.observe(imgs[0], "mouseover", toggleFeaturedProductImage);
			Event.observe(imgs[0], "mouseout", toggleFeaturedProductImage);
			*/
			var stars = $("mini-week").select(".product-star-anchor");
			Event.observe(stars[0], "mouseover", toggleFeaturedProductClass);
			Event.observe(stars[0], "mouseout", toggleFeaturedProductClass);
		});
	</script>
