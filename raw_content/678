		function multiSiteLoginCheck(str){
			//alert('multiSiteLoginCheck str: ' + str);
			
			if(str == 'not_logged_in'){
				
				alert('make ajax call to check for logins on other sites');
				
				$.ajax({
					url: "/ajax_process.php",
					type: 'GET',
					cache: false,
					data: (
						{
							task: 'check_multi_site_login'
						}
					),
					dataType: "json",
					timeout: 10000,
					success: function(json){
						var alert_str = 'multiSiteLoginCheck success' + "\n";
						alert_str += 'json.allow_login: ' + json.allow_login + "\n";
						alert_str += 'json.time: ' + json.time + "\n";
						alert_str += 'json.logged_in: ' + json.logged_in + "\n";
						//alert(alert_str);
						
						if(json.logged_in){
							alert('logged_in');
						}else{
							alert('NOT logged_in');
						}
					},
					error: function(){
						//alert('multiSiteLoginCheck error');
					},
					complete: function(){
						//alert('multiSiteLoginCheck complete');
					}
				});
				
			}else{
				//alert('we just logged in and loaded an iframe to set cookies on c.prime');
			}
		}
	</script>
	
