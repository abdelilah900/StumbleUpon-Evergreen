<script type="text/javascript">

    var myListeners = {
        handle_auth_login: function(data) {
            // Whatever called this requires a login. Send through registration loop.
            window.location = 'http://www.cbssports.com/login?master_product=2580&xurl=http%3A%2F%2Fwww.cbssports.com%2Fmcc%2Fauthors%2Fnew-author%3Fxurl%3Dhttp%3A%2F%2Fwww.cbssports.com%2Fcollegebasketball%2Fblog%2Feye-on-college-basketball%2F20091580%2Ftexas-techs-decision-on-gillispie-on-hold-%3FprofFR%3D1&lfxurl=http%3A%2F%2Fwww.cbssports.com%2Fmcc%2Fauthors%2Fnew-author%3Fxurl%3Dhttp%253A%252F%252Fwww.cbssports.com%252Fcollegebasketball%252Fblog%252Feye-on-college-basketball%252F20091580%252Ftexas-techs-decision-on-gillispie-on-hold-%253FprofFR%253D1';
        },
        handle_comment_shown: function(data) {
            var elem = data.comment.find('.lf_comment_user');
            if (elem) {
                data.comment.find('.hastooltip').removeClass('hastooltip');
                $('#livefyre .hastooltip,[title="Quick profile"]').removeAttr('title');
            }
        },
        handle_comments_loaded: function() {
            $('#lf_comments > .lf_comment_container').has('.lf_comment_user').find('.hastooltip').removeClass('hastooltip');
            $('#livefyre .hastooltip,[title="Quick profile"]').removeAttr('title');
        }
    };

    changeDOM = function() {
        // DOM hacks.  Change the UI for the dropdown box
        var userBox = $('.lf_user_box.lf_box');
        // Change link label
        $('.lf_user_links.lf_edit_profile_link',userBox).html('Edit Settings');
        // Nuke logout link
        userBox.find('.lf_logout_link').remove();
        // Add our own link to the MCC profile
        userBox.append('<li class="lf_user_links"><a style="color:inherit;text-decoration:none" target="_blank" href="'+userObj.profile.profile_url+'">Edit Profile</a></li>');
    };
 

    var userObj = {};

    $(document).ready( function () {

        var conv = LF({
            domain: 'cbssports.fyre.co',
            site_id: '309983',
            article_id: 'c:20091580',
            conv_meta: {
                article_url: 'http://www.cbssports.com/collegebasketball/blog/eye-on-college-basketball/20091580/texas-techs-decision-on-gillispie-on-hold-',
                title: 'Texas Tech\'s decision on Gillispie on hold ',
                sig: 'd0210d5bc71403dee1357981c9fa249c'
            }
        }).ready(function() {
            // Run this listener once on initial load
            myListeners.handle_comments_loaded();
            // Run the changes to the DOM
            LF.Dispatcher.addListener(myListeners);
            // Log in the user if we got a token for them
            // Change the DOM after login since things get re-rendered
            if (userObj.token) {
                LF.login(userObj, function() {
                    CBSi.log("Livefyre Login success! ", userObj);
                    changeDOM();
               }, function() {
                    CBSi.log("Livefyre LOGIN ERROR"); 
                    changeDOM();
                });
            }
            else {
                changeDOM();
            }
            // Load the comment counts for our bubbles
            $('div.commentsBubble').addClass('livefyre-commentcount').attr('data-lf-site-id','309983').attr('data-lf-article-id','c:20091580');
            LF.CommentCount({
                replacer: function(element, count) {
                    if (count > 1){
                        $(element).html('<a href="#commentsAnchor">' + count + ' &nbsp;&nbsp;| &nbsp;Comments</a>');
                        $(element).show();
                    }
                    else{
                        $(element).html('<a href="#commentsAnchor">' + count + ' &nbsp;&nbsp;| &nbsp;Comment</a>');
                        $(element).show();
                    }
                }
            });
       });
    });
    

</script>


</div>
<div id="storyRight" class="printerHide" section="story;rightRail">
    <div class="spacer10">&nbsp;</div>
