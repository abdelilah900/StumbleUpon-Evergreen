    /* Define Google Analytics asynchronous page tracking object in the form of a command array.
      This array acts as a queue, which is a "first-in,first-out" data structure.
      This allows for the queue to be assembled prior to the GA supporting JS library loading
    */
    var _gaq = _gaq || [];
    
    /*
      Set default tracker (set without a namespace prefix)
