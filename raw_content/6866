  if (f.cid == '-na-' || f.cid == '') { 
   f.cid = we.forecast[0].cid; 
   f.text = we.forecast[0].cond; 
   f.temp = (we.forecast[0].hi != '-na-' && we.forecast[0].hi != '') ? (we.forecast[0].hi + ' / ' + we.forecast[0].lo) : we.forecast[0].lo; 
  } 
  
