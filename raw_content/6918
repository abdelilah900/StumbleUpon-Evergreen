function mtUpdateSignInWidget(u) {
    var separator = '';
    var el = document.getElementById('signin-widget-content');
    var content = '';
    if (!el) return;
    if (u) {
        if (u && u.is_authenticated) {
            user = u;
            mtSaveUser();
        } else {
            // user really isn't logged in; so let's do this!
            return mtSignIn();
        }
    } else {
        u = mtGetUser();
    }
    if (u && u.name) {
        var url;
        if (u.is_authenticated) {
            if (u.is_author) {
                url = 'http://consumerist.com/cgi-bin/mt/mt-cp.cgi?__mode=view&blog_id=1';
                url += '&static=' + encodeURIComponent( location.href );
            } else {
                url = u.url;
            }
        } else if (u.url) {
            url = u.url;
        } else {
            url = null;
        }
        if (url)
            content += '<a id="h-welcome" href="' + url + '">' + "Welcome, " + u.name + '</a>';
        else
            content += u.name + '';
        if (u.is_author)
            content += '<a id="h-edit" href="http://consumerist.com/cgi-bin/mt/mt-cp.cgi?__mode=edit&blog_id=1&return_to=' + encodeURIComponent(document.URL) + '">Edit profile</a>';
        
        content += '<a id="h-logout" href="javascript:void(0)" onclick="return mtSignOutOnClick()">Sign out</a>';
        
    } else if (u && u.is_banned) {
        content = 'You do not have permission to sign in to this blog.';
    } else {
        content = '<a id="h-login" href="javascript:void(0)" onclick="return mtSignInOnClick(\'signin-widget-content\')">Login</a>';
        
            
                content += '<a id="h-register" href="http://consumerist.com/cgi-bin/mt/mt-cp.cgi?__mode=register&blog_id=1&return_to=' + encodeURIComponent(document.URL) + '">Register</a>';
            
        
