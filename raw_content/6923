					var value = parseFloat($("#offer").val());
					if(value + "" == "NaN") value = "0.00";
					$("#offer").val(value.toFixed(2));
					$("#other").attr("checked", "checked");
				});

				$("#offer").focus(function () {
					$("#other").attr("checked", "checked");
