<div id="wishabiBox"></div>


<script>
var x = document.location.href.split('/');
var y = x.splice(0,3);

var path = "";
var varNum = '';
var varDivContent ='';

for (i=0, len=x.length; i < len; i++){
	if (x[i].indexOf('.') < 0) {
	path+= x[i];
		if (x[i] != "") {path+= "/";}
	}
}


function getWishabi(path) {

	if (path == "news/local/"){
		//Local
		varNum = "195";
	} else if (path == "lanow/"){
		//Local - LA Now
		varNum = "196";
	} else if (path == "news/nation/nationnow/"){
		varNum = "197";
	} else if (path == "news/politics/"){
		varNum = "198";
	} else if (path == "news/politics/topoftheticket/"){
		varNum = "199";
	}  else if (path == "health/"){
		varNum = "216";
	} else if (path == "news/nationworld/nation/"){
		varNum = "212";
	} else if (path == "business/"){
		varNum = "213";
	} else if (path == "business/money/"){
		varNum = "200";
	} else if (path == "business/technology/"){
		varNum = "201";
	} else if (path == "sports/"){
		varNum = "214";
	} else if (path == "sports/sportsnow/"){
		varNum = "202";
	} else if (path == "sports/football/nfl/"){
		varNum = "203";
	} else if (path == "sports/basketball/nba/lakers/"){
		varNum = "215";
	} else if (path == "entertainment/movies/moviesnow/"){
		varNum = "204";
	} else if (path == "entertainment/tv/showtracker/"){
		varNum = "205";
	} else if (path == "entertainment/gossip/"){
		varNum = "206";
	} else if (path == "entertainment/envelope/cotown/"){
		varNum = "207";
	} else if (path == "health/boostershots/"){
		varNum = "208";
	}  else if (path == "features/home/"){
		varNum = "209";
	}  else if (path == "features/food/"){
		varNum = "210";
	} else if (path == "features/image/alltherage/"){
		varNum = "211";
	} else if (path == "travel/"){
		varNum = "217";
	} else {
		//not in list
                varNum = '';
	}
	if (varNum != '') {
	varDivContent = "<div id='circularhub_module_"+ varNum +"'></div><script src=http://api.circularhub.com/"+ varNum +"/442bb3f53c91e218/circularhub_module.js></scr"+"ipt>";

	jQuery('#wishabiBox').html(varDivContent);
  }
}

window.onload = getWishabi(path);


</script>
                </div>
            
            
			
			
			
						
            

        


    
        







