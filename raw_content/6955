	var lynchsig = '<div id="sigshell" class="social-sig"><img id="sigheadshot" class="runtimeTopic" src="http://www.trbimg.com/img-5029727e/turbine/la-headshots-for-social-bylines-20120813-photo-001/600" alt="" /><div class="sigleftfloat"><div id="sigtwitter" class="sigleftfloat"><a class="twitter-follow-button" href="https://twitter.com/renelynch" data-show-count="false">Follow@renelynch</a></div><span class="sigleftfloat"><iframe src="//www.facebook.com/plugins/subscribe.php?href=https%3A%2F%2Fwww.facebook.com%2Frenelynch&layout=button_count&show_faces=true&colorscheme=light&font&width=90&appId=129500067171162"scrolling="no" frameborder="0" id="sigfacebook" allowTransparency="true"></iframe></span><a class="sigleftfloat" href="https://plus.google.com/116843518703648900008" target="_blank"><img style="border: 0; width: 20px; height: 20px;" src="https://ssl.gstatic.com/images/icons/gplus-16.png" alt="Google+" /></a><div class="clear"></div><p><a href="mailto:rene.lynch@latimes.com" style="color:#2262CC">rene.lynch@latimes.com</a></p></div></div>';
	


