<!-- Article Toolbar and Chunky Buttons -->
<script>
    (function() {

        var div;            
        var map = {
            'comm'  : 'Comments',
            'sp'    : 'Single Page',
            'rpt'   : 'Reprints',
            'rss'   : 'RSS',
            'print' : 'Print',
            'email' : 'E-mail'
        }
        $.each($('.sl-tbar-link'), function(i, elm){
            $(elm).mouseover(function(){
                div = $(document.createElement('div'));
                div.addClass('sl-tbar-tip')
                div.html(map[$(this).attr('class').split(' ')[1].split('-')[2]]);
                div.css('left', ($(this).parent().position().left + $(this).parent().width() / 2) + 'px');
                div.append($(document.createElement('div')).addClass('sl-tbar-tip-indica sl-sprite-tbar'));
                $('.sl-tbar').append(div);
                div = null;
            });
            $(elm).mouseout(function(){
                $('.sl-tbar-tip').remove();
            });
        });
    })();
</script>

