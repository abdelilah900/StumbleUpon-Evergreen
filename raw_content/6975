    $(document).ready(function() {
        $("#postloadframe").load(function() {

            var iframe = document.getElementById("postloadframe");
            var tb = iframe.contentWindow.document.getElementById("top_external_banner").innerHTML;
            var m = iframe.contentWindow.document.getElementById("mpu").innerHTML;
            var ss = iframe.contentWindow.document.getElementById("sky_scraper").innerHTML;
            var ga = iframe.contentWindow.document.getElementById("g_ads").innerHTML;

            document.getElementById('top_external_banner').innerHTML = tb;
            document.getElementById('mpu').innerHTML = m;
            document.getElementById('sky_scraper').innerHTML = ss;
            document.getElementById('g_ads').innerHTML = ga;
        });
    }); 
</script>


       
<iframe id="postloadframe" frameborder="0" height="0"  width="0"
        src="/Delia_AdFrame_Content.aspx?caller=recipe">
    </iframe>



 
</body>
</html>
