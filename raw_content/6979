    var google_adnum = 0; 
    
    function google_ad_request_done(google_ads) {
      var ad_code = '';
      if (google_ads.length == 1) {
        ad_code += '';
        ad_code += '<p class="adsense_credit"><a href="' +
        google_info.feedback_url + '\" class="googleClickout">Ads by Google</a></p>';
