$J.getScript(url);



/* Handle Post-Loading Ajax */
if (pageLoadActions == undefined) {
  var pageLoadActions = [];
  var loaded = false;
}
onPageLoadHandler = function() {
  if (!loaded) {
    for (iterator = 0; iterator < pageLoadActions.length; iterator++) {
      pageLoadActions[iterator]();
    }
    loaded = true;
  }
  if (theLensOwner) {
    $J('.lens-owner').show();
  }
}
function hasPageLoaded() {
  if (typeof(pageBottomLoaded) != 'undefined') {
    onPageLoadHandler();
  }
}

// Either when the page loads, or after 12 seconds...whatever comes first
window.onload = onPageLoadHandler;
window.setTimeout(hasPageLoaded, 12000);


var __squid_like = {
  logged_in: isLoggedIn,
  member_likes: false,
  owner_viewer: false,
  lens_owner: 'e33cd14136d803dce75c6d7e4f0b6261',
  member_favorite: false,
  like_count: 0};

function doSquidLike(button) {
  allButtons = $J('.sl_btn');

  if (!allButtons.hasClass('disabled')) {

    if (__squid_like.logged_in) {
      if (theMember.id == __squid_like.lens_owner) {
        Squidoo.popover(button, {
          title: 'Oops!',
          content: "Sorry, you're not allowed to SquidLike your own lens. Try sharing it with a friend for a look!",
          placement: 'top',
          trigger: 'hover'
        }, 'show');

      } else {
        allButtons.addClass('disabled');
        var new_count;

        if (__squid_like.member_likes) {
          new_count = __squid_like.like_count - 1;
          $J('.sl_count_rt').html(new_count.toString());
          $J('li.social_like').removeClass('social_liked');

          __squid_like.like_count = new_count;
          __squid_like.member_likes = false;

        } else {
          new_count = __squid_like.like_count + 1;
          $J('.sl_count_rt').html(new_count.toString());
          $J('li.social_like').addClass('social_liked');

          __squid_like.like_count = new_count;
          __squid_like.member_likes = true;

        }

        SquidgamingJS.post(
          '/utility/like_lens/' + theLensName,
          // this logic is opposite because the state class is applied above 
          { 'action': $J('li.social_like').hasClass('social_liked') ? 'like' : 'unlike' },
          function (result) {
            allButtons.removeClass('disabled');
          }
        );
      }
    } else {
      // if not logged-in, redirect to login
      // this only happens if the Squidoo.login hasn't executed
      // by the time the user clicks the button
      document.location = '/member/login/'+theLensName;
    }
  }
}

