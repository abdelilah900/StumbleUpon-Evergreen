        function changeTerms (selection) { 
            if (selection != undefined) { 
                jQuery('.0yrContract').toggle(selection == 0); 
                jQuery('.1yrContract').toggle(selection == 1); 
                jQuery('.12yrContract').toggle(selection == 12); 
                jQuery('.24yrContract').toggle(selection == 24); 
            } 
        } 
        changeTerms(); 
        //jQuery('.priceBlock .splitCells:contains($0)').toggle(false); 
        jQuery('.priceBlock .termsblock:not(:has(div))').css('border-top', 'none'); 
        jQuery('.priceBlock #commitmentTerm').live('change', function (e) {changeTerms(e.currentTarget.value);}); 
        /*jQuery('.priceBlock #qtyAllow').change(function (e) { 
            jQuery('.cartItem').attr('data-qty',e.currentTarget.value); 
        });*/ 
        //jQuery('.sizeVariant').live('click', function () {location.href = jQuery(this).val();});       
        jQuery('.priceBlock .color .swatchOuter').live('click', function () { 
            //jQuery('.priceBlock .color .swatch_label span')[0].innerHTML = jQuery(this).data('color'); 
            //jQuery(this).parents('.cartItem').data('sku', jQuery(this).data('skuHolder')); 
            location.href = jQuery(this).attr('colorUrl'); 
        }); 
        jQuery('.priceBlock .color .swatchInner').mouseenter(function () { 
            var borderColor = jQuery(this).data('html-color') == '#FFFFFF' ? '#999999' : '#CCCCCC'; 
            jQuery(this).parent().css('border-color', '#000000'); 
            jQuery(this).css('border-color', borderColor); 
            }).mouseleave(function () { 
            var borderColor = jQuery(this).data('html-color') == '#FFFFFF' ? '#CCCCCC' : jQuery(this).data('html-color'); 
            jQuery(this).parent().css('border-color', '#CCCCCC'); 
            jQuery(this).css('border-color', borderColor); 
        }); 
        jQuery('.priceBlock .color .swatchInner').hover(); 
        jQuery('#hcontarctlength').val(jQuery("#commitmentTerm").val()); 
        // Begin SLI section 
        // SLI globals 
        var SLIcombineCtaSearchWtEvt=false, SLIcurrentPageNum = 1, SLIlastPageNum = 1, SLIcurrentPageNumPrint = 1, SLIcurrentDiv = '',  
			SLIInvDown = '', SLIsavedLoc = 'false', SLIhadError=false, SLIprintPreviewBound = false; 
        function InStoreAvailability(param) { 
            var handleCboxComplete = function() { 
				jQuery('#SLIerrorMessage').html(''); 
				jQuery('#SLIerrorMessage').toggle(false); 
				var handlePrintClick = function () {  
					ATT.cqWebtrendsli.linkClick(); 
					jQuery(document).bind('cbox_cleanup', cboxBindCleanup ); 
					var handlePrintModal = function(){  
						jQuery('#printcontent #sliSearch').toggle(false); 
						jQuery('#printcontent #sliSearchMessage').toggle(false); 
						jQuery('#printcontent #PrintStoreDetail').toggle(false); 
						SLIcurrentPageNum = SLIcurrentPageNumPrint; 
						SLIlastPageNum = SLIcurrentPageNumPrint; 
						jQuery(document).unbind('cbox_complete', handlePrintModal); 
					} 
					jQuery(document).bind('cbox_complete', handlePrintModal); 
				} 
				if ( ! SLIprintPreviewBound) { 
					ATT.util.printPreview({selector:"#PrintStoreDetail", width:"1100px", contentSelector:"#storeDataDiv"}); 
					SLIprintPreviewBound = true; 
					jQuery('#PrintStoreDetail').live('click', handlePrintClick); 
				} 
				if (param == 'vm') { 
					submitSLISearch(); 
				} 
				jQuery.colorbox.resize(); 
				jQuery.fn.morphForms(); 
			} 
			jQuery(document).unbind('cbox_complete',handleCboxComplete); 
			jQuery.colorbox({ 
                inline:true, 
                href:'#inStoreAvailability',  
                width: 700, 
                scrolling: false, 
                onComplete: handleCboxComplete, 
                onClosed: clearData 
            }); 
            if (typeof param == "undefined") { 
                ATT.cqWebtrendsli.modalOpen(); 
            } 
        } 
        function cboxBindCleanup(){ 
            var v_id = jQuery('#printcontent').attr('id');  
            if (v_id == 'printcontent') { 
                jQuery(document).bind('cbox_closed', cboxBindClose ); 
            } 
        } 
        function cboxBindClose(){ 
            jQuery(document).unbind('cbox_closed'); 
            jQuery(document).unbind('cbox_cleanup'); 
            InStoreAvailability(1); 
        } 
        function clearData() {  
            jQuery('#storeData').html(''); 
            resetPageNum(); 
        } 
        function submitSLISearch() { 
            ATT.modalSpinner.show(); 
            searchLoc = jQuery('#searchLocation'); 
            if (searchLoc.hasClass('hinting'))  searchLoc.val(''); 
            jQuery('#searchAddress').html(searchLoc.val());          
            var sliSearch = jQuery('#sliSearch');            
            jQuery.colorbox.resize(); 
            //jQuery('#inStoreAvailability').css('height', '600px'); 
            jQuery('#SLIerrorMessage').html('').hide(); 
            jQuery.post(sliSearch.attr('action'),sliSearch.serialize(),registerCallback, 'html'); 
        } 
        function registerCallback(data, textStatus, jqXHR) { 
            var v_statusCode='', v_SuccessFlag='', v_statusMsg='', v_storeInvDetails='', v_json='', isNoQtyResult = false; 
            var lastNum = SLIcurrentPageNum; 
            resetPageNum(); 
            searchLoc = jQuery('#searchLocation'); 
            if (searchLoc.hasClass('hinting') && searchLoc.val() == '')  searchLoc.val(searchLoc.attr('title')); 
            if (data.indexOf('status=error') > -1) { 
                v_statusMsg = data.replace('<div style="display:none">status=error</div>',''); 
                jQuery('#SLIerrorMessage').append(jQuery.trim(v_statusMsg)); 
                jQuery('#SLIerrorMessage').show(); 
                v_SuccessFlag = '0'; 
                jQuery('#storeData').html(''); 
                jQuery.colorbox.resize(); 
                ATT.modalSpinner.close(); 
                SLIlastPageNum = lastNum; 
                ATT.cqWebtrendsli.SearchButton( 
                    v_SuccessFlag, v_statusCode, userResp,   
                    v_json.wtStoreInvDetails, v_json.wtStoreInvResultsCt, v_json.wtInStkStoreCt,  
                    v_json.wtOOStkStoreCt, v_json.wtInvUnAvailStoreCt, v_json.wtMinDistInStkStore,  
                    v_json.wtMinDistOOStkStore, v_statusMsg,  
                    '', '', '', true 
                ); 
            } else { 
                jQuery('#sliModalTitle').html("In-Store Availability"); 
                jQuery('#sliSearchMessage').html("Showing availability at company-owned AT&T stores and kiosks only."); 
                jQuery('#sliSearchBoxLabel').html("Change location"); 
                jQuery('#storeData').html(''); 
                jQuery('#storeDataHead').show(); 
                jQuery('#sliBottomButton').show(); 
                jQuery('#SLIerrorMessage').html(''); 
                jQuery('#SLIerrorMessage').hide(); 
                if (data.length > 2000 && jQuery.browser.msie) {  
                    jQuery('#storeData')[0].innerHTML = data;  
                } else {  
                    jQuery('#storeData').html(data);  
                }  
                SLIInvDown=''; 
                v_statusMsg=''; 
                jQuery('#storeData').find('#sliErrorResp').each(function(idx,el) { 
                    v_statusMsg = el.innerHTML; 
                    jQuery('#SLIerrorMessage').append(jQuery.trim(v_statusMsg)).show(); 
                    SLIInvDown = '-InvDown'; 
                }); 
                displayStoreOnPriceBlock(); 
                //var v_addressInfo = jQuery('#SLIaddressInfo').html(); 
                //jQuery('#SLIdata').html(v_addressInfo); 
                jQuery.colorbox.resize(); 
                ATT.modalSpinner.close(); 
                v_storeInvDetails = jQuery('#storeInvDetails').html();  
                v_statusCode = '0'; 
                v_SuccessFlag = '1'; 
                if (v_statusMsg != '') { 
                    v_statusCode = ''; 
                    v_SuccessFlag = '0'; 
                } 
                v_json = JSON.parse(v_storeInvDetails); 
                var userResp = jQuery('#searchLocation').val(); 
                SLIlastPageNum = lastNum; 
                ATT.cqWebtrendsli.SearchButton( 
                    v_SuccessFlag, v_statusCode, userResp,   
                    v_json.wtStoreInvDetails, v_json.wtStoreInvResultsCt, v_json.wtInStkStoreCt,  
                    v_json.wtOOStkStoreCt, v_json.wtInvUnAvailStoreCt, v_json.wtMinDistInStkStore,  
                    v_json.wtMinDistOOStkStore, v_statusMsg, 
                    'HRock StoreResults'+SLIInvDown+'-1 Pg', 1, '', false 
                ); 
            }  
        } // end of registerCallback 
        function displayStoreOnPriceBlock() { 
            jQuery("#SLIdata").load( 
                '/shop/wireless/devices/htc/one-xplus-carbon-black.sliGetStore.xhr.html', 'skuId=sku6300321&pageType=deviceDetails', 
                function(response, status, xhr) {  
                    //alert("The SLI data has been loaded. ");  
                } 
            ); 
        } 
        function resetPageNum() { 
            SLIcurrentPageNum = 1; 
            SLIlastPageNum = 1; 
            SLIcurrentPageNumPrint = 1; 
            SLIcurrentDiv = '';                      
        } 
        function pagePrevNext (param, cqDataPath) { 
            SLIlastPageNum = SLIcurrentPageNum; 
            var selectNumber, v_value; 
            var v_id = jQuery('#printcontent').attr('id'); 
            SLIcurrentDiv = (v_id == 'printcontent') ? ('#' + v_id) : ''; 
            if (isNaN(param)) { 
                v_value = (param == 'p') ? -1 : 1; 
                selectNumber = ((v_id == 'printcontent') ? SLIcurrentPageNumPrint : SLIcurrentPageNum) + v_value; 
            } else { 
                selectNumber = param;  
            }  
            var previousNumber = (v_id == 'printcontent') ? SLIcurrentPageNumPrint : SLIcurrentPageNum; 
            var pageNumber = jQuery(SLIcurrentDiv+' #page'+selectNumber).attr('id');  
            if (pageNumber) { 
                jQuery(SLIcurrentDiv+' #page'+previousNumber).toggle(false); 
                jQuery(SLIcurrentDiv+' #page'+selectNumber).toggle(true); 
                jQuery(SLIcurrentDiv+' #pageNumTop'+previousNumber).css('border', '1px solid #FFFFFF'); 
                jQuery(SLIcurrentDiv+' #pageNumTop'+selectNumber).css('border', '1px solid #CCCCCC'); 
                jQuery(SLIcurrentDiv+' #pageNumBottom'+previousNumber).css('border', '1px solid #FFFFFF'); 
                jQuery(SLIcurrentDiv+' #pageNumBottom'+selectNumber).css('border', '1px solid #CCCCCC'); 
                if (v_id == 'printcontent') 
                    SLIcurrentPageNumPrint = selectNumber; 
                else  
                    SLIcurrentPageNum = selectNumber; 
            } else { 
                jQuery(SLIcurrentDiv).find('#page'+previousNumber).toggle(false); 
                jQuery(SLIcurrentDiv).find('#page'+selectNumber).toggle(true); 
                jQuery(SLIcurrentDiv).find('#pageNumTop'+previousNumber).css('border', '1px solid #FFFFFF'); 
                jQuery(SLIcurrentDiv).find('#pageNumTop'+selectNumber).css('border', '1px solid #CCCCCC'); 
                jQuery(SLIcurrentDiv).find('#pageNumBottom'+previousNumber).css('border', '1px solid #FFFFFF'); 
                jQuery(SLIcurrentDiv).find('#pageNumBottom'+selectNumber).css('border', '1px solid #CCCCCC'); 
                if (v_id == 'printcontent') 
                    SLIcurrentPageNumPrint = selectNumber; 
                else  
                    SLIcurrentPageNum = selectNumber;            
            } 
            jQuery.colorbox.resize(); 
            ATT.cqWebtrendsli.SearchResult('HRock StoreResults'+SLIInvDown+'-'+SLIcurrentPageNum+' Pg', SLIcurrentPageNum, false, cqDataPath+param+'.jsp'); 
        } // end of pagePrevNext 
        function viewMoreStore() { 
            InStoreAvailability('vm'); 
        } // End of viewMoreStore 
        // End SLI section 
        ATT.namespace('cqWebtrendsli'); 
        ATT.cqWebtrendsli = new function() { 
            var v_language = ('en_US' == 'en_US') ? 'English' : 'Spanish', v_StoreInvDetails = '', wtSLID = jQuery('meta[name="DCSext.wtSLID"]'),  
                wtAcctNum = jQuery('meta[name="DCSext.wtBAN"]'), wtCTN = jQuery('meta[name="DCSext.wtCTN"]'); 
            this.ecParams = function(wtargs) { 
                if (wtSLID.length === 1) { 
                    wtargs.push('DCSext.wtSLID'); 
                    wtargs.push(wtSLID.attr('content')); 
                } 
                if (wtAcctNum.length === 1) { 
                    wtargs.push('DCSext.wtAcctNum'); 
                    wtargs.push(wtAcctNum.attr('content')); 
                } else if (wtCTN.length == 1) { 
                    wtargs.push('DCSext.wtCTN'); 
                    wtargs.push(wtCTN.attr('content')); 
                } 
                return wtargs; 
            } 
            this.searchResultParams = function(wtargs, resultPage, n, wtNoHit, wtclick) { 
                var dcsuri = '/shop/virtual/storeresults'+SLIcurrentPageNum+'.html', wtclick = wtclick || jQuery('#sliSearch > img').data('cqpath'); 
                wtNoHit = wtNoHit || false; 
                wtargs.push('DCSext.wtZipCode'); 
                wtargs.push(ATT.globalVars.zip()); 
                wtargs.push('DCSext.wtBuyFlowCode'); 
                wtargs.push(ATT.globalVars.flowcode); 
                wtargs.push('DCSext.wtCustType'); 
                wtargs.push('Consumer'); 
                wtargs.push('DCSext.wtLanguage'); 
                wtargs.push(v_language); 
                wtargs.push('DCSext.wtPN'); 
                wtargs.push(resultPage); 
                wtargs.push('DCS.dcsuri'); 
                wtargs.push(dcsuri); 
                if (wtNoHit === 1 || wtNoHit === '1') { 
                    wtargs.push('DCSext.wtNoHit'); 
                    wtargs.push(wtNoHit); 
                } 
                return wtargs; 
            } 
            this.errorResultParams = function(wtargs) { 
                var dcsuri = '/shop/virtual/storeresultserror1.html'; 
                wtargs.push('DCSext.wtZipCode'); 
                wtargs.push(ATT.globalVars.zip()); 
                wtargs.push('DCSext.wtBuyFlowCode'); 
                wtargs.push(ATT.globalVars.flowcode); 
                wtargs.push('DCSext.wtCustType'); 
                wtargs.push('Consumer'); 
                wtargs.push('DCSext.wtLanguage'); 
                wtargs.push(v_language); 
                wtargs.push('DCSext.wtPN'); 
                wtargs.push('HRock StoreResults-NoResults-1 Pg'); 
                wtargs.push('DCS.dcsuri'); 
                wtargs.push(dcsuri); 
                return wtargs; 
            } 
            this.removeSuccessMeta = function removeSuccessMeta() { 
                jQuery('head > meta[name="DCSext.wtSuccessFlag"]').remove(); 
                jQuery('head > meta[name="DCSext.wtStatusCode"]').remove(); 
            } 
            this.InStoreAvailabilityLink = function (cqpath) { 
                var wtclick = cqpath; 
                var wtargs = ['DCS.dcsuri', window.location.pathname, 'DCSext.wtCQClickId', wtclick, 'DCSext.wtNoHit', 1, 'DCSext.wtBuyFlowCode', ATT.globalVars.flowcode, 
                    'DCSext.wtZipCode', ATT.globalVars.zip(), 'DCSext.wtCustType', 'Consumer', 'DCSext.wtCustType', 'Consumer']; 
                ATT.cqWebtrendsli.removeSuccessMeta(); 
                window.dcsMultiTrack.apply(this, wtargs);            
            } 
            this.modalOpen = function () {  
                var dcsuri = '/shop/virtual/storesearch.html'; 
                var wtargs2 = ['DCSext.wtSku', 'sku6300321', 'DCSext.wtLanguage', v_language, 'DCSext.wtBuyFlowCode', ATT.globalVars.flowcode, 
                    'DCSext.wtZipCode', ATT.globalVars.zip(), 'DCSext.wtCustType', 'Consumer', 'DCSext.wtPN', 'HRock StoreSearch Pg', 'DCS.dcsuri', dcsuri]; 
                ATT.cqWebtrendsli.removeSuccessMeta(); 
                window.dcsMultiTrack.apply(this, wtargs2); 
            } 
            this.SearchButton = function(wtSuccessFlag, wtStatusCode, wtUserResp, wtStoreInvDetails, wtStoreInvResultsCt, wtInStkStoreCt, wtOOStkStoreCt, wtInvUnAvailStoreCt, wtMinDistInStkStore, wtMinDistOOStkStore, wtStatusMsg, resultPage, n, wtNoHit, isError) { 
                var wtclick = jQuery('#sliSearch > img').data('cqpath'); 
                v_StoreInvDetails = wtStoreInvDetails; 
                var r = (ATT.util.getCookie("colam_ctn") || "");  
                var wtargs = ['DCSext.wtEvent', 'HRock_StoreInv_Check_Submit', 'DCSext.wtSuccessFlag', wtSuccessFlag, 'DCSext.wtStatusCode', wtStatusCode, 'DCSext.wtStatusMsg', wtStatusMsg,  
                    'DCSext.wtSku', 'sku6300321', 'DCSext.wtUserResp', wtUserResp, 'DCSext.wtBuyFlowCode',  ATT.globalVars.flowcode, 
                    'DCSext.wtStoreInvDetails', wtStoreInvDetails, 'DCSext.wtStoreInvResultsCt', wtStoreInvResultsCt, 
                    'DCSext.wtInStkStoreCt', wtInStkStoreCt, 'DCSext.wtOOStkStoreCt', wtOOStkStoreCt, 'DCSext.wtInvUnAvailStoreCt', wtInvUnAvailStoreCt, 
                    'DCSext.wtMinDistInStkStore', wtMinDistInStkStore, 'DCSext.wtMinDistOOStkStore', wtMinDistOOStkStore, 'DCSext.wtNoHit', '1', 'DCSext.wtCustType', 'Consumer']; 
                if (!SLIcombineCtaSearchWtEvt) { 
                    if (SLIsavedLoc == 'true') { 
                        wtargs.push('DCSext.wtLinkLoc'); 
                        wtargs.push('HRock SearchResultsModal Body'); 
                    } else { 
                        wtargs.push('DCSext.wtLinkLoc'); 
                        wtargs.push('HRock SearchModal Body'); 
                    } 
                    wtargs.push('DCSext.wtLinkName'); 
                    wtargs.push('Search'); 
                    wtargs.push('DCSext.wtCQClickId'); 
                    wtargs.push(wtclick); 
                    wtargs.push('DCS.dcsuri'); 
                    if (SLIhadError){ 
                         wtargs.push('/shop/virtual/storeresultserror1.html'); 
                    } else if (SLIsavedLoc == 'true') { 
                        wtargs.push('/shop/virtual/storeresults'+SLIlastPageNum+'.html'); 
                    } else  { 
                        wtargs.push('/shop/virtual/storesearch.html'); 
                    } 
                } else { 
                    var clkLink = jQuery('#ViewMoreStoreLink'); 
                    wtargs.push('DCSext.wtLinkName'); 
                    wtargs.push(clkLink.data('linkName')); 
                    wtargs.push('DCSext.wtLinkLoc'); 
                    wtargs.push(clkLink.data('linkLoc')); 
                    wtargs.push('DCSext.wtZipCode'); 
                    wtargs.push(ATT.globalVars.zip()); 
                    wtargs.push('DCS.dcsuri'); 
                    wtargs.push(window.location.pathname); 
                    SLIcombineCtaSearchWtEvt = false; 
                } 
                wtargs = ATT.cqWebtrendsli.ecParams(wtargs); 
                ATT.cqWebtrendsli.removeSuccessMeta(); 
                window.dcsMultiTrack.apply(this, wtargs);       
                if (isError) { 
                    ATT.cqWebtrendsli.SearchError(); 
                } else { 
                    ATT.cqWebtrendsli.SearchResult(resultPage, n, wtNoHit, false); 
                    SLIsavedLoc = 'true'; 
                }                      
            } 
            this.SearchResult = function(resultPage, n, wtNoHit, wtclick) { 
                var wtargs = []; 
                ATT.cqWebtrendsli.removeSuccessMeta(); 
                window.dcsMultiTrack.apply(this, ATT.cqWebtrendsli.searchResultParams(wtargs, resultPage, n, wtNoHit, wtclick)); 
                SLIhadError = false;                             
            } 
            this.SearchError = function() { 
                var wtargs = []; 
                ATT.cqWebtrendsli.removeSuccessMeta(); 
                window.dcsMultiTrack.apply(this, ATT.cqWebtrendsli.errorResultParams(wtargs)); 
                SLIhadError = true;                         
            } 
            this.linkClick = function() { 
                var dcsuri = (jQuery('#storeData').html() == '') ? '/shop/virtual/storeresultserror1.html' : '/shop/virtual/storeresults'+SLIcurrentPageNum+'.html'; 
                var wtargs = ['DCS.dcsuri',dcsuri,'DCSext.wtZipCode', ATT.globalVars.zip(), 'DCSext.wtBuyFlowCode', ATT.globalVars.flowcode, 'DCSext.wtCustType', 'Consumer', 
                    'DCSext.wtNoHit', '1', 'DCSext.wtLinkLoc', jQuery('#PrintStoreDetail').data('linkLoc'), 'DCSext.wtLinkName', jQuery('#PrintStoreDetail').data('linkName')]; 
                ATT.cqWebtrendsli.removeSuccessMeta(); 
                window.dcsMultiTrack.apply(this, wtargs);                            
            } 
            this.directionClick = function(v_StoreDirectionDetails) { 
                var dcsuri = '/shop/virtual/storeresults'+SLIcurrentPageNum+'.html' 
                var storeSearchOrigin = jQuery('#searchLocation').val(); 
                var wtargs = ['DCSext.wtEvent', 'HRock_StoreInv_Directions_Click', 'DCSext.wtSuccessFlag', '1', 'DCSext.wtNoHit', 1, 'DCSext.wtCustType', 'Consumer', 
                    'DCSext.wtBuyFlowCode', ATT.globalVars.flowcode, 'DCSext.wtStatusCode', '0', 'DCSext.wtSku', 'sku6300321',  
                    'DCSext.wtStoreDirectionDetails', v_StoreDirectionDetails, 'DCSext.wtPageNum', SLIcurrentPageNum, 'DCSext.wtUserResp', storeSearchOrigin, 
                    'DCSext.wtLinkName', 'Directions', 'DCSext.wtLinkLoc', 'HRock SearchResultsModal Body', 'DCS.dcsuri', dcsuri, 'DCSext.wtZipCode', ATT.globalVars.zip() 
                ]; 
                ATT.cqWebtrendsli.removeSuccessMeta(); 
                window.dcsMultiTrack.apply(this, ATT.cqWebtrendsli.ecParams(wtargs)); 
            } 
        } // end of ATT.cqWebtrendsli 
    </script> 
