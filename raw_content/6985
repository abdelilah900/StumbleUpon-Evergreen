<script type="text/javascript"> 
	var $ = jQuery; 
	if (typeof encodedReviewerId == "undefined"){ 
		var encodedReviewerId = ""; 
	} 
	$(document).ready(function(){ 
		function bvLogin(callback) { 
			var rInfo = $("#rInfo"); 
			$.post(rInfo.attr("action"),rInfo.serialize(),callback,"json"); 
		} 
		if(typeof $BV != "undefined"){ 
			$BV.configure("global", { 
				allowSamePageSubmission: true, 
				userToken: encodedReviewerId, 
				/*doShowContent: function() { 
				},*/ 
				doLogin: function(successCallback, successUrl) { 
					jQuery.when(ATT.globalVars.cartPromise).done(function() { 
						$("#bv-zip-group").toggle(!ATT.globalVars.cartContents.userZip); 
					}); 
					$("#BVContent").hide(); 
					$("#BVLogin").show(); 
					$(".submitReviewNext").click(function(){ 
						ATT.modalSpinner.show(); 
						bvLogin(function(resp) { 
							if (resp.status == "success") { 
								var encodedReviewerId = resp.data.encoded_user_string; 
								successCallback(encodedReviewerId); 
								$("#BVContent").show(); 
								$("#BVLogin").hide(); 
								$(".BVRRCustomLinksWrapper").hide(); 
							} else { 
								$(document).trigger("FormResponseReturned",resp); 
							} 
						ATT.modalSpinner.close(); 
						}); 
					}); 
				} 
				/*doShowSubmission: function() { 
					myExampleShowLightbox("Submission_Lighbox"); 
				}, 
				onSubmissionReturn: function() { 
					myExampleCloseLightbox("Submission_Lighbox"); 
				}, 
				doScrollSubmission: function() { 
					myExampleScrollToSubmission(); 
				return false; 
				}*/ 
			}); 
		}; 
		if(typeof $BV != "undefined"){ 
			$BV.ui("rr", "show_summary", { 
				productId: "sku6300321", 
				summaryContainerDiv: "BVRRSummaryContainerTab", 
				onEvent: function() { 
					// Hide the reviewsStarsDiv in the "Reviews" Tab if there are no reviews present 
					if ($("#BVRRSummaryContainerTab").is(":hidden")) { 
						$("#reviewsStarsDiv").hide(); 
					} 
				}  
			}); 
		}; 
		if(typeof $BV != "undefined"){ 
			$BV.ui("rr", "show_reviews", { 
				productId: "sku6300321",  
				submissionContainerDiv: "BVRRContainer" 
				/*doShowContent: function() { 
					alert('showReviews'); 
				}*/ 
			}); 
		}; 
		$("#serviceAgreementCheck").change(function () { 
			if ($("#serviceAgreementCheck").attr("checked")) { 
				$(".submitReviewNext").show(); 
				$(".submitReviewNextDisabled").hide(); 
			}else{ 
				$(".submitReviewNext").hide(); 
				$(".submitReviewNextDisabled").show(); 
			} 
		}) 
		.change(); 
		$(".submitReviewCancel").click(function () { 
			$("#BVContent").show(); 
			$("#BVLogin").hide(); 
		}) 
	}); 
