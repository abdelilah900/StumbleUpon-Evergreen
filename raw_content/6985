    <script> 
        jQuery(document).ready(function() { 
            jQuery("#priceBlockAjax").load( 
                '/shop/wireless/devices/htc/one-xplus-carbon-black.pricearea.xhr.html', 
                'skuId=sku6300321&pageType=deviceDetails',  
                function(response, status, xhr) {  
