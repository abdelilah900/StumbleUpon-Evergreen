<script type="text/javascript"> 
	var $ = jQuery, pathname = window.location.href; 
	$(document).ready(function() {  
		// on tab click, add/remove class to hide/show bottom border under the stars in the Reviews Tab 
		$("a","ul.tabs").bind("click", function(){  
			if($(this).parent().attr("id") === "reviewsLabelDiv"){ 
				$("#reviewsStarsDiv").addClass("reviewsStarsDivCurrent");  
			}else{ 
				$("#reviewsStarsDiv").removeClass("reviewsStarsDivCurrent"); 
			}; 
		}); 
	}); 
	(function(){ 
		//On page load, add class to hidebottom border under the stars in the Reviews Tab 
		if (pathname.indexOf("tab2") > -1 || pathname.indexOf("tab4") > -1) { 
			$("#reviewsStarsDiv").addClass("reviewsStarsDivCurrent");  
		} 
	}());	 
