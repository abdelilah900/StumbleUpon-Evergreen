<script type="text/javascript"> 
	if (window.location.href.match('-sim-')) { 
		ATT.ShoppingCart.hijack(10,'existingEquipmentModal', function (dfd, target, order) { 
			//add a deferred object to target addToCart button to hijack atc 
			thisDfd = dfd; 
			//popup modal and grab a handle to the html somehow 
			ATT.CartSummary.showExistingEquipmentModal('/shop/wireless/modals/shopmodals/jcr:content/mainpar/existingequipment.html?qty='+jQuery('#qtyAllow').val()); 
			//use the handle to bind to the submit button or whatever 
			jQuery('#imei-continue-btn').live('click', function () { 
				// add imei to data-extra-params 
				var	extraIMEIParam = "&imei=", imeis = jQuery('.imei-input'); 
				for (var i=0;i < imeis.length; i++) { 
					if (i != 0) extraIMEIParam += ","; 
					extraIMEIParam += encodeURIComponent(imeis[i].value); 
				} 
				order.params += extraIMEIParam; 
				//only after deferred is resolved will atc continue 
				thisDfd.resolve(); 
			}); 
			jQuery(document).bind('CartFailure', function() { 
				ATT.ShoppingCart.removeHijacksByName('existingEquipmentModal'); 
				contBtn = jQuery('#imei-continue-btn'); 
				contBtn.die(); 
				contBtn.live('click', function () { 
					var extraIMEIParam = "&imei="; 
					var imeis = jQuery('.imei-input'); 
					for (var i=0;i < imeis.length; i++) { 
						if (i != 0) extraIMEIParam += ","; 
						extraIMEIParam += encodeURIComponent(imeis[i].value); 
					} 
					jQuery('.addToCart').data('extraParams',extraIMEIParam); 
					jQuery('.addToCart').click(); 
				}); 
			}); 
		}); 
	} 
jQuery(document).ready(function(){ 
	if (window.location.href.match('-prepaid') || window.location.href.match('-gophone')) { 
		ATT.ShoppingCart.removeHijacksByName('customerTypeModal'); 
		jQuery('.addToCart, .addPackageToCart, .replaceCart').live('click',function(){ATT.ShoppingCart.buyFlowTypeShown();}); 
	} 
	jQuery(document).delegate('#searchLocation','keypress', function(e){ 
		if (e.which == 13) { 
			e.preventDefault(); 
			submitSLISearch(); 
		} 
	}); 
}); 
</script> 
