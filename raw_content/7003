<script type="text/javascript">

$(document).ready(function(){
		
	// equal height
	
	function equalHeight(group) {
		var tallest = 0;
		group.each(function() {
			var thisHeight = $(this).height();
			if(thisHeight > tallest) {
				tallest = thisHeight;
			}
		});
		group.height(tallest);
	}
	
	equalHeight($('#footer .widget'));
	
});

</script>
 
