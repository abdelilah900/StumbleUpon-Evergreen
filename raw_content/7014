var currentDate=(year+"-"+month+"-"+day);String(currentDate);if(pageName==''||pageName===undefined)
{s.pageName='health.howstuffworks.com/wellness/drugs-alcohol/hangover8.htm';}
else
{s.pageName=pageName;}
