//JCA - Documented 4/12/2010
//This is the universal function for displaying password retrieval.
//It sets the IFrame source and calls the showModal function.
function LaunchPassword(DesignType) {
    
    var designtypefield=document.getElementById('ctl00_pageHeader_designtypeHF');
    designtypefield.value=DesignType

    var iFrameObj = document.getElementById('passwordIF');
    var divObj = document.getElementById('password');
    iFrameObj.src="/popovers/password/Default.aspx"

    Popup.showModal('password');
    Popup.hide('login');


}

