    iFrameObj.src="/popovers/site_owner.aspx"
    Popup.showModal('SiteOwner');
}

//KL - Documented 5/27/2010
//This is the universal function for displaying the BudgetCalc Window
//It sets the IFrame source and calls the showModal function.
function LaunchBudgetCalc(DesignType) {
    var iFrameObj = document.getElementById('BudgetCalcIF');
    var divObj = document.getElementById('BudgetCalc');
    var budget, numOfPeople, TypeEvent
	 try {
	    budget = parent.document.aspnetForm.ctl00_sideBarCPH_shopByBudget_form1_budget.value;
	    numOfPeople = parent.document.aspnetForm.ctl00_sideBarCPH_shopByBudget_form1_numPeople.value;
	    if (parent.document.aspnetForm.ctl00_sideBarCPH_shopByBudget_form1_RadioButton1.checked == true) {
	    TypeEvent = parent.document.aspnetForm.ctl00_sideBarCPH_shopByBudget_form1_RadioButton1.value;
	    }else{
	    TypeEvent = parent.document.aspnetForm.ctl00_sideBarCPH_shopByBudget_form1_RadioButton2.value;
	    }
	 }
	 catch(err)
	 {
	    //KL 6/3/2010
	    //This was added because this function can be called from the footer, as well as
	    //the form that was intended to call it.  This simply sets the default values if 
	    //the form is unavailable.
	    budget = 0;
	    numOfPeople = 0;
	    TypeEvent = "Total";
	 }
    iFrameObj.src="/popovers/BudgetCalculator.aspx?budget=" + budget + "&numOfPeople=" + numOfPeople +"&TypeEvent=" + TypeEvent
    Popup.showModal('BudgetCalc');
}

