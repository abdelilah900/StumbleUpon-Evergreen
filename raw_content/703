//alert(document.getElementById("ctl00_pageHeader_promo_holder").style.background)
var timeoutId = window.setTimeout("ActivateCountDown('TimeOutModalContent', 120);",10680000); 
var _countDowncontainer=0;
var _currentSeconds=0;

function upDateSession(modalId)
{
    if(_currentSeconds <= 0)
    {
        Popup.hide(modalId); 
        window.location = '/'

    } else {
    
      upDateTimer();
    __doPostBack('ctl00_pageHeader_CartUpdatePanel');
    Popup.hide(modalId); 
    
    }
  
}

function upDateTimer()
{
window.clearTimeout(timeoutId);
timeoutId = window.setTimeout("ActivateCountDown('TimeOutModalContent', 120);",10680000); 
}


 var milisec=0 
 var seconds=30 
 
function timer()
{
 if (milisec<=0){
 milisec=9 
    seconds-=1 
 } 
 if (seconds<=-1){ 
    milisec=0 
    seconds+=1 
 } 
 else 
    milisec-=1 
    setTimeout("display()",100) 
} 

 function ActivateCountDown(strContainerID, initialValue)
 {    _countDowncontainer = document.getElementById(strContainerID);
      if (!_countDowncontainer)
      {
            alert("count down error: container does not exist: "+strContainerID+"\nmake sure html element with this ID exists");
            return;
      }        
      Popup.showModal('TimeOutModalDiv');
      SetCountdownText(initialValue);
      window.setTimeout("CountDownTick()", 1000);
      } 
      
      function CountDownTick()
      {    
      if (_currentSeconds <= 0)
      {
      _countDowncontainer.innerHTML = "Your session has expired.<br /><br />Please click the \"OK\" button to return to the home page.";
      return;
      }
      
      SetCountdownText(_currentSeconds-1);
      window.setTimeout("CountDownTick()", 1000);
      }
      
      function SetCountdownText(seconds)
      {   
       _currentSeconds = seconds;
       var minutes=parseInt(seconds/60);
       seconds = (seconds%60);
       var hours=parseInt(minutes/60);
       minutes = (minutes%60);
       var strText = AddZero(minutes) + ":" + AddZero(seconds);
       _countDowncontainer.innerHTML = "Your session will expire in " + strText + ".<br /><br />Click the \"Ok\" button to keep your session active.";
       
       } 
       
       function AddZero(num)
       {
           return ((num >= 0)&&(num < 10))?"0"+num:num+"";
       } 


	function remove_account()
    {
        
        var LO = document.getElementById('ctl00_pageHeader_LoggedOut');
        
        LO.value = 'True';
    }


</script>

