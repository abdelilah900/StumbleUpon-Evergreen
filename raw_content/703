<script language="javascript" type="text/javascript">

    var winLocationH = window.parent.location.href.toLowerCase();
	if(winLocationH != null)
	{
	    if(winLocationH.indexOf('https')==0) {
		    if(winLocationH.indexOf('checkout')==-1) {
		    window.location.href = winLocationH.replace("https", "http");
		    //alert(winLocationH.replace("https", "http"));
		    }
	    } else {
	         if(winLocationH.indexOf('checkout') > 0)
	            {
	                if(winLocationH.indexOf('https')==-1) 
	                    window.location.href = winLocationH.replace("http", "https");
	            }
	    }
    }

function userMovingWithin()
{
	try{
		movingWithinSite = true;	
	}
	catch(err){}
}

//SMB - Documented 3/16/2010
//This function fires when the user clicks Terms and Conditions link when saving a design.
function showTerms()
{
     tb_show("Design Terms","/popovers/designterms.aspx?height=200&width=200&KeepThis=true&TB_iframe=true&modal=true","");
}
//SMB - Documented 3/16/2010
//This function fires when the user clicks the Image Tips or Do's and Don'ts when adding a Face.
function showPhotoTips()
{
    tb_show("Photo Tips","/popovers/phototips.aspx?height=200&width=200&KeepThis=true&TB_iframe=true&modal=true","");
}

//SMB - Documented 3/16/2010
//This function fires when the user clicks the cancel button on either
//the login or registration screens.  It hides the DIV and calls the 
//TellFlashLoginStatus and passes it false to tell Flash that the user 
//clicked cancel.
function modalClose(DontTellFlashStatus)
{
	if (DontTellFlashStatus!=true)
	    TellFlashLoginStatus(false,'');
	var iFrameObj = document.getElementById('loginIF');
	var divObj = document.getElementById('login');
	iFrameObj.src="/blank_wait.html"
	var iFrameObj = document.getElementById('registerIF');
	var divObj = document.getElementById('register');
	iFrameObj.src="/blank_wait.html"
	Popup.hide('password');
	Popup.hide('login');
	Popup.hide('register');
	Popup.hide('shippingInfo');
	Popup.hide('holidayShippingInfo');
	Popup.hide('pricelistInfo');
	Popup.hide('ppPricelistInfo');	
	Popup.hide('TellAFriend');	
	Popup.hide('SiteOwner');
	Popup.hide('PromoDescription');
	Popup.hide('BudgetCalc');
	Popup.hide('volumeDiscounts');
	Popup.hide('Packaging_info');
	Popup.hide('dosanddonts');
	Popup.hide('imageTips');
	Popup.hide('Details');
	Popup.hide('termscondInfo');
}

//JCA - Documented 3/16/2010
//This function fires when the user clicks retrieve password link on the
//login form.
function ForgotPassword()
{
    var iFrameObj = document.getElementById('passwordIF');
    var divObj = document.getElementById('password');
    iFrameObj.src="/popovers/password/default.aspx"

    Popup.hide('login');
    Popup.show('password');
    
    return false;
}

//SMB - Documented 3/16/2010
//This function fires when the user clicks the submit button on either
//the login or registration screens.
function modalDone(blnAuthenticated,DesignType) {
            
    TellFlashLoginStatus(blnAuthenticated,DesignType);
    __doPostBack('ctl00_pageHeader_CartUpdatePanel');
    var iFrameObj = document.getElementById('loginIF');
    var divObj = document.getElementById('login');
    iFrameObj.src="/blank_wait.html"
    var iFrameObj = document.getElementById('registerIF');
    var divObj = document.getElementById('register');
    iFrameObj.src="/blank_wait.html"
    Popup.hide('register');
    Popup.hide('login');
}

//SMB - Documented 3/16/2010
//Returns the Flash object that pertains to the movieName parameter.
//Specifically is used for the Utility.
function getFlashMovie(movieName) {
	var isIE = navigator.appName.indexOf("Microsoft") != -1;	
	return (isIE) ? window[movieName] : document[movieName];
}


//SMB - Documented 3/16/2010
//This is called by modalClose and modalDone to notify flash
//if the user logged in successfully or not.
function TellFlashLoginStatus(blnAuthenticated,DesignType) {
	
    try{
		designtypefield=document.getElementById('ctl00_pageHeader_designtypeHF');
		getFlashMovie("flashshell").jsLogin(blnAuthenticated,designtypefield.value);
	}
	catch(err){}
	
}

//JCA - Documented 4/12/2010
//This is the universal function for displaying the shipping information
//It sets the IFrame source and calls the showModal function.
function LaunchShippingInfo(DesignType) {
    
    var iFrameObj = document.getElementById('shipinfoIF');
    var divObj = document.getElementById('shippingInfo');
    iFrameObj.src="/popovers/shipping_information.aspx"
    Popup.showModal('shippingInfo');
}

