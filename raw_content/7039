    var windowprops = "width=320,height=270,location=no,toolbar=no,menubar=no,scrollbars=yes,resizable=yes";    
    var CurrentURl=window.location.href.toLowerCase();
    var isOpen = true;
    var intCounter;    
    var pages = "recipes/seasonal-recipes/spring-recipes,recipes/seasonal-recipes/fresh-ingredients,recipes/seasonal-recipes/fresh-ingredients?keycode=zmag,recipes/ingredients/strawberry-recipes,recipes/ingredients/pea-recipes,recipes/ingredients/rhubarb-recipes,recipes/ingredients/asparagus-recipes,recipes/ingredients/artichoke-recipes";    
    var arrPages = pages.split(',');    
	for (intCounter=0;intCounter<arrPages.length;intCounter++)
	{
		if(CurrentURl.indexOf(arrPages[intCounter]) >= 0)
		{			
			isOpen = false;
			break;
		}
	} 
    if(document.cookie)
    {
       if((document.cookie.indexOf("TOH=entry_pop_up")<0) && (isOpen == true))
        {
            var today = new Date();
            var expire = new Date(); 
            expire.setTime(today.getTime() + 3600000*24*1);
            document.cookie = "TOH=entry_pop_up;domain=tasteofhome.com;expires="+expire.toGMTString()+"; path=/";            
            win = window.open(page, "pop", windowprops);
            win.blur();             		    
        }
    }
