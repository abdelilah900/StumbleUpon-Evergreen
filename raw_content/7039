<script type="text/javascript">

	function ScrollToReviewSection() {
		var reviewContainer = document.getElementById("ReviewHeroContainer");

		var selectedPosX = 0;
		var selectedPosY = 0;

		while (reviewContainer != null) {
			selectedPosX += reviewContainer.offsetLeft;
			selectedPosY += reviewContainer.offsetTop;
			reviewContainer = reviewContainer.offsetParent;
		}
		window.scrollTo(selectedPosX, selectedPosY);
	}

	
</script>




