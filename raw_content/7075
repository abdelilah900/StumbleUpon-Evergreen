				/* <![CDATA[ */
				var tags = new Array();
				
					tags[tags.length] = {name:"chris-christie", url:"http://topics.nj.com/tag/chris-christie/index.html", rank: "1"};
				
					tags[tags.length] = {name:"pension", url:"http://topics.nj.com/tag/pension/index.html", rank: "6"};
				
					tags[tags.length] = {name:"stephen-sweeney", url:"http://topics.nj.com/tag/stephen-sweeney/index.html", rank: "6"};
				
				function compareNames(a, b) {
					var nameA = a.name.toLowerCase( );
					var nameB = b.name.toLowerCase( );
					if (nameA < nameB) {return -1}
					if (nameA > nameB) {return 1}
					return 0;
				}
				tags.sort(compareNames);
				for (i = 0; i < tags.length; i++) {
					var obj = tags[i];
					jQuery("#TagHolder").append("<span class=\"rank" + obj.rank + "\"><a href=\"" + encodeURI(obj.url) + "\">" + obj.name + "</a></span>\n ");
				}
				/* ]]> */
				</script>
