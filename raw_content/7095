			has_bb = "False";				
		}
		else {
			if (has_bb > 0) {
			has_bb = "True";
			}				
		}	
		I09839.DM_cat("Woman's Day > Lifestyle");
		I09839.DM_addEncToLoc("Beauty Book Status",has_bb);
		I09839.DM_tag();
	});	
//-->
</SCRIPT>
