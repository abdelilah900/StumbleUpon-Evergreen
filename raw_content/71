<script type="text/javascript">
(function() {
	if( HPBrowser.isIE6() ) return false;

	var onIMGMouseOver = function (event, matchedEl) {
	
		if( "quickread_link" == matchedEl.className )
		{
			var entry_link = '';
			var entry_id = 0;

			if( null != matchedEl.parentNode.href.match(/_([n|b])_(\d{2})(\d+)\./) ) {
				entry_link = matchedEl.parentNode.href;
			} else if ( matchedEl && matchedEl.id && null != matchedEl.id.match(/image_(\d+)/) ) {
				entry_id = matchedEl.id.match(/image_(\d+)/);
				entry_link = $('read_post_'+entry_id[1]).innerHTML;
			} else if ( matchedEl.parentNode && matchedEl.parentNode.id && null != matchedEl.parentNode.id.match(/entry_id_(\d+)/) ) {
				entry_id = matchedEl.parentNode.id.match(/entry_id_(\d+)/);
				if( typeof entry_id[1] != "undefined"
					&& entry_id[1] != 0 ) 
				{
					entry_link = $('read_post_'+entry_id[1]).innerHTML;
				} else {
					return false;
				}
			} else {
				return false;
			}


			html = '<div class="quickread_hover" onclick="return QV.pop(this,'+'\''+entry_link+'\''+');"></div>';
	
			matchedEl.setAttribute('on_hover', 'yes');
			
			if(matchedEl.getAttribute('floating_id'))
				return false;
			FloatingPrompt.embed(
				matchedEl, 
				html, 
				undefined, 
				'top-right',
				{
					ignore_arrow:true,
					overlay_mouseout:false,
					width:119, 
					add_xy:[-119,0], 
					class_name: 'fp_quickread'
				}
			);
		}
	};
	
	if( $('featured_content') ) {
		// top content
		$('featured_content').onmouseover = function(event){
		
			event = event || window.event;
			
			//IE uses srcElement as the target
			var target = event.target || event.srcElement;    
			
			if( "IMG" != target.tagName )
				return false;
			
			onIMGMouseOver(target.tagName, target);
		};
	}
	if($('sidebar_right')) {
		// right column
		$('sidebar_right').onmouseover = function(event) {
			
			event = event || window.event;
			
			//IE uses srcElement as the target
			var target = event.target || event.srcElement;    
			
			if( "IMG" != target.tagName )
				return false;
				
			onIMGMouseOver(target.tagName, target);
		};
	}	
	

})();


</script>

