	/* use hot keys for nav switching */
	
	/* left arrow */
	var klLeft = new YAHOO.util.KeyListener(document, { ctrl:false, keys:37 }, function(event, element) {getBNPage('prev', (element[1].srcElement || element[1].target).nodeName );
	} );
	/* right arrow */
	var klRight = new YAHOO.util.KeyListener(document, { ctrl:false, keys:39 }, function(event, element) {getBNPage('next', (element[1].srcElement || element[1].target).nodeName );} );

	/* for fullscreen slidehow */
		
	/* init */
	klLeft.enable();
	klRight.enable();

</script>


