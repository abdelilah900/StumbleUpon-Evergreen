	var current_pic, storageName = 'fabShown';
	var shown = (hasLocalStorage() && localStorage.getItem(storageName)!=null) ? localStorage.getItem(storageName) : "";
	var fab_spinner = new bf_spinner('cute_spinner', {lines:15,length:30,width:10,radius:40,color:'#36D0F9',speed:1.4,trail:59,shadow:true,hwaccel:true});

	var next_pic = function() {
		if (fab_spinner.active()) {setTimeout("fab_spinner.stop()",500);}

		if (current_pic) {
			if (hasLocalStorage()) {
				// Store Last 50 'cutes'
				lsArray = shown.split(',');
				while (lsArray.length > 50) {lsArray.shift();} 
				lsArray.push(current_pic.campaignid);
				shown = lsArray.join(',');
				localStorage.setItem(storageName, shown);
			}
		}

		if ( fab_or_drab_list.length > 0 ) {
			current_pic = fab_or_drab_list.shift();
			if (shown) {
				while (shown.match(current_pic.campaignid) == current_pic.campaignid) {
					if (fab_or_drab_list.length > 0) {current_pic = fab_or_drab_list.shift();}
					else {close_fab(); return;}
				}
			}

			$('fab_or_drab_img').setAttribute('src', BF_STATIC.image_root + current_pic.image);
			$('fab_or_drab_title').update(current_pic.name);
			$('fab_or_drab_source').update(current_pic.source.replaceAll(' ', '&nbsp;'));
			if (!$('fab_or_drab_wrapper').visible()) { $('fab_or_drab_wrapper').show(); }
		}
		else {close_fab();}
	}
	
	var vote_pic = function(b) {
		fab_spinner.start();
		var badge = $(b).getAttribute('rel:badge');
		var params = {
			buzz_id: current_pic.campaignid,
			category: 'badge',
			uri: current_pic.username + '/' + current_pic.uri,
			action: 'vote',
			badge_id: $(b).getAttribute('rel:badge_id'),
			badge: badge
		};
		badge_vote_manager.ajax.request( badge_vote_manager.VOTE_CONTROLLER, {
			method: 'get',
			parameters: params,
			onSuccess: function(r) {
				// Share Buttons
				var share_message = badge == "fab" ? "Fab!" : "Drab!";
				$('fab_or_drab_results_shared').childElements().each(function(el) {$(el).remove();});
				$('fab_or_drab_results_shared').insert("<a class=\"button2 big facebook-share-button\" rel:gt_act=\"share/facebook/share\" type=\"box_count\" target=\"_new\" style=\"float:left;margin: 1px 10px 0 0;\" onclick=\"var sTop = window.screen.height/2-(218); var sLeft = window.screen.width/2-(313);window.open(this.href,'sharer','toolbar=0,status=0,width=626,height=436,top='+sTop+',left='+sLeft);return false;\" href=\"https://www.facebook.com/sharer/sharer.php?s=100&p%5Btitle%5D="+current_pic.name.replaceAll('"', "&quote;")+"&p%5Bsummary%5D="+share_message+"&p%5Burl%5D=http://"+fab_buzz_full_path()+"&p%5Bimages%5D%5B0%5D="+BF_STATIC.image_root + current_pic.image+"\"><span class=\"icon\">&nbsp;</span><span class=\"word\">Share</span></a>");
				$('fab_or_drab_results_shared').insert("<a rel:gt_act=\"share/pinterest\" target=\"_blank\" href=\"http://pinterest.com/pin/create/button/?url="+fab_buzz_full_path()+"&media="+BF_STATIC.image_root + current_pic.image+"&description="+share_message+"\" class=\"bf_dom shareBtn big\"><div class=\"pi_icon\"></div><div class=\"label\">Pin it</div></a>");

				// Update UI
				current_pic[params.badge + '_votes'] = Number(current_pic[params.badge + '_votes']) + 1;
				var fab_votes = Number(current_pic.fab_votes);
				var drab_votes = Number(current_pic.drab_votes);
				var total_votes = fab_votes + drab_votes;
				$('fab_or_drab_small_img').setAttribute('src', BF_STATIC.image_root + current_pic.image);
				$('fab_count').update(Math.round((fab_votes/total_votes)*100) + '% Fab');
				$('drab_count').update(Math.round((drab_votes/total_votes)*100) + '% Drab');
				$('fab_or_drab_results').show();

				// load next picture
				next_pic();
			}
		});
	}

	$$('#vote_fab, #vote_drab').each(function(b) {
		$(b).observe('click', function(){
			var user = new BF_User();
			if (user.isLoggedIn()) {vote_pic(b);}
			else { 
				bf_login.loggedInCallbacks.push(function() { vote_pic(b); });
				signin_anywhere.open({ el: $(b) }); 
			}
		});
	});
	
	var close_fab = function() {
		$('fab_or_drab_come_back_later').show();
		$$('#fab_or_drap_header, #fab_or_drab_img, #fab_or_drab_votes').each(function(el){
			$(el).hide();
		});
	}

	var fab_buzz_full_path = function() {
		return BFWebPath() + '/' + current_pic.username + '/' + current_pic.uri;
	}

	if (fab_or_drab_list.length > 0) { next_pic(); }
	else {close_fab();}
</script>
