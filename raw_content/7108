        jQuery(function($){
            CN.style.collections.setConfig({
                                                ajaxRequest     :   {
                                                                        cache               :   true
                                                                    },
                                                todaysShow      :   {
                                                                        dataURL             :   "http://www.style.com/rest/fashion-show/last24hours",
                                                                        tabHeaderText       :   "latest shows"
                                                                    },
                                                yesterdayShow   :   {
                                                                        dataURL             :   "http://www.style.com/rest/fashion-show/yesterday"
                                                                    },
                                                highlights      :   {
