<script>

    var _ghearst_vars = (function() {
    var metaKeyWords = (function(){
    var metaKeywordContent='';
    metaCollection = document.getElementsByTagName('meta');
    for (i=0;i<metaCollection.length;i++) {
        nameAttribute = metaCollection[i].name.search(/keywords/);
        if (nameAttribute!= -1) {
            metaKeywordContent = metaCollection[i].content;
            metaKeywordContent = metaKeywordContent.replace(/,/g,'%2C'); // to replace ,
            metaKeywordContent = metaKeywordContent.replace(/ /g,''); // to replace space
        }
    }
    return metaKeywordContent;
    })();
    return {
    'ams_ads_script_src': 'http://subscribe.style.com/ams/page-ads.js?ad_category_prefix=fashionshows&browser_path=%2Ffashionshows%2Fcomplete%2FS2013RTW-YSLRG&cat_prefixes=%2Cfashionshows%2Ccomplete%2CS2013RTW-YSLRG&keywords='+metaKeyWords+'&position_list=AMS_STY_CONTENTPAGE_NAVBAR_SUBSCRIBELINK%2CAMS_STY_CONTENTPAGE_RIGHTRAIL_A&site_prefix=style&subdomain=www&url_name=S2013RTW-YSLRG'
    };
    })();
