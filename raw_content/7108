            var tabhtml="",
                active,
                i=0;
            for(; i < tabIDs.length; i++) {
                active = (i == selectedTab) ? " active" : "";
                tabhtml+="<a id=\""+tabIDs[i]+"\"class=\"slider_tab"+active+"\">"+tabTitles[i]+"</a>";
            }
            el.html(tabhtml);
            if(selectedTab != 0){
                el.hide();
            }
        })(jQuery("#tab_nav_mu"))
