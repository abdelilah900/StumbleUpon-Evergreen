		if(jQuery('#carousel_13319213366fe0d54e88').outerHeight(true) < jQuery('#carousel_13319213366fe0d54e88 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_13319213366fe0d54e88').animate({
				height: jQuery('#carousel_13319213366fe0d54e88 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_13319213366fe0d54e88').children('li:last').detach();
		jQuery('#carousel_13319213366fe0d54e88').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_13319213366fe0d54e88').animate({
			height: jQuery('#carousel_13319213366fe0d54e88 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
</script>
<script type="text/javascript">
	</script>
</div>
					</div>
					<div class="clear"></div>




