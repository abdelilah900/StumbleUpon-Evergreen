            	function validateWebForm() {
            		var error = "";
            		if( (document.getElementById('fm_daily').checked == 0) && (document.getElementById('fm_monthly').checked == 0) ) {
            			error += "please select a newsletter\n";
            		}
            		if(error != "") {
            			alert(error);
            			return false;
            		} else {
                        return true;
                    }
            	}
