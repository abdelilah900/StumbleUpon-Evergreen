<script type='text/javascript'>
	jQuery(document).ready(function(){
		
		jQuery('a.loginbuttontrigger').click(function(evt){
			evt.preventDefault();
			var $this = jQuery(this);
			var pos = $this.position();
			var _left = (pos.left+($this.width()/2)) - 100;
			var _top = pos.top + $this.height();
			jQuery('#login_layer').css({left:_left+'px', top:_top+'px' }).fadeIn();
		});
		jQuery('a#close_login_button').click(function(evt){
			evt.preventDefault();
			jQuery('#login_layer').fadeOut();
		});
		
		jQuery('#frmlogin #username').focus(function(){
			if(jQuery(this).val() == "e-mail"){
				jQuery(this).val('');
			}
		});
		jQuery('#frmlogin #username').blur(function(){
			if(jQuery(this).val() == ""){
				jQuery(this).val('e-mail');
			}
		});
		jQuery('#frmlogin #fake_password').focus(function(){
			jQuery(this).hide();
			jQuery('#frmlogin #password').show().focus();
		});
		jQuery('#frmlogin #password').blur(function(){
			if(jQuery(this).val() == ""){
				jQuery(this).hide();
				jQuery('#frmlogin #fake_password').show();
			}
		});

	});	
	
	
function checklogin()
{
	if(jQuery.trim(jQuery('#frmlogin #username').val()) == "e-mail")
	{
		show_dialog("Please enter your Email Address","Form Error",{
               "Ok": function(){
               	window._common_dialog_layer.dialog('close');
               	jQuery('#frmlogin #username').focus();
               	}
            });
	}else	if(jQuery('#frmlogin #fake_password:visible').size() != 0)
	{
		show_dialog("Please enter your password","Form Error",{
               "Ok": function(){
               	window._common_dialog_layer.dialog('close');
               	jQuery('#frmlogin #fake_password').focus();
               	}
            });
	}else {
		var frm = jQuery('#frmlogin')[0];
		frm.onsubmit='';
		frm.submit();
	}
}
function do_login_prompt(obj){
	var $this = jQuery(obj);
	var pos = $this.position();
	var _left = (pos.left+($this.width()/2)) - 100;
	var _top = pos.top + $this.height();
	jQuery('#login_layer').css({left:_left+'px', top:_top+'px' }).fadeIn();
}
</script>
