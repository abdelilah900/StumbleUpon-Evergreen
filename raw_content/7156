<div id="content_well">




<!-- TMPL content_article -->
<!-- TMPL content_unit_supplement -->
<div class="supplement">
        
        <div class="image r2_b1GRA">
                <div class="cbt"><div class="cbl"></div><div class="cbr"></div></div>
                <div class="cbc">

<img
	
	src="/cm/marieclaire/images/wR/janet-mock-0611-2-mdn.jpg" alt="janet mock"  flipboard-src="/cm/marieclaire/images/D2/janet-mock-0611-2-xl.jpg"
