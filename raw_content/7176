<script type="text/javascript">
//<![CDATA[
function flagReasonChanged(article_id){
	if (document.getElementById('flagReasonsSelect').value == '') return false;
	
	if (document.getElementById('flagReasonsSelect').value == 'redirect'){
		document.getElementById('masterCategoriesDiv').style.display='block';
		document.getElementById('palagrizedUrlDiv').style.display='none';
	}
	else if (document.getElementById('flagReasonsSelect').value == 'plagiarism'){
		document.getElementById('palagrizedUrlDiv').style.display='block';
		document.getElementById('masterCategoriesDiv').style.display='none';
	}
	else
		doFlagIt(article_id);
		
}           
           
function doFlagIt( article_id)
{
   var reason = document.getElementById('flagReasonsSelect').value;
   var to_master_category_id = document.getElementById('masterCategoriesSelect').value;
   var palagrized_url = encodeURIComponent(document.getElementById('palagrizedUrl').value);

   if (reason == 'plagiarism' && palagrized_url.indexOf('http') < 0) return false;
   if (reason != 'plagiarism') palagrized_url = '';
	
   document.getElementById('flagit_div').innerHTML = '';
   document.getElementById('flagit_div').innerHTML = '<p>Thank you!</p>';
   var mysack = new sack( 
       "/wp-content/mu-plugins/w4l/w4l_flag_it_ajax.php" );    

  mysack.execute = 1;
  mysack.method = 'POST';
  mysack.setVar( "article_id", article_id );
  mysack.setVar( "by_writer_id", triond_writer_id);
  mysack.setVar( "reason", reason);
  mysack.setVar( "to_master_category_id", to_master_category_id);
  mysack.setVar( "palagrized_url", palagrized_url);
  mysack.onError = function() { alert('Ajax error in flagging' )};
  mysack.runAJAX();

  return true;

} // end of JavaScript function myplugin_cast_vote
//]]>
</script>
