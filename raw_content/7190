				if (google) {
					google.load('search', '1'); 
					google.setOnLoadCallback(function() { 
						attachAutoComplete('All-en-US');
					}); 
				}

				var attachAutoComplete = function(type) {
					if (google) {
						var autoCompleteIds = {
							'Articles-en-US': 	'001306696778704498477:clqfwtnu4gw',
							'Videos-en-US': 	'001306696778704498477:d-ner9pinps',
							'Members-en-US': 	'',
							'Web-en-US': 		'001306696778704498477:oqmey3bbw28',
							'Articles-en-GB': 	'001306696778704498477:e7pcf3zdvr4',
							'Videos-en-GB': 	'001306696778704498477:vsfounw1igo',
							'All-en-US': 		'001306696778704498477:folafxbmfpa',
							'All-en-GB': 		'001306696778704498477:y66jit2rb2c'
						}
						if (type == 'Members-en-US') //memebers
							return
						var id = autoCompleteIds[type]
						google.search.CustomSearchControl.attachAutoCompletion( 
							id, 
							$(".Search input:text")[0],
							'SearchHeader');
						google.search.CustomSearchControl.attachAutoCompletion( 
							id, 
							$(".Search input:text")[1], 
							'SearchFooter');
					}
				}
			</script>
		
