<script type="text/javascript">
 function mycarousel_initCallback(carousel) {
	 
	 function omniture(){
		 //set the referrer because a blank referrer will not do.
		 //since this is going to be preprocessed by drupal, we will probably want to use the site name or something more dynamic here. Hardcoding the site name is unnecessary, but I can't think of what the var is right now.
     s.referrer = 'http://www.thebigmoney.com';
		 //reset the omniture events variable.  This fixed a problem where the event variable was getting concatenated rather than overwritten.
     s.events='';
		 //do the omniture dance (it's an obfuscated function)
     s.t();
	 }

	 function changeAds(){
		 if (typeof window.placeAd2 == 'function' && typeof window.hackBin == 'function') {
		   //right sidebar big box flex ad
		   placeAd2(commercialNode,'rightflex','AJAX','');
		 
		   //header leaderboard ad
		   placeAd2(commercialNode,'leaderboard','AJAX','');
		 }
	 }

   jQuery('.jcarousel-control a').bind('click', function() {
     carousel.scroll(jQuery.jcarousel.intval(jQuery(this).text()));
     return false;
   });

   jQuery('.mycarousel-next').bind('click', function() {
	   omniture();
		 changeAds();
     carousel.next();
     return false;
   });

   jQuery('.mycarousel-prev').bind('click', function() {
		 omniture();
		 changeAds();
     carousel.prev();
     return false;
   });
 };


 $("#media-gallery-sub").css("color","#67712A"); //select sub-nav
 jQuery(document).ready(function() {
    jQuery('#mycarousel').jcarousel({
     scroll:1,
    initCallback: mycarousel_initCallback,
        // This tells jCarousel NOT to autobuild prev/next buttons
        buttonNextHTML: null,
        buttonPrevHTML: null
     });
    jQuery("#mycarousel").css("visibility","visible");
 });
</script>

	

