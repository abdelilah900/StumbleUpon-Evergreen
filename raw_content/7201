<script language="javascript" type="text/javascript">

function changeLargeTab(popularType) {
    if (popularType == '1') {
        jQuery('#poptab1').addClass("active");
        jQuery('#poptab2').removeClass("active");
        jQuery('#tabPanelsLarge1').css("display", "block");
        jQuery('#OpenBracket1').css("display", "inline");
        jQuery('#CloseBracket1').css("display", "inline");
        jQuery('#tabPanelsLarge2').css("display", "none");
        jQuery('#OpenBracket2').css("display", "none");
        jQuery('#CloseBracket2').css("display", "none");
        AnalyticsCustomEvent('CommunityClick', 'PopularTab');
    }
    else {
        jQuery('#poptab1').removeClass("active");
        jQuery('#poptab2').addClass("active");
        jQuery('#tabPanelsLarge1').css("display", "none");
        jQuery('#OpenBracket1').css("display", "none");
        jQuery('#CloseBracket1').css("display", "none");
        jQuery('#tabPanelsLarge2').css("display", "block");
        jQuery('#OpenBracket2').css("display", "inline");
        jQuery('#CloseBracket2').css("display", "inline");
        AnalyticsCustomEvent('CommunityClick', 'DiscussionTab');
    }
}

</script>

