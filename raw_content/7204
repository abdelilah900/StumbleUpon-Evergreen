		function openShadowbox(flash_array)
		{ 
			var _array = new Array();
			for (var i=0; i<flash_array.length; i++)
			{
				var _object = 
				{
					content: flash_array[i][0],
					player: flash_array[i][1],
					title: flash_array[i][2],
					width: flash_array[i][3],
					height: flash_array[i][4]
				};
				_array.push(_object);
			}
			Shadowbox.open(_array, {continuous: true});
		}
		
			$(function() {
		
		/*
				$("#share-link").showPopup({
					'onStart':shareWindow,
					'onClose':popupClose
				});
				$("#follow-link").showPopup({
					'onStart':popupStart,
					'onClosed':popupClose
				});	
		*/
		
				$("#share-link").click(function (e) {
					e.preventDefault();
					var element = $($(this).attr("href"));
					shareWindow(element)
				});
		
				$("#follow-link").click(function (e) {
					e.preventDefault();
					var element = $($(this).attr("href"));
					showPopup(element);
				});
		
				$("#overlay").click(function (e) {
					e.preventDefault();
					hidePopup();
				});
		
				$(".close").click(function (e) {
					e.preventDefault();
					hidePopup();
				});
		
				$( "#flashwrapper" ).css("padding-top","0px");
				$( "#flashwrapper" ).css("padding-right","0px");
				$( "#flashwrapper" ).css("padding-bottom","0px");
				$( "#flashwrapper" ).css("padding-left","0px");

				function position() {
				
					var scaling_type = "FULL";
					var halign = "center";
					var valign = "center";
					var padding_top = 0;
					var padding_right = 0;
					var padding_bottom = 0;
					var padding_left = 0;
					var fixed_width = 971;
					var fixed_height = 636;
					var min_width = 954;
					var min_height = 636;
					var max_width = 2880;
					var max_height = 2880;
					
					var myWidth = 0, myHeight = 0;
					if( typeof( window.innerWidth ) == 'number' )
					{
						//Non-IE
						myWidth = window.innerWidth;
						myHeight = window.innerHeight;
					} else if( document.documentElement && ( document.documentElement.clientWidth || document.documentElement.clientHeight ) )
					{
						//IE 6+ in 'standards compliant mode'
						myWidth = document.documentElement.clientWidth;
						myHeight = document.documentElement.clientHeight;
					} else if( document.body && ( document.body.clientWidth || document.body.clientHeight ) )
					{
						//IE 4 compatible
						myWidth = document.body.clientWidth;
						myHeight = document.body.clientHeight;
					}
					
					if ($.browser.msie  && parseInt($.browser.version) == 8) {
						myWidth = $(window).width();
						myHeight = $(window).height();
					}
					
					switch (scaling_type) {
		
						case "FIX":
							var html_width = myWidth - padding_left - padding_right;
							var html_height = myHeight - padding_top - padding_bottom;
							
							var myalign = halign + " " +valign;
							
							if (html_width < fixed_width) {
								myalign = "left " + valign;
							}
							if (html_height < fixed_height) {
								myalign = halign + " top";
							}
							if (html_width < fixed_width && html_height < fixed_height ) {
								myalign = "left top";
							}
							
							var html_width = fixed_width;
							var html_height = fixed_height;
							
							break;
				
						case "FULL": 
							var available_html_width = myWidth - padding_left - padding_right;
							var available_html_height = myHeight - padding_top - padding_bottom;
							
							var html_width = available_html_width;
							var html_height = available_html_height;
							
							var myalign = "center center";
												
							if (html_width < min_width && html_height < min_height ) {
								html_width = min_width;
								html_height = min_height;
							}
							if (html_width < min_width) {
								html_width = min_width;
							}
							if (html_height < min_height ) {
								html_height = min_height;
							}
							
							if (html_width > max_width && html_height > max_height) {
								html_width = max_width;
								html_height = max_height;
							}
							if (html_width > max_width) {
								html_width = max_width;
							}
							if (html_height > max_height) {
								html_height = max_height;
							}
						
							if (available_html_width <= min_width) {
								if (available_html_height <= min_height) {
									myalign = "left top";
								} else {
									myalign = "left center";
								}
							} else {
								if (available_html_height <= min_height) {
									myalign = "center top";
								} else {
									myalign = "center center";
								}
							}
							
							break;
							
						case "AR": 
							var available_html_width = myWidth - padding_left - padding_right;
							var available_html_height = myHeight - padding_top - padding_bottom;
							
							var html_width = available_html_width;
							var html_height = available_html_height;
							
							var myalign = halign + " " +valign;
												
							if (html_width < min_width && html_height < min_height ) {
								html_width = min_width;
								html_height = min_height;
							}
							if (html_width < min_width) {
								html_width = min_width;
							}
							if (html_height < min_height ) {
								html_height = min_height;
							}
							
							if (html_width > max_width && html_height > max_height) {
								html_width = max_width;
								html_height = max_height;
							}
							if (html_width > max_width) {
								html_width = max_width;
							}
							if (html_height > max_height) {
								html_height = max_height;
							}
						
							var aspect_ratio = min_width / min_height;
							var current_aspect_ratio = html_width / html_height;
							if (current_aspect_ratio > aspect_ratio) {
								html_width = Math.round(html_height * aspect_ratio);
							} else {
								html_height = Math.round(html_width / aspect_ratio);
							}
							
							if (available_html_width <= min_width) {
								if (available_html_height <= min_height) {
									myalign = "left top";
								} else {
									myalign = "left " + valign;
								}
							} else {
								if (available_html_height <= min_height) {
									myalign = halign + " top";
								}
							}
							
							break;
							
						default:
						
					}
					
					if ( (myWidth - padding_left - padding_right) > min_width ) {
						body_html_width = myWidth;
					} else {
						body_html_width = min_width + padding_left + padding_right;
					}
					if ( (myHeight - padding_top - padding_bottom) > min_height) {
						body_html_height = myHeight;
					} else {
						body_html_height = min_height + padding_top + padding_bottom;
					}

					$( "#flashwrapper" ).width(html_width);
					$( "#flashwrapper" ).height(html_height);
					
					if (padding_top == 0 && padding_right == 0 && padding_bottom == 0 && padding_left == 0 && scaling_type == "FULL") {
					} else {
						if ($.browser.msie  && parseInt($.browser.version) == 8) {
							$( "#flashwrapper" ).position({
								of: $(window),
								my: myalign,
								at: myalign,
								offset: "0 0",
								collision: "none none"
							});
						} else {
							$( "#flashwrapper" ).position({
								of: $(document),
								my: myalign,
								at: myalign,
								offset: "0 0",
								collision: "none none"
							});
						}
						
					}
						
				}
				$(window).resize(function() {
					position();
				});
				position();
			});
			
			$(document).ready(function () {
			
				$("#facebook-link").attr('allowTransparency', 'true');
				
			});	
		</script>
	
	</body>
</html>
