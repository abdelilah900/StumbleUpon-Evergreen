	<script type="text/javascript">
	function openShadowbox(flash_array)
	{ 
		var _array = new Array();
		for (var i=0; i<flash_array.length; i++)
		{
			var _object = 
			{
				content: flash_array[i][0],
				player: flash_array[i][1],
				title: flash_array[i][2],
				width: flash_array[i][3],
				height: flash_array[i][4]
			};
			_array.push(_object);
		}
		Shadowbox.open(_array, {continuous: true});
	}
	
	</script>
