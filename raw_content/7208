    //Helper text for non-logged-in users. No dependencies
    function makeHelpBubbleTextForSubscribe() {
      return '<div id="login5"><strong>Follow this user!</strong><br />'
        + 'To follow this user on Instructables,<br />'
        + '<a class="bright bold" href="/account/gopro?sourcea=subscribe&nxtPgName=Mega+Rocher&nxtPg=/id/Mega-Rocher/">'
        + 'sign up</a> or <a href="/account/login?nxtPgName=Mega+Rocher&nxtPg=/id/Mega-Rocher/">log in</a>.'
        + makeHelpBubbleLoginHTML('nxtPgName=Mega+Rocher&nxtPg=/id/Mega-Rocher/?newComment=true&skipPro=true', '', 'subscribe')
        + '</div>'
    }
