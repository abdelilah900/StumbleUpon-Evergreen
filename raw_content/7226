        <div class="clearer"></div>
        
    </div><!-- close navBar -->

</div><!-- close header -->

	<div id="content">
		
		<div id="threeColumn1">
			
		<!-- post include -->
		
			<div class="individualPost">

				<div class="tools" id="blogTools">

