<script language="javascript" type="text/javascript"><!--


function st(t, qid, tid, pw) {
	t.style.textDecoration = 'underline';
	t.style.color = '#c0c0c0';
	t.style.cursor = 'default';
	t.onclick = null;

	var a = [];
	a.push({name: 'quote_id', value: qid});
	a.push({name: 'tag_id', value: tid});
	a.push({name: 'password', value: pw});

	$.post('/votebox', a, function (ret, stat) {
	if (stat=='success') {
		t.innerHTML = ret;
	} else {
		t.innerHTML = 'Error!';
	}
	});
}

function sv2(t, qid, r) {
	c = t.parentNode;
	t.style.textDecoration='underline';
	d = 0;

	for (i = 0; i < t.parentNode.childNodes.length && d < 2; i++) {
	if (t.parentNode.childNodes[i].nodeName == 'A') {
		t.parentNode.childNodes[i].style.color = '#808080';
		t.parentNode.childNodes[i].style.cursor = 'default';
		t.parentNode.childNodes[i].onclick = null;
		t.parentNode.childNodes[i].href = 'javascript:;';
		d++;
	}
	}

	th = document.createElement('span');
	th.className = 'hh';
	th.id = 'stt'+qid;
	c.appendChild(th);
	th.innerHTML = ' Voting...';

	var form = document.vt;
	var els = form.elements;
	var a = [];

	document.getElementById('rh['+qid+']').value = r;

	document.vt.noout.value = 1;
	for(var i=0, max=els.length; i < max; i++) {
	var el = els[i];
	var n = el.name;
	if (!n) continue;

	if ((el.type == 'radio' || el.type == 'checkbox') && !el.checked)
		v = null;
	else
		v = el.value;

	if (v && v.constructor == Array) {
		for(var j=0, jmax=v.length; j < jmax; j++)
			a.push({name: n, value: v[j]});
	}
	else if (v !== null && typeof v != 'undefined')
		a.push({name: n, value: v});
	}
	a.push({name: 'cur_vote', value: qid});
	$.post('/votebox', a, function (ret, stat) {
	thh = document.getElementById('stt'+qid);
	if (stat=='success') {
		if (ret == 'Thanks!') {
			qs = document.getElementById('qs['+qid+']');
			if (qs) {
				if (qs.innerHTML != '?') {
					qs.innerHTML = parseInt(qs.innerHTML) + r;
					qs.style.color = '#c08000';
				}
				qvc = document.getElementById('qvc['+qid+']');
				if (qvc) {
					if (qvc.innerHTML != '') {
						qvc.innerHTML = '/' + (parseInt(qvc.innerHTML.substr(1)) + 1);
					} else {
						qvc.innerHTML = '/1';
					}
				}
				thh.innerHTML = '';
			} else {
				thh.innerHTML = ' '+ret;
			}
		} else {
			thh.innerHTML = ' '+ret;
		}
	} else {
		thh.innerHTML = ' Oops! Couldn\'t reach server.';
	}
	});
	document.vt.noout.value = 0;

	return false;
}
function dv(t, m) {
	t.innerHTML = m.contentWindow.document.body.innerHTML;
}
