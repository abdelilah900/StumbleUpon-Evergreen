          google.friendconnect.container.openSocialSiteId = siteId;
          google.friendconnect.requestSignOut();
        });
      }
    </script>
<script type="text/javascript">
    
    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/1071556851276427633/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/1071556851276427633/comments/default";
          
          holder.currentBlogUrl = "http://bonnielovescake.blogspot.com/";
          holder.currentBlogId = "1071556851276427633";
        
        return holder;
      });
    }
  </script>
