// Add the page method call as an onclick handler of load more Instagram photos button.
$("#loadInstagramPhotos").click(function() {
$.ajax({
type: "GET",
url: "/remote-instagram-call/",
data: {"max_id": $('#loadInstagramPhotos').attr('next_max_id'), "instagram-tag": "fitness"},
dataType: 'html',
success: function(data) {
    var nextId = $(data).siblings('#more_id').html();
    if(nextId > 0){
    $('#loadInstagramPhotos').attr('next_max_id',nextId);
    $(data).parent().find('#more_id').remove();
    s_265.t();
    }
    else {
    $('#loadInstagramPhotos').attr('next_max_id','');
    $('#loadInstagramPhotos').css('display','none');
    }
    $(data).appendTo($("#instagram-photos-list"));
    $('#instagram-photos-list #more_id').remove();
    $('#instagram-photos-list li:nth-child(3n) > ul').css('margin-right','0');
}
}); 
}); 

