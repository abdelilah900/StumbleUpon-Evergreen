// custom omniture for Instagram photo overlay
function omniAjaxPV(pageName,dept)
{
    omniObj = {
      pageName:pageName,
      prop1:dept
    };
    s_265.t(omniObj);
}


// Instagram Overlay and Sharebar
function loadSharebar(){
    $("[name='aol-share']").aolShare({
        services:['facebook','twitter','email','stumbleupon'],
        templates:{
            'twitter':{
                'twitterStatus':"url={{url}}&text=Check out: {{title}}&via=AOLMusic&hashtags=heidiklum"
            }
        },
        deferCount:0
    });

    // omniture ajax setup
    oApageName = s_265.prop2 + " individual photo";
    oAdept = "photo";
    omniAjaxPV(oApageName, oAdept);

    $('<div class="close-button closeClass">Close <b>X</b></div>').prependTo('#instagram-photo-by-id')
    .click(function(){
        $('#instagram-photo-container').jqmHide();
    });
}

$(function(){
	$(document).delegate("#instagram-photos-by-tag .mc-instagram-photo", "click", function(e){
                //console.log( this );
		$('#instagram-photo-container').empty();
		$('#instagram-photo-container').load(this.href+' #instagram-photo-by-id').jqm().jqmShow();
		window.setTimeout('loadSharebar()',1000);
		e.preventDefault();
	});
	$("#instagram-photos-list > li:nth-child(3n) > ul").css('margin-right','0');
});



//Photo Gallery Sharebar 

    function galleryInteract() {
        // Remove necessary elements  
            $(".aol-photo-gallery-fullscreen .aol-share, .aol-photo-gallery-fullscreen .aol-share-pinterest, #media-share, #pin-share").remove();
    
        // Set up all our values ... commment url required for gallery share even without fb comments

            var commentURL = $("ul.photos li.active a").attr("href");

            commentURL = "http://heidiklum.aol.com/" + commentURL;

             
            var pageURL = window.location.pathname;

            var mediaLink = $("ul.photos li.active a").html();
            mediaLink = mediaLink.replace(/'/g, "");            
            var mediaID = $("ul.photos li.active a").attr("data-media-id");
            var mediaTitle = $("ul.photos li.active a").attr("title");
            
            var pinTitle = $(".aol-photo-gallery-fullscreen ul.captions li h3:visible").text();
            

            // escape single quotes in title for gallery share append
            mediaTitle = mediaTitle.replace(/'/g, "&#39;"); 
            pinTitle = pinTitle.replace(/'/g, "&#39;");

            
            var imgUrl =  $("ul.photos li.active").css("background-image");
            imgUrl = imgUrl.replace(/"/g,"").replace(/url\(|\)$/ig, "");
            
            // Write in new share block
            $("#gallery-share").insertAfter($("#.aol-photo-gallery-fullscreen .captions"));
            $("#gallery-share").append("<div id='media-share'><a name='aol-share' class='aol-share' data-bid='787' data-pid='" + mediaID + "' href='mailto:yourfriend@email.com?subject=Check this out:" + mediaLink + "&body=" + commentURL + "' title='" + mediaTitle + "'>Share</a></div>");
       
            
        // Call aolShare function to rebuild share bar.
            $("[name='aol-share']").aolShare({
                deferCount: 0,
                services:['email','facebook','twitter','stumbleupon','s2c','plusone'],
                plugins:{'facebook':{useShortUrl:0}}
                }); 

         // Insert Pinterest into aolshare div
               $("#gallery-share").append("<div id='pin-share'><div class='aol-share-pinterest'><a class='pin-it-button' id='PinItButton' count-layout='none' target='_blank' href='http://pinterest.com/pin/create/button/?url=http://heidiklum.aol.com" + pageURL + "%3Fa_dgi=aolshare_pinterest&media="+ imgUrl+ "&description=" + pinTitle + "'>Pin It</a></div></div>");
               $("#pin-share").appendTo(".aol-share");

          // Style gallery share div... cannot hide, must be on page for google plus to work
               $('#gallery-share').css({'margin-top': '25px', 'padding': '15px 0 100px', 'border-bottom': '1px solid #292929', 'border-top': '1px solid #292929'});
               

     
    };

