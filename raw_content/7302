  var t = Badgeville.Settings.player;
  if (typeof t !== "undefined") {
    Badgeville.getAPI( 'player_rewards', {
      id: Badgeville.Settings.player.id
    }, function( data ) { 
      var displayed = 0; 
      var target = Badgeville('#myDiv'); 
      Badgeville.$.each( data.rewards, function() { 
        if ( Badgeville.Objects.is( this, Badgeville.Objects.BADGE ) ) { 
          Badgeville.Gears.reward( this ).appendTo(target); 
          displayed++; 
        } 
        if ( displayed == 3 ) { 
          return false;
        } 
      }); 
    }); 
        
  // get user points
  //var points_total = Badgeville.Settings.player.points_all;
  //$('#mnn_badgeville_total_points').html(points_total);
  var points = Badgeville.Settings.player.units;
