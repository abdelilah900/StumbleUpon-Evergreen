Badgeville.Queue.Filter.define( 'moreThan100Points', function() {
  var player = Badgeville.Objects.getPlayer( this.stack[ 0 ] );
  if ( !player || player.points_all <= 100 ) {
    this.remove( 0 );
  }
} );

