      var points_total = points.points.all;
      $('#mnn_badgeville_total_points').html(points_total);
    
      var tmp = points.unit_worldshare;
      if (typeof tmp !== "undefined") {
        var points_ws = points.unit_worldshare.all;
      }
      else {
        var points_ws = 0;
      }
    
    
      $('#mnn_badgeville_ws_points').html(points_ws);
