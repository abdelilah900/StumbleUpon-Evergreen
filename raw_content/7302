      $('input[name=bv_ws_points]').val(points_ws);
      $('#edit-bv-ws-points').attr("value", points_ws);  
      //alert("ws points = " + $('input[name=bv_ws_points]').val());
    
  }
       
  // chk if this is the user profile page
