function findSearch()
{
        updateParams();

        var searchNumber = document.searchForm.searchOption.selectedIndex;
        searchText = this.document.searchForm.search.value;
        words = searchText.split(" ");
        searchText = words.join("+");

        if(searchNumber == 0)
        {
                this.window.location = "/blcS.sc?search=" + searchText + "&cat=all";
                return false;
        }
        else if(searchNumber == 1)
        {
                this.window.location = "/blcS.sc?search=" + searchText + "&start=0&cat=web";
                return false;
        }
        else if(searchNumber == 4 && (searchText.length == 0 || searchText.toLowerCase() == "search"))
        {
                this.window.location = "/historicalarchives/?fl";
                return false;
        }
        else
        {
                document.searchForm.action =
                document.searchForm.searchOption.options[searchNumber].value;
        }
        return true;
}
function updateParams() {
                var optionNumber = document.searchForm.searchOption.selectedIndex;
                switch(optionNumber){
                        //case 0: document.searchForm.sorder.value = "L"; break;
