<script type="text/javascript">
  window.IE_HOVER_HACK = false;

  jQuery('#more-links .hover').mouseover(function (e, elem) {
    var hover = jQuery(e.currentTarget);
    if (hover.is(".hover")) {
      jQuery('.column-description a.hover').removeClass('current');
      hover.addClass('current');
      jQuery('.hover-list').hide();
      jQuery('#' + hover.attr('attached') + '-hover-list').show();
    }
  });

  // the timeout bs fixes flickering in IE7
  jQuery('#super-hack .nav-link').mouseover(function (e, elem) {
    var elem = jQuery(e.currentTarget);
    if (elem.is('.nav-link')) {
      clearTimeout(elem.data("timeout"));
      elem.data("timeout", setTimeout(function () {
        var dropdown = elem.find('.link-list'),
            offset   = elem.position();
        elem.addClass("over");
        dropdown.css({
          position: "absolute",
          top: offset.top + elem.height() - 2,
          left: offset.left
        });
      },
      50));
      if (document.activeElement.id == 'searchBarInput') {
        window.IE_HOVER_HACK = true;
        document.activeElement.blur();
      }
    }
  });

  jQuery('#super-hack .nav-link').mouseout(function (e, elem) {
    var elem = jQuery(e.currentTarget);
    if (elem.is('.nav-link')) {
      clearTimeout(elem.data("timeout"));
      elem.data("timeout", setTimeout(function () {elem.removeClass("over");}, 50));
      if (window.IE_HOVER_HACK) {
        window.IE_HOVER_HACK = false;
        jQuery('#searchBarInput').focus();
      }
    }
  });
</script>    </div>
