  (function () {
    var root = document.createElement("div"); root.id = "fb-root";
    document.getElementsByTagName("body")[0].appendChild(root);
    SG.load_script_async(('https:' == document.location.protocol ? 'https' : 'http') + "://connect.facebook.net/en_US/all.js");
  })();

  window.fbAsyncInit = function () {
    FB.init({
      appId: 'd77520f224200e405090bf6645fc79fb',
      status: true,
      cookie: true,
      xfbml:  true
    });

    // HACK don't register the auth.login listener until we know the initial
    // sessionChange event has fired
    FB.getLoginStatus(function(response) {
      FB.Event.subscribe('auth.login', function(response) {
        // A user has logged in, and a new cookie has been saved
        SG.fb_connect_cb();
      });

      if (response.session && !SG.is_authenticated()) {
        // on the off chance the user is logged into facebook and has already approved
        // the SeatGeek app, log them into seatgeek
        SG.fb_connect_cb();
      } else {
        // do nothing
      }
    });
  };

</script>
