<div id="get_email_updates" class="email_box">
    <script type="text/javascript">
        jQuery(function() {
            jQuery("#get_email_updates form").unbind('submit'); // HACK b/c this code seems to run twice on the event page
            jQuery("#get_email_updates form").submit(function(e) {
                var dataString = 'email=' + encodeURIComponent(jQuery('#email').val());
                dataString += '&ref=' + encodeURIComponent("homepage");
                jQuery.ajax({
                    type: "POST",
                    url: '/main/addemail/',
                    data: dataString,
                    beforeSend: function(data) {
                        jQuery('#email_success').fadeOut(200);
                        jQuery('#indicator').show();
                    },
                    success: function(data) {
                        jQuery('#indicator').hide()
                        jQuery('#email_success').html(data);
                        jQuery('#email_success').fadeIn(1000);
                        return false;
                    },
                    error: function() {
                        jQuery('#email_success').html("<p>An error occured. Please try submitting again.</p>");
                        jQuery('#email_success').fadeIn(200);
                    }
                });
                return false;
            });
        });
    </script>
    <form action="/main/addemail/" method="post">
          <p id="spamSucks">We’ll email you occasionally about deals on events in your area. We don’t spam. Promise.</p>
