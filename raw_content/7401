jQuery(function() {
    function pagerAnchorBblasafuilder(i, el) {
      return '<li class="slider-'+(i+1)+'"><a href="#"></a></li>';
    }
    jQuery('ul.slider').cycle({
      fx: 'fade',
      pager: '#pager',
      pause: 1,
      timeout: 8000,
      activePagerClass: 'active',
      pagerAnchorBuilder: function(i, el) {
        return '<li class="slider-'+(i+1)+'"><a href="#"></a></li>';
      }
    });
});
