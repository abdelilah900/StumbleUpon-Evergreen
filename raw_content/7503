// Store the existing Gear
var _property = Badgeville.Gears.property;
// Redefine the Gear so we can customize it
Badgeville.Gears.property = function() {
  // Call the original Gear and capture the result
  var ret = _property.apply( this, arguments );
  // If this is an <a> tag...
  if ( ret.attr( 'tagName' ) === 'A' ) {
    // Modify the result using JQuery to add rel="nofollow"
    ret.attr( 'rel', 'nofollow' );
 }
  // Return the result
  return ret;
};
