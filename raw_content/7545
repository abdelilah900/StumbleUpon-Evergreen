<script type="text/javascript">
	var chartbeatSection = new Array();
	chartbeatSection = sitezone.split("/");
	
	var sectionSize = chartbeatSection.length;
	if ( sectionSize > 1 ){
		chartbeatSection = chartbeatSection[0];
	}

	chartbeatSection = chartbeatSection.toString();
	chartbeatSection = chartbeatSection.split(".");
	
	sectionSize = chartbeatSection.length;
	if ( sectionSize > 1 ){
		if ( sectionSize > 2 ) {
			chartbeatSection = chartbeatSection[2];
		} else {
			chartbeatSection = chartbeatSection[1];
		}
	}
	
	if (chartbeatSection == null) {
		chartbeatSection = "";
	}


	var sectionAuthor = $("meta[name=byline]");
	sectionAuthor = sectionAuthor.attr("content");
	if (sectionAuthor != null) {
		sectionAuthor = sectionAuthor.replace(/\s+/g," ");
		sectionAuthor = sectionAuthor.replace(/By /gi, "");
		sectionAuthor = sectionAuthor.replace(/ and /gi,", ");
		sectionAuthor = sectionAuthor.replace(/ & /g,", ");
		
		if (sectionAuthor == null) {
			sectionAuthor = "";
		}
	} else {
		sectionAuthor = "";
	}


	var _sf_async_config={uid:19332,domain:"nypost.com"};
	_sf_async_config.sections = chartbeatSection.toString().toLowerCase();
	_sf_async_config.authors = sectionAuthor.toString().toUpperCase();
	_sf_async_config.path = window.location.pathname;
	
	(function(){
		function loadChartbeat() {
			window._sf_endpt=(new Date()).getTime();
			var e = document.createElement('script');
			e.setAttribute('language', 'javascript');
			e.setAttribute('type', 'text/javascript');
			e.setAttribute('src',
				(("https:" == document.location.protocol) ? "https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/" : "http://") +
				"static.chartbeat.com/js/chartbeat.js");
			document.body.appendChild(e);
		}
		var oldonload = window.onload;
		window.onload = (typeof window.onload != 'function') ?
			loadChartbeat : function() { oldonload(); loadChartbeat(); };
	})();
</script>
