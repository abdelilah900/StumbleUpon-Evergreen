    function trackTwitter(intent_event) {
        if (intent_event) {
            DW.levt('ria', 'log', {siteid: '175', mapp: 'social_share_bar', comptyp: 'app', comp: 'main', riaevent: 'click', objtyp: ' button', objnm: 'twitter_share'});
