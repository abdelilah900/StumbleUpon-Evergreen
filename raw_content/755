<script language="JavaScript" type="text/javascript">
function process_rating(id, review) {
    var element = document.getElementById('mcc_current_rating_'+id);
    var element_not = document.getElementById('mcc_not_current_rating_'+id);
    if(element && (element.style.width=='0px' || element.style.width=='')) {				// ...==0 prevents re-voting
        var http = getHTTPObject();
        http.open( "GET", "/mcc/reviews/set-review/" + id + "/" + (review*10) , true);
        http.send(null);
        element.style.width = parseInt(review*10) + '%';	// this sets the current rating
        element_not.style.width = element.style.width;          // this sets the current rating
        var hover = document.getElementById('mcc_hovers_'+id);
        var hover_not = document.getElementById('mcc_not_hovers_'+id);
        var display_was = hover.style.display;
        hover.style.display='none';
        hover_not.style.display=display_was;
    }
}
</script>
