function rf_ck(val,t){
	var myD = document.location.host;
	var now=new Date();
	var d = new Date(now.getFullYear(),now.getMonth(),now.getDate(),now.getMinutes()+t,now.getSeconds());
	var curCookie = "ZZZrf_ck=" + val +"; expires=" + d.toGMTString() + ";path=/; domain=" + myD;
	document.cookie = curCookie;
}
function rf_ck_refresh(){
	rf_ck(1,100);
	window.location.reload(true);
}
