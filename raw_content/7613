function on_country_change()
{
	var dropdown = document.getElementById('selected_country');
	var myindex  = dropdown.selectedIndex;
	var SelValue = dropdown.options[myindex].value;
	var baseURL  = 'http://www.worldlifeexpectancy.com/country-health-profile/'+SelValue;
	top.location.href = baseURL;
	return true;
}

