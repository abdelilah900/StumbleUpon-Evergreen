	$(document).ready(function() {
		var u, t;
		$('.share .fb').click(function(e) {
			u=location.href;
			t=document.title;
			window.open('http://www.facebook.com/sharer.php?u='+encodeURIComponent(u)+'&amp;t='+encodeURIComponent(t),
			null,
			'sharer','toolbar=0,status=0,width=626,height=436');
			e.preventDefault();
			return false;		
		});
		$('.share .tw').click(function(e) {	
    			var twUrl = 'http%3A%2F%2Fwww.rd.com%2Flaughs%2Fyou-know-when-its-time%2F';
			u=twUrl;
			//t= encodeURIComponent(window.document.title);
			t="You Know When It&#8217;s Time";
			window.open(
				'https://twitter.com/intent/tweet?related=readersdigest&url='+u+'&via=readersdigest&text='+ t +'&original_referer=' + u,
				null,
				'status=0,toolbar=0,width=626,height=436'
			);
			e.preventDefault();
			return false;		
		});
		$('.share .email').click(function(e){
			u=location.href;
			t=document.title;
