<script type="text/javascript">
    var conv_ready = false;
    var conv = LF({
        domain: 'myvoicenation.fyre.co',
        site_id: '304529',
        article_id: 'Mzc4NDcyNzQtNA==',
        conv_meta: {
            article_url: "http://www.laweekly.com/slideshow/kang-ho-dong-baekjeong-more-bbq-in-koreatown-37847274/4/",
