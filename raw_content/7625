<script type="text/javascript">
    jQuery(document).ready(function () {
        jQuery('body').prepend('<div id="jqEnlargeModal" class="jqmWindow"></div>');
        jQuery('#jqEnlargeModal').jqm();
        var titleLength = (jQuery('p#caption').text()).length;

        if(titleLength > 50){
            var abovePicHeight = jQuery('p#caption').height()+10;
            jQuery("#abovePic").css({height: abovePicHeight + 'px'});
            jQuery("p#enlarge").css({top: (abovePicHeight-jQuery("p#enlarge").height()-5) + 'px'});
        } else{
            if((jQuery('#thumbRange').length == 0) && !(jQuery('#enlarge').is(':visible')) && jQuery('#caption').html() == '') {
            	jQuery('#abovePic').css({height:'4px'});
                jQuery('#caption').css({display: 'none'});
            } else {
            	jQuery('#abovePic').css({height:'30px'});
            }
        }
    });
</script>
