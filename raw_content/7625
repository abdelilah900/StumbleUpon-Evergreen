<script type="text/javascript">
    function enlarge(link) {
        jQuery('#jqEnlargeModal').jqm({onLoad: function(hash) {
                jQuery('#mediumrect').hide();
            }
        });
        jQuery('#jqEnlargeModal').jqm({onHide: function(hash) {
            jQuery('#mediumrect').show();
            hash.w.fadeOut('2000',function(){ hash.o.remove(); });
            }
        });
            jQuery.get(link, function(data) {
                jQuery('#jqEnlargeModal').css({width: ''});
                jQuery('#jqEnlargeModal').jqmShow();
                jQuery('#jqEnlargeModal').html('');
                jQuery('#jqEnlargeModal').append(data);
                var setWidth = (jQuery('#jqEnlargeModal img').css('width'));
                jQuery('#jqEnlargeModal').css({width: setWidth});
                jQuery('#jqEnlargeModal').prepend('<div class="jqEnlargeClose" id="jqEnlargeX" style="margin:0px">X</div>');
                jQuery('#jqEnlargeModal').jqmAddClose('.jqEnlargeClose');
                jQuery('#enlargeHeader').html(jQuery("p#caption").text());
                var newWidth = jQuery('#jqEnlargeModal img').width();
                if(jQuery('#jqEnlargeModal img').width()>(screen.availWidth)){
                    var newLeft = 0;
                } else if(newWidth > jQuery(window).width()){
                    var newLeft = 0 + document.documentElement.scrollLeft;
                    if(!document.documentElement.scrollLeft){
                        var newLeft = 0 + document.body.scrollLeft;
                    }
                } else {
                    var newLeft = (jQuery(window).width()/2 - newWidth/2);
                    if(window.pageXOffset > 0){
                        newLeft += window.pageXOffset;
                    } else if(document.documentElement.scrollLeft > 0){
                        newLeft += document.documentElement.scrollLeft;
                    }
                }

                jQuery('#jqEnlargeModal').css({'margin-left':'0px', left: newLeft, top: '40%' });
                jQuery('.jqmOverlay').css({'z-index': (jQuery('#jqEnlargeModal').css("z-index"))-1});
                
                            });
        return false;
    }

    function getCurrentPosition() {
        return jQuery("#currentThumbIndex").text();
    }

    var galleryImagesTotal = 19;
    var currentPosition    = false;

    // Used for gallery contests so you can see the rating for an 
    // individual picture.
    function initRatings(dim) {
        jQuery.getJSON('/pictures/galleryItemRating/' + dim[2], function(rate) {
                            jQuery('#rateContainer').html('Vote: ');
                jQuery('#rateContest').html('').rating('/media/addVote/' + dim[2], {maxvalue:5, curvalue:rate.avg_rating, callback:clearRating, contest:true});
                jQuery('#rateContest .videoStar a').attr('title', 'Log in to Vote');
                jQuery('#contestVotes').html(rate.avg_rating + ' stars (' + rate.num_votes + ' votes)');
                jQuery('#rateContest .videoStar').unbind();
                jQuery('#rateContest .videoStar').removeClass('dyn');
                jQuery('#rateContest').removeClass('rating');
                jQuery('#rateContest .videoStar a').attr('onclick', "jQuery('#jqModal').jqm({ajax: '/user/inlineAuth?ajax=1'}); jQuery('#jqModal').jqmShow();");
                    });
    }

    // Update contest gallery image rating when user votes.
    function clearRating(data) {
        var curr = getCurrentPosition();
        var dim = jQuery('li[@jcarouselindex=' + curr + '] img:last').attr('dimensions').split(':');
        initRatings(dim);
    }

    function initImageChange(thumbIndex) {
        if (!gImages[(thumbIndex - 1)].complete)
            return false;

        jQuery('p#caption').html('');
        jQuery('#enlarge').hide();
        jQuery('#gallerycarousel li.activeGalleryItem').removeClass('activeGalleryItem');
        jQuery('li[@jcarouselindex=' + thumbIndex + ']').addClass('activeGalleryItem');
        jQuery('#currentThumbIndex').html(thumbIndex);
        jQuery('li[@jcarouselindex=' + thumbIndex + '] img').fadeTo('slow', 0.7);
        var newSrc = jQuery('li[@jcarouselindex=' + thumbIndex + '] img').attr('path');
        var dim    = jQuery('li[@jcarouselindex=' + thumbIndex + '] img').attr('dimensions').split(':');

        jQuery('#pictureMedia p[@id=currentPic]').animate({ opacity: 0 }, 250, function() {
            jQuery('#pictureMedia p[@id=currentPic] img').replaceWith(gImages[(thumbIndex - 1)]);
            jQuery('#pictureMedia p[@id=currentPic] img').css({ width:dim[0] + 'px', height:dim[1] + 'px'});
            jQuery('#pictureMedia p[@id=currentPic]').fadeTo(250, 1);
        });

        if (dim[3] > 610) {
            jQuery('#enlarge').show();
        }
        jQuery('p#caption').html(jQuery('li[@jcarouselindex=' + thumbIndex + '] img').attr('title'));
        jQuery('#enlarge a').attr({
            size: dim[0],
            href: "/pictures/enlarge/" + dim[2] + "/ajax=1/parent=38287/"
          });
        var titleLength = (jQuery('p#caption').text()).length;

        if (titleLength > 50) {
            var abovePicHeight = jQuery('p#caption').height()+10;
            jQuery('#abovePic').css({height: abovePicHeight + 'px'});
            jQuery('p#enlarge').css({top: (abovePicHeight-jQuery('p#enlarge').height()-5) + 'px'});
        } else {
            jQuery('#abovePic').css({height: '30px'});
            jQuery('p#enlarge').css({top: '0px'});
        }

                /* Reload zones */
        refreshEverythingDefault();

        return false;
    }
      
    function mycarousel_initCallback(carousel) {
        function thumbClick() {
        	var thumbIndex = jQuery(this).parent().attr('jcarouselindex');
            var curr       = getCurrentPosition();
            var curIndex   = parseInt(curr);

            if(!(thumbIndex > galleryImagesTotal)) {
                if (!gImages[thumbIndex - 1])
                    return false;

                if (!gImagesCollection[thumbIndex - 1].complete)
                     return false;
            }
            
            if (thumbIndex == carousel.last && thumbIndex < galleryImagesTotal)
                carousel.next();

            else if (thumbIndex == carousel.first && thumbIndex != 1) 
                carousel.prev();

            if (curIndex != thumbIndex)
                initImageChange(thumbIndex);

            return false;
        }
       
        function galleryPrevious() {
            var curr       = getCurrentPosition();
            var currIndex  = parseInt(curr);
            var thumbIndex = currIndex - 1;
           
            if (thumbIndex <= 0) {
                if (galleryImagesTotal > 5) {
            		if (gImages.length != galleryImagesTotal)
                        return false;

                    var carouselWidth = jQuery('#gallerycarousel').width();
                    //610 is the width of the carousel minus the 4 pixels of padding on the ends.
                    jQuery('#gallerycarousel').css({left: (0 - carouselWidth + 610) + 'px'});
                    carousel.last  = galleryImagesTotal;
                    carousel.first = galleryImagesTotal - 4;
                    thumbIndex     = galleryImagesTotal;
                } else {
                	var carouselWidth = jQuery('#gallerycarousel').width();
                    //610 is the width of the carousel minus the 4 pixels of padding on the ends.
                    jQuery('#gallerycarousel').css({left: 0});
                    carousel.last  = galleryImagesTotal;
                    carousel.first = 1;
                    thumbIndex     = galleryImagesTotal;
                }
            }
            if (thumbIndex == carousel.first || currIndex == carousel.first)
                carousel.prev();

            initImageChange(thumbIndex);
        }

        function galleryNext() {
            var curr       = getCurrentPosition();
            var currIndex  = parseInt(curr);
            var thumbIndex = currIndex + 1;

            if(!(thumbIndex > galleryImagesTotal)) {
                if (!gImages[thumbIndex - 1])
                    return false;

                if (!gImagesCollection[thumbIndex - 1].complete)
	                return false;
            }
            if (thumbIndex > galleryImagesTotal) {
                jQuery('#gallerycarousel').css({left: '0px'});
                carousel.last  = 5;
                carousel.first = 1;
                thumbIndex     = 1;
            }

            if (thumbIndex == carousel.last || currIndex == carousel.last) {
                if (!gImages[(thumbIndex - 1)].complete)
                    return false;

                carousel.next();
            }

            initImageChange(thumbIndex);
        }
        jQuery('.jcarousel-control a').bind('click', thumbClick);
        jQuery('#currentPic').bind('click', galleryNext);
        jQuery('#mycarousel-next').bind('click', galleryNext);
        jQuery('#mycarousel-prev').bind('click', galleryPrevious);

        /* Allow arrow key navigation */
        jQuery(document).bind('keyup', function(event) {
            if (event.keyCode == 37) {
                var curr       = getCurrentPosition();
                var currIndex  = parseInt(curr);
                var thumbIndex = currIndex - 1;
                
                if (thumbIndex <= 0) {
                	if(galleryImagesTotal > 5) {
                		if (gImages.length != galleryImagesTotal)
                            return false;
                        
                        var carouselWidth = jQuery('#gallerycarousel').width();
                        //610 is the width of the carousel minus the 4 pixels of padding on the ends.
                        jQuery('#gallerycarousel').css({left: (0 - carouselWidth + 610) + 'px'});
                        carousel.last  = galleryImagesTotal;
                        carousel.first = galleryImagesTotal - 4;
                        thumbIndex     = galleryImagesTotal;
                    } else {
                    	var carouselWidth = jQuery('#gallerycarousel').width();
                        //610 is the width of the carousel minus the 4 pixels of padding on the ends.
                        jQuery('#gallerycarousel').css({left: 0});
                        carousel.last  = galleryImagesTotal;
                        carousel.first = 1;
                        thumbIndex     = galleryImagesTotal;
                    }
                }
                if (thumbIndex == carousel.first || currIndex == carousel.first)
                    carousel.prev();

                initImageChange(thumbIndex);
            }
            if (event.keyCode == 39) {
                var curr       = getCurrentPosition();
                var currIndex  = parseInt(curr);
                var thumbIndex = currIndex + 1;

                if(!(thumbIndex > galleryImagesTotal)) {
                    if (!gImages[thumbIndex - 1])
                        return false;
                    
                    if (!gImagesCollection[thumbIndex - 1].complete)
                         return false;
                }

                if (thumbIndex > galleryImagesTotal) {
                    jQuery('#gallerycarousel').css({left: '0px'});
                    carousel.last  = 5;
                    carousel.first = 1;
                    thumbIndex     = 1;
                }

                if (thumbIndex == carousel.last || currIndex == carousel.last) {
                	if (!gImagesCollection[(thumbIndex - 1)].complete)
                        return false;
                    
                    carousel.next();
                }

                initImageChange(thumbIndex);
            }
        });
    }
    var arVersion = navigator.appVersion.split("MSIE")
    var version   = parseFloat(arVersion[1])
    if ((version < 7)) {
        jQuery('.navText').html('');
    }
            jQuery('#gallerycarousel li:first').addClass('activeGalleryItem');
        jQuery('#gallerycarousel li:first img').fadeTo('slow', 0.5);
        jQuery('#gallerycarousel').jcarousel({
            vertical: false,
            size: galleryImagesTotal,
            animation: 'fast',
            initCallback: mycarousel_initCallback,
            buttonNextHTML: null,
            buttonPrevHTML: null,
            scroll: 4
        });
        jQuery('#pictureItem').addClass('pictureItem');
        var cnt = galleryImagesTotal;
        if (cnt < 5) {
            var wd     = cnt * 114;
            var plusWd = cnt * 8 + 2;
         
            jQuery('div.jcarousel-clip').width(wd + plusWd + 'px');
            jQuery('#galleryNav').width(wd + plusWd + 'px');
            jQuery('.jcarousel-skin-tango.jcarousel-container-horizontal').width(wd + plusWd + 'px');
        }
                jQuery('p#caption').html(jQuery('li[@jcarouselindex=1] img:last').attr('title'));
        var titleLength = (jQuery('p#caption').text()).length;
        if (titleLength > 50) {
            var abovePicHeight = jQuery('p#caption').height() + 10;
            jQuery('#abovePic').css({height: abovePicHeight + 'px'});
            jQuery('p#enlarge').css({top: (abovePicHeight-jQuery('p#enlarge').height() - 5) + 'px'});
        } else{
            jQuery('#abovePic').css({height:'30px'});
        }
        
        /* Lets load all the images into browser cache */
        var gImages            = [];
        var gImagesCollection  = jQuery('a.galleryItem img.gThumb');
        var galleryItemTotal   = gImagesCollection.length;
        var currentImageToLoad = 0;
    
        function loadImagesInOrder() {            
        	gImages[currentImageToLoad] = new Image();
            gImages[currentImageToLoad].onload = function() {     	
            	gImagesCollection[currentImageToLoad].onload = function() {   
                    jQuery('img[@path=' + gImages[currentImageToLoad].src + ']').prev().remove();
                    jQuery('img[@path=' + gImages[currentImageToLoad].src + ']').css({ display: 'inline' });

                    currentImageToLoad ++;
                    
                    if(currentImageToLoad < galleryItemTotal)
                        loadImagesInOrder();
                }
            	gImagesCollection[currentImageToLoad].src = jQuery(gImagesCollection[currentImageToLoad]).attr('thumb');
            };           
            gImages[currentImageToLoad].src = jQuery(gImagesCollection[currentImageToLoad]).attr('path');
        }
        loadImagesInOrder();       
    </script>
