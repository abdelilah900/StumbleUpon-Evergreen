	var check_cookie=checkCookie();
	if (!(jQuery.browser.msie && jQuery.browser.version == 7.0)) {
  		jQuery(document).ready(function($) {
    		$(".signin").mouseover(function() {
    			clearTimeout(timeout);
      			$('div#subscribe-overlay').show();
	  			$("#close-subscribe-btn").click(function (){
        			clearTimeout(timeout);
        			$('div#subscribe-overlay').hide();
        			return false;
    			});
      			var timeout = setTimeout(function () {$("div#subscribe-overlay").hide();}, 3000);
      			$('#subscribe-form_div input').focus(function () {clearTimeout(timeout);});
    		});
  		});
	}

	if(check_cookie=="0") {
		setCookie('username','yes');
		if (!(jQuery.browser.msie && jQuery.browser.version == 7.0)) {
			jQuery(document).ready(function($) {
				$('div#elleapps-overlay').show();
				$("#close-elleapps-btn").click(function (){
					$('div#elleapps-overlay').hide();
		        	return false;
		    	});
			});
