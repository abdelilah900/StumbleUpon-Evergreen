<script type="text/javascript">

window.onscroll = function()
{
	if( window.XMLHttpRequest ) { 
	
			if (document.documentElement.scrollTop > 115 || self.pageYOffset > 115) {
			$('#quick-tool').css('position', 'fixed');
			$('#quick-tool').css('top', '50px');
			$('#quick-tool').css('z-index', '999');
		} else if (document.documentElement.scrollTop < 115 || self.pageYOffset < 115) {
			$('#quick-tool').css('position', 'absolute');
			$('#quick-tool').css('top', '');
		}
		}
}
</script>

<script type="text/javascript"> 
(function() {
	
	function $(id) {
	return document.getElementById(id);
}

function setStyleDisplay(id, status) {
	$(id).style.display = status;
}

function runNow(a, t) {
	a = a || 0.1;
	t = t || 16;

	var x1 = 0;
	var y1 = 0;
	var x2 = 0;
	var y2 = 0;
	var x3 = 0;
	var y3 = 0;

	if (document.documentElement) {
		x1 = document.documentElement.scrollLeft || 0;
		y1 = document.documentElement.scrollTop || 0;
	}
	if (document.body) {
		x2 = document.body.scrollLeft || 0;
		y2 = document.body.scrollTop || 0;
	}
	var x3 = window.scrollX || 0;
	var y3 = window.scrollY || 0;

	var x = Math.max(x1, Math.max(x2, x3));
	var y = Math.max(y1, Math.max(y2, y3));

	var speed = 1 + a;
	window.scrollTo(Math.floor(x / speed), Math.floor(y / speed));
	if(x > 0 || y > 0) {
		var f = "AlmostE.runNow(" + a + ", " + t + ")";
		window.setTimeout(f, t);
	}
}

window['AlmostE'] = {};
window['AlmostE']['$'] = $;
window['AlmostE']['setStyleDisplay'] = setStyleDisplay;
window['AlmostE']['runNow'] = runNow;
})();
</script>
