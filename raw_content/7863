	<a href="javascript:;" class="share-icon bf_dom" id="pinterest" rel:bf_bucket_data="{'pin-share':{'id':'1755726_561886'}}"  rel:sub_title='They&#39;re High Impact!' rel:gt_act="share/pinterest/list-item:9" rel:gt_label=""></a>
	
	<a href="javascript:;" class="share-icon bf_dom" id="twitter" rel:bf_bucket_data="{'tw-share':{'id':'1755726_561886'}}" rel:sub_title='They&#39;re High Impact!' rel:gt_act="share/twitter/list-item:9" rel:gt_label=""></a>
