	function voted(data)
	{
	    $("#votecontainer").text("Vote Registered. Thanks!");
       	$("#floatLayer").hide('slow');
	}

	function attach_voting(id)
	{
        $(id).click(function(event) {        
            var bc_link = $(id).attr("href");
            voted(null);
            $.post(bc_link);
            event.preventDefault();
        });
	}

     $(document).ready(function(){
        /* floating layer */
    	start_persistent_div();
	    $("#floatLayer").show('slow');

    	attach_voting("#voteuplink");
	    attach_voting("#votedownlink");
    	attach_voting("#votebadlink");
     });
    
