        function showMessage(message) {
	      if($j("#error")) {
		    $j("#error").remove();
	       };
          //  document.getElementById('message')[0].innerHTML = message;
        $j("#comments_container").prepend("<h4 id='error' class='red mt20'>" + message + "</h4>");
        }
        
        function adminComment(action, id) {
	
            var actionUrl = '/xhr/comments/'+ action + '?cid=' + id;
			        $j.ajax({
				      type: 'POST',
				      url: actionUrl,
				      success: function(e){
					    window.location = (location + ('?comment=new#comments_container'));
			            location.reload(true);
				      },
				      error: function(e){
					  	if (e.status == 400) {
			              showMessage( e.responseText );
			            }
			            else {
			              showMessage('We\'re having some app problems right now. Please try again in a bit.');
			             };
			          }
			      });
			      return false;
        }
</script>

