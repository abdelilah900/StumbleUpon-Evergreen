function didLoadAllComponents(chowComponents) {
    for (var chowComponent in chowComponents) {
        if (chowComponents[chowComponent] !== true){return false;}
    }
    return true;
}

function executeSuccessCallback(chowComponent) {
    if (chowComponent) { chowComponents[chowComponent] = true; }
    if (didLoadAllComponents(chowComponents)) {
        initChowJS();
    }
}

function initChowJS() {
   initChowJSCounter++; 
   if(initChowJSCounter > 100) return; 
   try{    
   
    chowcomments.init(); 
    chowcomments.needsHash=true;
