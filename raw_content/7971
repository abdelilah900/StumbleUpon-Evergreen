}  
    catch(err) {  
        setTimeout('initChowJS()', 50); 
    }  
}

function callGetScript(chowComponent) {
    var sc = document.createElement('script'); sc.type='text/javascript'; sc.src=chowComponents[chowComponent]; var s =  document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(sc, s); executeSuccessCallback(chowComponent);
}

