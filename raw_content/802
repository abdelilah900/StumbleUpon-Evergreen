<script>
	function showArchivePaging(archive_articles_per_page,archive_start,archive_total,pagination_cntr) {
		var archive_totalpages = Math.ceil(archive_total / archive_articles_per_page);
		var current_page = Math.floor(archive_start / archive_articles_per_page) + 1;
		var page_num;
		var writeMe = '';
		var start_page = (current_page < 4) ? 2 : current_page - 2;
		var end_page = (current_page > archive_totalpages - 3) ? archive_totalpages - 1 : current_page + 2;
		var archive_start_end = ((archive_start + archive_articles_per_page) <= archive_total) ? (archive_start + archive_articles_per_page) : archive_total ;

		if (!(archive_totalpages == 1 && current_page == 1))
		{
			writeMe += (current_page == 1) ? '<div class="no_bdr">1</div>' : '<a class="has_bdr" href="/archive/tips/0/' + archive_articles_per_page + '/issue_date/desc/">1</a>';
			writeMe += (current_page > 4) ? '<div class="ellipsis">...</div>' : '';
		}
		
		for (page_num = start_page; page_num <= end_page; page_num++){
		var start_article = archive_articles_per_page * (page_num - 1);
		var page_url = start_article + "/" + archive_articles_per_page;
		writeMe +=  (page_num == current_page) ?  ("<div class='no_bdr'>"+page_num+"</div>") : ("<a class=\"has_bdr\" href='/archive/tips/" + page_url + "/issue_date/desc/'>" + page_num + "</a>");
		}

		if (archive_totalpages > 1) {
		writeMe += (current_page < archive_totalpages - 3) ? '<div class="ellipsis">...</div>' : '';
		
		writeMe += (current_page == archive_totalpages) ? '<div class="no_bdr">' + archive_totalpages + '</div>' : '<a class="has_bdr" href="/archive/tips/' + archive_articles_per_page * (archive_totalpages - 1) + '/' + archive_articles_per_page + '/issue_date/desc/">' + archive_totalpages + '</a>';	
		}
		document.getElementById(pagination_cntr).innerHTML += writeMe;
	}
	
</script>

