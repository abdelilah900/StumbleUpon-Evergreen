					<h1>U.S.-Costa Rica World Cup Qualifier</h1>
					
					<!-- share -->
<script type="text/javascript">
	/* SI-5384 */
	function getTitleMeta() {
		var metas = document.getElementsByTagName('meta');
		var storyHeadline = '';
		var i = '';
		for(i = 0; i < metas.length; i++) {
			if(metas[i].getAttribute('property') == "og:title") {
				storyHeadline = metas[i].getAttribute('content');
				break;
			}
		}
		if(storyHeadline == '') {
			storyHeadline = top.document.title;
		}
		if(storyHeadline.indexOf(" | SI.com") != -1) {
			storyHeadline = storyHeadline.replace(" | SI.com", "");
		} else if(storyHeadline.indexOf(" - SI.com") != -1) {
			storyHeadline = storyHeadline.replace(" - SI.com", "");
		}

		return storyHeadline;
	}

	$(document).ready(function() {
		$('#shareBtn').hover(function() {
			$('.wdt_button_min #share_popup').toggle();
		});
		$('.wdt_button_min #share_popup').hover(function() {
			$('.wdt_button_min #share_popup').toggle();
		});
		var metas = document.getElementsByTagName('meta');
		var shareTitle = getTitleMeta();
		var shareDesc = '';
		for(var i = 0; i < metas.length; i++) {
			if(metas[i].getAttribute('name') == "description") {
				shareDesc = metas[i].getAttribute('content');
				break;
			}
		}
		if(shareDesc == '') {
			shareDesc = encodeURIComponent(top.document.title);
		}
		shareTitle = encodeURIComponent(shareTitle);
		var shareURL = encodeURIComponent(top.document.URL);

		$('#tumblr_share').attr('href', 'http://www.tumblr.com/share/link?url='+shareURL+'&amp;name='+shareTitle+'&amp;description='+shareDesc);
		$('#stumbleupon_share').attr('href', 'http://www.stumbleupon.com/submit?url='+shareURL+'&amp;title='+shareTitle);
		$('#reddit_share').attr('href', 'http://reddit.com/submit?url='+shareURL);
		$('#digg_share').attr('href', 'http://digg.com/submit?url='+shareURL);
		$('#delicious_share').attr('href', 'http://del.icio.us/post?url='+shareURL+'&amp;title='+shareTitle+'&amp;notes='+shareDesc);
	});
</script>
