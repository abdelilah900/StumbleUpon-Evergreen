</div><!-- END box danas-izbor --><script type="text/javascript">

	// if( top != self ) { top.window.location.href = self.window.location.href; }

	function scur( el ) {
		var p = el.value.indexOf( '@' );
		if( 0 != p ) { return true; }
		if( el.setSelectionRange ) {
			el.focus();
			el.setSelectionRange( 0, 0 );
		}
		else {
			if( el.createTextRange ) {
				range = el.createTextRange();
				range.collapse(true);
				range.moveEnd('character',0);
				range.moveStart('character',0);
				range.select();
			}
			else { el.focus(); }
		}
	}

	function CheckAll( form ) {
		if( form.ego_user.value == '' ) {
			alert( 'Upišite e-mail adressu!' );
			form.ego_user.focus();
			return false;
		}

		if( form.ego_secret.value == '' ) {
			alert( 'Upišite lozinku!' );
			form.ego_secret.focus();
			return false;
		}
		return true;
	}

	function ol() {
		if( (document.log_centrum.ego_user.value == '') || (document.log_centrum.ego_user.value == '@net.hr') ) { document.log_centrum.ego_user.focus(); }
		else { document.log_centrum.ego_secret.focus(); }
	}

</script>
