<script type="text/javascript">
if (BrowserDetect.browser == "Internet Explorer"){
document.write('<a class="box snl-lnk home" href="javascript:history.go(0);" onClick="this.style.behavior=\'url(#default#homepage)\';this.setHomePage(\'http://www.net.hr\');">');
document.write('Neka ovo bude vaša početna stranica</a>');
}
else if (BrowserDetect.browser == "Firefox" && BrowserDetect.OS == "Windows")	{
document.write('<a class="box snl-lnk home" href="http://www.net.hr" title="net.hr">Odvucite ovo na <em>Home</em> ikonu kako bi net.hr postao vaša početna stranica</a>');
}
else if (BrowserDetect.browser == "Firefox" && BrowserDetect.OS == "Mac")	{
document.write('<a class="box snl-lnk home" href="http://www.net.hr" title="net.hr">Odvucite ovo na <em>Home</em> ikonu kako bi net.hr postao vaša početna stranica</a>');
}
else if (BrowserDetect.browser == "Chrome" && BrowserDetect.OS == "Windows")	{
document.write('<a class="box snl-lnk home" href="#" onClick="window.alert(\'Nažalost Vaš browser ne podržava ovu opciju!\\nProbajte ovako:\\n\\nKliknite na ikonu (francuski ključ) s gornje desne strane Vašeg browsera i odaberite Options.\\nU otvorenom prozoru pod Home Page odaberite Open this page i ovdje ukoliko Vam već nije upisano upišite:\\nhttp://www.net.hr/\\n\\nVaš net.hr\')">');
document.write('Neka ovo bude vaša početna stranica</a>');
}
