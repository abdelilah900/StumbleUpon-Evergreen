<script language="javascript" type="text/javascript">
//<![CDATA[
	//Div parente
var mainMediaHDiv = document.getElementById('mediaH');
var mainMBulletsUl = document.getElementById('mediaHUlBullets');
var currentMediaSlide = 1;
var numberMSlides = 9;
function goToMSlide(n)
{
	var n = n ? n : 1;
	//Cacher toutes les divs
	var subMedias = mainMediaHDiv.getElementsByTagName('div');
	for(var i=0; i<subMedias.length; i++)
	{
		if(subMedias[i].id.substring(0,14) == "mediaHContent-")
		{
			subMedias[i].style.display = "none";
		}
	}
	//Mettre tous les bullets en blanc
	var bullets = mainMBulletsUl.getElementsByTagName('img');
	for(var i=0; i<bullets.length; i++)
	{
		if(bullets[i].id.substring(0,12) == "mediaBullet-")
		{
			bullets[i].src = "http://l.liberation.fr/img/common/bt_boule_off.gif";
		}
	}
	//Setter currentMediaSlide
	currentMediaSlide = n;
	//montrer la div courante
	var content = document.getElementById('mediaHContent-'+n);
	content.style.display = "block";	
	//Mettre le bullet rouge là où qui faut
	var oneBullet = document.getElementById('mediaBullet-'+n);
	oneBullet.src = "http://l.liberation.fr/img/common/bt_boule_on.gif";
}
function slideMediaH(mode)
{
	
	//Définir la div à montrer
	var goTo = 1;//Valeur par défaut
	if(mode == 'next')
	{
		if(currentMediaSlide >= numberMSlides)
			goTo = 1;
		else
			goTo = currentMediaSlide + 1;
	}
	else if(mode == 'prev')
	{
		if(currentMediaSlide == 1)
			goTo = numberMSlides;
		else
			goTo = currentMediaSlide - 1;		
	}
	goToMSlide(goTo);
}
goToMSlide();
//]]>
</script>
