<script language="javascript" type="text/javascript">
//<![CDATA[
var contentPos = 1;
var shakeTab = new Array();
var poirotDiv = document.getElementById('shakerRicardo');
Array.prototype.sortRandom = function()
{
	this.sort(function(elmt1, elmt2) { return ((2 * Math.round(Math.random())) - 1) });
}

var nbrBlocs = 7;
for(var i=1; i<=nbrBlocs; i++)
{
	shakeTab[i] = i;
}
shakeTab.sortRandom();

function shakeIt()
{
	if(contentPos > nbrBlocs)
		contentPos = 1;
	var contents = poirotDiv.getElementsByTagName("DIV");
	for(var i=0; i<contents.length; i++)
	{
		if(contents[i].id.substring(0,6) == "e1b59-")
		{
			contents[i].style.display = "none";
		}
	}
	var content = document.getElementById("e1b59-"+shakeTab[contentPos-1]);
	content.style.display = "block";
	contentPos++;
}
shakeIt();
//]]>
</script>
