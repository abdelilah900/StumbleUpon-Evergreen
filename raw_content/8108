<script language="javascript" type="text/javascript">
	/*Div parente*/
var mainLibevillesHDiv = document.getElementById('sliderLibeVilles');
var mainLVBulletsUl = document.getElementById('libeVillesUlBullets');
var currentLibeVillesSlide = 1;
var numberLVSlides = 9;
function goToLibeVillesSlide(n)
{
	var n = n ? n : 1;
	/*Cacher toutes les divs*/
	var subLibeVilles = mainLibevillesHDiv.getElementsByTagName('ul');
	for(var i=0; i<subLibeVilles.length; i++)
	{
		if(subLibeVilles[i].id.substring(0,20) == "libeVillesContentUl-")
		{
			subLibeVilles[i].style.display = "none";
		}
	}
	/*Mettre tous les bullets en blanc*/
	var bullets = mainLVBulletsUl.getElementsByTagName('img');
	for(var i=0; i<bullets.length; i++)
	{
		if(bullets[i].id.substring(0,17) == "libeVillesBullet-")
		{
			bullets[i].src = "http://l.liberation.fr/img/common/bt_boule_off.gif";
		}
	}
	/*Setter currentLibeVillesSlide*/
	currentLibeVillesSlide = n;
	/*montrer la div courante*/
	var content = document.getElementById('libeVillesContentUl-'+n);
	content.style.display = "block";	
	/*Mettre le bullet rouge là où qui faut*/
	var oneBullet = document.getElementById('libeVillesBullet-'+n);
	oneBullet.src = "http://l.liberation.fr/img/common/bt_boule_on.gif";
}
function slideLibeVilles(mode)
{
	
	/*Définir la div à montrer*/
	var goTo = 1;/*Valeur par défaut*/
	if(mode == 'next')
	{
		if(currentLibeVillesSlide >= numberLVSlides)
			goTo = 1;
		else
			goTo = currentLibeVillesSlide + 1;
	}
	else if(mode == 'prev')
	{
		if(currentLibeVillesSlide == 1)
			goTo = numberLVSlides;
		else
			goTo = currentLibeVillesSlide - 1;		
	}
	goToLibeVillesSlide(goTo);
}
goToLibeVillesSlide();
</script>
 

