<script type="text/javascript">
      var pubs = [
      {url: "http://flux.liberation.fr/viagogo/frame/libe", chance: 31, height:280},
      {url: "http://flux.liberation.fr/fnac/frame/home/libe/", chance: 23, height:375}, 
      {url: "http://blocguidescharme.liberation.fr/sites/affiliates/liberation/Flux/BlocCentralLiberationGDC.aspx", chance: 46, height:250}];


      function show_pub(iframe) {
      document.write("<iframe src='" + iframe.url  + "' width='300px' height='"+ iframe.height +"px' frameborder='0' scrolling='no' style='border: none;'>Votre navigateur ne supporte pas les iframes</iframe>");
      }


      var randomNumber = Math.random() * 100;

      var previous = 0;
      
      for(i = 0; i < pubs.length; i++) {
		     pub = pubs[i];
		     if ((previous <= randomNumber) &&
			 (randomNumber < previous + pub.chance)) {
			     show_pub(pub);
		      }
                      previous = previous + pub.chance;
      }
</script>
