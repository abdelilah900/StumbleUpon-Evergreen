	 <script language="javascript">        
        function sendHit(id,str){          
            var m1=null,m2=null,m3=null,m4=null,m5=null,m6=null,m7=null;
            var m8=null,m9=null,m10=null,m11=null,m12=null,m13=null,m14=null; 
            var xtplyr = document.getElementById(id);
            var xtcfg = xtplyr.getConfig();           
            for(var itm in xtcfg) {
                switch(itm){
                    case 'xttype': {m1=xtcfg[itm];break}
                    case 'xtn2': {m2=xtcfg[itm];break}
                    case 'xtrichname': {m3=xtcfg[itm];break}                                
                    case 'xtcustom': {m5=xtcfg[itm];break}
                    case 'xtrefresh': {m6=xtcfg[itm];break}
                    case 'xtduration': {m7=xtcfg[itm];break} 
                    case 'xtplayer': {m8=xtcfg[itm];break}
                    case 'xtquality': {m9=xtcfg[itm];break}
                    case 'xtstream': {m10=xtcfg[itm];break}
                    case 'xtlocation': {m11=xtcfg[itm];break}
                    case 'xtmode': {m12=xtcfg[itm];break}
                    case 'xtsize': {m13=xtcfg[itm];break} 
                    case 'xtextension': {m14=xtcfg[itm];break}    
                    default:{break}    
                } 
            }	                       
            if(str.indexOf('STATE')>=0){ 
                var oldstate='',newstate='';  
                var xtVal=str.toLowerCase().substring(str.indexOf('(')+1,str.length-1).split(',');                            
                for(var i=0;i<xtVal.length;i++){         
                    if(xtVal[i].split(':')[0]=='newstate'){newstate=xtVal[i].split(':')[1];}
                    if(xtVal[i].split(':')[0]=='oldstate'){oldstate=xtVal[i].split(':')[1];}
                }         
                switch(newstate+oldstate){
                    case 'idleplaying': {m4='stop';break}
                    case 'idlepaused': {m4='stop';break}
                    case 'playingidle': {m4='play';break}
                    case 'playingpaused': {m4='play';break}
                    case 'pausedplaying': {m4='pause';break}                          
                    default:{break}    
                }                                     
            }             
            if(m4!=null){   
	        xt_rm(m1,m2,m3,m4,m5,m6,m7,m8,m9,m10,m11,m12,m13,m14);		    
	        m4=null;       
            }               
        }
    </script>						
