      depeches[54]["publicationHour"] = "00:34";
      depeches[54]["source"] = "AFP";
      depeches[54]["istoday"] = "";
        
  function bloc_depeches(page){
    var out = "";
    var nombre_pages = 10;
    var nombre_par_page = 6;
    var from = (page-1)*nombre_par_page;
    var to = from+nombre_par_page;

    for(var i=from; i < to; i++){ 
      if(depeches[i] != null){
        var date = depeches[i]["publicationDate"] + " " + depeches[i]["publicationHour"];
        if(depeches[i]["istoday"]){
          date = depeches[i]["publicationHour"];
        }
        if(depeches[i]["source"] == "AFP"){
          out += "<li>" + date + " " + depeches[i]["source"] + "<a href=\"http://www.liberation.fr" +depeches[i]["url"]+ "\">" + depeches[i]["title"] + "</a></li>";
        }else if(depeches[i]["source"] == "REUTERS"){
          out += "<li>" + date + " " + depeches[i]["source"] + "<a href=\"http://www.liberation.fr" +depeches[i]["url"]+ "\">" + depeches[i]["title"] + "</a></li>";
        }
      }
    }
    
    /* Affiche les dépèches dans la liste UL */
    var container = document.getElementById("liste_depeches");
    container.innerHTML = out;
    
    /* Enregistre le numéro de la page courante */
    if(page > nombre_pages){
      page = 1;
      bloc_depeches(page);
    }else if(page < 1){
      page = nombre_pages;
      bloc_depeches(page);
    }
    bloc_depeches_indice = page;
    
    /* Remet tout les boutons en blanc */
    var boutons = document.getElementsByTagName("IMG");
    for(var i=0; i<boutons.length; i++){
      if(boutons[i].className == "bloc_depeches_bouton"){
        boutons[i].src = "http://l.liberation.fr/img/common/bt_boule_off.gif";
      }
    }
    /* Met le bouton selctionné en rouge */
    var bouton = document.getElementById("bloc_depeches_page_"+page);
    bouton.src = "http://l.liberation.fr/img/common/bt_boule_on.gif";

  }
</script>
<div id="e1b3-1" class="blocE1B-3" style="display: block;">
