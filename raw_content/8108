<script language="javascript" type="text/javascript">
	/*Div parente*/
	var mainForumsHDiv = document.getElementById('blocE1B-32-forumlist');
	var mainFBulletsUl = document.getElementById('forumHUlBullets');
	var currentForumSlide = 1;
	var numberFSlides = 13;
	
	function goToFSlide(n)
	{
		var n = n ? n : 1;
		/*Cacher toutes les divs*/ 
		var subForums = mainForumsHDiv.getElementsByTagName('div');
		for(var i=0; i<subForums.length; i++)
		{
			if(subForums[i].id.substring(0,15) == "listForumSlide-")
			{
				subForums[i].style.display = "none";
			}
		}
		/*Mettre tous les bullets en blanc*/
		var bullets = mainFBulletsUl.getElementsByTagName('img');
		for(var i=0; i<bullets.length; i++)
		{
			if(bullets[i].id.substring(0,13) == "forumsBullet-")
			{
				bullets[i].src = "http://l.liberation.fr/img/common/bt_boule_off.gif";
			}
		}
		/*Setter currentForumSlide*/
		currentForumSlide = n;
		/*montrer la div courante*/
		var content = document.getElementById('listForumSlide-'+n);
		content.style.display = "block";	
		/*Mettre le bullet rouge là où qui faut*/
		var oneBullet = document.getElementById('forumsBullet-'+n);
		oneBullet.src = "http://l.liberation.fr/img/common/bt_boule_on.gif";
	}
	function slideForumH(mode)
	{
		
		/*Définir la div à montrer*/
		var goTo = 1;/*Valeur par défaut*/
		if(mode == 'next')
		{
			if(currentForumSlide >= numberFSlides)
				goTo = 1;
			else
				goTo = currentForumSlide + 1;
		}
		else if(mode == 'prev')
		{
			if(currentForumSlide == 1)
				goTo = numberFSlides;
			else
				goTo = currentForumSlide - 1;		
		}
		goToFSlide(goTo);
	}
	goToFSlide(1);
</script>

