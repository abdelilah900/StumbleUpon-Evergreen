    $(function(){

        
        $("#profileImg").attr("src", 'https://graph.facebook.com/210633705636159/picture?type=small').fadeIn();

        $(".email-signup-container .user-email").val('ENTER EMAIL HERE');

        $(".email-signup-container .user-email").focus(function(){
            var userEmail = $(this).val();
            if (userEmail == 'ENTER EMAIL HERE'){
                $(this).val('');
            }
        });

        $(".email-signup-container .user-email").blur(function(){
            var userEmail = $(this).val();
            if (trim(userEmail) == ''){
                $(this).val('ENTER EMAIL HERE');
            }
        });
    });
	
