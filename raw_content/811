			function roundIt(myElem){
				
					if (!myElem.parent().hasClass('cornerBoxInner')){
						var myMargin = {
							top:myElem.css("marginTop"),
							right:myElem.css("marginRight"),
							bottom:myElem.css("marginBottom"),
							left:myElem.css("marginLeft")
						};
						myElem.css("margin","0");
					  myElem.wrap(function(){
							return '<div class="cornerBox" style="width:' + myElem.innerWidth() + 'px;" />'
							}).before('<div class="corner TL"></div><div class="corner TR"></div><div class="corner BL"></div><div class="corner BR"></div>').wrap('<div class="cornerBoxInner" />');

						myElem.parents('.cornerBox').css({"marginTop":myMargin.top,"marginRight":myMargin.right,"marginBottom":myMargin.bottom,"marginLeft":myMargin.left});
						if (myElem.is(":hidden"))
							myElem.parents('.cornerBox').hide(); /* needed for EMWA button toggle */
					}
				
			}
