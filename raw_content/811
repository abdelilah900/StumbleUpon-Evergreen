<script type="text/javascript">
	var thumbnailUtils = function($) {
		/* private methods */
		var $thumbCollection = null; /* holders for all thumbnails */

		var _toggleBackToTop = function(scrollTop) {
			if($(document).scrollTop() > 0){
				$("#back-to-top").fadeIn('slow');
			}else{
				$("#back-to-top").fadeOut('slow');
			}
		};

		var _displayProductImage = function(scrollTop) {
			var startPoint = (scrollTop-100) < 0 ? 0 : (scrollTop-100); /* scroll top less a buffer */
			var stopPoint = scrollTop + $(window).height();
			var thumbSeries = []; /* the collection to retrieve */

			for(i=0; i<$thumbCollection.length; i++) {
				if ($thumbCollection.eq(i).hasClass("js_productLoading")) {
					var _offset = $thumbCollection.eq(i).offset();
					if (_offset.top >= startPoint) {
						if (_offset.top > stopPoint)
							break;
						thumbSeries.push($thumbCollection.eq(i));
					}
				}
			}

			for(j=0; j<thumbSeries.length; j++) {
				var $currentProd = thumbSeries[j];
				if ($currentProd.length > 0) {
					var $currentProductImage = $currentProd.find(".js_productImageLoad");
					if ($currentProductImage.length > 0) {
						//update the source of the image tag to be that of the product image
						$currentProductImage.attr("src", $currentProductImage.attr("data-productimagesrc"));
						//remove the class that hsows the product loading image as the background image
						$currentProd.removeClass("js_productLoading");
					}
				}
			}
		};
		return {
			/* public methods */
			init : function() {
				$thumbCollection = $(".js_itemWrap");

				$("#back-to-top").click(function(){
					$("html, body").animate({ scrollTop: 0 }, "slow");
					  return false;
				});
				
				$(window).bind("scrollstop", function () {
					var st = $(document).scrollTop();
					_toggleBackToTop(st);
					_displayProductImage(st);
				}).trigger("scrollstop"); /* explicitly fire it once if the user is already scrolled down and they refresh the page */

			}
		};
	}($);

	$(document).ready(function entityThumbnailInit() {
		thumbnailUtils.init();

		$(".jsSeeMoreColors").click(function(e){
			e.preventDefault();
			var $currentProduct = $(this).parents(".Quicklook");
			var z = parseInt($currentProduct.css("z-index"));
			z = (z > 0) ? z : 0;
			var $remainingSwatches = $(".remainingSwatches", $currentProduct);
			if ($remainingSwatches.is(":hidden")) {
				$currentProduct.css("z-index",z+10);/* since all cells are position relative, we need to make this one higher so the remaining swatches will always be on top */
				$remainingSwatches.show();
				e.stopPropagation();
				$("body").bind("click.remainingSwatches", function(e){
					var clicked = $(e.target);
					if (!clicked.parents().hasClass("remainingSwatches")) {
						$remainingSwatches.hide();
						$currentProduct.css("z-index",parseInt($currentProduct.css("z-index"))-10);
						$("body").unbind("click.remainingSwatches");
					}
				});
			} else {
				$remainingSwatches.hide();
				$currentProduct.css("z-index",z-10);
				$("body").unbind("click.remainingSwatches");
			}
		});
	});

</script></div>
		</div>
		<div class="clr"></div>
		</div>
</div>
<script type="text/javascript" language="JavaScript">
   var parentCatName = 'New';
   var catName = 'Sequins &amp; Sparkle';
   var subCatName = '';

   var pageName = parentCatName;
   if (catName != '')
	   pageName = pageName+":"+ catName;
   if(subCatName != '')
    	pageName = pageName+":"+ subCatName;
   
   s.pageName = pageName;
   s.server = "null";
   s.channel = parentCatName;
   s.prop1 = parentCatName + ":" + catName;
   s.prop8 = subCatName != '' ? "sub category" : "category";
   if (subCatName == '') {
	   s.prop9=parentCatName + ":" + catName;	
		s.eVar35=parentCatName + ":" + catName;
   } else {
   		s.prop9=parentCatName + ":" + catName + ":" + subCatName; 
   		s.eVar35=parentCatName + ":" + catName+ ":" + subCatName;
   }
  
	s.eVar10 = "";
   
	s.eVar34 = parentCatName;	

   var s_code=s.t();if(s_code)document.write(s_code);
</script>
