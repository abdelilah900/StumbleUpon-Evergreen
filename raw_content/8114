<script type="text/javascript">cnnInitOverlay();</script>

<script type="text/javascript">

var CNN_SocialManager = (function () {
	var poll_timer,
		poll_tries = 0,
		socialNodes,
		socialNodesCounter = -1,

	// init
		init = function () {
			socialNodes = $('cnnBlogContentArea').select('.comments');
			if ( socialNodes ) {
				poll_timer = window.setInterval( pollForFB, 500 );	// wait for FB
			}
		},

	// pollForFB -- wait for FB before rendering social icons
		pollForFB = function () {
			poll_tries++;
			if ( typeof FB === "object" ) {
				window.clearInterval( poll_timer );
				getSocial();
			}
			else if ( poll_tries > 15 ) {
				window.clearInterval( poll_timer );
			}
		},

	// getSocial -- makes calls to Facebook and Disqus for numbers
		getSocial = function () {
			var FBQueries = [];
			
			socialNodes.each(function (node) {
				var href = node.href.replace('#comments',''),
					container;
				
				socialNodesCounter++;
				
				container = new Element( 'span', { 'class':'cnnSocialContainer','id': 'cnnSocialContext-' + socialNodesCounter, 'style': 'display: none' } ).update(
					'<img src="http://i.cdn.turner.com/cnn/.element/img/3.0/sect/blogs/eatocracy/gfx/icon_facebook_overwhite_small.gif" width="13" height="14" alt="" border="0"><span class="cnnFBCount" id="cnnFBCount-' + socialNodesCounter + '"><\/span>'
				);
				
				node.insert( {'before':container} );
				
					FBQueries.push( 'SELECT total_count FROM link_stat WHERE url="' + href + '"' );
				
				
			});
			
			// for Facebook numbers
			if ( FBQueries.length > 0 ) {
				FB.api(
					{
						method: 'fql.multiquery',
						queries : FBQueries
					},
					FBcallback
				);
			}
			
		},
		
	// FBcallback -- FB makes an async call
		FBcallback = function (obj) {
			var index = 0;
			for ( ; index <= socialNodesCounter; index++ ) {
				$('cnnFBCount-'+index).update( obj[index].fql_result_set[0].total_count );
				$('cnnSocialContext-'+index).show();
			}
		}
	;

	return ({
		init: init
	});
})();

Event.observe( window, 'load', CNN_SocialManager.init );
</script>

</body>
</html>