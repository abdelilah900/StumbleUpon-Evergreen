dojo.addOnLoad(function(){
try {
var comet = new hsn.Comet('ip-204-236-182-111.chill.com', '80', '80', '80', '', '');
comet.startCometService();
dojo.doc.comet = comet;
//Check for any comet subscriptions to add
if(dojo.doc.cometSubscriptions){
dojo.forEach(dojo.doc.cometSubscriptions, function(c){
comet.subscribe(c)
})
//Clean up
dojo.doc.cometSubscriptions = null
}
dojo.addOnUnload(function() {
try {
comet.stopCometService();
} catch(e){
//error unloading danbox
}
});
} catch(e) {
//console && console.warn && console.warn('Error connecting to Danbox ' + e);
}
});

