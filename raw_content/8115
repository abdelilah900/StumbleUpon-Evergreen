//load FB
window.fbAsyncInit = function() {
FB.init({appId: '187663324592154',
channelUrl:'http://d3ar1agdggt2nn.cloudfront.net/static-versioned/5ee12707f54a4af13db5808826dd8040.html',
oauth: true,
status: true,
cookie: true,
xfbml: true});
fbSDKLoaded=true;
var postFBParse = function(){
var els = [];
if (document.getElementsByClassName){
els = document.getElementsByClassName('fb-fbml-frame')
} else if (document.querySelectorAll) {
els = document.querySelectorAll('.fb-fbml-frame')
} else {
var node = document.getElementById('fbLikeSplash')
if (node){
els.push(node)
}
}
FB.getLoginStatus(function(r){
if (r.status=='unknown'){
for (var i=0;i<els.length;i++){
els[i].style.display='none'
}
} else {
for (var i=0;i<els.length;i++){
els[i].style.visibility=''
}
}
})
}
FB.XFBML.parse(null, postFBParse)
};
