function hasCookieSupport() {
	window.document.cookie = "cookieTester=1; expires=Tue, 01 Mar 2033 17:21:05 GMT; path=/";
	if (window.document.cookie.indexOf("cookieTester=1") !== -1) {
		return true;
	} else {
		return false;
	}
}
