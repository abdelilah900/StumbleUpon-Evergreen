try
{
SetActiveCat();
}
catch(e)
{}
function SetActiveCategory()
{
var str= window.location.href.replace(window.location.host,"").replace("http://","");
var str2 = str.substring(0,str.lastIndexOf("/"));
var catItem;
var catID;
if(str2 !="")
{
var arrFolders = str2.split("/");
for(var i=1; i<arrFolders.length; i++)
{  catID = arrFolders[i] + "Folder";
catItem = document.getElementById(catID); 
if (catItem!=null)
{ 
catItem.setAttribute("className","categoryNavActive");
catItem.setAttribute("class","categoryNavActive");
}
}
}
}
</script>

