<script type="text/javascript">
	var mh_flyout_callState1 = 0;	
	function displayMenu1(e)
	{
		hideMenuImmed();
		var gmnuObj = this;
		jQuery(this).attr('ON','1');
		jQuery(this).removeAttr('OFF');
		setTimeout(function() {
			var mnuObj = gmnuObj;
			if(jQuery(mnuObj).attr('ON') == '1')
			{
				jQuery('.mh_flyout_menu1').css('display','none');
				jQuery('#mh_header_flyout_subscribe').css('display','block');
				jQuery(mnuObj).removeAttr('ON');
			}
			},
			500
		);
	}
	function hideMenu1(e)
	{
		mh_flyout_callState1=0;
		var gmnuObj = this;
		jQuery(this).attr('OFF','1');
		jQuery(this).removeAttr('ON');
		setTimeout(function() {
			var mnuObj = gmnuObj;
			if((mh_flyout_callState1==0) && (jQuery(mnuObj).attr('OFF') == '1'))
			{
				jQuery('#mh_header_flyout_subscribe').css('display','none');
				jQuery(mnuObj).removeAttr('OFF');
			}
			},
			500
		);
	}
	
	function setCallOnState1(e)
	{
		mh_flyout_callState1 = 1;
	}
	function setCallOffState1(e)
	{
		mh_flyout_callState1 = 0;
		var mnuId = "#mh_header_subnav_subscribe";
		jQuery(mnuId).trigger("mouseout");
	}
	
	function hideMenuImmed1(e)
	{
		mh_flyout_callState1=0;
		jQuery('#mh_header_flyout_subscribe').css('display','none');
	}

	var mh_flyout_callState2 = 0;	
	function displayMenu2(e)
	{
		hideMenuImmed();
		var gmnuObj = this;
		jQuery(this).attr('ON','1');
		jQuery(this).removeAttr('OFF');
		setTimeout(function() {
			var mnuObj = gmnuObj;
			if(jQuery(mnuObj).attr('ON') == '1')
			{
				jQuery('.mh_flyout_menu2').css('display','none');
				jQuery('#mh_header_flyout_bootcamp').css('display','block');
				jQuery(mnuObj).removeAttr('ON');
			}
			},
			500
		);
	}
	function hideMenu2(e)
	{
		mh_flyout_callState2=0;
		var gmnuObj = this;
		jQuery(this).attr('OFF','1');
		jQuery(this).removeAttr('ON');
		setTimeout(function() {
			var mnuObj = gmnuObj;
			if((mh_flyout_callState2==0) && (jQuery(mnuObj).attr('OFF') == '1'))
			{
				jQuery('#mh_header_flyout_bootcamp').css('display','none');
				jQuery(mnuObj).removeAttr('OFF');
			}
			},
			500
		);
	}
	
	function setCallOnState2(e)
	{
		mh_flyout_callState2 = 1;
	}
	function setCallOffState2(e)
	{
		mh_flyout_callState2 = 0;
		var mnuId = "#mh_header_subnav_bootcamp";
		jQuery(mnuId).trigger("mouseout");
	}
	
	function hideMenuImmed2(e)
	{
		mh_flyout_callState2=0;
		jQuery('#mh_header_flyout_bootcamp').css('display','none');
	}

$(document).ready( function() {
	$('.mh_flyout_menu2').hover(
		setCallOnState2,
		setCallOffState2
	);
	$('#mh_header_subnav_bootcamp').hover(
 		 displayMenu2,
 		 hideMenu2
 	);
	 
 	$('#hp-main-nav').hover(
	hideMenuImmed2,
	function(){}
	);
});

	var mh_flyout_callState3 = 0;	
