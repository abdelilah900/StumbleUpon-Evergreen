<script type="text/javascript">
  // check for iphone
  var regex = new RegExp("Mozilla/5.0(.*)AppleWebKit/(.*) \\(KHTML, like Gecko\\) Version/.* Mobile/(.*) Safari/(.*)");
  var matches = regex.exec(navigator.userAgent);

  if (matches) {
    var iTunesLink = "";
    var AppPromo = "";

    // check for iPad
    if( matches[1].indexOf("(iPad") == 1 ){
      iTunesLink = "http://itunes.apple.com/us/app/huffington-post-for-ipad/id376304186?mt=8";
      AppPromo = "<center><h1>HuffingtonPost now has an <strong>iPad App</strong><br><br><a href=\"" +
                        iTunesLink + "\">Download and install it Here</a></h1></center>";
    }else{
      iTunesLink = "http://itunes.apple.com/WebObjects/MZStore.woa/wa/viewSoftware?id=306621789&mt=8";
      AppPromo = "<center><h1>HuffingtonPost now has an iPhone App<br><br><a href=\"" +
                        iTunesLink + "\">Download and install it Here</a></h1></center>";

    }
    document.write("<div><br>" + AppPromo + "</div><br><div class=\"clear full\"></div>");
  }
  
	//Initialize our user agent string to lower case.
	var uagent = navigator.userAgent.toLowerCase();
	  
	if(uagent.search("android") > -1) {
  		androidLink = 'http://market.android.com/details?id=com.huffingtonpost.android';
  		androidPromo = "<center><div class='arial_44 bold'><img alt=\'Android\' align='top' src=\'/images/icons/trans-android-big.png\' /><a href=\"" + androidLink + "\" class='color_444444'>HuffPost Has An Android App</a><br>" +
                        "<a href=\"" + androidLink + "\" class='block' style='margin-top:-45px'>Download and install it here</a></div></center>";
		document.write("<div><br>" + androidPromo + "</div><br><div class=\"clear full\"></div>");
	} else if (uagent.search("blackberry") > -1) {
  		bbLink = 'http://appworld.blackberry.com/webstore/content/19143';
  		bbPromo = "<center><div class='arial_44 bold'><img alt=\'Blackberry\' align='top' src=\'/images/icons/bb.png\' /><a href=\"" + bbLink + "\" class='color_444444'>HuffPost Has A Blackberry App</a><br>" +
                        "<a href=\"" + bbLink + "\" class='block'>Download and install it here</a></div></center>";			
		document.write("<div style='line-height:normal;'>" + bbPromo + "</div><br><div class=\"clear full\"></div>");
	}
</script>

