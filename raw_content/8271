                	<div id="content">
                    	
                                                
                        <div style="width:336px;height:280px;float:left;margin:0 10px 10px 0;">
                        <script type="text/javascript">
							google_ad_client = "ca-pub-1335720096597487";
							google_ad_width = 336;
							google_ad_height = 280;
							google_ad_format = "336x280_as";
							google_ad_type = "text";
							google_ad_channel = "";
							google_color_border = "FFFFFF";
