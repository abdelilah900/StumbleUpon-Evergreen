	function storeAdZoneObjInfo(adZoneObj) {
		var azval = 'mt_adzone@' + adZoneObj['mt_adzone'];
		azval = escape(azval);
		saveVirtualCookie('mt_az',azval);
	}

	function readAdZoneObjInfo(adZoneObj) {
		var val = readVirtualCookie('mt_az');
		val = unescape(val);
		if(val.length>5) {
			var vals = val.split('|');
			for(a=0;a<vals.length;a++) {
				var data = vals[a].split('@');
				adZoneObj[data[0]] = data[1];
			}
		}
	}
	
	function injectBreadCrumb(currentChannelInfo) {
		val = readVirtualCookie('mt_lncInfo');
		val = unescape(val);
		
		if(val.length>5) {
			var bcs = val.split('@');
			var titles = bcs[0].split('|'); 
			var urls = bcs[1].split('|');
			var htmlBC = '';
			for(var a=0;a<titles.length;a++) {
				htmlBC = htmlBC + ' <a href="' + urls[a] + '">' + titles[a] + '</a>  &#187 ';
			}
			htmlBC = htmlBC + '<span id="ms-global-breadcrumbs-youarehere" class="ms-global-breadcrumbs-youarehere"> ' +  currentChannelInfo['mt_chnT'] + '</span>';
			document.getElementById('ms-global-breadcrumbs').innerHTML = htmlBC;
		}
		
	}	
	function ct_querySt(key) {
	    ct_qstr = window.location.search.substring(1);
	    ct_qstr_tokens = ct_qstr.split("&");
	    for (i=0;i<ct_qstr_tokens.length;i++) {
	        ct_str = ct_qstr_tokens[i].split("=");
	        if (ct_str[0] == key) {
	            return ct_str[1];
	        }
	    }
	}
	
