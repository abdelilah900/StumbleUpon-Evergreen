			Event.observe(document.body, 'click', function(event) { 
				var elt = Event.element(event); 
				if((typeof(elt)!='undefined') && elt && (typeof($(elt).up)!='undefined') && $(elt).up) {
				if (('a' == elt.tagName) || ('A' == elt.tagName) || ('a' == $(elt).up(0).tagName) || ('A' == $(elt).up(0).tagName)) {
					saveVirtualCookie('mt_lncInfo', cok_val);
					storeAdZoneObjInfo(lncInfo);
				}
				}
			});
