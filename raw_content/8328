<!--Make the Placeholder attribute work in IE and other legacy browsers
	See http://www.cssnewbie.com/cross-browser-support-for-html5-placeholder-text-in-forms/
-->
<script type="text/javascript">
 jQuery(function() {
	if(!jQuery.support.placeholder) { 
		var active = document.activeElement;
		//Text Inputs
		jQuery('form :text').focus(function () {
			if (jQuery(this).attr('placeholder') != '' && jQuery(this).val() == jQuery(this).attr('placeholder')) {
				jQuery(this).val('').removeClass('hasPlaceholder');
			}
		}).blur(function () {
			if (jQuery(this).attr('placeholder') != '' && (jQuery(this).val() == '' || jQuery(this).val() == jQuery(this).attr('placeholder'))) {
				jQuery(this).val(jQuery(this).attr('placeholder')).addClass('hasPlaceholder');
			}
		});
		jQuery('form :text').blur();
		//Textareas
		jQuery('form textarea').focus(function () {
			if (jQuery(this).attr('placeholder') != '' && jQuery(this).val() == jQuery(this).attr('placeholder')) {
				jQuery(this).val('').removeClass('hasPlaceholder');
			}
		}).blur(function () {
			if (jQuery(this).attr('placeholder') != '' && (jQuery(this).val() == '' || jQuery(this).val() == jQuery(this).attr('placeholder'))) {
				jQuery(this).val(jQuery(this).attr('placeholder')).addClass('hasPlaceholder');
			}
		});
		jQuery('form textarea').blur();
		
		jQuery(active).focus();
		jQuery('form').submit(function () {
			jQuery(this).find('.hasPlaceholder').each(function() { jQuery(this).val(''); });
		});
	}
});
</script>

	
	

