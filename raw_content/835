<script type="text/javascript">
jQuery(function(){
jQuery(document).ready(function(){
	jQuery(".menu > li").click(function(e){
		switch(e.target.id){
			case "recent":
				//change status & style menu
				jQuery("#recent").addClass("active");
				jQuery("#popular").removeClass("active");
				jQuery("#rcomments").removeClass("active");
				jQuery("#tags").removeClass("active");
				//display selected division, hide others
				jQuery("div.recent").fadeIn();
				jQuery("div.popular").css("display", "none");
				jQuery("div.rcomments").css("display", "none");
				jQuery("div.tags").css("display", "none");
			break;
			case "popular":
				//change status & style menu
				jQuery("#recent").removeClass("active");
				jQuery("#popular").addClass("active");
				jQuery("#rcomments").removeClass("active");
				jQuery("#tags").removeClass("active");
				//display selected division, hide others
				jQuery("div.popular").fadeIn();
				jQuery("div.recent").css("display", "none");
				jQuery("div.rcomments").css("display", "none");
				jQuery("div.tags").css("display", "none");
			break;
			case "rcomments":
				//change status & style menu
				jQuery("#recent").removeClass("active");
				jQuery("#popular").removeClass("active");
				jQuery("#rcomments").addClass("active");
				jQuery("#tags").removeClass("active");
				//display selected division, hide others
				jQuery("div.rcomments").fadeIn();
				jQuery("div.recent").css("display", "none");
				jQuery("div.popular").css("display", "none");
				jQuery("div.tags").css("display", "none");
			break;
			case "tags":
				//change status & style menu
				jQuery("#recent").removeClass("active");
				jQuery("#popular").removeClass("active");
				jQuery("#rcomments").removeClass("active");
				jQuery("#tags").addClass("active");
				//display selected division, hide others
				jQuery("div.tags").fadeIn();
				jQuery("div.rcomments").css("display", "none");
				jQuery("div.recent").css("display", "none");
				jQuery("div.popular").css("display", "none");				
			break;			
		}
		//alert(e.target.id);
		return false;
	});
});
});
</script>
