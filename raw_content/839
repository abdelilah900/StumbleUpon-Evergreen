                    if (typeof (GS_googleAddAdSenseService) == "undefined") {
                        AnalyticsCustomEvent("AdServe", "Blocked");
                    }
                    else {
                        AnalyticsCustomEvent("AdServe", "Served");
                    }
                }
            </script>
        
    </div>
</div>


        </div>
    </span>


<!-- END: GoogleAdManagerControl -->


<!-- END: DisplayAd -->
                    </div>
                
                
<script type="text/javascript">
    var StorInSec_activePage = 0;
    jQuery(document).ready(function () {
        jQuery(".jcarousel-tempButton").css("display", "none");
        jQuery('#StorInSec')
        jQuery('#StorInSec').jcarousel({ 
            scroll: 5, 
            animation: "fast",
            itemFirstInCallback: {onBeforeAnimation: StorInSec_updateDots}
        });
    });
    function StorInSec_updateDots(carouselInstance,liObject,positionIndex,action){
        if(action != "init"){
            if (positionIndex == 1){
                StorInSec_activePage = 0;
            }else{
                StorInSec_activePage = Math.ceil((positionIndex -1)/5);
            }
            AnalyticsCustomEvent("StoryInSectionCarouselLatest",action,"13-More Crime & Courts Stories");
            StorInSec_setBullets();
        }
    }
    function StorInSec_ScrollTo(item,animate,page){
            jQuery('#StorInSec').jcarousel("scroll",item,animate);
    }
    function StorInSec_setBullets(){
        jQuery(".bullets_StorInSec").removeClass("active").addClass("inactive");
        jQuery(".bullets_StorInSec.bullet_" + StorInSec_activePage).removeClass("inactive").addClass("active");
    }
</script>
