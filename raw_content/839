<script type="text/javascript" >
function vote(categoryId, categoryName) {
    var userType = "U";
    var userId = 0;
    var visitorId;
    if (userId == 0) {
        userType = "V";
        visitorId = 325357633;
    }
    var promptShare = false;
    var autoShare = false;
    var neverShare = false;
    var showShare = !promptShare &&  !autoShare && !neverShare;
    if (promptShare){
        
        void(0);
    }
    var storydesc = jQuery("meta[name=description]").attr("content");
    var url = '/controlpage.aspx?control=StoryCategory&storyid=' + storyId + '&storycategoryid=' + categoryId + '&PageTitle=' + document.title + '&storyheadline=' + storyHeadline + '&userid=' + userId + '&visitorid=' + visitorId + '&type=' + userType + "&showshare=" + showShare + "&autoshare=" + autoShare + "&storydescription=" + storydesc;
    AjaxReplace(url, jQuery('#voteDivContent'));
    
}
</script>


