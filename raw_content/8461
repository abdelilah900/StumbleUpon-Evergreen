<script type="text/javascript">
	var promoted_switch = function() {
		if( typeof(promoted_match) != 'undefined' && $(promoted_match) ) {
			$(promoted_match).hide();
			$('promoted-extra-hidden').show();
		}
	}
	promoted_switch();	
</script>

