<script type="text/javascript">
function initialize() {
  	/* HEADER NEWS RSS */
    if ( typeof(hnews) == 'string' && hnews && $("#nwshd").length ) {
      var feed = new google.feeds.Feed(hnews);
      feed.setNumEntries(8);
      feed.load(loadNewsHeader);
    }
	/* Paper RSS */
    if ( typeof(paperRSS) == 'string' && paperRSS && $("#feed-paper").length ) {
      var feed = new google.feeds.Feed(paperRSS);
      feed.setNumEntries(8);
      feed.load(loadRssPaper);
    }
	/* Paper twitter timeline */
    if ( typeof(paperTwitter) == 'string' && paperTwitter && $("#timeline-paper") ) {
      loadTwitterData(paperTwitter);
    }
	/* kiosko facebook livebox */
    if ( $("#fb-frame") ) {
      fb = $("#fb-frame");
      w=""; c=4;
      if ( fb ) {
        w = fb.outerWidth();
        c = parseInt((w-20)/55);
      }
      fb.attr('src',"http://www.facebook.com/plugins/likebox.php?id=243510920909&width="+ w +"&connections="+ c +"&stream=false&header=false&height=220");
    }
}
google.setOnLoadCallback(initialize);
google.load("feeds", "1");
</script>

