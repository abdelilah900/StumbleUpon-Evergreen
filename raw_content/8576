	<script type="text/javascript">
		function setTitle() {
			if($('#the_daily_four_promo_title_1').get(0)) {
				$('#the_daily_four_title').html($('#the_daily_four_promo_title_1').html());
				$('#the_daily_four_promo_1').addClass('active');
				return;
			}

			setTimeout('setTitle()', 1000);
		}
		
		$(document).ready(function()
		{
			$("#the_daily_four_promos .promo").hover(
				function () {
					var titleId = $(this).attr('id').replace('_promo_', '_promo_title_');
					
					$('#the_daily_four_title').html(
						$('#' + titleId).html()
					);

					$("#the_daily_four_promos .promo").removeClass('active');
					$(this).addClass('active');
				},
				function () {
				}
			);

			setTitle();
		});
	</script>

