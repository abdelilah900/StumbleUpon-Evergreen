<!-- Fade Category Navigation Background -->
<script type="text/javascript">
var navBG = jQuery("#cats .nav li").css("background-color");
var fadeColor = "#eaeaea";
jQuery('#cats .nav li').css({ backgroundColor : navBG }, 0 );
jQuery("#cats .nav li").hover(function () {
jQuery(this).stop().animate( { backgroundColor:fadeColor }, 444 )
}, function () {
jQuery(this).stop().animate( {backgroundColor: navBG }, 333 )
});

</script>

