	function validate_email(field,alerttxt) { 	
		with (field) { 		
			apos=value.indexOf("@"); 		
			dotpos=value.lastIndexOf("."); 		
			if (apos<1||dotpos-apos<2) { 			
				alert(alerttxt); 			
				field.focus(); 			
				return false; 		 
				} 		else {
					return true;
					} 	
					} 
					
				}
		String.prototype.trim = function() { return this.replace(/^\s*(\b.*\b|)\s*$/, "$1");	};
		function mandatoryText(input,fieldName){ 	if(input.value.trim()==''||input==null){		
		alert('Please enter your '+fieldName+'.');
		input.focus(); 		
		return false; 	
		} else { 		return true; 	} } 
		function validForm(){ 	if(!mandatoryText(document.getElementById('email'), 'E-mail address')) return; 	
		if(!validate_email(document.getElementById('email'),"Not a valid e-mail address!")) return;
			document.getElementById('user-regbyemail').submit();
		}
</script>
			
