			    <div id="bb-dropdown" class="dropdown">
        <a  href="#">BROWSE BY<span class="ui-icon ui-icon-888888 ui-icon-triangle-1-s"></span></a>
        <ul class="hidden">
            <li><a  class="selected" id="bb-tile" href="#">TILES</a></li>
            <li><a  class="" id="bb-excerpt" href="#">EXCERPTS</a></li>
            <li><a  class="" id="bb-article" href="#">ARTICLES</a></li>
        </ul>
    </div>
    <script type="text/javascript">
    jQuery("#bb-dropdown>ul>li>a").click(function(e) {
        e.preventDefault();
        var param = /(browseby=)(\w*)/,
            value = jQuery(this).attr("id").substr(3),
            url = location.href.replace(/page\/[0-9]*\//, "");
        url = url.replace(/#.*/, "");
        jQuery.cookie("wp-settings-browseby", value, {path: "/", expires: 100});
        if( param.test(location.href) ) { url = url.replace(param, '$1' + value); }
        else if( /\?/.test(location.href) ) { url = url + "&browseby=" + value; }
        else { url = url + "?browseby=" + value; }
        location.href = url;
    });
    </script>
