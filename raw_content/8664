			<div id="panel-tabs-container">
			<ul id="panel-tabs">
						<li class="panel-tab" id="panel-tab-div-0" onClick="selectPanel(0);" style="background-color:#7baee1;"><span>0</span></li>
						<li class="panel-tab" id="panel-tab-div-1" onClick="selectPanel(1);" style="background-color:#e0efff;"><span>1</span></li>
						<li class="panel-tab" id="panel-tab-div-2" onClick="selectPanel(2);" style="background-color:#e0efff;"><span>2</span></li>
						<li class="panel-tab" id="panel-tab-div-3" onClick="selectPanel(3);" style="background-color:#e0efff;"><span>3</span></li>
						<li class="panel-tab" id="panel-tab-div-4" onClick="selectPanel(4);" style="background-color:#e0efff;"><span>4</span></li>
						<li class="panel-tab" id="panel-tab-div-5" onClick="selectPanel(5);" style="background-color:#e0efff;"><span>5</span></li>
						<li class="panel-tab" id="panel-tab-div-6" onClick="selectPanel(6);" style="background-color:#e0efff;"><span>6</span></li>
						<li class="panel-tab" id="panel-tab-div-7" onClick="selectPanel(7);" style="background-color:#e0efff;"><span>7</span></li>
						<li class="panel-tab" id="panel-tab-div-8" onClick="selectPanel(8);" style="background-color:#e0efff;"><span>8</span></li>
						<li class="panel-tab" id="panel-tab-div-9" onClick="selectPanel(9);" style="background-color:#e0efff;"><span>9</span></li>
						<li class="panel-tab" id="panel-tab-div-10" onClick="selectPanel(10);" style="background-color:#e0efff;"><span>10</span></li>
						<li class="panel-tab" id="panel-tab-div-11" onClick="selectPanel(11);" style="background-color:#e0efff;"><span>11</span></li>
			<li class="panel-tab-play-pause" id="play-pause" onClick="playPause();"><span>last</span></li>
			</ul>
			</div>
			</div>
			<script language="javascript">
				var x = $('#panel-scroller ul#panels li:lt(1)').clone();
				$('#panel-scroller ul#panels').append(x);
				
				var numberOfPanels = document.getElementById("panels").getElementsByTagName("li").length;
				var myTimeoutArray = new Array(numberOfPanels);
				var zeroIndexNumberOfPanels = numberOfPanels - 1;
				var currentPanel = 1;
				var marginL = 0;
				var currentAnimate = 1;
				var animationStopped = 0;
				
				function playPause() {
					if (animationStopped == 0) {
						stopTimers();
						animationStopped = 1;
					} else {
						runTimers();
						animationStopped = 0;
					}
				}
	
				function stopTimers() {
					$('#play-pause').css('background-image','url(play-panel-sl-v2.gif)');
					currentPanel = currentAnimate;
	
					for (i = 1; i <= numberOfPanels; i++) {
						clearTimeout(myTimeoutArray[i]);	
					}
				}
	
				function selectPanel(thisPanel) {
					stopTimers();
					for (i = 0; i < numberOfPanels; i++) {
						eval("$('#panel-tab-div-" + i + "').css('background-color','#e0efff');");
					}
					eval("$('#panel-tab-div-" + thisPanel + "').css('background-color','#7baee1');");
					currentPanel = thisPanel + 1;
					currentAnimate = thisPanel + 1;
					var panelMargin = thisPanel * -330;
					eval("$('#panel-scroller').find('ul').animate({marginLeft:" + panelMargin + "},800);");
				}
	
				function animatePanel(mL, panelId) {
					currentAnimate = panelId;
					var lastPanel = 0;
					var NumPanelsMinusTwo = numberOfPanels - 2;
					
					if (mL == 0) {
						//we are at the beginning again, start the timers
						$('#panel-scroller').find('ul').css('margin-left',0);
						eval("$('#panel-tab-div-" + panelId + "').css('background-color','#7baee1');");
						runTimers();
					} else {
						eval("$('#panel-scroller').find('ul').animate({marginLeft:" + mL + "},800);");
						lastPanel = panelId - 1;
						if (panelId == zeroIndexNumberOfPanels) {
							eval("$('#panel-tab-div-" + NumPanelsMinusTwo + "').css('background-color','#e0efff');");
							eval("$('#panel-tab-div-0').css('background-color','#7baee1');");
						} else {
							eval("$('#panel-tab-div-" + lastPanel + "').css('background-color','#e0efff');");
							eval("$('#panel-tab-div-" + panelId + "').css('background-color','#7baee1');");
						}
					}
				}
	
				function runTimers() {
					if (numberOfPanels > 1) {
						$('#play-pause').css('background-image','url(pause-panel-sl-v2.gif)');
				
						timeoutValue = 0;
						loopCount = 1;
	
						for (i = currentPanel; i <= numberOfPanels; i++) {
						
							marginL = i * -330;
							timeoutValue = loopCount * 8000;
							
							if (i == numberOfPanels) {
								timeoutValue = timeoutValue - 1000;
							}
	
							if (i == numberOfPanels) {
								marginL = 0;
							}
							myTimeoutArray[i] = setTimeout("animatePanel(" + marginL + "," + i + ")",timeoutValue);
							loopCount ++;
						}
						loopCount = 1;
						currentPanel = 1;
					}
				}
				runTimers();
			</script>
		</div>

  	
  
