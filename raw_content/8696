<script>
    function checkIfLikeIt(id)
    {
        if( document.getElementById(id).src == 'http://cdn3.faniq.com/img/thumbs_like.png'
           || document.getElementById(id).src == 'http://cdn4.faniq.com/img/thumbs_like_over.png')
            return true;
        return false;
    }
    
    function addBookmark()
    {
        title = "FanIQ | Sports Videos, Blogs, News, Rumors, Picks, Predictions, Forums and Trivia";
        url = "http://www.faniq.com";
        if (window.sidebar)
            window.sidebar.addPanel(title, url,"");
        else if( window.external )
            window.external.AddFavorite( url, title );
    }
</script>


