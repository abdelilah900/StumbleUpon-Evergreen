			if (FBLoginCheck && ! isLoggedIn) {
				// check to see if user is already fb connected - if so, automatically log them in
				FB.getLoginStatus(function(response) {
					if (response.status === 'connected' && response.authResponse) {
				 		if (typeof disableFBAutoLogin != 'undefined' && disableFBAutoLogin == true) {                
			                return;
			            }
			            new Ajax.Request('/ajax/facebook/auto_signin.php', {onSuccess : function(response) {
			                var result;
			                try {
			                    result = response.responseText.evalJSON();
			                    if (result.success) {
			                        window.location.href = window.location.href;
			                    }
			                } catch (err) {}
			            }});
					}
				});
			}
			
		  };

