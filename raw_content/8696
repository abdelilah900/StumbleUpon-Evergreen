<script>

var fadeInOutDuration = .5;

function displayGenericPopupRectangular (title, message, time) {
    var t = document.getElementById("genericPopup-rectangular-titleSpan");
    var m = document.getElementById("genericPopup-rectangular-MessageSpan");
    t.innerHTML = title;
    m.innerHTML = message;

    var popup = $("genericPopup-rectangular");
    popup.appear({duration: fadeInOutDuration});
    window.setTimeout(closeGenericPopupRectangular, time);
}

function closeGenericPopupRectangular() {
    var popup = $("genericPopup-rectangular");
    popup.fade({duration: fadeInOutDuration});
    if (typeof closeGenericPopupRectangularCallback == 'function') {
        closeGenericPopupRectangularCallback();
    }
}

function displayGenericPopupWide(type, message, time) {
    var bgDiv = document.getElementById("genericPopup-wide-bgDiv");
    var iconDiv = document.getElementById("genericPopup-wide-iconDiv");

    if (type == 'error') {
        bgDiv.className = "bgSliceRed";
        iconDiv.className ="iconCircleRedX24";
    } else if (type == 'warning') {
        bgDiv.className = "bgSliceYellow";
        iconDiv.className ="iconTriangleExcl24";
    } else if (type == 'success') {
        bgDiv.className = "bgSliceGreen";
        iconDiv.className ="iconCircleGreenCheck24";
    } else {
        bgDiv.className = "bgSliceBlue";
        iconDiv.className ="iconCircleBlueI24";
    }

    document.getElementById("genericPopup-wide-messageSpan").innerHTML = message;

    var popup = $("genericPopup-wide");
    popup.appear({duration: fadeInOutDuration});
    window.setTimeout(closeGenericPopupWide, time);
}

function closeGenericPopupWide() {
    var popup = $("genericPopup-wide");
    popup.fade({duration: fadeInOutDuration});
}

</script>


<!-- chartbeat analytics code -->

<script type="text/javascript">
var _sf_async_config={uid:29956,domain:"faniq.com"};
(function(){
  function loadChartbeat() {
    window._sf_endpt=(new Date()).getTime();
    var e = document.createElement('script');
    e.setAttribute('language', 'javascript');
    e.setAttribute('type', 'text/javascript');
    e.setAttribute('src',
       (("https:" == document.location.protocol) ? "https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/" : "http://static.chartbeat.com/") +
       "js/chartbeat.js");
    document.body.appendChild(e);
  }
  var oldonload = window.onload;
  window.onload = (typeof window.onload != 'function') ?
     loadChartbeat : function() { oldonload(); loadChartbeat(); };
})();
</script>

<!-- end chartbeat analytics code -->

