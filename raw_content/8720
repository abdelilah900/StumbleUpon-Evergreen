<script type="text/javascript">
    function CityListMapToggleCities() {
        $('#city-map-popular-cities').toggle(200);
        if ($('#city-map-more-less').text() == "Less") {
            $('#city-map-more-less').text('More');
        } else {
            $('#city-map-more-less').text('Less');
        }
    }
</script>
