function authbold() {
if ($("#GH_hat_tools .GH_hat_LI a").eq(0).attr("name") =="aol-ma-display-name"){

$("#GH_hat_tools .GH_hat_LI a").eq(0).css("font-weight","bold");

$("#GH_hat_tools .GH_hat_LI a").eq(0).css("color","#000000");

}
}

