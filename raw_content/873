function addborder(){

   $('#aol-newsletter-follow form').css('border-top','2px solid white');

   $('#aol-newsletter-follow form').css('padding-top','5px');

   $('#aol-newsletter-follow form').css('padding-bottom','46px');

   $('#aol-newsletter-follow .message').css('padding-bottom','10px');

}

function addborderpage(){

   $('#aol-newsletter-post form').css('border-top','2px solid white');

   $('#aol-newsletter-post form').css('padding-top','10px');

}


