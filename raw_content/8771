	 <!-- storyCarousel -->
	 <script type="text/javascript">
	 	function storyCarousel_initCallback(carousel) {
	 		jQuery('.carouselNav a').bind('click', function() {
		    	var myClass = jQuery(this).attr('class');
		    	if(myClass.indexOf("thumb")!=-1){
		    		var index = myClass.substring(myClass.length-1);
		        	carousel.scroll(jQuery.jcarousel.intval(index));
		        } else if(myClass.indexOf("next")!=-1){
			        	carousel.next();
		        } else {
			        	carousel.prev();
		        }
		        return false;
		    });
		};
	
		function storyCarousel_init(){
			if(jQuery("#storyCarousel").parent().is(":visible")){
				jQuery("#storyCarousel").show(1,function(){
					jQuery("#storyCarousel #contentPanel").jcarousel({
		        		scroll: 6,
		        		visible:6,
		        		initCallback: storyCarousel_initCallback,
		        		buttonNextHTML: null,
		        		buttonPrevHTML: null
		    		});
				});
		    } else {
		    	setTimeout(storyCarousel_init,100);
		    }		
		}
	
		jQuery(document).ready(function() {
				setTimeout(storyCarousel_init,500);
		});
	
	 </script>
	
	
	<div id="storyCarousel" class="module section storyCarousel radius_5 ">
	<span class="modTitle">Editors&#039; Picks</span>
		<ul class="carouselNav">
			<li class="left"><a href="javascript:void(0);" class="carouselNav-prev roundedcorners-5px-topleft roundedcorners-5px-bottomleft">&nbsp;</a></li>
			<li class="right"><a href="javascript:void(0);" class="carouselNav-next roundedcorners-5px-topright roundedcorners-5px-bottomright">&nbsp;</a></li>
		</ul>
		<div class="contentPanel">
			<ul id="contentPanel" class="footerArticleCarousel">
				
		 			<li style="width: 161px;">
		 				<div class="image">
							
									<a href="/article/248830/in_defense_of_jailbreaking.html"><img src="http://zapp3.staticworld.net/news/graphics/218703-gt_jailbreak_thumb_original.jpg" /></a>
							
