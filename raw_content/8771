	<script>
		function submitNewsletterSubscription(){
			var postData=$("#newsletterModuleForm").serialize();
			if(postData.indexOf("prodid")==-1){
				alert("Please Select at least one Newsletter");
				return false;
			}
			var requestOptions={
						url: '/newsletters/pcw_module_processor',
						data: postData,
						success: function(data,textStatus){
								$("#newsletters").fadeOut("slow",function(){
										$(this).html(data).fadeIn("slow");
									})
							}
				};
			$.ajax(requestOptions)
		}	
	</script>

</div> <!--  END #newsletters -->

