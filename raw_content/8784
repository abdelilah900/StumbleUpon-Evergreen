	<script type="text/javascript">
		function $w(ele){
			return document.getElementById(ele);
		}
		function intAd(slot) {
			acode = $("#adMon1").html();
			findCode = acode.split("// ");
			if(!findCode[1]){
				finalBreak="Ad is Serving";
				window.showMonster = true;
			} else {
				finalBreak=findCode[1].substr(0, 6);
				window.showMonster = false;
			}
			if(finalBreak == "no ads" || finalBreak == ""){
				$("#adMonster1").hide();
				$("#doubleIntCont").show();
				yld_mgr.place_ad_here("adPos" + slot);
			}
		}
	</script>
	
