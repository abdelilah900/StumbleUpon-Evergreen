	function loadGalAd() {
		if(navigator.platform != 'iPad' && navigator.platform != 'iPhone' && navigator.platform != 'iPod'){
			 document.getElementById("cboxGalAd").innerHTML = "<iframe id='galleryAd' src='http://renoob.com/stuffs/728x90.php?adpageid=20201001' width='728' height='90' frameborder='0' scrolling='no'></iframe>";
        //    document.getElementById("cboxGalAd").innerHTML =" ";
		}
	}
 
    function $w(ele){
        return document.getElementById(ele);
    }
    
</script>


<!-- just int ads function -->

<!-- int ads if no monster ad function -->
<script type="text/javascript">
    
    function intAd(slot) {

        acode = $w('monsterAd').innerHTML;

        findCode = acode.split("// ");
        if(!findCode[1]){
            finalBreak="Ad is Serving";
            //alert(finalBreak);
        } else {
            finalBreak=findCode[1].substr(0, 6);
            //alert(finalBreak);
        }
        if(finalBreak == "no ads"){
            $w('monsterAd').style.display = "none";
            $w('intAd' + slot).style.display = "block";
            yld_mgr.place_ad_here("adPos" + slot);
        } else {
            //alert("Ads Avail");
        }
        
    }
    
</script>


