<script language="javascript">
$(document).ready(function(){
	$.ajax({
		type: "GET",
		
		url: "/api_static/msn-lifestyle-feed.xml",///api_static/msn-lifestyle-feed.xml
		
		success:function(parsedData){
		    var xml = parsedData;
				toutItemList = $(xml).find('item'),
				html = '';
			
			$(toutItemList).each(function(i){
				var itemTitle = $(this).find('title').text(),
					itemLink = $(this).find('link').text(),
					itemType = $(this).find("[nodeName='xnw:linktype']").text() || '';
				
				if(i == 0){
					var xnwBox = $(this).find("[nodeName='xnw:box']"),
						xnwImage = $(xnwBox).find("[nodeName='xnw:image']").text(),
						xnwText = $("[nodeName='xnw:richtext']", xnwBox).text();
						
						
												
					html += "<div class='headline' class='"  + itemType + "'><a href=\"" + itemLink + "\"><img src=\"" + xnwImage + "\"></a>" + xnwText + "</div><div class='clear'></div>";
				} else {
					if (i == toutItemList.length - 1){
						html += "<div class='li_links last "  + itemType + "'><a target=\"_blank\" href=\"" + itemLink + "\">" + itemTitle + "</a></div>";
					} else {
						html += "<div class='li_links "  + itemType + "'><a target=\"_blank\" href=\"" + itemLink + "\">" + itemTitle + "</a></div>";
					}
				}
			});
			$('#msn_tout').html(html);
			$('#msn_tout .headline p:first').remove();
			$('#lifestyle_fd').html("<a href=\"http://lifestyle.msn.com/FeedPageFinal.aspx?page=http://www.delish.com&format=rss\">Today\'s Picks</a>");
		}

	});	
});
</script>



