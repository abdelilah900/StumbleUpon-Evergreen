<script type="text/javascript">
function ans_tab(i) {
  var d = document;
  d.getElementById('ans_tab_1').style.color = '#668C1F';
  d.getElementById('ans_tab_2').style.color = '#668C1F';
  d.getElementById('ans_tab_3').style.color = '#668C1F';
  d.getElementById('ans_tab_' + i).style.color = '#000';
  d.getElementById('ans_tab_1').className = '';
  d.getElementById('ans_tab_2').className = '';
  d.getElementById('ans_tab_3').className = '';
  d.getElementById('ans_tab_' + i).style.color = '#000';
  d.getElementById('ans_tab_' + i).className = 'active';
  d.getElementById('ans_module_1').style.display = 'none';
  d.getElementById('ans_module_2').style.display = 'none';
  d.getElementById('ans_module_3').style.display = 'none';
  d.getElementById('ans_module_' + i).style.display = 'block';
}
</script>

<script type="text/javascript">
function ans_limitChars_2(element) {
    var defaultText = "In a nutshell, what would you like to ask?";	
    var currentText = element.value;
    var truncatedString = currentText.substr(0,100);

    if (truncatedString == defaultText) {
        document.getElementById("ans_char_count_2").innerHTML = "You have 100 characters remaining";
        element.value = '';
        return true;
    }

    document.getElementById("ans_char_count_2").innerHTML = "You have " + (100 - truncatedString.length) + " characters remaining";

    if (truncatedString != currentText) {
        element.value = truncatedString;
    }
}
</script>

<script type="text/javascript">function ansunifiedTracking(){s.events=s.appendList(s.events,'event53',',','1');}</script>

      
		
	
