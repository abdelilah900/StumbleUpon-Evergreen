<script language="javascript">
function refreshAds(){	
	//Hearst tracking
    if (top.s) {pageviewTracking();}
	//MSN tracking
	if (top.s_msn) {
        if ( s_msn.referrer == location.href )
        {
        s_msn.referrer='';
        }
        s_msn.t();
	}
		$.track.trackInfo.userStatic.requestId = null;
		$.track.trackPage(); /** to fire impression event **/
		var spans = document.getElementsByTagName('span');
        var adDivs = new Array();
        for(var i=0;i<spans.length;i++){s
                if(spans[i].id.indexOf('ams_del') > -1 && spans[i].firstChild){
                        if ((spans[i].firstChild.nodeName == 'DIV') && (!!spans[i].firstChild.id)){
                                adDivs.push(spans[i].firstChild);
								
                        }
                }
        }
        for(var i=0;i<adDivs.length;i++){
                dapMgr.displayAd(dapMgr.getAdItemIndex(adDivs[i].id));
				
        }
		
}
</script>


