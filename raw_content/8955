    <span id="ams_delish_networktout"><script type="text/javascript">
if ( window.location.href.substring(7,12) == "games" )
{
	var ntwrkURL = "http://games.delish.com/proxy.aspx?url=http://www.delish.com/delish-network-tout.json"; // URL of the content article in Magnus
	var ntwrkImgPath = "http://games.delish.com/proxy.aspx?url=http://www.delish.com/cm/shared/smarttout/delish/"; // path to the directory containing the image files
}
else
{
	var ntwrkURL = "/delish-network-tout.json"; // URL of the content article in Magnus
	var ntwrkImgPath = "/cm/shared/smarttout/delish/"; // path to the directory containing the image files
}
//
// EDIT BELOW THIS POINT FOR DELISH REQUIREMENTS PER DEBRA 1/9/09
// div structure MUST stay standardized!
//
$(document).ready(function(){
	$.ajax({
	type: "GET",
	url: ntwrkURL,
	dataType: "json",
	error: function(XMLHttpRequest, textStatus, errorThrown){
		$("#smarttout_wrap").hide();
	},
	success: function(data){
		var tabObj = {};
		tabObj = data;
		var nettouttitle = tabObj.touttitle;
		$.each(tabObj.tabs, function(i,item){
                var titlepromo = item.featuredtitle;
				$("#smarttout_content").append("<div id='tab"+i+"'><div id='smarttout_picks_no_tabbed'>"+item.title+"</div><div id='featured_link'></div><div class='smarttout_link'><ul id='tab0addi'></ul></div></div>");
				if (titlepromo){
					$('#featured_link').css('display', 'block');
					$("#featured_link").append("<div class='headline'><a href='"+item.featuredurl+"'><img src='"+item.featuredimage+"'></a><p><a href='"+item.featuredurl+"'>"+item.featuredtitle+"</a></p><p class='description'><a href='"+item.featuredurl+"'>"+item.featureddesc+"</a></p></div><div class='clear'></div>");
				} else {
					$('#featured_link').css('display', 'none');
				}
				for (var j=0; j < item.addi.length; j++) {
						$("#tab"+i+"addi").append("<li><a href='"+item.addi[j].addurl+"' target='"+item.addi[j].addwin+"'><span>"+item.addi[j].addname+"</span></a></li>");
				}
		});
	}
});
});
</script>

