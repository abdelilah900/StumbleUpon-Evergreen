<script>
//WEB SEARCH JS

//Validate Web Search, so empty searches are not executed
function validate (form) {
	if (form.q.value == "" || form.q.value.replace(/^\s*|\s*$/g,'') == "") {
		alert("Please enter a valid search term.");
		form.q.focus();
		return false ;
	}
	return true ;
}

function searchtabs_rf(swap){

	if (swap == 'websearch') {
		$("#websearch").show();
		$("#sitesearch").hide();
		$("#recipesearch").hide();
		$("#web_tab").removeClass("inactive").addClass("active");
		$("#site_tab").removeClass("active").addClass("inactive");
		$("#recipe_tab").removeClass("active").addClass("inactive");
		$("#websearch_b").show();
		$("#sitesearch_b").hide();
		$("#recipesearch_b").hide();
		$("#web_tab_b").removeClass("inactive").addClass("active");
		$("#site_tab_b").removeClass("active").addClass("inactive");
		$("#recipe_tab_b").removeClass("active").addClass("inactive");
	}
	else if (swap == 'sitesearch') {
		$("#websearch").hide();
		$("#sitesearch").show();
		$("#recipesearch").hide();
		$("#web_tab").removeClass("active").addClass("inactive");
		$("#site_tab").removeClass("inactive").addClass("active");
		$("#recipe_tab").removeClass("active").addClass("inactive");
		$("#websearch_b").hide();
		$("#sitesearch_b").show();
		$("#recipesearch_b").hide();
		$("#web_tab_b").removeClass("active").addClass("inactive");
		$("#site_tab_b").removeClass("inactive").addClass("active");
		$("#recipe_tab_b").removeClass("active").addClass("inactive");
		var autocomplete = '/rampsearch/autocomplete/delish?q=';
		$('#gen_search').click(function(){setAutoComplete('gen_search', 'gen_results', autocomplete);});
	}
	else if (swap == 'recipesearch') {
		$("#websearch").hide();
		$("#sitesearch").hide();
		$("#recipesearch").show();
		$("#web_tab").removeClass("active").addClass("inactive");
		$("#site_tab").removeClass("active").addClass("inactive");
		$("#recipe_tab").removeClass("inactive").addClass("active");
		$("#websearch_b").hide();
		$("#sitesearch_b").hide();
		$("#recipesearch_b").show();
		$("#web_tab_b").removeClass("active").addClass("inactive");
		$("#site_tab_b").removeClass("active").addClass("inactive");
		$("#recipe_tab_b").removeClass("inactive").addClass("active");
	}
}

//BOTTOM RF SEACH BOX
function searchtabs_rf_b(swap) {
	
	if (swap == 'websearch_b') {
		$("#websearch_b").show();
		$("#sitesearch_b").hide();
		$("#recipesearch_b").hide();
		$("#web_tab_b").removeClass("inactive").addClass("active");
		$("#site_tab_b").removeClass("active").addClass("inactive");
		$("#recipe_tab_b").removeClass("active").addClass("inactive");
		$("#websearch").show();
		$("#sitesearch").hide();
		$("#recipesearch").hide();
		$("#web_tab").removeClass("inactive").addClass("active");
		$("#site_tab").removeClass("active").addClass("inactive");
		$("#recipe_tab").removeClass("active").addClass("inactive");
	}
	else if (swap == 'sitesearch_b') {
		$("#websearch_b").hide();
		$("#sitesearch_b").show();
		$("#recipesearch_b").hide();
		$("#web_tab_b").removeClass("active").addClass("inactive");
		$("#site_tab_b").removeClass("inactive").addClass("active");
		$("#recipe_tab_b").removeClass("active").addClass("inactive");
		$("#websearch").hide();
		$("#sitesearch").show();
		$("#recipesearch").hide();
		$("#web_tab").removeClass("active").addClass("inactive");
		$("#site_tab").removeClass("inactive").addClass("active");
		$("#recipe_tab").removeClass("active").addClass("inactive");
		var autocomplete = '/rampsearch/autocomplete/delish?q=';
		$('#gen_search_bottom').click(function(){setAutoComplete('gen_search_bottom', 'gen_results_bottom', autocomplete);});
	}
	else if (swap == 'recipesearch_b') {
		$("#websearch_b").hide();
		$("#sitesearch_b").hide();
		$("#recipesearch_b").show();
		$("#web_tab_b").removeClass("active").addClass("inactive");
		$("#site_tab_b").removeClass("active").addClass("inactive");
		$("#recipe_tab_b").removeClass("inactive").addClass("active");
		$("#websearch").hide();
		$("#sitesearch").hide();
		$("#recipesearch").show();
		$("#web_tab").removeClass("active").addClass("inactive");
		$("#site_tab").removeClass("active").addClass("inactive");
		$("#recipe_tab").removeClass("inactive").addClass("active");
	}
}

function clickclear(thisfield, defaulttext) {
if (thisfield.value == defaulttext) {
thisfield.value = "";
}
}

function clickrecall(thisfield, defaulttext) {
if (thisfield.value == "") {
thisfield.value = defaulttext;
}
} 
</script>
