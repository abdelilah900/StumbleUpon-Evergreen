jQuery('DIV.imgcredits').live('mouseenter', function(event){ jQuery("div", this).fadeIn(300); });
jQuery('DIV.imgcredits').live('mouseleave', function(event){ jQuery("div", this).fadeOut(300); });
jQuery('DIV.imgbox').live('mouseenter', function(){
	jQuery(".credits", this).fadeIn(300);
	jQuery(".sordering", this).attr('class','sordering act');
	if( jQuery(".tools", this).html() == '') { jQuery(".tools", this).html('<span class="load"></span>'); }
	jQuery(".tools", this).css('display','block');
	jQuery(".rating", this).css('display','block');

	getpicturedata(postid,this);
