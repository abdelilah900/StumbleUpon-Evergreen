<script>

jQuery(document).ready(function() { 


jQuery('.hover').live('mouseleave', function(event){
	jQuery('.mkavatarbtn').hide();
});
jQuery('.hover').live('mouseenter', function(){
	jQuery('.mkavatarbtn').show();
});

jQuery('.commentimage').live('mouseleave', function(event){
	jQuery('.commimg').fadeOut(300);
	jQuery(this).css('text-decoration','none');
});

jQuery('span.commentimage').live('mouseenter', function()
{
	var img_number=jQuery(this).html();
	img_number=img_number.substr(1);
	if (jQuery('a[name=pic'+img_number+']').length)
	{	
		var imgsrc=jQuery('a[name=pic'+img_number+']').parents(".imgbox").find("img:first").attr('src');
		var imgHeight=jQuery('a[name=pic'+img_number+']').parents(".imgbox").find("img:first").css('height');
		var imgWidth=jQuery('a[name=pic'+img_number+']').parents(".imgbox").find("img:first").css('width');
		if(imgHeight>imgWidth) {
			var imgSize="height:250px;";
			var imgTop=125;
			var imgLeft=230;
		} else {
			var imgSize="width:250px;";
			var imgTop=94;
			var imgLeft=260;
		}
		var offset = jQuery(this).offset();
		jQuery(this).css('text-decoration','underline');
		jQuery('.commimg').css({'left':offset.left-imgLeft,'top':offset.top-imgTop}).html('<img src="'+imgsrc+'" style="'+imgSize+'"/>').fadeIn(400);
	}
});

jQuery('div.pic_paging a').live('mouseleave', function(event){
	jQuery('.thumbimg').hide();
});

jQuery('div.pic_paging a').live('mouseenter', function()
{
	var imgsrc=jQuery(this).attr('rel');
	if (imgsrc.length>1)
	{
		var offset = jQuery(this).offset();
		jQuery('.thumbimg').css({'left':offset.left-20,'top':offset.top-210}).html('<img src="'+imgsrc+'" />').show();
	}
});


jQuery('span.commentimage').live('click', function()
{
	document.location.href='#pic'+jQuery(this).html().substr(1);
});


jQuery('span,div,img').tipsy({gravity: 's', live:true});


jQuery('#lentatop').cycle({ 
    fx:     'scrollUp', 
    timeout: 0,
    prev:   '#lenta_left', 
    next:   '#lenta_right', 
    height: 'auto', 
    speed:  'fast',
});

jQuery('#bignewspost').cycle({ 
    fx:     'scrollVert', 
    timeout: 0,
    prev:   '#leftNav', 
    next:   '#rightNav', 
    height: 'auto', 
    speed:  'fast',
});
       


	var news_expand = jQuery.cookie('news_expand');
	if (news_expand!=undefined) showfull(news_expand);

	setTimeout('jQuery("#rightNav").click()',4000);  // After 4 seconds scroll to DAILY pic dump 



});
</script>


