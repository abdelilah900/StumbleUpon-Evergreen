	if (jQuery(".sordering a[name^=pic]", this).html().substr(0,1)!="#")
	jQuery(".sordering a[name^=pic]", this).html('#'+jQuery(".sordering a[name^=pic]", this).html());
});
jQuery('DIV.imgbox').live('mouseleave', function(){
	jQuery(".credits", this).fadeOut(300);
	jQuery(".sordering", this).attr('class','sordering');
	jQuery(".tools", this).css('display','none');
	if (jQuery(".sordering a[name^=pic]",this).html()) jQuery(".sordering a[name^=pic]",this).html(jQuery(".sordering a[name^=pic]",this).html().substr(1));
	jQuery(".rating", this).css('display','none');
	});

