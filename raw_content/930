<script id="TopsCommentTemplate" type="text/html">
<div class="comment">
	<div class="user">
		<div class="userphoto">
			<img src="{{= UserImageUrl }}" class="profilephotosmall" alt="User Profile Photo" width="75" />
		</div>
		<p class="username"><a href="/users/{{= UserName }}" 
			title="View thestar.com public profile" 
			class="td-username td-comment_username">{{= UserName }}</a></p>
		<p class="date">{{= $.showDate(LastModifiedTimestamp) }}</p>
    </div>
	<div class="text">
		<div class="spacer"></div>
		<h3><span>{{= Title }}</span></h3>
		<p class="text"><span class="Text">{{= Text }}</span></p>
		<ul id="rate_{{= ID }}">
			<li class="first">
				{{= $.asyncLink(ID, EncryptedID, $.encrypted().agree, "Agree") }}
				<span id="agree_{{= ID }}">{{= $.asyncCount(PositiveResponseCount, ID, $.encrypted().agree) }}</span></li>
                <li class="sep">|</li>
			<li>
				{{= $.asyncLink(ID, EncryptedID, $.encrypted().disagree, "Disagree") }} 
				<span id="disagree_{{= ID }}">{{= $.asyncCount(NegativeResponseCount, ID, $.encrypted().disagree) }}</span></li>
                 <li class="sep">|</li>
			<li><span class="alertlink">! {{= $.asyncLink(ID, EncryptedID, $.encrypted().offensive, "Alert a moderator") }}</span></li>
		</ul>
		<input type="hidden" id="vote_{{= ID }}" />
	</div>
    <div style="clear:both"></div>
</div>
</script>
<script type="text/javascript">
	var articleID = 648303;
	var commentService = "http://comments.thestar.topscms.com/";
	
	$(document).ready(function () {
		try {
			
			//Enable display on page load.
			if(document.location.hash == "#comments")
				showComments();

			//Hide comments if a tab is clicked.
			$("ul.ts-article_tabs > li > a").click(function(event) {
				hideComments();
			});
			
			//not all pages have this tab, if the tab isn't there, this is not necessary
			if(typeof OrderDataTab != "undefined")
			{
			    //Show comments on any comment link.
			    $("a[href$='#comments']").click(function(event) {
				    TD.TabToggle.showTab('comments',OrderDataTab);
				    TD.ProjectTheStar.OPAAd.Hide();
				    showComments();
			    });
			}
		}
		catch(e) {
			$("#TopsCommentFilter").hide();
			$("#TopsCommentList").html("Commenting is currently unavailable");
		}
	});
	

	//Populate comments.
	function showComments() {
		try {
			$("#TopsCommentFilter").show();	
			$("#TopsCommentList").show();
			
			//Populate comments.
			$.loadComments(
			articleID,
			commentService,
			{
				template: "#TopsCommentTemplate",
				container: "#TopsCommentContainer",
				pageSize: 10
			});
		}
		catch (err) {
			$("#TopsCommentFilter").hide();
			$("#TopsCommentList").html("Comments are currently unavailable");
		}
	}
	
	//Obfuscate comments.
	function hideComments() {
		$("#TopsCommentFilter").hide();
		$("#TopsCommentList").hide();
	}
</script>
				
		</div>
	
	<div id="dataTabinteractive" style="display:none" class="ts-article">
        
	</div>
	
    <script type="text/javascript">
        var OrderDataTab = new Array("article", "photo", "video", "comments", "interactive");
        TD.TabToggle.init(OrderDataTab);
    </script>

    </div>
    
    
