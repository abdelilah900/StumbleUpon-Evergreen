   /* <![CDATA[ */
   $(document).ready(function() {
       var u = mtGetUser();
       if (u && u.is_authenticated) {
           mtUpdateScores();
       } else {
           $('.scoring').addClass('scoring-scorable');
       }
       
       	/* ------- Utilities BEGIN --------- */
		// remove the default text on any search input box with the class "userField"
		$(".userField").click(function() { 
			    if ($(this).val() == 'Search Serious Eats'||'Search for Recipes' || 'Search Talk' || 'Search New York' || 'Search Sweets' || 'Search Drinks' || 'Search Slice' || 'Search AHT' || 'Search Photograzing' || 'Search Chicago') {   
			        $(this).data('original', $(this).val()).val('');
					$(this).addClass("selected");
			    }
			});
		// put the default text back into any search input box with the class "userField" if the user hasn't typed anything
		$(".userField").blur(function() { 
			    if ($(this).val() == '') {   
					$(this).removeClass("selected");
			        $(this).val($(this).data('original'));
			    }
			});
		// show and hide the newsletter form signup box
		$("#subscribe-button").click(function(){
			$("#newsletter-form").toggle("fast");
			$(this).text($(this).text() == 'Go' ? 'Close' : 'Go');
		});
		/* ------- Utilities END --------- */

   });
   //mtAttachEvent('load', mtUpdateScores);
   mtAttachEvent('usersignin', mtUpdateScores);
   /* ]]> */
   </script>


