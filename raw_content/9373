    <div id="product-details">
        <h1>
            <a href="/Shop/Designers/Alexander_McQueen" title="View all Alexander McQueen">Alexander McQueen</a>
        </h1>
        <h2>
            Long Skull whipsnake clutch</h2>

        <div id="price">
                $2,590</div>
        </div>
    <div id="tabbed-info">
        <div id="tab-view">
            <div id="demo" class="yui-navset">
                <ul class="yui-nav">
                    <li class="selected editors">
                        <a href="#tab1">
                            <span class="tl"></span>
