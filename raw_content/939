<script type="text/javascript">

//<![CDATA[


function commodityFontString(input_string) {
	return input_string;
}


function update_pod_basket(total) {

	$.ajax({
   	   type: "POST",
   	   url: "/facebox_overlay/facebox_overlay.php",
   	   data: "&url=/basket/|1340|710|||1",
   	   success: function(msg){
	      		$(function(){
	      			$.facebox(msg);
   			});
			}
		});

		if($('#pod_basket_total_buffer').html()){
			var	old_basket_total = parseInt($('#pod_basket_total_buffer').html());
		}
		var basket_total =  parseInt(total);

		update = update_function({total_items: basket_total});

		$("#pod_basket_total").html(update);
		$("#pod_basket_total_buffer").html(basket_total);
		$("#pod-nav-cart, .cart-left, .cart-items, .cart-right").addClass("active");
}


function remove_from_pod_basket(delete_id)
{

	var basket_total = 0;

	$.ajax({
		type: "POST",
 	   	url: "/basket/",
 	   	data: "?add=add&ajax=yes&prod_desc_id[]=9999999999&quantity=1&from=podUpdate",
 	   	dataType: "xml",
 	   	success: function(msg){
	      	$(function(){

	      		//handle the xml object
		      	$(msg).find('info').each(function(){
	 				var title = $(this).find('num_items').text();
	 				basket_total = parseInt(title);
				});
	      		update = update_function({total_items: basket_total});
	      		document.getElementById('pod_basket_total').innerHTML = update;
	      		$("#pod_basket_total_buffer").html(basket_total);
	      		if(basket_total == 0){
	      			$("#pod-nav-cart, .cart-left, .cart-items, .cart-right").removeClass("active");
	      		}
 			});
		}
	});


}

function update_function(data) {
	var str = "";
	if (data.total_items && data.total_items > 0) {
		str = '<span class="letter letter-'+data.total_items+'"><span>'+data.total_items+'</span></span>';
	} else {
		str = '<span class="letter letter-0"><span>0</span></span>';
	}

	return str;
};

function getPodOverlay(params) {
	 var url = params;
	 var details = url.split("|");
	 $.ajax({
      	   'type': "POST",
      	   'url': "/facebox_overlay/facebox_overlay.php",
      	   'data': "&url="+url,
      	   'success': function(msg){
	      		$.facebox(msg);
      			if( details[5] == 0 ){
      				$('#facebox .footer').css({'display' : 'none'});
      			}
  			}
  		});
}
// ]]>

</script>
