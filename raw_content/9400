              <script type="text/javascript">

jQuery(document).ready(function(){setTimeout(updateDailiesPanel, 5000);});
function updateDailiesPanel() {
	var base = 'linkImage';
	var total = jQuery('#buttons_switch .buttons_single_panel').length;
	var current = parseInt(jQuery('#buttons_switch').attr('current'));
	if (current == NaN || current == undefined) { current = 1; }
	var next = current + 1;
	if (next > total) { next = 1; }
	jQuery('#'+ base + current).fadeOut(500, function(){
		document.getElementById(base + current).style.display = 'none';
		jQuery('#'+ base + next).fadeIn(500);
		jQuery('#buttons_switch').attr('current', next);
	});
	setTimeout(updateDailiesPanel, 5000);
}

</script>
