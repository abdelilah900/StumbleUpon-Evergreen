<script language="JavaScript" type="text/javascript">
var pagename = "";
s.prop3=node_page_name.toLowerCase();

			
  if(typeof(aid) !== 'undefined' && aid != 'NULL'){
		s.prop4 = aid;
	}
			
  if(typeof(zone2) !== 'undefined' && zone2 != 'NULL'){
		s.prop5 = zone2;
	}
			
  if(typeof(channel) !== 'undefined' && channel != 'NULL'){
		s.prop6 = channel.substring(11);
	}
			
	if((typeof(channel) !== 'undefined') && (typeof(subch) !== 'undefined')  && channel != 'NULL' && subch != 'NULL'){
		s.prop7= channel.substring(11) + ':' + subch;
	}
	
	if(typeof(channel) !== 'undefined' && channel != 'NULL') {
		pagename = 'el' + ':' + channel.substring(11);
			
		if(typeof(subch) !== 'undefined' && subch != 'NULL') {
			pagename += ':' + subch;
		} else if((typeof(zone1) !== 'undefined') && (typeof(subch) !== 'undefined') && zone1 != 'NULL' && subch != 'NULL') {
			pagename += ':' + zone1;
		}
		
		
		
		
				
		
				
		
	}		
       

	//special case for newsletter
	if(omniture_pagename == "newsletter signup") {
		pagename="el";
	}
</script>	




