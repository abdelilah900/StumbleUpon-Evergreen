<script type="text/javascript">
	function check_email (strng, id) {
		var error="";
		if (strng == "") {
			document.getElementById(id).style.backgroundColor = "#ffdce4"; 
		}
		var emailFilter=/^.+@.+\..{2,3}$/;
		if (!(emailFilter.test(strng))) { 
			error = "Please enter a valid email address.\n"; }
		else {
			//test email for illegal characters
			var illegalChars= /[\(\)\<\>\,\;\:\\\"\[\]]/
			if (strng.match(illegalChars)) {
				error = "The email address contains illegal characters.\n";
				document.getElementById(id).style.backgroundColor = "#ffdce4";
			}
			else {
				document.getElementById(id).style.backgroundColor = "transparent"; 
			}
		}
		return error;    
	}

	function check_newsletter_signup() {
		var why = "";
		var ChBox1=document.getElementById('newsletter_checkbox');
		if (!ChBox1.checked){
			why += "Please check the box if you would like to recevie the Street Chic newsletter.\n";
		}
		why += check_email(document.getElementById("Email").value, "Email");
		if (why != "") {
			alert(why);                
			return false;
		}    
		$("#submit_signup").click();
	}
	</script>
