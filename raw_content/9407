            var offset = $(".share-rail-wrapper").offset();
            var topPadding = 15;
            $(window).scroll(function() {
                if ($(window).scrollTop() > offset.top) {
                    $(".share-rail-wrapper").css('position', 'fixed');
                    $(".share-rail-wrapper").css('top', 15);
                } else {
