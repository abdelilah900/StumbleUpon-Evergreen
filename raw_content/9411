<script>
new Shifter('shifter-wrapper', $$('#shifter-content .shifter'), $$('a.shifter-control'), {duration:0.5});
function selectShiftElement(shifter, sport)
{
$$('#shifter-content .shifter').each(function(slide, index) {
slide.className = "shifter";
});
shifter.className = "shifter selected";
$$('#results_scroller .boiteResScroller').each(function(slide, index) {
slide.style.display = "none";
});
//$(sport).style.display = "block";
$(sport).appear({duration: 0.4});
//new Effect.Scale(sport, 100, {scaleX:true, scaleY:false, scaleFromCenter:false, scaleFrom:0, scaleMode: {originalHeight:35,originalWidth:550},duration:0.5});
}
</script>
