        	<script language="javascript" type="text/javascript">
	unikIdForLink = function (obj){
	    var thisLink = obj.href;
		var linkSplit = thisLink.split("?");
		var maxVal = 9999999999;
		var minVal = 1111111111;
		var randVal = minVal+(Math.random()*(maxVal-minVal));
		var UID = typeof floatVal=='undefined'?Math.round(randVal):randVal.toFixed(floatVal);
		document.location = linkSplit[0] + "?uniqueid=" + UID + "&" + linkSplit[1];
		return false;
	}
</script>
