    beforeload = (new Date()).getTime();
    function pageloadingtime(){
        afterload = (new Date()).getTime();
        secondes = (afterload-beforeload)/1000;
        document.getElementById('loadingtime_frame').src = 'http://loadtime.rds.ca/dot.gif?load=' + secondes + '&rnd=' + Math.random() + "&page=index";
    }
