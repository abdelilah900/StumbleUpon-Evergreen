        <script language="JavaScript" type="text/javascript">
               function redirectConcours () {        
                       var URL = "http://www.rds.ca/cgi-bin/Identification?pagedyn=concours&idx=202&action=formulaire&site=rds";
                       var maxVal = 9999999999;
                       var minVal = 1111111111;
                       var randVal = minVal+(Math.random()*(maxVal-minVal));
                       var UID = typeof floatVal=='undefined'?Math.round(randVal):randVal.toFixed(floatVal);                        
                       document.location = URL + "&uniqueid=" + UID; 
               }
        </script>
		

