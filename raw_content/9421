	vidOutput : function(div, command, data) {
		var totalVids = data.total_count;
		switch (command) {
			case "find_playlist_by_id" :
				theSource = data.videos;
				break;
			default:
				theSource = data.items;
		}
		$("#" + div + "").empty();
		if (div !== "relatedvideos") {
			if (totalVids) {
				allVids = totalVids;
				backAction = -1;
				nextAction = 1;
			} else {
				allVids = theSource.length;
				backAction = '\"back\"';
				nextAction = '\"next\"';
			}	
		}
		if (div === "searchvidoutput" && theSource.length === 0) {
			searchVidsNull();
		}
		$.each(theSource, function(i,item){
			$("#" + div + "").append("<div class='video_wrapper left'><a href='#v" + item.id + "' onclick='showVid(" + item.id + ")'><img src='" + item.thumbnailURL + "' width='120' height='90' class='video' /></a><div class='text'><a href='#v" + item.id + "' onclick='showVid(" + item.id + ")'>" + item.name + "</a></div><div class='time'>" + formatTime(item.length) + "</div></div>");
		});
		if (div !== "relatedvideos") {
			(div === "categoryvidoutput") ? pgsize = bcVidObj.channelPgSize : pgsize = bcVidObj.generalPgSize;
			vidPgFst = (vidCurrentPg * pgsize) + 1;
			if (!totalVids) {
				vidPgLst = (vidCurrentPg * pgsize) + pgsize;
				vidPagination(div, vidPgFst, vidPgLst, allVids);
			} else {
				vidPgLst = (vidCurrentPg * pgsize) + theSource.length;
			}
			vidPaginationOutput();
		}
	},
