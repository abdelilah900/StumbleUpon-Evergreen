							function sendVote(form,id,url) {
						    	var formElements = new Array();
						    	var ajaxUrl = "";
						    	if( form != null ){
							        var x=0;
							        for(var i=0;i<form.elements.length;i++) {
							            if(form.elements[i].type != 'radio') {
							              formElements[x++]=form.elements[i];
							            }
							            else if(form.elements[i].checked && form.elements[i].checked != 'true') {
							              formElements[x++]=form.elements[i];
							            }
							        }
							        ajaxUrl = url+'?service=ajax&action=poll&cache=false';
						        }else{
						       		formElements = null;
						       		ajaxUrl = url+'?service=ajax&action=poll';
						        }

								var $container = $('#poll_container_'+id);
								var container_width = $('#poll_container_'+id).width() + 'px';
								var container_height = $('#poll_container_'+id).height() + 'px';
								$.ajax({
								    url: ajaxUrl,
								    data: formElements,
								    success: function( data ){

									    $container
									    	//.css( { 'height': container_height, 'overflow': 'hidden'} )
											.fadeOut(
												500,
												function(){
													$container
														.html( data )
														.find(".poll_container div span").hide();

													$container
														.show()
														.find(".poll_container div")
														.css( {'width': '1px', 'overflow': 'hidden'} )
														.animate( {width: container_width}, 2000, function(){
															$(this).find("span").show();
														});
												}
											);

								    },
								    error: function( e ){

								    }
								});

						        return false;
						   }
						</script>
