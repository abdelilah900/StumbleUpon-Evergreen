<script>
var aiAllowedKeyCodes = new Array(8,9,13,35,36,37,38,39,40,46);

Array.prototype.indexOf = function(searchString)
{
	for(result=[], x=0; x<this.length; x++){
		if(this[x] == searchString){
			result[result.length]=x;
		}
	}
	return result.length ? result : -1;
};


Array.prototype.inArray = function(val) {
for (var i in this) { if (this[i] == val) return i; }
return false;
}

function getRadioValue(o)
{
    for (var i=0; i<o.length; i++)
	{
		if (o[i].checked)
		return o[i].value;
	}

    return false;
  }


function checkKey(e, bAllowNumerical, bAllowChar, aiSpecialChars, obj)
{
    var bRet = false;

    var iKeyCode = window.event ? window.event.keyCode : e.which;
    var bShift = window.event ? window.event.shiftKey : e.shiftKey;
    var bCtrl = window.event ? window.event.ctrlKey : e.ctrlKey;

	window.status = iKeyCode;

    if (bAllowNumerical && ((iKeyCode > 47 && iKeyCode < 58) || (iKeyCode > 95 && iKeyCode < 106)) && !bShift)
    {
        bRet = true;
    }

    if (bAllowChar && (iKeyCode > 64 && iKeyCode < 90) && !bShift)
    {
        bRet = true;
    }

    if (aiAllowedKeyCodes.indexOf(iKeyCode) != -1)
    {
        bRet = true;
    }

    if (aiSpecialChars.length > 0)
    {
        if (aiSpecialChars.indexOf(iKeyCode) != -1)
        {
            bRet = true;
        }
    }

    if (bCtrl && (iKeyCode == 67 || iKeyCode == 86 || iKeyCode == 88 || iKeyCode == 65))
    {
        bRet = true;
    }

    return bRet;
}


function setThousendPoint(e, obj)
    {
	  	  var iKeyCode = window.event ? window.event.keyCode : e.which;
		  var bShift = window.event ? window.event.shiftKey : e.shiftKey;

	      if ((((iKeyCode > 47 && iKeyCode < 58) || (iKeyCode > 95 && iKeyCode < 106)) && !bShift) || (aiAllowedKeyCodes.indexOf(iKeyCode) != -1))
	     {
          var strVal = obj.value.replace(/\./g,"");
          var bNegative = 0;
          if (strVal.match(/-/))
            {
              strVal = strVal.replace(/-/g,"");
              bNegative = 1;
            }

          var iLen = strVal.length;

          if (iLen<4 && obj.value.match(/\./g))
		  {
            obj.value = ((bNegative==1)?"-":"")+strVal;
		  }
          else if (iLen > 3)
            {
              var strNewVal = ((bNegative==1)?"-":"")+strVal.substr(0,iLen%3);
              for (i=iLen%3;i<iLen;i+=3)
                strNewVal += ((i!=0)?".":"")+strVal.substr(i,3);

              obj.value = strNewVal;
            }
			}
    }

function c24_strom_track(node)
{
  var valid = true;
  var aiValidDurations = new Array(500,750,1000,1250,1500,1750,2000,2250,2500,2750,3000,3250,3500,3750,4000,5000,6000);
  var zipcode = document.getElementsByName("zipcode")[0];
	var totalconsumption = document.getElementsByName("totalconsumption")[0];

  if (! (Math.ceil(zipcode.value)<=99999 && zipcode.value.length==5) )
	{
		alert(unescape('Bitte geben Sie eine g%FCltige Postleitzahl ein.'));
		valid = false;
	}

	else if (!aiValidDurations.inArray(totalconsumption.options[totalconsumption.selectedIndex].value))
		{
			alert(unescape('Bitte w%E4hlen Sie den f%FCr Sie richtigen Jahresverbrauch %FCber die Dropdown-Box.'));
			valid = false;
		}

  else if (valid){
    var url = node.action;
    var params = new Array();
    for (var i=0;i<node.elements.length;i++) {
        var e = node.elements[i];
        if (e.name != '' && e.value != '') {
            params.push(e.name +'='+ e.value);
        }
    }
    var url_params  = params.length > 0 ? '?'+params.join('&') : '';
    document.location.href = url + url_params;
  }
    return false;

}
</script>
