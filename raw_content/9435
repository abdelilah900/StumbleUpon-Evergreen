		var seo_comments = $('#comments-list .legacy');
		var facebook_comments = 0;
		var max_comments = 5;
		var remainder = max_comments - facebook_comments;
		var logged	= false;
		if (facebook_comments >= 5) {
			$('#comments-list').remove();
		} else {
			$('#comments-list .facebook:lt('+ (remainder + 1) +')').remove();
		}
		FB.Event.subscribe('comment.create', function(response) {
			$.post('http://www.ehow.com/services/comment/refresh/', { xid:'aG93XzQ0OTEwNDBfYmFrZS1tZWx0ZWQtaWNlLWNyZWFtLW11ZmZpbnMuaHRtbA'});
					});
	});
	
	$("#Comments").onVisible({ callback: function() {
		$(".comment img").lazyImage();
	}});


</script>
