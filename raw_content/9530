    "      <div class=\"last-slide\">\n      \n        \n      \n        <h2>Related Slideshows<\/h2>\n        \n                              <div>\n              <a href=\"\/diy-adorable-headbands\"><img src=\"http:\/\/static2.refinery29.com\/bin\/entry\/fc0\/280x335\/143752\/thumb.jpg\" alt=\"thumb\" width=\"280\" height=\"335\"  \/><\/a>\n              <h2 class=\"italic\"><a href=\"\/diy-adorable-headbands\">Crowning Glory! Two Headband Projects To Top Off Any Look<\/a><\/h2>\n            <\/div>\n            \n          \n        \n              \n        \n                          <div>\n              <a href=\"\/best-eye-creams-for-your-twenties\"><img src=\"http:\/\/static1.refinery29.com\/bin\/entry\/900\/280x335\/107954\/skinceuticals-thumb.jpg\" alt=\"skinceuticals-thumb\" width=\"280\" height=\"335\"  \/><\/a>\n              <h2 class=\"italic\"><a href=\"\/best-eye-creams-for-your-twenties\">5 Nourishing Eye Creams For 20-Somethings<\/a><\/h2>\n            <\/div>\n            \n          \n        \n              \n        \n        \n      <\/div>\n    "
  ];  
  
      
    var default_idx = parseInt(R29.url.hash.getVar("slide")) || 2;
    
    /** Load up the R29 slideshow **/
    var slideshow = new R29.slideshow( $('.slides'), slides, default_idx, 
      function(page, is_init) {
        
        if (this.page == 1) {
          $("#slideshow-previous").fadeOut();
        } else {
          $("#slideshow-previous").fadeIn();
          
        }
        
        if (this.page == this.length) {
          $("#slideshow-next").fadeOut();
        } else {
          $("#slideshow-next").fadeIn();
        }
        
        //Set the page number at the top and the hash
        $('#slideshow-page').text(page);
        document.title = 'Photo ' + page + '- ' + "You Totally Got This: 4 Easy Braids For Rookies";
        // $('meta[name="description"]').attr('content', 'View Photo ' + page + '- ' + "You Totally Got This: 4 Easy Braids For Rookies");
        R29.url.hash.setVar("slide", page);
  
        //Refreshing
        if (!is_init) {
          R29.ads.refresh('.ad');
          R29.tracking.refresh();
        }      
      }
    );
    
    /** Load up this shoddy jcarousel thing **/
    
    $('#slideshow-nav-carousel').jcarousel({
      scroll: 1,
      start: default_idx,
      wrap: null,
      itemFallbackDimension: 80, //Default width of 80px
      initCallback: 
        function (carousel, state) {
        
          if (state != 'init') {
           return;
          }
          // set fullscreen hash to the proper slide id
          var permium_href = $('.fullscreen_inslide').attr('href').split('#')[0] + "#slide-" + default_idx;
          $('.fullscreen_inslide').attr('href', permium_href);
          
          $('.slide-select').bind('click', function() {
            var carousel     = jQuery('#slideshow-carousel').data('jcarousel');
            //carousel.scroll(jQuery.jcarousel.intval($(this).attr('image_idx')));
            
            slideshow.goto($(this).attr('image_idx'));
            
            $('.slideshow-nav-carousel li').removeClass('selected');
            $(this).parent().addClass('selected');
            return false;
          });
        
          $('#slideshow-next').bind('click', function() {
          
            var nav_carousel = jQuery('#slideshow-nav-carousel').data('jcarousel');
            var buffer    = 3;
      
            slideshow.next();
            var new_index = slideshow.page;
            
            var permium_href = $('.fullscreen_inslide').attr('href').split('#')[0] + "#slide-" + slideshow.page;
            
            $('.fullscreen_inslide').attr('href', permium_href);
            
            
            if (new_index <= (buffer + 1)) {
              new_index = 1;
            } else if ((new_index + buffer) < slideshow.length) {
              new_index -= buffer;
            }       
            
            nav_carousel.scroll(new_index);
            $('.slideshow-nav-carousel li').removeClass('selected');
            $('.slideshow-nav-carousel a[image_idx='+ slideshow.page +']').parent().addClass('selected');
            
            return false;
          });
          
          $('#slideshow-previous').bind('click', function() {
           
            var nav_carousel = jQuery('#slideshow-nav-carousel').data('jcarousel');
            
            slideshow.prev();
             
            var new_index = slideshow.page;
            var buffer    = 3;
            
            var permium_href = $('.fullscreen_inslide').attr('href').split('#')[0] + "#slide-" + slideshow.page;
            
            $('.fullscreen_inslide').attr('href', permium_href);
            
            if (new_index <= (buffer + 1)) {
              new_index = 1;
            } else if (new_index + buffer < slideshow.length) {
              new_index -= buffer;
            }   
            
            nav_carousel.scroll(new_index);
            $('.slideshow-nav-carousel li').removeClass('selected');
            $('.slideshow-nav-carousel a[image_idx='+ slideshow.page +']').parent().addClass('selected');
            
            return false;
          });
          
          
          $('#slideshow-nav-next', carousel.container).bind('click', function() {  
            var carousel     = jQuery('#slideshow-nav-carousel').data('jcarousel');    
            carousel.next();
            return false;
          });
          
          $('#slideshow-nav-previous', carousel.container).bind('click', function() {
            var carousel     = jQuery('#slideshow-nav-carousel').data('jcarousel');
                               
            carousel.prev();
            return false;
          });
        
        },
      buttonNextHTML: null,buttonPrevHTML: null
    });
    
  
  
  $('.slideshow .fullscreen').click(function () {
  	_gaq.push(['_trackEvent', 'Fullscreen Slideshow Click', 'before starting slideshow', '25192',,true]);
  });

  $('.slideshow .fullscreen_inslide').click(function () {
  	_gaq.push(['_trackEvent', 'Fullscreen Slideshow Click', 'after starting slideshow', '25192',,true]);
  });
  
  $('.entry .h1').click(function () { document.location = document.location.href.split('/slideshow')[0]; }).css('cursor', 'pointer');
  
</script>
	  
	  
