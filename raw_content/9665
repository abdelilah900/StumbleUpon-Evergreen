<script>
    var version=navigator.appVersion;
    var Vcheck=version.lastIndexOf("WebKit");
    var docHeight=$(document).height();
    var docHeightAdj=docHeight+140;
    var docHeightAdjLong=docHeightAdj*3.3;
    var backgroundheight=docHeight-122;
    var urlinQues=document.URL;
    var urlCheck=urlinQues.lastIndexOf("vosges_chocolate_in_the_news");
    
    if( Vcheck == -1)$("#backgroundImg").css("height", backgroundheight);
    else if(docHeightAdj > 3700)$("#backgroundImg").css("height", docHeightAdjLong); 
    else if(docHeightAdj >2000 && urlCheck > 0)$("#backgroundImg").css("height", docHeightAdjLong);
    else $("#backgroundImg").css("height", docHeightAdj); 
</script>




