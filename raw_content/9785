								  var js, fjs = d.getElementsByTagName(s)[0];
								  if (d.getElementById(id)) return;
								  js = d.createElement(s); js.id = id;
								  js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=236867953043249";
								  fjs.parentNode.insertBefore(js, fjs);
								}(document, 'script', 'facebook-jssdk'));
								
								/* doLinks script */
								function linkify(inputText, options) {
								
								inputText = inputText.replace(/\u200B/g, "");
								
								//URLs starting with http://, https://, or ftp://
								var replacePattern1 = /(href="|">|\s>)?(https?|ftp):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;i]*[-A-Z0-9+&@#\/%=~_|i]/gim;
								var replacedText = inputText.replace(replacePattern1, function($0,$1){ return $1?$0:'<a href="' + $0 + '" target="_blank">'+ $0+ '</a>';});
								
								return replacedText;
								}
								
								$.fn.doLinks = function(){
									this.each(function(){
										$(this).html(linkify($(this).html()));
									});
								}
								
								$(function(){$('#fb-activity ul').find('li').doLinks();});
								
								$('#fb-activity ul').find('em:empty').text('0');
								</script>
								
