<script type="text/javascript">
document.observe('dom:loaded', function() {
    $$('.hpArrow').each(function(arrow) {
        if (Utils.isWindows()) {
            arrow.setStyle({
                '-webkit-backface-visibility': 'hidden'
            });
        }
        
        arrow.observe('mouseover', function() {
            arrow.down('span,img').rotate(parseInt(arrow.readAttribute('data-rotate')));
        });
        arrow.observe('mouseout', function() {
            arrow.down('span,img').rotate(0);
        });
    });
})
</script>
