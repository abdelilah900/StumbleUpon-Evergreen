<script type="text/javascript">
    document.observe('dom:loaded', function() {
        var _height = 0;
        $$('.adminMessage').each(function(am) {
            _height += am.getHeight();
        });
        if (_height) {
            var _topBar = $$('.topBar').first();
            var _header = $$('.header').first();
            var _contentInner = $$('.contentInner').first();
            
            var _snapper = function() {
                _topBar.setStyle({'position': 'absolute', 'top': _height + 'px'});
                _header.setStyle({'position': 'absolute', 'top': ((_contentInner ? 0 : _height) + _topBar.getHeight()) + 'px'});
            }
            _snapper();
            document.observe('scroll', function() {
                if (document.body.scrollTop < _height) {
                    _snapper();
                }
                else {
                    _topBar.setStyle({'position': 'fixed', 'top': '0px'});
                    _header.setStyle({'position': 'fixed', 'top': _topBar.getHeight() + 'px'});
                }
            });
        }
    });
</script>    
