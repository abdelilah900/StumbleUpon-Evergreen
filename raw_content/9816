<script type="text/javascript">
var jsReady = false;
var pageObj = {};
var imageObj = {};


var featureList = [];

var flashVars = "v=1&pathname=" + document.location.pathname + "&" + document.location.search.substring(1) + "";
var flashFilePath = "http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/Global/ss_data_5.swf" + "?" + flashVars;

var flashvars = false;
var params = {
    menu: "false",
    play: "true",
    loop: "true",
    allowScriptAccess: "always",
    flashvars: flashVars
};
var attributes = {
    id: "ssDataProxy"
};

swfobject.embedSWF(flashFilePath, "ssData", "1", "1", "9.0.0", "http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/Global/expressInstall.swf", flashvars, params, attributes);

function pageInit() {
    jsReady = true;
    jQuery("#devOutput").append("JavaScript is ready.");

    jQuery("div#slideshowHolder").hide();    
    jQuery(".partnerCompWrapper").remove();
    jQuery("div#divButtons").remove();
    jQuery("div#divButtonsBottom").remove();
}

function isReady() {
    // flash will make this call before it tries to call any of the other external interface calls
    return jsReady;
}

function ssImageData(pageTitle, slide, total, FileName, Width, Height, NoResize, MediaRoot2, Credits, Caption, Alt) {

    pageObj.title = pageTitle;
    pageObj.slide = slide;
    pageObj.total = total;
    pageObj.DisplayTotal = "" + (parseInt(total) + 1) + "";
    pageObj.resolutionMode = false;
    pageObj.nextURL = '?slide=1';
    // this will be filled by the resolution slide
    // these are ints in case we need them
    pageObj.s = parseInt(slide);
    pageObj.t = parseInt(total);
    pageObj.dt = parseInt(total) + 1;

    imageObj.FileName = FileName;
    imageObj.Width = Width;
    imageObj.Height = Height;
    imageObj.NoResize = NoResize;
    imageObj.MediaRoot2 = MediaRoot2;
    imageObj.Credits = Credits;
    imageObj.Caption = Caption;
    imageObj.Alt = Alt;

    // set resolution mode state
    if (pageObj.s == pageObj.t + 1) {
        pageObj.resolutionMode = true;
    }

    ssSwapContent();

    return true;
}


function ssFilmstripData(href, linktype, FileName, Width, Height, NoResize, MediaRoot2, AlternateText, Headline, Description, AssetLinks) {

    f = {};

    f.href = href;
    f.linktype = linktype;

    f.FileName = FileName;
    f.Width = Width;
    f.Height = Height;
    f.NoResize = NoResize;
    f.MediaRoot2 = MediaRoot2;
    f.AlternateText = AlternateText;

    f.Headline = Headline;
    f.Description = Description;
    f.AssetLinks = AssetLinks;

    var imgHref = '';
    if (f.linktype.toLowerCase() == 'internal') {
        imgHref = '/id/' + f.href + '';
    } else {
        imgHref = f.href;
    }
    var imgProps = ' width="' + f.Width + '" height="' + f.Height + '" border="0" hspace="0" vspace="0"';

    var h = '<div class="filmstripBox">';
    h += '<div class="cFont clr FL_BI">';
    h += ' <ul class="ll_bullet FL_ART">';
    h += '  <li class="nobullet cFont cf11 clr FL_ART_LI">';
    h += '   <a href="' + imgHref + '">';
    h += '<img src="' + f.MediaRoot2 + '/i' + f.FileName + '" alt="' + f.AlternateText + '" ' + imgProps + ' />';
    h += '</a><br>';
    h += '  </li>';
    h += ' </ul>';
    h += ' <ul class="ll_bullet FL_TITLE">';
    h += '  <li class="nobullet cFont cf11 clr FL_TAG_LI">';

 var newHeadline = f.Headline;
    if (jQuery(f.Headline).find("a:first").attr("linktype").toLowerCase() == 'internal') {
  var fsId = jQuery(f.Headline).find("a:first").attr("href");
  if(fsId.split("/").length > 1){
   var pathArray = fsId.split("/");
   for (i = 0; i < pathArray.length; i++) {
    if (pathArray[i] == "id") {
     fsId = jQuery.trim(pathArray[i+1]);
     break;
    }
   }
  }
  var link = jQuery(f.Headline).find("a:first").attr("href","/id/"+fsId+"");
  newHeadline = jQuery(f.Headline).find("a:first").html(link).html();
    }

    h += '  <span class="nobullet cFont cf11 clr cstrong FL_TITLE_LI">' + newHeadline + '</span>';
    h += '  </li>';
    h += ' </ul>';
    h += '<ul class="ll_bullet FL_DESP"><li class="nobullet cFont cf11 clr">';
    h += f.Description;
    h += '</li></ul>';
    h += '</div>';
    h += '</div>';

    jQuery("#ssFilmstripBody").append(h);

    featureList.push(f);
    return true;
}

function ssResolution(nextURL) {
    pageObj.nextURL = nextURL;
    return true;
}

function ssSwapContent() {

    // we always want the title
    jQuery("#ssPageTitle").html(pageObj.title);

    if (pageObj.resolutionMode) {
        // BUILD OUT THE RESOLUTION STATE
        var headline = jQuery("#ssPageTitle").text();
        jQuery("#ssPageTitle").empty();
  jQuery("#ssPhotoImgHolder").remove();
  jQuery("#ssPhotoAlt").remove();
        var replayImage = 'http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/Global/ssReplaySlideshow.gif';
        jQuery("<img/>").attr("id", "ssReplaySlideshow").attr("src", replayImage).appendTo("#ssPageTitle");
        jQuery("#ssPageTitle").append(headline).css({"font-size": "12px","font-weight":"bold"});
        jQuery("#ssPhotoNumber").html(pageObj.slide + '&nbsp;of&nbsp;');
        jQuery("#ssPhotoCount").html(pageObj.DisplayTotal);
        var cssObj = {
            'border-top': '0px solid #fff',
            'margin-top': '0px'
        }
        jQuery("#CNBCFilmstrip2").css(cssObj);

  var deckContentText = jQuery('.cnbc_sbhdln:first').text();
  var deckContent = '';
  if (deckContentText.length >= 1){
   deckContent = jQuery('.cnbc_sbhdln:first').html();
  } else {
   deckContent = '<ul><li><a href="?slide=1">Replay Slideshow</a></li></ul>';
  }

        jQuery('#ssPhotoCaption').addClass("ssDeckLinks")
        .append('<div id="ssMoreRelated">More related to:</div>')
        .append('<div id="ssMoreRelatedTitle">' + headline + '</div>')
        .append(deckContent);
        ssShowTextCheck();

    } else {

        // CREATE THE IMAGE
  var imageAlign = "left";
        // jQuery("#ssPhotoImg").css("width", imageDetails.w + "px");
        if (imageObj.Width < imageObj.Height && imageObj.Width < 640) {
            imageAlign = "center";
        }        

        if (imageObj.NoResize == "true") {
            jQuery("<img/>")
            .attr("id", "ssImage")
            .attr("src", imageObj.MediaRoot2 + '/i' + imageObj.FileName)
            .attr("width", imageObj.Width)
            .attr("height", imageObj.Height)
   .attr("align",imageAlign)
   .attr("border","0")
            .appendTo("#ssPhotoImg");
        } else {
            var fileNameExtPos = imageObj.FileName.lastIndexOf('.');
            var fileNameExt = imageObj.FileName.substring(fileNameExtPos);
            var fileNameBase = imageObj.FileName.substring(0, fileNameExtPos);
            var fileName = fileNameBase + '.aim640' + fileNameExt;
            jQuery("<img/>")
            .attr("id", "ssImage")
            .attr("src", imageObj.MediaRoot2 + '/j' + fileName)
            .attr("width", imageObj.Width)
            .attr("height", imageObj.Height)
   .attr("align",imageAlign)
   .attr("border","0")
            .appendTo("#ssPhotoImg");
        }
        jQuery("#ssPhotoNumber").html(pageObj.slide + '&nbsp;of&nbsp;');
        jQuery("#ssPhotoCount").html(pageObj.DisplayTotal);

        if (imageObj.Credits != '') {
            jQuery("#ssPhotoCredit").html(imageObj.Credits);
        }
        if (imageObj.Caption != '') {
            jQuery("#ssPhotoCaption").html(imageObj.Caption);
        }
        if (imageObj.Alt != '') {
            jQuery("#ssPhotoAlt").html(imageObj.Alt);
        }


    }
    // ADD THE MAIN NEXT AND BACK BUTTONS
    // THIS IS JUST THE IMAGES. WE WILL APPLY THE EVENTS LATER
    if (pageObj.slide == 1) {
        // first slide
        // blank
        jQuery("<img/>").attr("src", "http://media.cnbc.com/i/CNBC/CNBC_Images/blank.gif")
        .attr("width", "44")
        .attr("height", "44")
        .attr("border", "0")
        .appendTo("#ssPhotoLinks");
        // next
        jQuery("<img/>").attr("src", "http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/ss_btnOffNext.gif")
        .attr("width", "44")
        .attr("height", "44")
        .attr("border", "0")
        .attr("id", "ssNextBtn")
        .appendTo("#ssPhotoLinks");

    } else {
        // all the rest
        // BACK
        jQuery("<img/>").attr("src", "http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/ss_btnOffBack.gif")
        .attr("width", "44")
        .attr("height", "44")
        .attr("border", "0")
        .attr("id", "ssBackBtn").appendTo("#ssPhotoLinks");
        // NEXT
        jQuery("<img/>").attr("src", "http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/ss_btnOffNext.gif")
        .attr("width", "44")
        .attr("height", "44")
        .attr("border", "0")
        .attr("id", "ssNextBtn").appendTo("#ssPhotoLinks");
    }

    jQuery('#cnbc_textbody').replaceWith(jQuery('#ssWide').html());
    jQuery('#ss_right_rail').replaceWith(jQuery('#ssThin').html());

    ssShowTextCheck();
}
// NEXT AND BACK
function ssNext() {
    if (pageObj.resolutionMode) {
    if (featureList.length >=1){
 
     var path = document.location.pathname;
     var pathArray = path.split("/");
     var wbId = '';
     for (i = 0; i < pathArray.length; i++) {
      if (pathArray[i] == "id") {
       wbId = jQuery.trim(pathArray[i+1]);
       break;
      }
     }
 
     for (j = 0; j < featureList.length; j++) {
      if(jQuery.trim(featureList[j].href) == wbId){
       if(j == featureList.length -1){
        pageObj.nextURL = '/id/' + featureList[0].href + '';
       } else {
        pageObj.nextURL = '/id/' + featureList[j+1].href + '';      
       }
       break;
      }
     }
 
    }
        document.location = pageObj.nextURL;
    } else {
        document.location = '?slide=' + (pageObj.s + 1);
    }
}
function ssBack() {
    if (pageObj.s > 1) {
        document.location = '?slide=' + (pageObj.s - 1);
    }
}

// SLIDE CAPTION
function ssShowTextCheck() {
    if (jQuery("#ssPhotoCaptionScroll").height() < jQuery("#ssPhotoCaption").height()) {
        jQuery("<img/>").attr("id", "ssShowTextBtn").attr("src", 'http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/Global/ssReadMoreShow_1.gif')
     .attr("width", "75")
     .attr("height", "16")
     .attr("border", "0")
     .appendTo("#ssReadMore");
    }
}


// JQUERY DOC READY
jQuery(document).ready(function() {

    // output *************************************************************
    jQuery('#cnbc_textbody').empty();

    // bindings
    jQuery(document).bind('NEXT_OVER',
    function(e) {
        jQuery("#ssNextBtn").attr("src", "http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/ss_btnOnNext.gif");
  window.status = 'Next Slide';
    });
    jQuery(document).bind('NEXT_OUT',
    function(e) {
        jQuery("#ssNextBtn").attr("src", "http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/ss_btnOffNext.gif");
  window.status = '';
    });
    jQuery(document).bind('NEXT_CLICK',
    function(e) {
        ssNext();
    });
    jQuery(document).bind('BACK_OVER',
    function(e) {
        jQuery("#ssBackBtn").attr("src", "http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/ss_btnOnBack.gif");
  window.status = 'Previous Slide';
    });
    jQuery(document).bind('BACK_OUT',
    function(e) {
        jQuery("#ssBackBtn").attr("src", "http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/ss_btnOffBack.gif");
  window.status = '';
    });
    jQuery(document).bind('BACK_CLICK',
    function(e) {
        ssBack();
    });
    // SHOW HIDE TEXT
    jQuery(document).bind('SHOW_TEXT',
    function(e) {

        var captionHeight = jQuery("#ssPhotoCaption").height();
        if (jQuery("#ssPhotoCaptionScroll").height() >= captionHeight) {
            // we need to bring it down
            jQuery("#ssPhotoCaptionScroll").animate({
                height: 142
            },
            500);
            jQuery("#ssShowTextBtn").attr("src", 'http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/Global/ssReadMoreShow_1.gif');

        } else if (jQuery("#ssPhotoCaptionScroll").height() < captionHeight) {
            jQuery("#ssPhotoCaptionScroll").animate({
                height: captionHeight
            },
            500);
            jQuery("#ssShowTextBtn").attr("src", 'http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/Global/ssReadMoreHide_1.gif');
        }
    });

    // UI events to triggers
    // this is the onload of the image
    // NEXT button
    jQuery("#ssNextBtn").livequery("click",
    function(e) {
        jQuery(document).trigger('NEXT_CLICK');
        e.preventDefault();
    });
    jQuery("#ssNextBtn").livequery("mouseover",
    function(e) {
        jQuery(document).trigger('NEXT_OVER');
    });
    jQuery("#ssNextBtn").livequery("mouseout",
    function(e) {
        jQuery(document).trigger('NEXT_OUT');
    });
    // BACK button
    jQuery("#ssBackBtn").livequery("click",
    function(e) {
        jQuery(document).trigger('BACK_CLICK');
        e.preventDefault();
    });
    jQuery("#ssBackBtn").livequery("mouseover",
    function(e) {
        jQuery(document).trigger('BACK_OVER');
    });
    jQuery("#ssBackBtn").livequery("mouseout",
    function(e) {
        jQuery(document).trigger('BACK_OUT');
    });
    // blur, focus, load, resize, scroll, unload, beforeunload, click, dblclick,  mousedown, mouseup, mousemove, mouseover, mouseout, mouseenter, mouseleave, change, select,  submit, keydown, keypress, keyup, error
    // this is the onload of the image
    jQuery("#ssImage").livequery("load",
    function(e) {

        // jQuery(this).hide();
        // jQuery(this).fadeIn('slow');
    });

    jQuery("#ssImage").livequery("click",
    function(e) {
        jQuery(document).trigger('NEXT_CLICK');
        e.preventDefault();
    });
    jQuery("#ssImage").livequery("mousemove",
    function(e) {
        jQuery(document).trigger('NEXT_OVER');
        e.preventDefault();
    });
    jQuery("#ssImage").livequery("mouseleave",
    function(e) {
        jQuery(document).trigger('NEXT_OUT');
    });

    // KEYBOARD ACTION
    jQuery(document).bind('keyup', 'right',
    function(e) {
        jQuery(document).trigger('NEXT_CLICK');
        e.preventDefault();
    });
    jQuery(document).bind('keydown', 'right',function (e){
     jQuery(document).trigger('NEXT_OVER');
     e.preventDefault();
    });
    jQuery(document).bind('keyup', 'left',
    function(e) {
        jQuery(document).trigger('BACK_CLICK');
        e.preventDefault();
    });
    jQuery(document).bind('keydown', 'left',function (e){
  jQuery(document).trigger('BACK_OVER');
  e.preventDefault();
    });
    jQuery(document).bind('keydown', 'down',
    function(e) {
        jQuery(document).trigger('SHOW_TEXT');
        e.preventDefault();
    });
    jQuery(document).bind('keydown', 'up',
    function(e) {
        jQuery(document).trigger('SHOW_TEXT');
        e.preventDefault();
    });


    // HIDE AND EXPAND THE TEXT
    jQuery("#ssShowTextBtn").livequery("click",
    function(e) {
        jQuery(document).trigger('SHOW_TEXT');
        e.preventDefault();
    });
    jQuery("#ssShowTextBtn").livequery("mouseover",
    function(e) {
  window.status = "Show/Hide Text";
    });
    jQuery("#ssShowTextBtn").livequery("mouseout",
    function(e) {
  window.status = "";
    });
 // REPLAY SLIDESHOW
    jQuery("#ssReplaySlideshow").livequery("click",
    function(e) {
        // jQuery(document).trigger('NEXT_CLICK');
  document.location = "?slide=1";
    });
    jQuery("#ssReplaySlideshow").livequery("mouseover",
    function(e) {
        jQuery(e.target).attr('src','http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/Global/ssReplaySlideshowOver.gif');
    });
    jQuery("#ssReplaySlideshow").livequery("mouseout",
    function(e) {
        jQuery(e.target).attr('src','http://media.cnbc.com/i/CNBC/Components/Slideshow_V2/Global/ssReplaySlideshow.gif');
    });

    pageInit();

    gigya.services.socialize.showShareBarUI(conf,{   
        userAction:ua,  
        shareButtons: [{provider:'facebook-like',action:'recommend'},{provider:'twitter'},{provider:'google-plusone'},{provider:'linkedin'},{provider:'share',enableCount:false}], 
        containerID: 'divSSButtons'
    });  

});
</script>

