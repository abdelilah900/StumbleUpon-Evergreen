			<script src="http://tredir.go.com/capmon/GetDE?set=j&param=metro" type="text/javascript"></script>
			<script type="text/javascript">
				(function($){
					
					var stubhub = {
						metro: metro,
						buildMod: function(){
							var sport = 'mlb';
							$.ajax({
								url: 'http://espn.go.com/travel/stubhub/api/events/' + sport + '/index', 
								data: {location: stubhub.metro},
								dataType: 'json',
								success: function(response){
									$eventList = $('#event-list');
									$eventList.append('<li class="first"><h5>' + stubhub.formatMetro(stubhub.metro) + ' MLB Events</h5></li>');
									if(typeof(response) === 'object'){
										if(response.success && typeof(response.data) === 'object'){
											var events = response.data.events;
											if(typeof(events) === 'object' && events.length > 0){
												var i, event, eventDate, eventHtml;
												var dowList = ['Sun','Mon','Tue','Wed','Thu','Fri','Sat'];
												for(i in events){
													if(i < 3){
														event = events[i];
														eventDate = new Date(event.date);
														
														// build event HTML
														eventHtml = '<li class="first">';
														eventHtml += '<div class="ticket-date-cont">';
														eventHtml += '<span class="ticket-date">' + dowList[eventDate.getDay()] + ', ' + (eventDate.getMonth() + 1) + '/' + eventDate.getDate() + '</span><br/>' + eventDate.toLocaleTimeString().replace(':00 ',' ');
														eventHtml += '</div>';
														eventHtml += '<dl style="margin: 0;">';
														eventHtml += '<dt><a href="' + event.stubhubBuyLink + '" onclick="this.href=this.href+\'?gcid=C12289x447&keyword=StubHub\'">' + event.shortDescription + '</a></dt>';
														eventHtml += '<dd><b><a href="' + event.stubhubBuyLink + '" onclick="this.href=this.href+\'?gcid=C12289x447&keyword=StubHub\'">Buy</a></b> | <b><a href="' + event.stubhubSellLink + '">Sell</a></b></dd>';
														eventHtml += '</dl></li>';
														
														$eventList.append(eventHtml);
													}
												}									
												$eventList.show();
												$('#fallback-form').hide();
											}
										}
									}
								}
							});
						},
						formatMetro: function(metro){
							var formattedMetro = '';
							var cities = metro.split('/');
							var city, cityParts, cityPart;
							var i, k;
							for(i in cities){
								city = cities[i];
								cityParts = city.split(' ');
								if(i > 0){
									formattedMetro += '/';
								}
								for(k in cityParts){
									// capitalize the first letter
									cityPart = cityParts[k];							
									if(k > 0){
										formattedMetro += ' ';
									}
									formattedMetro += cityPart.substring(0,1).toUpperCase() + cityPart.substring(1).toLowerCase();
								}
							}
							
							return formattedMetro;				
						}
					};
					
					$(document).ready(function(){
						stubhub.buildMod();
					});					
				})(jQuery);

			</script>
