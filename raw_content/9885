    <script type="text/javascript">

        jQuery(document).ready(function() { 
            
            postnum = 0;
            
            jQuery('.post').each(function(index) {
                perma = 'empty';
                perma = jQuery(this).find('.plink').attr('href');
                postnum = postnum + 1; 
                jQuery(this).addClass('' + perma + ''); 
                // alert('Post ' + postnum + ': ' + perma);
                
                if (jQuery(this).find('h3').length > 0) {
                    h3value = jQuery(this).find('h3').html();
                    jQuery(this).find('h3').html('<a href="'+perma+'">' + h3value + '</a>');
                }                 
                
                
            });   
            
            setTimeout('removeComment()', 1500);

            jQuery('.post_info_bottom').prepend('From: ');
            jQuery('.post_info_bottom a:last-child').show();

        
            jQuery(".photoset").each(function() { 
                var newSrc = jQuery(this).attr("src").replace('500','660');
                jQuery(this).attr("src", newSrc);
                ifrh = jQuery(this).attr('height');
                newh = (ifrh/500)*660;
                jQuery(this).css("height", newh);
            });   


        });
        
        function removeComment() {
            jQuery('li.comments .dsq-comment-count').each(function() {
                initval = jQuery(this).html();
                newval = initval.replace(" Comments", "");
                finalval = newval.replace(" Comment", "");
                jQuery(this).html(finalval);
            });
            
        }
        

    </script>
    
    
<script type="text/javascript">
                function loadPageVar(sVar) {
                                return unescape(window.location.search.replace(new RegExp("^(?:.*[&\\?]" + escape(sVar).replace(/[\.\+\*]/g, "\\$&") + "(?:\\=([^&]*))?)?.*$", "i"), "$1"));
                }
                                
                jQuery(document).ready(function() {
                                /* FIGURE OUT WHAT PAGE YOU ARE ON AND MAKE THAT ACCESSIBLE */
                                var blogURL = "http://" + document.domain +   "/";
                                var currentURL = window.location.protocol + "//" + window.location.host + window.location.pathname + window.location.search;
                                
                                if (currentURL.indexOf("www.tumblr.com/edit/") > 0) {
                                                var postID = window.location.pathname.replace("/edit/", "");
                                                currentURL = jQuery("#post-"+postID+"-url").attr("href");
                                                blogURL = "http://" + loadPageVar("redirect_to").split("/")[2] + ".tumblr.com/";
                                }
                                var currentPage = currentURL.replace(blogURL, "");
                                
                                /* ADD CSS CLASSES TO BODY DEPENDING OF CURRENT PAGE */
                                if (currentURL != blogURL) {
                                                var classnames = currentPage.split("/");
                                                classnames.push(currentPage.replace(/\//gi, "-"));
                                                for (var i = 0; i < classnames.length; i++) {
                                                                jQuery("body").addClass(classnames[i]);
                                                }
                                } else {
                                                jQuery("body").addClass("home");
                                }
                                
                                /* LOAD TAGGED POSTS AND APPEND THEM TO SOME DIV */
                                var container = "#spotlight-post";                                                                                                         //where to insert the posts
                                var tag = "spotlight";                                                                                                                                         //which tags
                                var time = Math.round(new Date().getTime() / 1000 / 300);                         //min cache time (optional)
                                
                                
                                var parserURL = "http://ajax.googleapis.com/ajax/services/feed/load?v=1.0&num=10&q=";
                                var url = parserURL + encodeURIComponent(blogURL+"tagged/"+tag+"/rss?t="+time)+"&callback=?";
                                jQuery.getJSON(url, function(data) {
                                                var items = [];
                                                var entries = data.responseData.feed.entries;
                                                for (var i = 0; i < entries.length; i++) {
                                                                var categories = [];
                                                                var item = '';
                                                                var title = entries[i].title;
                                                                var content = entries[i].content;
                                                                
                                                                                
item += '<a href="'+entries[i].link+'">';

item += content;
                                                                                
item += '</a>';



for (var j = 0; j < entries[i].categories.length; j++) {
                                                                                                var category = entries[i].categories[j];
                                                                                                categories.push('<a href="'+blogURL+'tagged/'+category+'">'+category+'</a>');
                                                                                }
                                                                                                                                                
                                                                
                                                                items.push(item);
                                                }
                                                jQuery('<div/>', {
                                                                html: items.join('')
                                                }).appendTo(container);
                                });

  
    getImage();
    
  
    });
    
function getImage() {
    spotimageheight = setTimeout('spotImage()', 1000);
}



function spotImage() {
    jQuery('#spotlight-post img').attr('id','spotlight-image');
    var spimg = document.getElementById('spotlight-image'); 
    var spimgh = spimg.clientHeight;
    var spotheight = spimgh + 94;
    jQuery('#sidebar #spotlight-post').height(spotheight);
    jQuery('#spotlight-post').addClass('spotlight-load');
    jQuery('#spotlight-post').delay(100).animate({
    opacity: 1
  }, 500, function() { 
    // Animation complete.
  });;    
}


    
</script>

    
