    window.onload = function() {
        var facebookShare=document.createElement('script');
        facebookShare.setAttribute("type","text/javascript");
        facebookShare.setAttribute("src", "http://static.ak.fbcdn.net/connect.php/js/FB.Share");
        jQuery('#fbShare').append(facebookShare);
        
        var twitter=document.createElement('script');
        twitter.setAttribute("type","text/javascript");
        twitter.setAttribute("src", "http://platform.twitter.com/widgets.js");
        jQuery('#tweet').append(twitter);    
    }
    
    var tflag = 1;
    function toggleDescription() {   	
    	if (tflag == 0) {  // show first 168 characters
            jQuery('#longDescriptionString').hide();
            jQuery('#shortDescriptionString').show();
            jQuery('#mediaDescription').css({'height':'60px'});      	
        	tflag = 1;
    	} else {
            jQuery('#mediaDescription').css({'height':'auto'});
            jQuery('#shortDescriptionString').hide();
            jQuery('#longDescriptionString').show();
        	tflag = 0;
    	}
    }
    var tagflag = 1;
    function toggleTags() {   	
    	if (tagflag == 0) {  // show first 80 characters
    		jQuery('#longTagString').hide();
    		jQuery('#shortTagString').show();
   		    jQuery('#mediaTags').css({'height':'32px'});
        	tagflag = 1;
    	} else {
            jQuery('#mediaTags').css({'height':'auto'});
        	jQuery('#shortTagString').hide();
        	jQuery('#longTagString').show();
        	tagflag = 0;
    	}
    }
